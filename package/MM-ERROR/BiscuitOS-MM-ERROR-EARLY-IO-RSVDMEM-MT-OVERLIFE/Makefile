PK_MM_ERR_EARLYIOMEM_OVERLIFE_NAME        := BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-OVERLIFE
PK_MM_ERR_EARLYIOMEM_OVERLIFE_VERSION     := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_VERSION)
PK_MM_ERR_EARLYIOMEM_OVERLIFE_SITE        := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_URL)
PK_MM_ERR_EARLYIOMEM_OVERLIFE_GITHUB      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_GITHUB)
PK_MM_ERR_EARLYIOMEM_OVERLIFE_SUBNAME     :=
PK_MM_ERR_EARLYIOMEM_OVERLIFE_BUILDARCH   := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_BUILDARCH)
PK_MM_ERR_EARLYIOMEM_OVERLIFE_DIR         := $(srctree)/package/MM-ERROR/BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-OVERLIFE
PK_MM_ERR_EARLYIOMEM_OVERLIFE_PATCH       := $(PK_MM_ERR_EARLYIOMEM_OVERLIFE_DIR)/patch
PK_MM_ERR_EARLYIOMEM_OVERLIFE_BSBIT       := BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-OVERLIFE-$(PK_MM_ERR_EARLYIOMEM_OVERLIFE_VERSION).bsbit
PK_MM_ERR_EARLYIOMEM_OVERLIFE_OUT         :=
PK_MM_ERR_EARLYIOMEM_OVERLIFE_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_MM_ERR_EARLYIOMEM_OVERLIFE_TAR         := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_TAR)
PK_MM_ERR_EARLYIOMEM_OVERLIFE_PACKAGE     :=
PK_MM_ERR_EARLYIOMEM_OVERLIFE_CONFIG      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_CONFIG)
SUB_TARGET                  += $(PK_MM_ERR_EARLYIOMEM_OVERLIFE_DIR)/build

PK_MM_ERR_EARLYIOMEM_OVERLIFE_sh = $(CONFIG_SHELL) $(PK_MM_ERR_EARLYIOMEM_OVERLIFE_SCRIPTS) \
                 $1X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERLIFE_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERLIFE_VERSION)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERLIFE_SITE)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERLIFE_GITHUB)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERLIFE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERLIFE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERLIFE_SUBNAME)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_CONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_LIBPATH)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_CPPFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_DPKCONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_CFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_LDFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_CXXFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_ASFLAGS)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERLIFE_BSBIT)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERLIFE_BUILDARCH)X \
		 $(PK_MM_ERR_EARLYIOMEM_OVERLIFE_CSRCFILE)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_SRCFILE)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERLIFE_CSRCFILE)

$(PK_MM_ERR_EARLYIOMEM_OVERLIFE_DIR)/build: FORCE
	$(Q)$(call PK_MM_ERR_EARLYIOMEM_OVERLIFE_sh,$(srctree))
