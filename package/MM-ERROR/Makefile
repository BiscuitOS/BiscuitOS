MM-ERROR-$(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS)		+= BiscuitOS-MM-ERROR-Confuse-GFP-Flags/
MM-ERROR-$(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP)	+= BiscuitOS-MM-ERROR-Confuse-GFP-Flags-PCP/
MM-ERROR-$(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY)	+= BiscuitOS-MM-ERROR-Confuse-GFP-Flags-Buddy/
MM-ERROR-$(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB)	+= BiscuitOS-MM-ERROR-Confuse-GFP-Flags-SLAB/
MM-ERROR-$(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC)	+= BiscuitOS-MM-ERROR-Confuse-GFP-Flags-VMALLOC/
MM-ERROR-$(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW)		+= BiscuitOS-MM-ERROR-Permanent-Overflow/

PACKAGE_MAKEDIR := $(patsubst %/, $(srctree)/package/MM-ERROR/%/Makefile, $(MM-ERROR-y))

include $(PACKAGE_MAKEDIR)
