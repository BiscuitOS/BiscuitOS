PK_MM_ERR_PERMANENT_OVERFLOW_NAME        := BiscuitOS-MM-ERROR-Permanent-Overflow
PK_MM_ERR_PERMANENT_OVERFLOW_VERSION     := $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_VERSION)
PK_MM_ERR_PERMANENT_OVERFLOW_SITE        := $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_URL)
PK_MM_ERR_PERMANENT_OVERFLOW_GITHUB      := $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_GITHUB)
PK_MM_ERR_PERMANENT_OVERFLOW_SUBNAME     :=
PK_MM_ERR_PERMANENT_OVERFLOW_BUILDARCH   := $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_BUILDARCH)
PK_MM_ERR_PERMANENT_OVERFLOW_DIR         := $(srctree)/package/MM-ERROR/BiscuitOS-MM-ERROR-Permanent-Overflow
PK_MM_ERR_PERMANENT_OVERFLOW_PATCH       := $(PK_MM_ERR_PERMANENT_OVERFLOW_DIR)/patch
PK_MM_ERR_PERMANENT_OVERFLOW_BSBIT       := BiscuitOS-MM-ERROR-Permanent-Overflow-$(PK_MM_ERR_PERMANENT_OVERFLOW_VERSION).bsbit
PK_MM_ERR_PERMANENT_OVERFLOW_OUT         :=
PK_MM_ERR_PERMANENT_OVERFLOW_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_MM_ERR_PERMANENT_OVERFLOW_TAR         := $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_TAR)
PK_MM_ERR_PERMANENT_OVERFLOW_PACKAGE     :=
PK_MM_ERR_PERMANENT_OVERFLOW_CONFIG      := $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_CONFIG)
SUB_TARGET                  += $(PK_MM_ERR_PERMANENT_OVERFLOW_DIR)/build

PK_MM_ERR_PERMANENT_OVERFLOW_sh = $(CONFIG_SHELL) $(PK_MM_ERR_PERMANENT_OVERFLOW_SCRIPTS) \
                 $1X \
                 $(PK_MM_ERR_PERMANENT_OVERFLOW_NAME)X \
                 $(PK_MM_ERR_PERMANENT_OVERFLOW_VERSION)X \
                 $(PK_MM_ERR_PERMANENT_OVERFLOW_SITE)X \
                 $(PK_MM_ERR_PERMANENT_OVERFLOW_GITHUB)X \
                 $(PK_MM_ERR_PERMANENT_OVERFLOW_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MM_ERR_PERMANENT_OVERFLOW_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MM_ERR_PERMANENT_OVERFLOW_SUBNAME)X \
                 $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_CONFIG)X \
                 $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_LIBPATH)X \
                 $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_CPPFLAGS)X \
                 $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_DPKCONFIG)X \
                 $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_CFLAGS)X \
                 $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_LDFLAGS)X \
                 $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_CXXFLAGS)X \
                 $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_ASFLAGS)X \
                 $(PK_MM_ERR_PERMANENT_OVERFLOW_BSBIT)X \
                 $(PK_MM_ERR_PERMANENT_OVERFLOW_BUILDARCH)X \
		 $(PK_MM_ERR_PERMANENT_OVERFLOW_CSRCFILE)X \
                 $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_SRCFILE)X \
                 $(CONFIG_PK_MM_ERR_PERMANENT_OVERFLOW_CSRCFILE)

$(PK_MM_ERR_PERMANENT_OVERFLOW_DIR)/build: FORCE
	$(Q)$(call PK_MM_ERR_PERMANENT_OVERFLOW_sh,$(srctree))
