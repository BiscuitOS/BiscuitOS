PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_SITE        := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_URL)
PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_GITHUB      := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_GITHUB)
PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_SUBNAME     :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_BUILDARCH   := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_BUILDARCH)
PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_DIR         := $(srctree)/package/MM-ERROR/BiscuitOS-MM-ERROR-Confuse-GFP-Flags-SLAB
PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_PATCH       := $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_DIR)/patch
PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_BSBIT       := BiscuitOS-MM-ERROR-Confuse-GFP-Flags-SLAB-$(PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_VERSION).bsbit
PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_OUT         :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_TAR         := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_TAR)
PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_PACKAGE     :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_CONFIG      := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_CONFIG)
SUB_TARGET                  += $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_DIR)/build

PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_sh = $(CONFIG_SHELL) $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_SCRIPTS) \
                 $1X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_NAME)X \
