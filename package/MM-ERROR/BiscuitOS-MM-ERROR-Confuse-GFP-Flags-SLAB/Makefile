                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_SITE)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_GITHUB)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_SUBNAME)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_CONFIG)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_LIBPATH)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_CPPFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_DPKCONFIG)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_CFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_LDFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_CXXFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_ASFLAGS)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_BSBIT)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_BUILDARCH)X \
		 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_CSRCFILE)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_SRCFILE)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_CSRCFILE)

$(PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_DIR)/build: FORCE
	$(Q)$(call PK_MM_ERR_CONFUSE_GFP_FLAGS_SLAB_sh,$(srctree))
