PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_NAME        := BiscuitOS-MM-ERROR-Confuse-GFP-Flags-VMALLOC
PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_VERSION     := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_VERSION)
PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_SITE        := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_URL)
PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_GITHUB      := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_GITHUB)
PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_SUBNAME     :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_BUILDARCH   := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_BUILDARCH)
PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_DIR         := $(srctree)/package/MM-ERROR/BiscuitOS-MM-ERROR-Confuse-GFP-Flags-VMALLOC
PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_PATCH       := $(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_DIR)/patch
PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_BSBIT       := BiscuitOS-MM-ERROR-Confuse-GFP-Flags-VMALLOC-$(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_VERSION).bsbit
PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_OUT         :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_TAR         := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_TAR)
PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_PACKAGE     :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_CONFIG      := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_CONFIG)
SUB_TARGET                  += $(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_DIR)/build

PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_sh = $(CONFIG_SHELL) $(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_SCRIPTS) \
                 $1X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_NAME)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_VERSION)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_SITE)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_GITHUB)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_SUBNAME)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_CONFIG)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_LIBPATH)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_CPPFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_DPKCONFIG)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_CFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_LDFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_CXXFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_ASFLAGS)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_BSBIT)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_BUILDARCH)X \
		 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_CSRCFILE)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_SRCFILE)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_CSRCFILE)

$(PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_DIR)/build: FORCE
	$(Q)$(call PK_MM_ERR_CONFUSE_GFP_FLAGS_VMALLOC_sh,$(srctree))
