PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_NAME        := BiscuitOS-MM-ERROR-Confuse-GFP-Flags-Buddy
PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_VERSION     := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_VERSION)
PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_SITE        := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_URL)
PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_GITHUB      := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_GITHUB)
PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_SUBNAME     :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_BUILDARCH   := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_BUILDARCH)
PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_DIR         := $(srctree)/package/MM-ERROR/BiscuitOS-MM-ERROR-Confuse-GFP-Flags-Buddy
PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_PATCH       := $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_DIR)/patch
PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_BSBIT       := BiscuitOS-MM-ERROR-Confuse-GFP-Flags-Buddy-$(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_VERSION).bsbit
PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_OUT         :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_TAR         := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_TAR)
PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_PACKAGE     :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_CONFIG      := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_CONFIG)
SUB_TARGET                  += $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_DIR)/build

PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_sh = $(CONFIG_SHELL) $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_SCRIPTS) \
                 $1X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_NAME)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_VERSION)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_SITE)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_GITHUB)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_SUBNAME)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_CONFIG)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_LIBPATH)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_CPPFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_DPKCONFIG)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_CFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_LDFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_CXXFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_ASFLAGS)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_BSBIT)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_BUILDARCH)X \
		 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_CSRCFILE)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_SRCFILE)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_CSRCFILE)

$(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_DIR)/build: FORCE
	$(Q)$(call PK_MM_ERR_CONFUSE_GFP_FLAGS_BUDDY_sh,$(srctree))
