PK_MM_ERR_EARLYIOMEM_OVERSLOT_NAME        := BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-OVER-SLOT
PK_MM_ERR_EARLYIOMEM_OVERSLOT_VERSION     := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_VERSION)
PK_MM_ERR_EARLYIOMEM_OVERSLOT_SITE        := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_URL)
PK_MM_ERR_EARLYIOMEM_OVERSLOT_GITHUB      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_GITHUB)
PK_MM_ERR_EARLYIOMEM_OVERSLOT_SUBNAME     :=
PK_MM_ERR_EARLYIOMEM_OVERSLOT_BUILDARCH   := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_BUILDARCH)
PK_MM_ERR_EARLYIOMEM_OVERSLOT_DIR         := $(srctree)/package/MM-ERROR/BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-OVER-SLOT
PK_MM_ERR_EARLYIOMEM_OVERSLOT_PATCH       := $(PK_MM_ERR_EARLYIOMEM_OVERSLOT_DIR)/patch
PK_MM_ERR_EARLYIOMEM_OVERSLOT_BSBIT       := BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-OVER-SLOT-$(PK_MM_ERR_EARLYIOMEM_OVERSLOT_VERSION).bsbit
PK_MM_ERR_EARLYIOMEM_OVERSLOT_OUT         :=
PK_MM_ERR_EARLYIOMEM_OVERSLOT_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MM_ERR_EARLYIOMEM_OVERSLOT_TAR         := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_TAR)
PK_MM_ERR_EARLYIOMEM_OVERSLOT_PACKAGE     :=
PK_MM_ERR_EARLYIOMEM_OVERSLOT_CONFIG      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_CONFIG)
SUB_TARGET                  += $(PK_MM_ERR_EARLYIOMEM_OVERSLOT_DIR)/build

PK_MM_ERR_EARLYIOMEM_OVERSLOT_sh = $(CONFIG_SHELL) $(PK_MM_ERR_EARLYIOMEM_OVERSLOT_SCRIPTS) \
                 $1X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERSLOT_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERSLOT_VERSION)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERSLOT_SITE)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERSLOT_GITHUB)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERSLOT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERSLOT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERSLOT_SUBNAME)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_CONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_LIBPATH)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_CPPFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_DPKCONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_CFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_LDFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_CXXFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_ASFLAGS)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERSLOT_BSBIT)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERSLOT_BUILDARCH)X \
		 $(PK_MM_ERR_EARLYIOMEM_OVERSLOT_CSRCFILE)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_SRCFILE)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERSLOT_CSRCFILE)

$(PK_MM_ERR_EARLYIOMEM_OVERSLOT_DIR)/build: FORCE
	$(Q)$(call PK_MM_ERR_EARLYIOMEM_OVERSLOT_sh,$(srctree))
