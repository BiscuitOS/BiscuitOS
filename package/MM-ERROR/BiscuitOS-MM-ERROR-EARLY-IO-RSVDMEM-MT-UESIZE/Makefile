PK_MM_ERR_EARLYIOMEM_UESIZE_NAME        := BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-UESIZE
PK_MM_ERR_EARLYIOMEM_UESIZE_VERSION     := $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_VERSION)
PK_MM_ERR_EARLYIOMEM_UESIZE_SITE        := $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_URL)
PK_MM_ERR_EARLYIOMEM_UESIZE_GITHUB      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_GITHUB)
PK_MM_ERR_EARLYIOMEM_UESIZE_SUBNAME     :=
PK_MM_ERR_EARLYIOMEM_UESIZE_BUILDARCH   := $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_BUILDARCH)
PK_MM_ERR_EARLYIOMEM_UESIZE_DIR         := $(srctree)/package/MM-ERROR/BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-UESIZE
PK_MM_ERR_EARLYIOMEM_UESIZE_PATCH       := $(PK_MM_ERR_EARLYIOMEM_UESIZE_DIR)/patch
PK_MM_ERR_EARLYIOMEM_UESIZE_BSBIT       := BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-UESIZE-$(PK_MM_ERR_EARLYIOMEM_UESIZE_VERSION).bsbit
PK_MM_ERR_EARLYIOMEM_UESIZE_OUT         :=
PK_MM_ERR_EARLYIOMEM_UESIZE_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MM_ERR_EARLYIOMEM_UESIZE_TAR         := $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_TAR)
PK_MM_ERR_EARLYIOMEM_UESIZE_PACKAGE     :=
PK_MM_ERR_EARLYIOMEM_UESIZE_CONFIG      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_CONFIG)
SUB_TARGET                  += $(PK_MM_ERR_EARLYIOMEM_UESIZE_DIR)/build

PK_MM_ERR_EARLYIOMEM_UESIZE_sh = $(CONFIG_SHELL) $(PK_MM_ERR_EARLYIOMEM_UESIZE_SCRIPTS) \
                 $1X \
                 $(PK_MM_ERR_EARLYIOMEM_UESIZE_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_UESIZE_VERSION)X \
                 $(PK_MM_ERR_EARLYIOMEM_UESIZE_SITE)X \
                 $(PK_MM_ERR_EARLYIOMEM_UESIZE_GITHUB)X \
                 $(PK_MM_ERR_EARLYIOMEM_UESIZE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_UESIZE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MM_ERR_EARLYIOMEM_UESIZE_SUBNAME)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_CONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_LIBPATH)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_CPPFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_DPKCONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_CFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_LDFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_CXXFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_ASFLAGS)X \
                 $(PK_MM_ERR_EARLYIOMEM_UESIZE_BSBIT)X \
                 $(PK_MM_ERR_EARLYIOMEM_UESIZE_BUILDARCH)X \
		 $(PK_MM_ERR_EARLYIOMEM_UESIZE_CSRCFILE)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_SRCFILE)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UESIZE_CSRCFILE)

$(PK_MM_ERR_EARLYIOMEM_UESIZE_DIR)/build: FORCE
	$(Q)$(call PK_MM_ERR_EARLYIOMEM_UESIZE_sh,$(srctree))
