PK_MM_ERR_EARLYIOMEM_REFREE_NAME        := BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-REFREE
PK_MM_ERR_EARLYIOMEM_REFREE_VERSION     := $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_VERSION)
PK_MM_ERR_EARLYIOMEM_REFREE_SITE        := $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_URL)
PK_MM_ERR_EARLYIOMEM_REFREE_GITHUB      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_GITHUB)
PK_MM_ERR_EARLYIOMEM_REFREE_SUBNAME     :=
PK_MM_ERR_EARLYIOMEM_REFREE_BUILDARCH   := $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_BUILDARCH)
PK_MM_ERR_EARLYIOMEM_REFREE_DIR         := $(srctree)/package/MM-ERROR/BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-REFREE
PK_MM_ERR_EARLYIOMEM_REFREE_PATCH       := $(PK_MM_ERR_EARLYIOMEM_REFREE_DIR)/patch
PK_MM_ERR_EARLYIOMEM_REFREE_BSBIT       := BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-REFREE-$(PK_MM_ERR_EARLYIOMEM_REFREE_VERSION).bsbit
PK_MM_ERR_EARLYIOMEM_REFREE_OUT         :=
PK_MM_ERR_EARLYIOMEM_REFREE_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MM_ERR_EARLYIOMEM_REFREE_TAR         := $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_TAR)
PK_MM_ERR_EARLYIOMEM_REFREE_PACKAGE     :=
PK_MM_ERR_EARLYIOMEM_REFREE_CONFIG      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_CONFIG)
SUB_TARGET                  += $(PK_MM_ERR_EARLYIOMEM_REFREE_DIR)/build

PK_MM_ERR_EARLYIOMEM_REFREE_sh = $(CONFIG_SHELL) $(PK_MM_ERR_EARLYIOMEM_REFREE_SCRIPTS) \
                 $1X \
                 $(PK_MM_ERR_EARLYIOMEM_REFREE_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_REFREE_VERSION)X \
                 $(PK_MM_ERR_EARLYIOMEM_REFREE_SITE)X \
                 $(PK_MM_ERR_EARLYIOMEM_REFREE_GITHUB)X \
                 $(PK_MM_ERR_EARLYIOMEM_REFREE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_REFREE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MM_ERR_EARLYIOMEM_REFREE_SUBNAME)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_CONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_LIBPATH)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_CPPFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_DPKCONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_CFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_LDFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_CXXFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_ASFLAGS)X \
                 $(PK_MM_ERR_EARLYIOMEM_REFREE_BSBIT)X \
                 $(PK_MM_ERR_EARLYIOMEM_REFREE_BUILDARCH)X \
		 $(PK_MM_ERR_EARLYIOMEM_REFREE_CSRCFILE)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_SRCFILE)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_REFREE_CSRCFILE)

$(PK_MM_ERR_EARLYIOMEM_REFREE_DIR)/build: FORCE
	$(Q)$(call PK_MM_ERR_EARLYIOMEM_REFREE_sh,$(srctree))
