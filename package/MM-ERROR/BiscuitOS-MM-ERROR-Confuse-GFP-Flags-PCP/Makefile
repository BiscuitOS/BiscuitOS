PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_NAME        := BiscuitOS-MM-ERROR-Confuse-GFP-Flags-PCP
PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_VERSION     := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_VERSION)
PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_SITE        := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_URL)
PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_GITHUB      := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_GITHUB)
PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_SUBNAME     :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_BUILDARCH   := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_BUILDARCH)
PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_DIR         := $(srctree)/package/MM-ERROR/BiscuitOS-MM-ERROR-Confuse-GFP-Flags-PCP
PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_PATCH       := $(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_DIR)/patch
PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_BSBIT       := BiscuitOS-MM-ERROR-Confuse-GFP-Flags-PCP-$(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_VERSION).bsbit
PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_OUT         :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_TAR         := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_TAR)
PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_PACKAGE     :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_CONFIG      := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_CONFIG)
SUB_TARGET                  += $(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_DIR)/build

PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_sh = $(CONFIG_SHELL) $(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_SCRIPTS) \
                 $1X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_NAME)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_VERSION)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_SITE)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_GITHUB)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_SUBNAME)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_CONFIG)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_LIBPATH)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_CPPFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_DPKCONFIG)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_CFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_LDFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_CXXFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_ASFLAGS)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_BSBIT)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_BUILDARCH)X \
		 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_CSRCFILE)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_SRCFILE)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_CSRCFILE)

$(PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_DIR)/build: FORCE
	$(Q)$(call PK_MM_ERR_CONFUSE_GFP_FLAGS_PCP_sh,$(srctree))
