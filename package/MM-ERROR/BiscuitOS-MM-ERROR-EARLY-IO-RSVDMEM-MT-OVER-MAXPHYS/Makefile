PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_NAME        := BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-OVER-MAXPHYS
PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_VERSION     := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_VERSION)
PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_SITE        := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_URL)
PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_GITHUB      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_GITHUB)
PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_SUBNAME     :=
PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_BUILDARCH   := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_BUILDARCH)
PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_DIR         := $(srctree)/package/MM-ERROR/BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-OVER-MAXPHYS
PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_PATCH       := $(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_DIR)/patch
PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_BSBIT       := BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-OVER-MAXPHYS-$(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_VERSION).bsbit
PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_OUT         :=
PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_TAR         := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_TAR)
PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_PACKAGE     :=
PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_CONFIG      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_CONFIG)
SUB_TARGET                  += $(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_DIR)/build

PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_sh = $(CONFIG_SHELL) $(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_SCRIPTS) \
                 $1X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_VERSION)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_SITE)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_GITHUB)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_SUBNAME)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_CONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_LIBPATH)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_CPPFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_DPKCONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_CFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_LDFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_CXXFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_ASFLAGS)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_BSBIT)X \
                 $(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_BUILDARCH)X \
		 $(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_CSRCFILE)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_SRCFILE)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_CSRCFILE)

$(PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_DIR)/build: FORCE
	$(Q)$(call PK_MM_ERR_EARLYIOMEM_OVERMAXPHYS_sh,$(srctree))
