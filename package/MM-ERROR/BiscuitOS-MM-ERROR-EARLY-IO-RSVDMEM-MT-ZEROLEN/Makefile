PK_MM_ERR_EARLYIOMEM_ZEROLEN_GITHUB      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_ZEROLEN_GITHUB)
PK_MM_ERR_EARLYIOMEM_ZEROLEN_SUBNAME     :=
PK_MM_ERR_EARLYIOMEM_ZEROLEN_BUILDARCH   := $(CONFIG_PK_MM_ERR_EARLYIOMEM_ZEROLEN_BUILDARCH)
PK_MM_ERR_EARLYIOMEM_ZEROLEN_DIR         := $(srctree)/package/MM-ERROR/BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-ZEROLEN
PK_MM_ERR_EARLYIOMEM_ZEROLEN_PATCH       := $(PK_MM_ERR_EARLYIOMEM_ZEROLEN_DIR)/patch
PK_MM_ERR_EARLYIOMEM_ZEROLEN_BSBIT       := BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-ZEROLEN-$(PK_MM_ERR_EARLYIOMEM_ZEROLEN_VERSION).bsbit
PK_MM_ERR_EARLYIOMEM_ZEROLEN_OUT         :=
PK_MM_ERR_EARLYIOMEM_ZEROLEN_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MM_ERR_EARLYIOMEM_ZEROLEN_TAR         := $(CONFIG_PK_MM_ERR_EARLYIOMEM_ZEROLEN_TAR)
PK_MM_ERR_EARLYIOMEM_ZEROLEN_PACKAGE     :=
PK_MM_ERR_EARLYIOMEM_ZEROLEN_CONFIG      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_ZEROLEN_CONFIG)
SUB_TARGET                  += $(PK_MM_ERR_EARLYIOMEM_ZEROLEN_DIR)/build

PK_MM_ERR_EARLYIOMEM_ZEROLEN_sh = $(CONFIG_SHELL) $(PK_MM_ERR_EARLYIOMEM_ZEROLEN_SCRIPTS) \
                 $1X \
                 $(PK_MM_ERR_EARLYIOMEM_ZEROLEN_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_ZEROLEN_VERSION)X \
                 $(PK_MM_ERR_EARLYIOMEM_ZEROLEN_SITE)X \
                 $(PK_MM_ERR_EARLYIOMEM_ZEROLEN_GITHUB)X \
                 $(PK_MM_ERR_EARLYIOMEM_ZEROLEN_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_ZEROLEN_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MM_ERR_EARLYIOMEM_ZEROLEN_SUBNAME)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_ZEROLEN_CONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_ZEROLEN_LIBPATH)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_ZEROLEN_CPPFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_ZEROLEN_DPKCONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_ZEROLEN_CFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_ZEROLEN_LDFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_ZEROLEN_CXXFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_ZEROLEN_ASFLAGS)X \
                 $(PK_MM_ERR_EARLYIOMEM_ZEROLEN_BSBIT)X \
                 $(PK_MM_ERR_EARLYIOMEM_ZEROLEN_BUILDARCH)X \
		 $(PK_MM_ERR_EARLYIOMEM_ZEROLEN_CSRCFILE)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_ZEROLEN_SRCFILE)X \
