                 $(PK_MM_ERR_EARLYIOMEM_WP_SITE)X \
                 $(PK_MM_ERR_EARLYIOMEM_WP_GITHUB)X \
                 $(PK_MM_ERR_EARLYIOMEM_WP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_WP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MM_ERR_EARLYIOMEM_WP_SUBNAME)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_WP_CONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_WP_LIBPATH)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_WP_CPPFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_WP_DPKCONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_WP_CFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_WP_LDFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_WP_CXXFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_WP_ASFLAGS)X \
                 $(PK_MM_ERR_EARLYIOMEM_WP_BSBIT)X \
                 $(PK_MM_ERR_EARLYIOMEM_WP_BUILDARCH)X \
		 $(PK_MM_ERR_EARLYIOMEM_WP_CSRCFILE)X \
