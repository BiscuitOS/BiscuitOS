PK_MM_ERR_EARLYIOMEM_WP_DIR         := $(srctree)/package/MM-ERROR/BiscuitOS-MM-ERROR-WP-ON-EARLY-IO-RSVDMEM-MT
PK_MM_ERR_EARLYIOMEM_WP_PATCH       := $(PK_MM_ERR_EARLYIOMEM_WP_DIR)/patch
PK_MM_ERR_EARLYIOMEM_WP_BSBIT       := BiscuitOS-MM-ERROR-WP-ON-EARLY-IO-RSVDMEM-MT-$(PK_MM_ERR_EARLYIOMEM_WP_VERSION).bsbit
PK_MM_ERR_EARLYIOMEM_WP_OUT         :=
