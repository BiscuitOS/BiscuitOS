PK_MM_ERR_EARLYIOMEM_WP_PACKAGE     :=
PK_MM_ERR_EARLYIOMEM_WP_CONFIG      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_WP_CONFIG)
SUB_TARGET                  += $(PK_MM_ERR_EARLYIOMEM_WP_DIR)/build

PK_MM_ERR_EARLYIOMEM_WP_sh = $(CONFIG_SHELL) $(PK_MM_ERR_EARLYIOMEM_WP_SCRIPTS) \
                 $1X \
