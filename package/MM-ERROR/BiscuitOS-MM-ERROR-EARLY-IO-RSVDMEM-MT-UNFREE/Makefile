PK_MM_ERR_EARLYIOMEM_UNFREE_SITE        := $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_URL)
PK_MM_ERR_EARLYIOMEM_UNFREE_GITHUB      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_GITHUB)
PK_MM_ERR_EARLYIOMEM_UNFREE_SUBNAME     :=
PK_MM_ERR_EARLYIOMEM_UNFREE_BUILDARCH   := $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_BUILDARCH)
PK_MM_ERR_EARLYIOMEM_UNFREE_DIR         := $(srctree)/package/MM-ERROR/BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-UNFREE
PK_MM_ERR_EARLYIOMEM_UNFREE_PATCH       := $(PK_MM_ERR_EARLYIOMEM_UNFREE_DIR)/patch
PK_MM_ERR_EARLYIOMEM_UNFREE_BSBIT       := BiscuitOS-MM-ERROR-EARLY-IO-RSVDMEM-MT-UNFREE-$(PK_MM_ERR_EARLYIOMEM_UNFREE_VERSION).bsbit
PK_MM_ERR_EARLYIOMEM_UNFREE_OUT         :=
PK_MM_ERR_EARLYIOMEM_UNFREE_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MM_ERR_EARLYIOMEM_UNFREE_TAR         := $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_TAR)
PK_MM_ERR_EARLYIOMEM_UNFREE_PACKAGE     :=
PK_MM_ERR_EARLYIOMEM_UNFREE_CONFIG      := $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_CONFIG)
SUB_TARGET                  += $(PK_MM_ERR_EARLYIOMEM_UNFREE_DIR)/build

PK_MM_ERR_EARLYIOMEM_UNFREE_sh = $(CONFIG_SHELL) $(PK_MM_ERR_EARLYIOMEM_UNFREE_SCRIPTS) \
                 $1X \
                 $(PK_MM_ERR_EARLYIOMEM_UNFREE_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_UNFREE_VERSION)X \
                 $(PK_MM_ERR_EARLYIOMEM_UNFREE_SITE)X \
                 $(PK_MM_ERR_EARLYIOMEM_UNFREE_GITHUB)X \
                 $(PK_MM_ERR_EARLYIOMEM_UNFREE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MM_ERR_EARLYIOMEM_UNFREE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MM_ERR_EARLYIOMEM_UNFREE_SUBNAME)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_CONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_LIBPATH)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_CPPFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_DPKCONFIG)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_CFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_LDFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_CXXFLAGS)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_ASFLAGS)X \
                 $(PK_MM_ERR_EARLYIOMEM_UNFREE_BSBIT)X \
                 $(PK_MM_ERR_EARLYIOMEM_UNFREE_BUILDARCH)X \
		 $(PK_MM_ERR_EARLYIOMEM_UNFREE_CSRCFILE)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_SRCFILE)X \
                 $(CONFIG_PK_MM_ERR_EARLYIOMEM_UNFREE_CSRCFILE)

$(PK_MM_ERR_EARLYIOMEM_UNFREE_DIR)/build: FORCE
	$(Q)$(call PK_MM_ERR_EARLYIOMEM_UNFREE_sh,$(srctree))
