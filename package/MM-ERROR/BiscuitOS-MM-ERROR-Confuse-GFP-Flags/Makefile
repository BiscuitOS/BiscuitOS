PK_MM_ERR_CONFUSE_GFP_FLAGS_NAME        := BiscuitOS-MM-ERROR-Confuse-GFP-Flags
PK_MM_ERR_CONFUSE_GFP_FLAGS_VERSION     := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_VERSION)
PK_MM_ERR_CONFUSE_GFP_FLAGS_SITE        := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_URL)
PK_MM_ERR_CONFUSE_GFP_FLAGS_GITHUB      := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_GITHUB)
PK_MM_ERR_CONFUSE_GFP_FLAGS_SUBNAME     :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_BUILDARCH   := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_BUILDARCH)
PK_MM_ERR_CONFUSE_GFP_FLAGS_DIR         := $(srctree)/package/MM-ERROR/BiscuitOS-MM-ERROR-Confuse-GFP-Flags
PK_MM_ERR_CONFUSE_GFP_FLAGS_PATCH       := $(PK_MM_ERR_CONFUSE_GFP_FLAGS_DIR)/patch
PK_MM_ERR_CONFUSE_GFP_FLAGS_BSBIT       := BiscuitOS-MM-ERROR-Confuse-GFP-Flags-$(PK_MM_ERR_CONFUSE_GFP_FLAGS_VERSION).bsbit
PK_MM_ERR_CONFUSE_GFP_FLAGS_OUT         :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MM_ERR_CONFUSE_GFP_FLAGS_TAR         := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_TAR)
PK_MM_ERR_CONFUSE_GFP_FLAGS_PACKAGE     :=
PK_MM_ERR_CONFUSE_GFP_FLAGS_CONFIG      := $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_CONFIG)
SUB_TARGET                  += $(PK_MM_ERR_CONFUSE_GFP_FLAGS_DIR)/build

PK_MM_ERR_CONFUSE_GFP_FLAGS_sh = $(CONFIG_SHELL) $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SCRIPTS) \
                 $1X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_NAME)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_VERSION)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SITE)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_GITHUB)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_SUBNAME)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_CONFIG)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_LIBPATH)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_CPPFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_DPKCONFIG)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_CFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_LDFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_CXXFLAGS)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_ASFLAGS)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BSBIT)X \
                 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_BUILDARCH)X \
		 $(PK_MM_ERR_CONFUSE_GFP_FLAGS_CSRCFILE)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_SRCFILE)X \
                 $(CONFIG_PK_MM_ERR_CONFUSE_GFP_FLAGS_CSRCFILE)

$(PK_MM_ERR_CONFUSE_GFP_FLAGS_DIR)/build: FORCE
	$(Q)$(call PK_MM_ERR_CONFUSE_GFP_FLAGS_sh,$(srctree))
