PK_EARLY_IOREMAP_IOUNMAP_NAME        := BiscuitOS-EARLY-IOREMAP-early_iounmap
PK_EARLY_IOREMAP_IOUNMAP_VERSION     := $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_VERSION)
PK_EARLY_IOREMAP_IOUNMAP_SITE        := $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_URL)
PK_EARLY_IOREMAP_IOUNMAP_GITHUB      := $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_GITHUB)
PK_EARLY_IOREMAP_IOUNMAP_SUBNAME     :=
PK_EARLY_IOREMAP_IOUNMAP_BUILDARCH   := $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_BUILDARCH)
PK_EARLY_IOREMAP_IOUNMAP_DIR         := $(srctree)/package/EARLY-IOREMAP/BiscuitOS-EARLY-IOREMAP-early_iounmap
PK_EARLY_IOREMAP_IOUNMAP_PATCH       := $(PK_EARLY_IOREMAP_IOUNMAP_DIR)/patch
PK_EARLY_IOREMAP_IOUNMAP_BSBIT       := BiscuitOS-EARLY-IOREMAP-early_iounmap-$(PK_EARLY_IOREMAP_IOUNMAP_VERSION).bsbit
PK_EARLY_IOREMAP_IOUNMAP_OUT         :=
PK_EARLY_IOREMAP_IOUNMAP_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_EARLY_IOREMAP_IOUNMAP_TAR         := $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_TAR)
PK_EARLY_IOREMAP_IOUNMAP_PACKAGE     :=
PK_EARLY_IOREMAP_IOUNMAP_CONFIG      := $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_CONFIG)
SUB_TARGET                  += $(PK_EARLY_IOREMAP_IOUNMAP_DIR)/build

PK_EARLY_IOREMAP_IOUNMAP_sh = $(CONFIG_SHELL) $(PK_EARLY_IOREMAP_IOUNMAP_SCRIPTS) \
                 $1X \
                 $(PK_EARLY_IOREMAP_IOUNMAP_NAME)X \
                 $(PK_EARLY_IOREMAP_IOUNMAP_VERSION)X \
                 $(PK_EARLY_IOREMAP_IOUNMAP_SITE)X \
                 $(PK_EARLY_IOREMAP_IOUNMAP_GITHUB)X \
                 $(PK_EARLY_IOREMAP_IOUNMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_EARLY_IOREMAP_IOUNMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_EARLY_IOREMAP_IOUNMAP_SUBNAME)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_CONFIG)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_LIBPATH)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_CPPFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_DPKCONFIG)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_CFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_LDFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_CXXFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_ASFLAGS)X \
                 $(PK_EARLY_IOREMAP_IOUNMAP_BSBIT)X \
                 $(PK_EARLY_IOREMAP_IOUNMAP_BUILDARCH)X \
		 $(PK_EARLY_IOREMAP_IOUNMAP_CSRCFILE)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_SRCFILE)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOUNMAP_CSRCFILE)

$(PK_EARLY_IOREMAP_IOUNMAP_DIR)/build: FORCE
	$(Q)$(call PK_EARLY_IOREMAP_IOUNMAP_sh,$(srctree))
