# BiscuitOS-EARLY-IOREMAP-early_memremap
#
################################################
PK_EARLY_IOREMAP_MEMREMAP_NAME        := BiscuitOS-EARLY-IOREMAP-early_memremap
PK_EARLY_IOREMAP_MEMREMAP_VERSION     := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_VERSION)
PK_EARLY_IOREMAP_MEMREMAP_SITE        := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_URL)
PK_EARLY_IOREMAP_MEMREMAP_GITHUB      := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_GITHUB)
PK_EARLY_IOREMAP_MEMREMAP_SUBNAME     :=
PK_EARLY_IOREMAP_MEMREMAP_BUILDARCH   := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_BUILDARCH)
PK_EARLY_IOREMAP_MEMREMAP_DIR         := $(srctree)/package/EARLY-IOREMAP/BiscuitOS-EARLY-IOREMAP-early_memremap
PK_EARLY_IOREMAP_MEMREMAP_PATCH       := $(PK_EARLY_IOREMAP_MEMREMAP_DIR)/patch
PK_EARLY_IOREMAP_MEMREMAP_BSBIT       := BiscuitOS-EARLY-IOREMAP-early_memremap-$(PK_EARLY_IOREMAP_MEMREMAP_VERSION).bsbit
PK_EARLY_IOREMAP_MEMREMAP_OUT         :=
PK_EARLY_IOREMAP_MEMREMAP_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_EARLY_IOREMAP_MEMREMAP_TAR         := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_TAR)
PK_EARLY_IOREMAP_MEMREMAP_PACKAGE     :=
PK_EARLY_IOREMAP_MEMREMAP_CONFIG      := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_CONFIG)
SUB_TARGET                  += $(PK_EARLY_IOREMAP_MEMREMAP_DIR)/build

PK_EARLY_IOREMAP_MEMREMAP_sh = $(CONFIG_SHELL) $(PK_EARLY_IOREMAP_MEMREMAP_SCRIPTS) \
                 $1X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_NAME)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_VERSION)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_SITE)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_GITHUB)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_SUBNAME)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_CONFIG)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_LIBPATH)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_CPPFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_DPKCONFIG)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_CFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_LDFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_CXXFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_ASFLAGS)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_BSBIT)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_BUILDARCH)X \
		 $(PK_EARLY_IOREMAP_MEMREMAP_CSRCFILE)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_SRCFILE)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_CSRCFILE)

$(PK_EARLY_IOREMAP_MEMREMAP_DIR)/build: FORCE
	$(Q)$(call PK_EARLY_IOREMAP_MEMREMAP_sh,$(srctree))
