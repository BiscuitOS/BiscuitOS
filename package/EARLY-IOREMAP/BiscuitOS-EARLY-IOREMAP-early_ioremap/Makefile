PK_EARLY_IOREMAP_IOREMAP_NAME        := BiscuitOS-EARLY-IOREMAP-early_ioremap
PK_EARLY_IOREMAP_IOREMAP_VERSION     := $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_VERSION)
PK_EARLY_IOREMAP_IOREMAP_SITE        := $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_URL)
PK_EARLY_IOREMAP_IOREMAP_GITHUB      := $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_GITHUB)
PK_EARLY_IOREMAP_IOREMAP_SUBNAME     :=
PK_EARLY_IOREMAP_IOREMAP_BUILDARCH   := $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_BUILDARCH)
PK_EARLY_IOREMAP_IOREMAP_DIR         := $(srctree)/package/EARLY-IOREMAP/BiscuitOS-EARLY-IOREMAP-early_ioremap
PK_EARLY_IOREMAP_IOREMAP_PATCH       := $(PK_EARLY_IOREMAP_IOREMAP_DIR)/patch
PK_EARLY_IOREMAP_IOREMAP_BSBIT       := BiscuitOS-EARLY-IOREMAP-early_ioremap-$(PK_EARLY_IOREMAP_IOREMAP_VERSION).bsbit
PK_EARLY_IOREMAP_IOREMAP_OUT         :=
PK_EARLY_IOREMAP_IOREMAP_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_EARLY_IOREMAP_IOREMAP_TAR         := $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_TAR)
PK_EARLY_IOREMAP_IOREMAP_PACKAGE     :=
PK_EARLY_IOREMAP_IOREMAP_CONFIG      := $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_CONFIG)
SUB_TARGET                  += $(PK_EARLY_IOREMAP_IOREMAP_DIR)/build

PK_EARLY_IOREMAP_IOREMAP_sh = $(CONFIG_SHELL) $(PK_EARLY_IOREMAP_IOREMAP_SCRIPTS) \
                 $1X \
                 $(PK_EARLY_IOREMAP_IOREMAP_NAME)X \
                 $(PK_EARLY_IOREMAP_IOREMAP_VERSION)X \
                 $(PK_EARLY_IOREMAP_IOREMAP_SITE)X \
                 $(PK_EARLY_IOREMAP_IOREMAP_GITHUB)X \
                 $(PK_EARLY_IOREMAP_IOREMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_EARLY_IOREMAP_IOREMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_EARLY_IOREMAP_IOREMAP_SUBNAME)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_CONFIG)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_LIBPATH)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_CPPFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_DPKCONFIG)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_CFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_LDFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_CXXFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_ASFLAGS)X \
                 $(PK_EARLY_IOREMAP_IOREMAP_BSBIT)X \
                 $(PK_EARLY_IOREMAP_IOREMAP_BUILDARCH)X \
		 $(PK_EARLY_IOREMAP_IOREMAP_CSRCFILE)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_SRCFILE)X \
                 $(CONFIG_PK_EARLY_IOREMAP_IOREMAP_CSRCFILE)

$(PK_EARLY_IOREMAP_IOREMAP_DIR)/build: FORCE
	$(Q)$(call PK_EARLY_IOREMAP_IOREMAP_sh,$(srctree))
