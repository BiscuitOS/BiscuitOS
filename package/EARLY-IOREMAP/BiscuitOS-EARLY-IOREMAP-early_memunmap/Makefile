################################################
#
# BiscuitOS-EARLY-IOREMAP-early_memunmap
#
################################################
PK_EARLY_IOREMAP_MEMUNMAP_NAME        := BiscuitOS-EARLY-IOREMAP-early_memunmap
PK_EARLY_IOREMAP_MEMUNMAP_VERSION     := $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_VERSION)
PK_EARLY_IOREMAP_MEMUNMAP_SITE        := $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_URL)
PK_EARLY_IOREMAP_MEMUNMAP_GITHUB      := $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_GITHUB)
PK_EARLY_IOREMAP_MEMUNMAP_SUBNAME     :=
PK_EARLY_IOREMAP_MEMUNMAP_BUILDARCH   := $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_BUILDARCH)
PK_EARLY_IOREMAP_MEMUNMAP_DIR         := $(srctree)/package/EARLY-IOREMAP/BiscuitOS-EARLY-IOREMAP-early_memunmap
PK_EARLY_IOREMAP_MEMUNMAP_PATCH       := $(PK_EARLY_IOREMAP_MEMUNMAP_DIR)/patch
PK_EARLY_IOREMAP_MEMUNMAP_BSBIT       := BiscuitOS-EARLY-IOREMAP-early_memunmap-$(PK_EARLY_IOREMAP_MEMUNMAP_VERSION).bsbit
PK_EARLY_IOREMAP_MEMUNMAP_OUT         :=
PK_EARLY_IOREMAP_MEMUNMAP_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_EARLY_IOREMAP_MEMUNMAP_TAR         := $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_TAR)
PK_EARLY_IOREMAP_MEMUNMAP_PACKAGE     :=
PK_EARLY_IOREMAP_MEMUNMAP_CONFIG      := $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_CONFIG)
SUB_TARGET                  += $(PK_EARLY_IOREMAP_MEMUNMAP_DIR)/build

PK_EARLY_IOREMAP_MEMUNMAP_sh = $(CONFIG_SHELL) $(PK_EARLY_IOREMAP_MEMUNMAP_SCRIPTS) \
                 $1X \
                 $(PK_EARLY_IOREMAP_MEMUNMAP_NAME)X \
                 $(PK_EARLY_IOREMAP_MEMUNMAP_VERSION)X \
                 $(PK_EARLY_IOREMAP_MEMUNMAP_SITE)X \
                 $(PK_EARLY_IOREMAP_MEMUNMAP_GITHUB)X \
                 $(PK_EARLY_IOREMAP_MEMUNMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_EARLY_IOREMAP_MEMUNMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_EARLY_IOREMAP_MEMUNMAP_SUBNAME)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_CONFIG)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_LIBPATH)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_CPPFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_DPKCONFIG)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_CFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_LDFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_CXXFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_ASFLAGS)X \
                 $(PK_EARLY_IOREMAP_MEMUNMAP_BSBIT)X \
                 $(PK_EARLY_IOREMAP_MEMUNMAP_BUILDARCH)X \
		 $(PK_EARLY_IOREMAP_MEMUNMAP_CSRCFILE)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_SRCFILE)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP_CSRCFILE)

$(PK_EARLY_IOREMAP_MEMUNMAP_DIR)/build: FORCE
	$(Q)$(call PK_EARLY_IOREMAP_MEMUNMAP_sh,$(srctree))
