PK_EARLY_IOREMAP_CPYMEM_NAME        := BiscuitOS-EARLY-IOREMAP-copy_from_early_mem
PK_EARLY_IOREMAP_CPYMEM_VERSION     := $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_VERSION)
PK_EARLY_IOREMAP_CPYMEM_SITE        := $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_URL)
PK_EARLY_IOREMAP_CPYMEM_GITHUB      := $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_GITHUB)
PK_EARLY_IOREMAP_CPYMEM_SUBNAME     :=
PK_EARLY_IOREMAP_CPYMEM_BUILDARCH   := $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_BUILDARCH)
PK_EARLY_IOREMAP_CPYMEM_DIR         := $(srctree)/package/EARLY-IOREMAP/BiscuitOS-EARLY-IOREMAP-copy_from_early_mem
PK_EARLY_IOREMAP_CPYMEM_PATCH       := $(PK_EARLY_IOREMAP_CPYMEM_DIR)/patch
PK_EARLY_IOREMAP_CPYMEM_BSBIT       := BiscuitOS-EARLY-IOREMAP-copy_from_early_mem-$(PK_EARLY_IOREMAP_CPYMEM_VERSION).bsbit
PK_EARLY_IOREMAP_CPYMEM_OUT         :=
PK_EARLY_IOREMAP_CPYMEM_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_EARLY_IOREMAP_CPYMEM_TAR         := $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_TAR)
PK_EARLY_IOREMAP_CPYMEM_PACKAGE     :=
PK_EARLY_IOREMAP_CPYMEM_CONFIG      := $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_CONFIG)
SUB_TARGET                  += $(PK_EARLY_IOREMAP_CPYMEM_DIR)/build

PK_EARLY_IOREMAP_CPYMEM_sh = $(CONFIG_SHELL) $(PK_EARLY_IOREMAP_CPYMEM_SCRIPTS) \
                 $1X \
                 $(PK_EARLY_IOREMAP_CPYMEM_NAME)X \
                 $(PK_EARLY_IOREMAP_CPYMEM_VERSION)X \
                 $(PK_EARLY_IOREMAP_CPYMEM_SITE)X \
                 $(PK_EARLY_IOREMAP_CPYMEM_GITHUB)X \
                 $(PK_EARLY_IOREMAP_CPYMEM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_EARLY_IOREMAP_CPYMEM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_EARLY_IOREMAP_CPYMEM_SUBNAME)X \
                 $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_CONFIG)X \
                 $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_LIBPATH)X \
                 $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_CPPFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_DPKCONFIG)X \
                 $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_CFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_LDFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_CXXFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_ASFLAGS)X \
                 $(PK_EARLY_IOREMAP_CPYMEM_BSBIT)X \
                 $(PK_EARLY_IOREMAP_CPYMEM_BUILDARCH)X \
		 $(PK_EARLY_IOREMAP_CPYMEM_CSRCFILE)X \
                 $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_SRCFILE)X \
                 $(CONFIG_PK_EARLY_IOREMAP_CPYMEM_CSRCFILE)

$(PK_EARLY_IOREMAP_CPYMEM_DIR)/build: FORCE
	$(Q)$(call PK_EARLY_IOREMAP_CPYMEM_sh,$(srctree))
