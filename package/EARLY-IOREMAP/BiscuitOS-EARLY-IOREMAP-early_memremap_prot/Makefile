PK_EARLY_IOREMAP_MEMREMAP_PROT_NAME        := BiscuitOS-EARLY-IOREMAP-early_memremap_prot
PK_EARLY_IOREMAP_MEMREMAP_PROT_VERSION     := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_VERSION)
PK_EARLY_IOREMAP_MEMREMAP_PROT_SITE        := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_URL)
PK_EARLY_IOREMAP_MEMREMAP_PROT_GITHUB      := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_GITHUB)
PK_EARLY_IOREMAP_MEMREMAP_PROT_SUBNAME     :=
PK_EARLY_IOREMAP_MEMREMAP_PROT_BUILDARCH   := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_BUILDARCH)
PK_EARLY_IOREMAP_MEMREMAP_PROT_DIR         := $(srctree)/package/EARLY-IOREMAP/BiscuitOS-EARLY-IOREMAP-early_memremap_prot
PK_EARLY_IOREMAP_MEMREMAP_PROT_PATCH       := $(PK_EARLY_IOREMAP_MEMREMAP_PROT_DIR)/patch
PK_EARLY_IOREMAP_MEMREMAP_PROT_BSBIT       := BiscuitOS-EARLY-IOREMAP-early_memremap_prot-$(PK_EARLY_IOREMAP_MEMREMAP_PROT_VERSION).bsbit
PK_EARLY_IOREMAP_MEMREMAP_PROT_OUT         :=
PK_EARLY_IOREMAP_MEMREMAP_PROT_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_EARLY_IOREMAP_MEMREMAP_PROT_TAR         := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_TAR)
PK_EARLY_IOREMAP_MEMREMAP_PROT_PACKAGE     :=
PK_EARLY_IOREMAP_MEMREMAP_PROT_CONFIG      := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_CONFIG)
SUB_TARGET                  += $(PK_EARLY_IOREMAP_MEMREMAP_PROT_DIR)/build

PK_EARLY_IOREMAP_MEMREMAP_PROT_sh = $(CONFIG_SHELL) $(PK_EARLY_IOREMAP_MEMREMAP_PROT_SCRIPTS) \
                 $1X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_PROT_NAME)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_PROT_VERSION)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_PROT_SITE)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_PROT_GITHUB)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_PROT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_PROT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_PROT_SUBNAME)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_CONFIG)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_LIBPATH)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_CPPFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_DPKCONFIG)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_CFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_LDFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_CXXFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_ASFLAGS)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_PROT_BSBIT)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_PROT_BUILDARCH)X \
		 $(PK_EARLY_IOREMAP_MEMREMAP_PROT_CSRCFILE)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_SRCFILE)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_PROT_CSRCFILE)

$(PK_EARLY_IOREMAP_MEMREMAP_PROT_DIR)/build: FORCE
	$(Q)$(call PK_EARLY_IOREMAP_MEMREMAP_PROT_sh,$(srctree))
