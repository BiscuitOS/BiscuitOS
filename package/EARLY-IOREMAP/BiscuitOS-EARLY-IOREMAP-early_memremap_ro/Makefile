PK_EARLY_IOREMAP_MEMREMAP_RO_NAME        := BiscuitOS-EARLY-IOREMAP-early_memremap_ro
PK_EARLY_IOREMAP_MEMREMAP_RO_VERSION     := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_VERSION)
PK_EARLY_IOREMAP_MEMREMAP_RO_SITE        := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_URL)
PK_EARLY_IOREMAP_MEMREMAP_RO_GITHUB      := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_GITHUB)
PK_EARLY_IOREMAP_MEMREMAP_RO_SUBNAME     :=
PK_EARLY_IOREMAP_MEMREMAP_RO_BUILDARCH   := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_BUILDARCH)
PK_EARLY_IOREMAP_MEMREMAP_RO_DIR         := $(srctree)/package/EARLY-IOREMAP/BiscuitOS-EARLY-IOREMAP-early_memremap_ro
PK_EARLY_IOREMAP_MEMREMAP_RO_PATCH       := $(PK_EARLY_IOREMAP_MEMREMAP_RO_DIR)/patch
PK_EARLY_IOREMAP_MEMREMAP_RO_BSBIT       := BiscuitOS-EARLY-IOREMAP-early_memremap_ro-$(PK_EARLY_IOREMAP_MEMREMAP_RO_VERSION).bsbit
PK_EARLY_IOREMAP_MEMREMAP_RO_OUT         :=
PK_EARLY_IOREMAP_MEMREMAP_RO_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_EARLY_IOREMAP_MEMREMAP_RO_TAR         := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_TAR)
PK_EARLY_IOREMAP_MEMREMAP_RO_PACKAGE     :=
PK_EARLY_IOREMAP_MEMREMAP_RO_CONFIG      := $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_CONFIG)
SUB_TARGET                  += $(PK_EARLY_IOREMAP_MEMREMAP_RO_DIR)/build

PK_EARLY_IOREMAP_MEMREMAP_RO_sh = $(CONFIG_SHELL) $(PK_EARLY_IOREMAP_MEMREMAP_RO_SCRIPTS) \
                 $1X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_RO_NAME)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_RO_VERSION)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_RO_SITE)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_RO_GITHUB)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_RO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_RO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_RO_SUBNAME)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_CONFIG)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_LIBPATH)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_CPPFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_DPKCONFIG)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_CFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_LDFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_CXXFLAGS)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_ASFLAGS)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_RO_BSBIT)X \
                 $(PK_EARLY_IOREMAP_MEMREMAP_RO_BUILDARCH)X \
		 $(PK_EARLY_IOREMAP_MEMREMAP_RO_CSRCFILE)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_SRCFILE)X \
                 $(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO_CSRCFILE)

$(PK_EARLY_IOREMAP_MEMREMAP_RO_DIR)/build: FORCE
	$(Q)$(call PK_EARLY_IOREMAP_MEMREMAP_RO_sh,$(srctree))
