EARLY-IOREMAP-$(CONFIG_PK_EARLY_IOREMAP_MEMUNMAP)	+= BiscuitOS-EARLY-IOREMAP-early_memunmap/
EARLY-IOREMAP-$(CONFIG_PK_EARLY_IOREMAP_MEMREMAP)	+= BiscuitOS-EARLY-IOREMAP-early_memremap/
EARLY-IOREMAP-$(CONFIG_PK_EARLY_IOREMAP_MEMREMAP_RO)	+= BiscuitOS-EARLY-IOREMAP-early_memremap_ro/
