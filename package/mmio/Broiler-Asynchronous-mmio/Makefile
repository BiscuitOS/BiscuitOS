PK_MMIO_ASYNC_BROILER_NAME        := Broiler-Asynchronous-mmio
PK_MMIO_ASYNC_BROILER_VERSION     := $(CONFIG_PK_MMIO_ASYNC_BROILER_VERSION)
PK_MMIO_ASYNC_BROILER_SITE        := $(CONFIG_PK_MMIO_ASYNC_BROILER_URL)
PK_MMIO_ASYNC_BROILER_GITHUB      := $(CONFIG_PK_MMIO_ASYNC_BROILER_GITHUB)
PK_MMIO_ASYNC_BROILER_SUBNAME     :=
PK_MMIO_ASYNC_BROILER_BUILDARCH   := $(CONFIG_PK_MMIO_ASYNC_BROILER_BUILDARCH)
PK_MMIO_ASYNC_BROILER_DIR         := $(srctree)/package/mmio/Broiler-Asynchronous-mmio
PK_MMIO_ASYNC_BROILER_PATCH       := $(PK_MMIO_ASYNC_BROILER_DIR)/patch
PK_MMIO_ASYNC_BROILER_BSBIT       := Broiler-Asynchronous-mmio-$(PK_MMIO_ASYNC_BROILER_VERSION).bsbit
PK_MMIO_ASYNC_BROILER_OUT         :=
PK_MMIO_ASYNC_BROILER_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MMIO_ASYNC_BROILER_TAR         := $(CONFIG_PK_MMIO_ASYNC_BROILER_TAR)
PK_MMIO_ASYNC_BROILER_PACKAGE     :=
PK_MMIO_ASYNC_BROILER_CONFIG      := $(CONFIG_PK_MMIO_ASYNC_BROILER_CONFIG)
SUB_TARGET                  += $(PK_MMIO_ASYNC_BROILER_DIR)/build

PK_MMIO_ASYNC_BROILER_sh = $(CONFIG_SHELL) $(PK_MMIO_ASYNC_BROILER_SCRIPTS) \
                 $1X \
                 $(PK_MMIO_ASYNC_BROILER_NAME)X \
                 $(PK_MMIO_ASYNC_BROILER_VERSION)X \
                 $(PK_MMIO_ASYNC_BROILER_SITE)X \
                 $(PK_MMIO_ASYNC_BROILER_GITHUB)X \
                 $(PK_MMIO_ASYNC_BROILER_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MMIO_ASYNC_BROILER_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MMIO_ASYNC_BROILER_SUBNAME)X \
                 $(CONFIG_PK_MMIO_ASYNC_BROILER_CONFIG)X \
                 $(CONFIG_PK_MMIO_ASYNC_BROILER_LIBPATH)X \
                 $(CONFIG_PK_MMIO_ASYNC_BROILER_CPPFLAGS)X \
                 $(CONFIG_PK_MMIO_ASYNC_BROILER_DPKCONFIG)X \
                 $(CONFIG_PK_MMIO_ASYNC_BROILER_CFLAGS)X \
                 $(CONFIG_PK_MMIO_ASYNC_BROILER_LDFLAGS)X \
                 $(CONFIG_PK_MMIO_ASYNC_BROILER_CXXFLAGS)X \
                 $(CONFIG_PK_MMIO_ASYNC_BROILER_ASFLAGS)X \
                 $(PK_MMIO_ASYNC_BROILER_BSBIT)X \
                 $(PK_MMIO_ASYNC_BROILER_BUILDARCH)X \
		 $(PK_MMIO_ASYNC_BROILER_CSRCFILE)X \
                 $(CONFIG_PK_MMIO_ASYNC_BROILER_SRCFILE)X \
                 $(CONFIG_PK_MMIO_ASYNC_BROILER_CSRCFILE)

$(PK_MMIO_ASYNC_BROILER_DIR)/build: FORCE
	$(Q)$(call PK_MMIO_ASYNC_BROILER_sh,$(srctree))
