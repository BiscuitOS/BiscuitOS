################################################
#
# BiscuitOS-NUMA-MPOL-PREFERRED-MANY
#
################################################
PK_NUMA_MPOL_PREFERRED_MANY_NAME        := BiscuitOS-NUMA-MPOL-PREFERRED-MANY
PK_NUMA_MPOL_PREFERRED_MANY_VERSION     := $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_VERSION)
PK_NUMA_MPOL_PREFERRED_MANY_SITE        := $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_URL)
PK_NUMA_MPOL_PREFERRED_MANY_GITHUB      := $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_GITHUB)
PK_NUMA_MPOL_PREFERRED_MANY_SUBNAME     :=
PK_NUMA_MPOL_PREFERRED_MANY_BUILDARCH   := $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_BUILDARCH)
PK_NUMA_MPOL_PREFERRED_MANY_DIR         := $(srctree)/package/NUMA/BiscuitOS-NUMA-MPOL-PREFERRED-MANY
PK_NUMA_MPOL_PREFERRED_MANY_PATCH       := $(PK_NUMA_MPOL_PREFERRED_MANY_DIR)/patch
PK_NUMA_MPOL_PREFERRED_MANY_BSBIT       := BiscuitOS-NUMA-MPOL-PREFERRED-MANY-$(PK_NUMA_MPOL_PREFERRED_MANY_VERSION).bsbit
PK_NUMA_MPOL_PREFERRED_MANY_OUT         :=
PK_NUMA_MPOL_PREFERRED_MANY_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_NUMA_MPOL_PREFERRED_MANY_TAR         := $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_TAR)
PK_NUMA_MPOL_PREFERRED_MANY_PACKAGE     :=
PK_NUMA_MPOL_PREFERRED_MANY_CONFIG      := $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_CONFIG)
SUB_TARGET                  += $(PK_NUMA_MPOL_PREFERRED_MANY_DIR)/build

PK_NUMA_MPOL_PREFERRED_MANY_sh = $(CONFIG_SHELL) $(PK_NUMA_MPOL_PREFERRED_MANY_SCRIPTS) \
                 $1X \
                 $(PK_NUMA_MPOL_PREFERRED_MANY_NAME)X \
                 $(PK_NUMA_MPOL_PREFERRED_MANY_VERSION)X \
                 $(PK_NUMA_MPOL_PREFERRED_MANY_SITE)X \
                 $(PK_NUMA_MPOL_PREFERRED_MANY_GITHUB)X \
                 $(PK_NUMA_MPOL_PREFERRED_MANY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_NUMA_MPOL_PREFERRED_MANY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_NUMA_MPOL_PREFERRED_MANY_SUBNAME)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_CONFIG)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_LIBPATH)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_CPPFLAGS)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_DPKCONFIG)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_CFLAGS)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_LDFLAGS)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_CXXFLAGS)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_ASFLAGS)X \
                 $(PK_NUMA_MPOL_PREFERRED_MANY_BSBIT)X \
                 $(PK_NUMA_MPOL_PREFERRED_MANY_BUILDARCH)X \
		 $(PK_NUMA_MPOL_PREFERRED_MANY_CSRCFILE)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_SRCFILE)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_MANY_CSRCFILE)

$(PK_NUMA_MPOL_PREFERRED_MANY_DIR)/build: FORCE
	$(Q)$(call PK_NUMA_MPOL_PREFERRED_MANY_sh,$(srctree))
