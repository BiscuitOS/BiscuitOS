PK_NUMA_MPOL_PREFERRED_NAME        := BiscuitOS-NUMA-MPOL-PREFERRED
PK_NUMA_MPOL_PREFERRED_VERSION     := $(CONFIG_PK_NUMA_MPOL_PREFERRED_VERSION)
PK_NUMA_MPOL_PREFERRED_SITE        := $(CONFIG_PK_NUMA_MPOL_PREFERRED_URL)
PK_NUMA_MPOL_PREFERRED_GITHUB      := $(CONFIG_PK_NUMA_MPOL_PREFERRED_GITHUB)
PK_NUMA_MPOL_PREFERRED_SUBNAME     :=
PK_NUMA_MPOL_PREFERRED_BUILDARCH   := $(CONFIG_PK_NUMA_MPOL_PREFERRED_BUILDARCH)
PK_NUMA_MPOL_PREFERRED_DIR         := $(srctree)/package/NUMA/BiscuitOS-NUMA-MPOL-PREFERRED
PK_NUMA_MPOL_PREFERRED_PATCH       := $(PK_NUMA_MPOL_PREFERRED_DIR)/patch
PK_NUMA_MPOL_PREFERRED_BSBIT       := BiscuitOS-NUMA-MPOL-PREFERRED-$(PK_NUMA_MPOL_PREFERRED_VERSION).bsbit
PK_NUMA_MPOL_PREFERRED_OUT         :=
PK_NUMA_MPOL_PREFERRED_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_NUMA_MPOL_PREFERRED_TAR         := $(CONFIG_PK_NUMA_MPOL_PREFERRED_TAR)
PK_NUMA_MPOL_PREFERRED_PACKAGE     :=
PK_NUMA_MPOL_PREFERRED_CONFIG      := $(CONFIG_PK_NUMA_MPOL_PREFERRED_CONFIG)
SUB_TARGET                  += $(PK_NUMA_MPOL_PREFERRED_DIR)/build

PK_NUMA_MPOL_PREFERRED_sh = $(CONFIG_SHELL) $(PK_NUMA_MPOL_PREFERRED_SCRIPTS) \
                 $1X \
                 $(PK_NUMA_MPOL_PREFERRED_NAME)X \
                 $(PK_NUMA_MPOL_PREFERRED_VERSION)X \
                 $(PK_NUMA_MPOL_PREFERRED_SITE)X \
                 $(PK_NUMA_MPOL_PREFERRED_GITHUB)X \
                 $(PK_NUMA_MPOL_PREFERRED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_NUMA_MPOL_PREFERRED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_NUMA_MPOL_PREFERRED_SUBNAME)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_CONFIG)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_LIBPATH)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_CPPFLAGS)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_DPKCONFIG)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_CFLAGS)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_LDFLAGS)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_CXXFLAGS)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_ASFLAGS)X \
                 $(PK_NUMA_MPOL_PREFERRED_BSBIT)X \
                 $(PK_NUMA_MPOL_PREFERRED_BUILDARCH)X \
		 $(PK_NUMA_MPOL_PREFERRED_CSRCFILE)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_SRCFILE)X \
                 $(CONFIG_PK_NUMA_MPOL_PREFERRED_CSRCFILE)

$(PK_NUMA_MPOL_PREFERRED_DIR)/build: FORCE
	$(Q)$(call PK_NUMA_MPOL_PREFERRED_sh,$(srctree))
