################################################
#
# BiscuitOS-NUMA-MPOL-INTERLEAVE
#
################################################
PK_NUMA_MPOL_INTERLEAVE_NAME        := BiscuitOS-NUMA-MPOL-INTERLEAVE
PK_NUMA_MPOL_INTERLEAVE_VERSION     := $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_VERSION)
PK_NUMA_MPOL_INTERLEAVE_SITE        := $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_URL)
PK_NUMA_MPOL_INTERLEAVE_GITHUB      := $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_GITHUB)
PK_NUMA_MPOL_INTERLEAVE_SUBNAME     :=
PK_NUMA_MPOL_INTERLEAVE_BUILDARCH   := $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_BUILDARCH)
PK_NUMA_MPOL_INTERLEAVE_DIR         := $(srctree)/package/NUMA/BiscuitOS-NUMA-MPOL-INTERLEAVE
PK_NUMA_MPOL_INTERLEAVE_PATCH       := $(PK_NUMA_MPOL_INTERLEAVE_DIR)/patch
PK_NUMA_MPOL_INTERLEAVE_BSBIT       := BiscuitOS-NUMA-MPOL-INTERLEAVE-$(PK_NUMA_MPOL_INTERLEAVE_VERSION).bsbit
PK_NUMA_MPOL_INTERLEAVE_OUT         :=
PK_NUMA_MPOL_INTERLEAVE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_NUMA_MPOL_INTERLEAVE_TAR         := $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_TAR)
PK_NUMA_MPOL_INTERLEAVE_PACKAGE     :=
PK_NUMA_MPOL_INTERLEAVE_CONFIG      := $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_CONFIG)
SUB_TARGET                  += $(PK_NUMA_MPOL_INTERLEAVE_DIR)/build

PK_NUMA_MPOL_INTERLEAVE_sh = $(CONFIG_SHELL) $(PK_NUMA_MPOL_INTERLEAVE_SCRIPTS) \
                 $1X \
                 $(PK_NUMA_MPOL_INTERLEAVE_NAME)X \
                 $(PK_NUMA_MPOL_INTERLEAVE_VERSION)X \
                 $(PK_NUMA_MPOL_INTERLEAVE_SITE)X \
                 $(PK_NUMA_MPOL_INTERLEAVE_GITHUB)X \
                 $(PK_NUMA_MPOL_INTERLEAVE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_NUMA_MPOL_INTERLEAVE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_NUMA_MPOL_INTERLEAVE_SUBNAME)X \
                 $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_CONFIG)X \
                 $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_LIBPATH)X \
                 $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_CPPFLAGS)X \
                 $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_DPKCONFIG)X \
                 $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_CFLAGS)X \
                 $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_LDFLAGS)X \
                 $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_CXXFLAGS)X \
                 $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_ASFLAGS)X \
                 $(PK_NUMA_MPOL_INTERLEAVE_BSBIT)X \
                 $(PK_NUMA_MPOL_INTERLEAVE_BUILDARCH)X \
		 $(PK_NUMA_MPOL_INTERLEAVE_CSRCFILE)X \
                 $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_SRCFILE)X \
                 $(CONFIG_PK_NUMA_MPOL_INTERLEAVE_CSRCFILE)

$(PK_NUMA_MPOL_INTERLEAVE_DIR)/build: FORCE
	$(Q)$(call PK_NUMA_MPOL_INTERLEAVE_sh,$(srctree))
