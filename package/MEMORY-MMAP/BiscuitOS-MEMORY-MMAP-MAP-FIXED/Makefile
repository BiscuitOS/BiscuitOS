PK_MEMORY_MMAP_MAP_FIXED_NAME        := BiscuitOS-MEMORY-MMAP-MAP-FIXED
PK_MEMORY_MMAP_MAP_FIXED_VERSION     := $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_VERSION)
PK_MEMORY_MMAP_MAP_FIXED_SITE        := $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_URL)
PK_MEMORY_MMAP_MAP_FIXED_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_GITHUB)
PK_MEMORY_MMAP_MAP_FIXED_SUBNAME     :=
PK_MEMORY_MMAP_MAP_FIXED_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_BUILDARCH)
PK_MEMORY_MMAP_MAP_FIXED_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-MAP-FIXED
PK_MEMORY_MMAP_MAP_FIXED_PATCH       := $(PK_MEMORY_MMAP_MAP_FIXED_DIR)/patch
PK_MEMORY_MMAP_MAP_FIXED_BSBIT       := BiscuitOS-MEMORY-MMAP-MAP-FIXED-$(PK_MEMORY_MMAP_MAP_FIXED_VERSION).bsbit
PK_MEMORY_MMAP_MAP_FIXED_OUT         :=
PK_MEMORY_MMAP_MAP_FIXED_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_MAP_FIXED_TAR         := $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_TAR)
PK_MEMORY_MMAP_MAP_FIXED_PACKAGE     :=
PK_MEMORY_MMAP_MAP_FIXED_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_MAP_FIXED_DIR)/build

PK_MEMORY_MMAP_MAP_FIXED_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_MAP_FIXED_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_MAP_FIXED_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_FIXED_VERSION)X \
                 $(PK_MEMORY_MMAP_MAP_FIXED_SITE)X \
                 $(PK_MEMORY_MMAP_MAP_FIXED_GITHUB)X \
                 $(PK_MEMORY_MMAP_MAP_FIXED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_FIXED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_MAP_FIXED_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_MAP_FIXED_BSBIT)X \
                 $(PK_MEMORY_MMAP_MAP_FIXED_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_MAP_FIXED_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_FIXED_CSRCFILE)

$(PK_MEMORY_MMAP_MAP_FIXED_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_MAP_FIXED_sh,$(srctree))
