PK_MEMORY_MMAP_VMA_VM_EXEC_NAME        := BiscuitOS-MEMORY-MMAP-VMA-VM-EXEC
PK_MEMORY_MMAP_VMA_VM_EXEC_VERSION     := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_VERSION)
PK_MEMORY_MMAP_VMA_VM_EXEC_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_URL)
PK_MEMORY_MMAP_VMA_VM_EXEC_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_GITHUB)
PK_MEMORY_MMAP_VMA_VM_EXEC_SUBNAME     :=
PK_MEMORY_MMAP_VMA_VM_EXEC_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_BUILDARCH)
PK_MEMORY_MMAP_VMA_VM_EXEC_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMA-VM-EXEC
PK_MEMORY_MMAP_VMA_VM_EXEC_PATCH       := $(PK_MEMORY_MMAP_VMA_VM_EXEC_DIR)/patch
PK_MEMORY_MMAP_VMA_VM_EXEC_BSBIT       := BiscuitOS-MEMORY-MMAP-VMA-VM-EXEC-$(PK_MEMORY_MMAP_VMA_VM_EXEC_VERSION).bsbit
PK_MEMORY_MMAP_VMA_VM_EXEC_OUT         :=
PK_MEMORY_MMAP_VMA_VM_EXEC_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_VMA_VM_EXEC_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_TAR)
PK_MEMORY_MMAP_VMA_VM_EXEC_PACKAGE     :=
PK_MEMORY_MMAP_VMA_VM_EXEC_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMA_VM_EXEC_DIR)/build

PK_MEMORY_MMAP_VMA_VM_EXEC_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMA_VM_EXEC_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMA_VM_EXEC_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_EXEC_VERSION)X \
                 $(PK_MEMORY_MMAP_VMA_VM_EXEC_SITE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_EXEC_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMA_VM_EXEC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_EXEC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_EXEC_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMA_VM_EXEC_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMA_VM_EXEC_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMA_VM_EXEC_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_EXEC_CSRCFILE)

$(PK_MEMORY_MMAP_VMA_VM_EXEC_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMA_VM_EXEC_sh,$(srctree))
