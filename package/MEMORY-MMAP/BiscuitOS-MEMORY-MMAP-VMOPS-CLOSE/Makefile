PK_MEMORY_MMAP_VMOPS_CLOSE_NAME        := BiscuitOS-MEMORY-MMAP-VMOPS-CLOSE
PK_MEMORY_MMAP_VMOPS_CLOSE_VERSION     := $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_VERSION)
PK_MEMORY_MMAP_VMOPS_CLOSE_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_URL)
PK_MEMORY_MMAP_VMOPS_CLOSE_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_GITHUB)
PK_MEMORY_MMAP_VMOPS_CLOSE_SUBNAME     :=
PK_MEMORY_MMAP_VMOPS_CLOSE_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_BUILDARCH)
PK_MEMORY_MMAP_VMOPS_CLOSE_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMOPS-CLOSE
PK_MEMORY_MMAP_VMOPS_CLOSE_PATCH       := $(PK_MEMORY_MMAP_VMOPS_CLOSE_DIR)/patch
PK_MEMORY_MMAP_VMOPS_CLOSE_BSBIT       := BiscuitOS-MEMORY-MMAP-VMOPS-CLOSE-$(PK_MEMORY_MMAP_VMOPS_CLOSE_VERSION).bsbit
PK_MEMORY_MMAP_VMOPS_CLOSE_OUT         :=
PK_MEMORY_MMAP_VMOPS_CLOSE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMORY_MMAP_VMOPS_CLOSE_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_TAR)
PK_MEMORY_MMAP_VMOPS_CLOSE_PACKAGE     :=
PK_MEMORY_MMAP_VMOPS_CLOSE_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMOPS_CLOSE_DIR)/build

PK_MEMORY_MMAP_VMOPS_CLOSE_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMOPS_CLOSE_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMOPS_CLOSE_NAME)X \
                 $(PK_MEMORY_MMAP_VMOPS_CLOSE_VERSION)X \
                 $(PK_MEMORY_MMAP_VMOPS_CLOSE_SITE)X \
                 $(PK_MEMORY_MMAP_VMOPS_CLOSE_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMOPS_CLOSE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMOPS_CLOSE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMOPS_CLOSE_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMOPS_CLOSE_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMOPS_CLOSE_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMOPS_CLOSE_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_CLOSE_CSRCFILE)

$(PK_MEMORY_MMAP_VMOPS_CLOSE_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMOPS_CLOSE_sh,$(srctree))
