PK_MEMORY_MMAP_VMA_VM_NEXT_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_URL)
PK_MEMORY_MMAP_VMA_VM_NEXT_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_GITHUB)
PK_MEMORY_MMAP_VMA_VM_NEXT_SUBNAME     :=
PK_MEMORY_MMAP_VMA_VM_NEXT_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_BUILDARCH)
PK_MEMORY_MMAP_VMA_VM_NEXT_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMA-VM-NEXT
PK_MEMORY_MMAP_VMA_VM_NEXT_PATCH       := $(PK_MEMORY_MMAP_VMA_VM_NEXT_DIR)/patch
PK_MEMORY_MMAP_VMA_VM_NEXT_BSBIT       := BiscuitOS-MEMORY-MMAP-VMA-VM-NEXT-$(PK_MEMORY_MMAP_VMA_VM_NEXT_VERSION).bsbit
PK_MEMORY_MMAP_VMA_VM_NEXT_OUT         :=
PK_MEMORY_MMAP_VMA_VM_NEXT_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMORY_MMAP_VMA_VM_NEXT_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_TAR)
PK_MEMORY_MMAP_VMA_VM_NEXT_PACKAGE     :=
PK_MEMORY_MMAP_VMA_VM_NEXT_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMA_VM_NEXT_DIR)/build

PK_MEMORY_MMAP_VMA_VM_NEXT_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMA_VM_NEXT_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMA_VM_NEXT_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NEXT_VERSION)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NEXT_SITE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NEXT_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NEXT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NEXT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NEXT_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NEXT_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NEXT_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMA_VM_NEXT_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NEXT_CSRCFILE)

$(PK_MEMORY_MMAP_VMA_VM_NEXT_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMA_VM_NEXT_sh,$(srctree))
