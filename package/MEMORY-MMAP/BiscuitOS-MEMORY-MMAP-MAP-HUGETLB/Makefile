PK_MEMORY_MMAP_MAP_HUGETLB_NAME        := BiscuitOS-MEMORY-MMAP-MAP-HUGETLB
PK_MEMORY_MMAP_MAP_HUGETLB_VERSION     := $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_VERSION)
PK_MEMORY_MMAP_MAP_HUGETLB_SITE        := $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_URL)
PK_MEMORY_MMAP_MAP_HUGETLB_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_GITHUB)
PK_MEMORY_MMAP_MAP_HUGETLB_SUBNAME     :=
PK_MEMORY_MMAP_MAP_HUGETLB_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_BUILDARCH)
PK_MEMORY_MMAP_MAP_HUGETLB_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-MAP-HUGETLB
PK_MEMORY_MMAP_MAP_HUGETLB_PATCH       := $(PK_MEMORY_MMAP_MAP_HUGETLB_DIR)/patch
PK_MEMORY_MMAP_MAP_HUGETLB_BSBIT       := BiscuitOS-MEMORY-MMAP-MAP-HUGETLB-$(PK_MEMORY_MMAP_MAP_HUGETLB_VERSION).bsbit
PK_MEMORY_MMAP_MAP_HUGETLB_OUT         :=
PK_MEMORY_MMAP_MAP_HUGETLB_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_MAP_HUGETLB_TAR         := $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_TAR)
PK_MEMORY_MMAP_MAP_HUGETLB_PACKAGE     :=
PK_MEMORY_MMAP_MAP_HUGETLB_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_MAP_HUGETLB_DIR)/build

PK_MEMORY_MMAP_MAP_HUGETLB_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_MAP_HUGETLB_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_MAP_HUGETLB_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_HUGETLB_VERSION)X \
                 $(PK_MEMORY_MMAP_MAP_HUGETLB_SITE)X \
                 $(PK_MEMORY_MMAP_MAP_HUGETLB_GITHUB)X \
                 $(PK_MEMORY_MMAP_MAP_HUGETLB_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_HUGETLB_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_MAP_HUGETLB_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_MAP_HUGETLB_BSBIT)X \
                 $(PK_MEMORY_MMAP_MAP_HUGETLB_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_MAP_HUGETLB_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_HUGETLB_CSRCFILE)

$(PK_MEMORY_MMAP_MAP_HUGETLB_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_MAP_HUGETLB_sh,$(srctree))
