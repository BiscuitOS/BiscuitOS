PK_MEMMAP_INFO_NUMA_NODE_NAME        := BiscuitOS-MEMMAP-INFO-NUMA-NODE
PK_MEMMAP_INFO_NUMA_NODE_VERSION     := $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_VERSION)
PK_MEMMAP_INFO_NUMA_NODE_SITE        := $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_URL)
PK_MEMMAP_INFO_NUMA_NODE_GITHUB      := $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_GITHUB)
PK_MEMMAP_INFO_NUMA_NODE_SUBNAME     :=
PK_MEMMAP_INFO_NUMA_NODE_BUILDARCH   := $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_BUILDARCH)
PK_MEMMAP_INFO_NUMA_NODE_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMMAP-INFO-NUMA-NODE
PK_MEMMAP_INFO_NUMA_NODE_PATCH       := $(PK_MEMMAP_INFO_NUMA_NODE_DIR)/patch
PK_MEMMAP_INFO_NUMA_NODE_BSBIT       := BiscuitOS-MEMMAP-INFO-NUMA-NODE-$(PK_MEMMAP_INFO_NUMA_NODE_VERSION).bsbit
PK_MEMMAP_INFO_NUMA_NODE_OUT         :=
PK_MEMMAP_INFO_NUMA_NODE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMMAP_INFO_NUMA_NODE_TAR         := $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_TAR)
PK_MEMMAP_INFO_NUMA_NODE_PACKAGE     :=
PK_MEMMAP_INFO_NUMA_NODE_CONFIG      := $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_CONFIG)
SUB_TARGET                  += $(PK_MEMMAP_INFO_NUMA_NODE_DIR)/build

PK_MEMMAP_INFO_NUMA_NODE_sh = $(CONFIG_SHELL) $(PK_MEMMAP_INFO_NUMA_NODE_SCRIPTS) \
                 $1X \
                 $(PK_MEMMAP_INFO_NUMA_NODE_NAME)X \
                 $(PK_MEMMAP_INFO_NUMA_NODE_VERSION)X \
                 $(PK_MEMMAP_INFO_NUMA_NODE_SITE)X \
                 $(PK_MEMMAP_INFO_NUMA_NODE_GITHUB)X \
                 $(PK_MEMMAP_INFO_NUMA_NODE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMMAP_INFO_NUMA_NODE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMMAP_INFO_NUMA_NODE_SUBNAME)X \
                 $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_CONFIG)X \
                 $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_LIBPATH)X \
                 $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_CFLAGS)X \
                 $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_LDFLAGS)X \
                 $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_ASFLAGS)X \
                 $(PK_MEMMAP_INFO_NUMA_NODE_BSBIT)X \
                 $(PK_MEMMAP_INFO_NUMA_NODE_BUILDARCH)X \
		 $(PK_MEMMAP_INFO_NUMA_NODE_CSRCFILE)X \
                 $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_SRCFILE)X \
                 $(CONFIG_PK_MEMMAP_INFO_NUMA_NODE_CSRCFILE)

$(PK_MEMMAP_INFO_NUMA_NODE_DIR)/build: FORCE
	$(Q)$(call PK_MEMMAP_INFO_NUMA_NODE_sh,$(srctree))
