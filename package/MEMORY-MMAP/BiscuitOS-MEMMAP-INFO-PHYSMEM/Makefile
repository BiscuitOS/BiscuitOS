################################################
PK_MEMMAP_INFO_PHYSMEM_NAME        := BiscuitOS-MEMMAP-INFO-PHYSMEM
PK_MEMMAP_INFO_PHYSMEM_VERSION     := $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_VERSION)
PK_MEMMAP_INFO_PHYSMEM_SITE        := $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_URL)
PK_MEMMAP_INFO_PHYSMEM_GITHUB      := $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_GITHUB)
PK_MEMMAP_INFO_PHYSMEM_SUBNAME     :=
PK_MEMMAP_INFO_PHYSMEM_BUILDARCH   := $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_BUILDARCH)
PK_MEMMAP_INFO_PHYSMEM_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMMAP-INFO-PHYSMEM
PK_MEMMAP_INFO_PHYSMEM_PATCH       := $(PK_MEMMAP_INFO_PHYSMEM_DIR)/patch
PK_MEMMAP_INFO_PHYSMEM_BSBIT       := BiscuitOS-MEMMAP-INFO-PHYSMEM-$(PK_MEMMAP_INFO_PHYSMEM_VERSION).bsbit
PK_MEMMAP_INFO_PHYSMEM_OUT         :=
PK_MEMMAP_INFO_PHYSMEM_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMMAP_INFO_PHYSMEM_TAR         := $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_TAR)
PK_MEMMAP_INFO_PHYSMEM_PACKAGE     :=
PK_MEMMAP_INFO_PHYSMEM_CONFIG      := $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_CONFIG)
SUB_TARGET                  += $(PK_MEMMAP_INFO_PHYSMEM_DIR)/build

PK_MEMMAP_INFO_PHYSMEM_sh = $(CONFIG_SHELL) $(PK_MEMMAP_INFO_PHYSMEM_SCRIPTS) \
                 $1X \
                 $(PK_MEMMAP_INFO_PHYSMEM_NAME)X \
                 $(PK_MEMMAP_INFO_PHYSMEM_VERSION)X \
                 $(PK_MEMMAP_INFO_PHYSMEM_SITE)X \
                 $(PK_MEMMAP_INFO_PHYSMEM_GITHUB)X \
                 $(PK_MEMMAP_INFO_PHYSMEM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMMAP_INFO_PHYSMEM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMMAP_INFO_PHYSMEM_SUBNAME)X \
                 $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_CONFIG)X \
                 $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_LIBPATH)X \
                 $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_CPPFLAGS)X \
                 $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_DPKCONFIG)X \
                 $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_CFLAGS)X \
                 $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_LDFLAGS)X \
                 $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_CXXFLAGS)X \
                 $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_ASFLAGS)X \
                 $(PK_MEMMAP_INFO_PHYSMEM_BSBIT)X \
                 $(PK_MEMMAP_INFO_PHYSMEM_BUILDARCH)X \
		 $(PK_MEMMAP_INFO_PHYSMEM_CSRCFILE)X \
                 $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_SRCFILE)X \
                 $(CONFIG_PK_MEMMAP_INFO_PHYSMEM_CSRCFILE)

$(PK_MEMMAP_INFO_PHYSMEM_DIR)/build: FORCE
	$(Q)$(call PK_MEMMAP_INFO_PHYSMEM_sh,$(srctree))
