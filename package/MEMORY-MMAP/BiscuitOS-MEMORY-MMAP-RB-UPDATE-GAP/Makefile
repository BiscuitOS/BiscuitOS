                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_RB_UPDATE_GAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_RB_UPDATE_GAP_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_RB_UPDATE_GAP_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_RB_UPDATE_GAP_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_RB_UPDATE_GAP_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_RB_UPDATE_GAP_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_RB_UPDATE_GAP_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_RB_UPDATE_GAP_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_RB_UPDATE_GAP_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_RB_UPDATE_GAP_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_RB_UPDATE_GAP_BSBIT)X \
                 $(PK_MEMORY_MMAP_RB_UPDATE_GAP_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_RB_UPDATE_GAP_CSRCFILE)X \
