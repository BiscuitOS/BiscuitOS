PK_MEMORY_MMAP_RB_UPDATE_GAP_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_RB_UPDATE_GAP_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_RB_UPDATE_GAP_DIR)/build

PK_MEMORY_MMAP_RB_UPDATE_GAP_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_RB_UPDATE_GAP_SCRIPTS) \
                 $1X \
