################################################
#
# BiscuitOS-MEMORY-MMAP-SPECIAL-MAPPING
#
################################################
PK_MEMORY_MMAP_SPECIAL_MAPPING_NAME        := BiscuitOS-MEMORY-MMAP-SPECIAL-MAPPING
PK_MEMORY_MMAP_SPECIAL_MAPPING_VERSION     := $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_VERSION)
PK_MEMORY_MMAP_SPECIAL_MAPPING_SITE        := $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_URL)
PK_MEMORY_MMAP_SPECIAL_MAPPING_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_GITHUB)
PK_MEMORY_MMAP_SPECIAL_MAPPING_SUBNAME     :=
PK_MEMORY_MMAP_SPECIAL_MAPPING_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_BUILDARCH)
PK_MEMORY_MMAP_SPECIAL_MAPPING_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-SPECIAL-MAPPING
PK_MEMORY_MMAP_SPECIAL_MAPPING_PATCH       := $(PK_MEMORY_MMAP_SPECIAL_MAPPING_DIR)/patch
PK_MEMORY_MMAP_SPECIAL_MAPPING_BSBIT       := BiscuitOS-MEMORY-MMAP-SPECIAL-MAPPING-$(PK_MEMORY_MMAP_SPECIAL_MAPPING_VERSION).bsbit
PK_MEMORY_MMAP_SPECIAL_MAPPING_OUT         :=
PK_MEMORY_MMAP_SPECIAL_MAPPING_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_MEMORY_MMAP_SPECIAL_MAPPING_TAR         := $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_TAR)
PK_MEMORY_MMAP_SPECIAL_MAPPING_PACKAGE     :=
PK_MEMORY_MMAP_SPECIAL_MAPPING_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_SPECIAL_MAPPING_DIR)/build

PK_MEMORY_MMAP_SPECIAL_MAPPING_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_SPECIAL_MAPPING_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_NAME)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_VERSION)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_SITE)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_GITHUB)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_BSBIT)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_CSRCFILE)

$(PK_MEMORY_MMAP_SPECIAL_MAPPING_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_SPECIAL_MAPPING_sh,$(srctree))
