PK_MEMORY_GUP_LOOGTERM_NAME        := BiscuitOS-MEMORY-GUP-LOOGTERM
PK_MEMORY_GUP_LOOGTERM_VERSION     := $(CONFIG_PK_MEMORY_GUP_LOOGTERM_VERSION)
PK_MEMORY_GUP_LOOGTERM_SITE        := $(CONFIG_PK_MEMORY_GUP_LOOGTERM_URL)
PK_MEMORY_GUP_LOOGTERM_GITHUB      := $(CONFIG_PK_MEMORY_GUP_LOOGTERM_GITHUB)
PK_MEMORY_GUP_LOOGTERM_SUBNAME     :=
PK_MEMORY_GUP_LOOGTERM_BUILDARCH   := $(CONFIG_PK_MEMORY_GUP_LOOGTERM_BUILDARCH)
PK_MEMORY_GUP_LOOGTERM_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-GUP-LOOGTERM
PK_MEMORY_GUP_LOOGTERM_PATCH       := $(PK_MEMORY_GUP_LOOGTERM_DIR)/patch
PK_MEMORY_GUP_LOOGTERM_BSBIT       := BiscuitOS-MEMORY-GUP-LOOGTERM-$(PK_MEMORY_GUP_LOOGTERM_VERSION).bsbit
PK_MEMORY_GUP_LOOGTERM_OUT         :=
PK_MEMORY_GUP_LOOGTERM_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMORY_GUP_LOOGTERM_TAR         := $(CONFIG_PK_MEMORY_GUP_LOOGTERM_TAR)
PK_MEMORY_GUP_LOOGTERM_PACKAGE     :=
PK_MEMORY_GUP_LOOGTERM_CONFIG      := $(CONFIG_PK_MEMORY_GUP_LOOGTERM_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_GUP_LOOGTERM_DIR)/build

PK_MEMORY_GUP_LOOGTERM_sh = $(CONFIG_SHELL) $(PK_MEMORY_GUP_LOOGTERM_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_GUP_LOOGTERM_NAME)X \
                 $(PK_MEMORY_GUP_LOOGTERM_VERSION)X \
                 $(PK_MEMORY_GUP_LOOGTERM_SITE)X \
                 $(PK_MEMORY_GUP_LOOGTERM_GITHUB)X \
                 $(PK_MEMORY_GUP_LOOGTERM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_GUP_LOOGTERM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_GUP_LOOGTERM_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_GUP_LOOGTERM_CONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_LOOGTERM_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_GUP_LOOGTERM_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_LOOGTERM_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_LOOGTERM_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_LOOGTERM_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_LOOGTERM_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_LOOGTERM_ASFLAGS)X \
                 $(PK_MEMORY_GUP_LOOGTERM_BSBIT)X \
                 $(PK_MEMORY_GUP_LOOGTERM_BUILDARCH)X \
		 $(PK_MEMORY_GUP_LOOGTERM_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_LOOGTERM_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_LOOGTERM_CSRCFILE)

$(PK_MEMORY_GUP_LOOGTERM_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_GUP_LOOGTERM_sh,$(srctree))
