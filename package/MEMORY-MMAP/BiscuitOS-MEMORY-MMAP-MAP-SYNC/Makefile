PK_MEMORY_MMAP_MAP_SYNC_NAME        := BiscuitOS-MEMORY-MMAP-MAP-SYNC
PK_MEMORY_MMAP_MAP_SYNC_VERSION     := $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_VERSION)
PK_MEMORY_MMAP_MAP_SYNC_SITE        := $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_URL)
PK_MEMORY_MMAP_MAP_SYNC_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_GITHUB)
PK_MEMORY_MMAP_MAP_SYNC_SUBNAME     :=
PK_MEMORY_MMAP_MAP_SYNC_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_BUILDARCH)
PK_MEMORY_MMAP_MAP_SYNC_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-MAP-SYNC
PK_MEMORY_MMAP_MAP_SYNC_PATCH       := $(PK_MEMORY_MMAP_MAP_SYNC_DIR)/patch
PK_MEMORY_MMAP_MAP_SYNC_BSBIT       := BiscuitOS-MEMORY-MMAP-MAP-SYNC-$(PK_MEMORY_MMAP_MAP_SYNC_VERSION).bsbit
PK_MEMORY_MMAP_MAP_SYNC_OUT         :=
PK_MEMORY_MMAP_MAP_SYNC_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_MAP_SYNC_TAR         := $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_TAR)
PK_MEMORY_MMAP_MAP_SYNC_PACKAGE     :=
PK_MEMORY_MMAP_MAP_SYNC_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_MAP_SYNC_DIR)/build

PK_MEMORY_MMAP_MAP_SYNC_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_MAP_SYNC_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_MAP_SYNC_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_SYNC_VERSION)X \
                 $(PK_MEMORY_MMAP_MAP_SYNC_SITE)X \
                 $(PK_MEMORY_MMAP_MAP_SYNC_GITHUB)X \
                 $(PK_MEMORY_MMAP_MAP_SYNC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_SYNC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_MAP_SYNC_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_MAP_SYNC_BSBIT)X \
                 $(PK_MEMORY_MMAP_MAP_SYNC_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_MAP_SYNC_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SYNC_CSRCFILE)

$(PK_MEMORY_MMAP_MAP_SYNC_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_MAP_SYNC_sh,$(srctree))
