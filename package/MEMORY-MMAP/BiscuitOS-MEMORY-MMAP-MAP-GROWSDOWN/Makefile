PK_MEMORY_MMAP_MAP_GROWSDOWN_NAME        := BiscuitOS-MEMORY-MMAP-MAP-GROWSDOWN
PK_MEMORY_MMAP_MAP_GROWSDOWN_VERSION     := $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_VERSION)
PK_MEMORY_MMAP_MAP_GROWSDOWN_SITE        := $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_URL)
PK_MEMORY_MMAP_MAP_GROWSDOWN_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_GITHUB)
PK_MEMORY_MMAP_MAP_GROWSDOWN_SUBNAME     :=
PK_MEMORY_MMAP_MAP_GROWSDOWN_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_BUILDARCH)
PK_MEMORY_MMAP_MAP_GROWSDOWN_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-MAP-GROWSDOWN
PK_MEMORY_MMAP_MAP_GROWSDOWN_PATCH       := $(PK_MEMORY_MMAP_MAP_GROWSDOWN_DIR)/patch
PK_MEMORY_MMAP_MAP_GROWSDOWN_BSBIT       := BiscuitOS-MEMORY-MMAP-MAP-GROWSDOWN-$(PK_MEMORY_MMAP_MAP_GROWSDOWN_VERSION).bsbit
PK_MEMORY_MMAP_MAP_GROWSDOWN_OUT         :=
PK_MEMORY_MMAP_MAP_GROWSDOWN_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_MAP_GROWSDOWN_TAR         := $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_TAR)
PK_MEMORY_MMAP_MAP_GROWSDOWN_PACKAGE     :=
PK_MEMORY_MMAP_MAP_GROWSDOWN_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_MAP_GROWSDOWN_DIR)/build

PK_MEMORY_MMAP_MAP_GROWSDOWN_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_MAP_GROWSDOWN_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_MAP_GROWSDOWN_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_GROWSDOWN_VERSION)X \
                 $(PK_MEMORY_MMAP_MAP_GROWSDOWN_SITE)X \
                 $(PK_MEMORY_MMAP_MAP_GROWSDOWN_GITHUB)X \
                 $(PK_MEMORY_MMAP_MAP_GROWSDOWN_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_GROWSDOWN_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_MAP_GROWSDOWN_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_MAP_GROWSDOWN_BSBIT)X \
                 $(PK_MEMORY_MMAP_MAP_GROWSDOWN_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_MAP_GROWSDOWN_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_GROWSDOWN_CSRCFILE)

$(PK_MEMORY_MMAP_MAP_GROWSDOWN_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_MAP_GROWSDOWN_sh,$(srctree))
