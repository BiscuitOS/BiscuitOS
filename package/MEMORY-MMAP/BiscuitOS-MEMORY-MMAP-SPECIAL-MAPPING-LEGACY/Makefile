################################################
#
# BiscuitOS-MEMORY-MMAP-SPECIAL-MAPPING-LEGACY
#
################################################
PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_NAME        := BiscuitOS-MEMORY-MMAP-SPECIAL-MAPPING-LEGACY
PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_VERSION     := $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_VERSION)
PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_SITE        := $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_URL)
PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_GITHUB)
PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_SUBNAME     :=
PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_BUILDARCH)
PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-SPECIAL-MAPPING-LEGACY
PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_PATCH       := $(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_DIR)/patch
PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_BSBIT       := BiscuitOS-MEMORY-MMAP-SPECIAL-MAPPING-LEGACY-$(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_VERSION).bsbit
PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_OUT         :=
PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_TAR         := $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_TAR)
PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_PACKAGE     :=
PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_DIR)/build

PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_NAME)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_VERSION)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_SITE)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_GITHUB)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_BSBIT)X \
                 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_CSRCFILE)

$(PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_SPECIAL_MAPPING_LEGACY_sh,$(srctree))
