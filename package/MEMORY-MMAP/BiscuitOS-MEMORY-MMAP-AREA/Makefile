# BiscuitOS-MEMORY-MMAP-AREA
#
################################################
PK_MEMORY_MMAP_AREA_NAME        := BiscuitOS-MEMORY-MMAP-AREA
PK_MEMORY_MMAP_AREA_VERSION     := $(CONFIG_PK_MEMORY_MMAP_AREA_VERSION)
PK_MEMORY_MMAP_AREA_SITE        := $(CONFIG_PK_MEMORY_MMAP_AREA_URL)
PK_MEMORY_MMAP_AREA_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_AREA_GITHUB)
PK_MEMORY_MMAP_AREA_SUBNAME     :=
PK_MEMORY_MMAP_AREA_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_AREA_BUILDARCH)
PK_MEMORY_MMAP_AREA_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-AREA
PK_MEMORY_MMAP_AREA_PATCH       := $(PK_MEMORY_MMAP_AREA_DIR)/patch
PK_MEMORY_MMAP_AREA_BSBIT       := BiscuitOS-MEMORY-MMAP-AREA-$(PK_MEMORY_MMAP_AREA_VERSION).bsbit
PK_MEMORY_MMAP_AREA_OUT         :=
PK_MEMORY_MMAP_AREA_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_AREA_TAR         := $(CONFIG_PK_MEMORY_MMAP_AREA_TAR)
PK_MEMORY_MMAP_AREA_PACKAGE     :=
PK_MEMORY_MMAP_AREA_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_AREA_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_AREA_DIR)/build

PK_MEMORY_MMAP_AREA_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_AREA_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_AREA_NAME)X \
                 $(PK_MEMORY_MMAP_AREA_VERSION)X \
                 $(PK_MEMORY_MMAP_AREA_SITE)X \
                 $(PK_MEMORY_MMAP_AREA_GITHUB)X \
                 $(PK_MEMORY_MMAP_AREA_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_AREA_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_AREA_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_AREA_BSBIT)X \
                 $(PK_MEMORY_MMAP_AREA_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_AREA_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_CSRCFILE)

$(PK_MEMORY_MMAP_AREA_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_AREA_sh,$(srctree))
