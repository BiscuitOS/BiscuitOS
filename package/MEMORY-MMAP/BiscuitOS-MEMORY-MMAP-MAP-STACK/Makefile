PK_MEMORY_MMAP_MAP_STACK_NAME        := BiscuitOS-MEMORY-MMAP-MAP-STACK
PK_MEMORY_MMAP_MAP_STACK_VERSION     := $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_VERSION)
PK_MEMORY_MMAP_MAP_STACK_SITE        := $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_URL)
PK_MEMORY_MMAP_MAP_STACK_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_GITHUB)
PK_MEMORY_MMAP_MAP_STACK_SUBNAME     :=
PK_MEMORY_MMAP_MAP_STACK_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_BUILDARCH)
PK_MEMORY_MMAP_MAP_STACK_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-MAP-STACK
PK_MEMORY_MMAP_MAP_STACK_PATCH       := $(PK_MEMORY_MMAP_MAP_STACK_DIR)/patch
PK_MEMORY_MMAP_MAP_STACK_BSBIT       := BiscuitOS-MEMORY-MMAP-MAP-STACK-$(PK_MEMORY_MMAP_MAP_STACK_VERSION).bsbit
PK_MEMORY_MMAP_MAP_STACK_OUT         :=
PK_MEMORY_MMAP_MAP_STACK_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_MAP_STACK_TAR         := $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_TAR)
PK_MEMORY_MMAP_MAP_STACK_PACKAGE     :=
PK_MEMORY_MMAP_MAP_STACK_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_MAP_STACK_DIR)/build

PK_MEMORY_MMAP_MAP_STACK_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_MAP_STACK_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_MAP_STACK_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_STACK_VERSION)X \
                 $(PK_MEMORY_MMAP_MAP_STACK_SITE)X \
                 $(PK_MEMORY_MMAP_MAP_STACK_GITHUB)X \
                 $(PK_MEMORY_MMAP_MAP_STACK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_STACK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_MAP_STACK_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_MAP_STACK_BSBIT)X \
                 $(PK_MEMORY_MMAP_MAP_STACK_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_MAP_STACK_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_STACK_CSRCFILE)

$(PK_MEMORY_MMAP_MAP_STACK_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_MAP_STACK_sh,$(srctree))
