PK_MEMORY_GUP_FOLL_WRITE_NAME        := BiscuitOS-MEMORY-GUP-FOLL-WRITE
PK_MEMORY_GUP_FOLL_WRITE_VERSION     := $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_VERSION)
PK_MEMORY_GUP_FOLL_WRITE_SITE        := $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_URL)
PK_MEMORY_GUP_FOLL_WRITE_GITHUB      := $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_GITHUB)
PK_MEMORY_GUP_FOLL_WRITE_SUBNAME     :=
PK_MEMORY_GUP_FOLL_WRITE_BUILDARCH   := $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_BUILDARCH)
PK_MEMORY_GUP_FOLL_WRITE_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-GUP-FOLL-WRITE
PK_MEMORY_GUP_FOLL_WRITE_PATCH       := $(PK_MEMORY_GUP_FOLL_WRITE_DIR)/patch
PK_MEMORY_GUP_FOLL_WRITE_BSBIT       := BiscuitOS-MEMORY-GUP-FOLL-WRITE-$(PK_MEMORY_GUP_FOLL_WRITE_VERSION).bsbit
PK_MEMORY_GUP_FOLL_WRITE_OUT         :=
PK_MEMORY_GUP_FOLL_WRITE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMORY_GUP_FOLL_WRITE_TAR         := $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_TAR)
PK_MEMORY_GUP_FOLL_WRITE_PACKAGE     :=
PK_MEMORY_GUP_FOLL_WRITE_CONFIG      := $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_GUP_FOLL_WRITE_DIR)/build

PK_MEMORY_GUP_FOLL_WRITE_sh = $(CONFIG_SHELL) $(PK_MEMORY_GUP_FOLL_WRITE_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_GUP_FOLL_WRITE_NAME)X \
                 $(PK_MEMORY_GUP_FOLL_WRITE_VERSION)X \
                 $(PK_MEMORY_GUP_FOLL_WRITE_SITE)X \
                 $(PK_MEMORY_GUP_FOLL_WRITE_GITHUB)X \
                 $(PK_MEMORY_GUP_FOLL_WRITE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_GUP_FOLL_WRITE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_GUP_FOLL_WRITE_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_CONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_ASFLAGS)X \
                 $(PK_MEMORY_GUP_FOLL_WRITE_BSBIT)X \
                 $(PK_MEMORY_GUP_FOLL_WRITE_BUILDARCH)X \
		 $(PK_MEMORY_GUP_FOLL_WRITE_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_WRITE_CSRCFILE)

$(PK_MEMORY_GUP_FOLL_WRITE_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_GUP_FOLL_WRITE_sh,$(srctree))
