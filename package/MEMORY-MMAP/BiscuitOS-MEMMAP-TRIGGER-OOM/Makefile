PK_MEMMAP_TRIGGER_OOM_NAME        := BiscuitOS-MEMMAP-TRIGGER-OOM
PK_MEMMAP_TRIGGER_OOM_VERSION     := $(CONFIG_PK_MEMMAP_TRIGGER_OOM_VERSION)
PK_MEMMAP_TRIGGER_OOM_SITE        := $(CONFIG_PK_MEMMAP_TRIGGER_OOM_URL)
PK_MEMMAP_TRIGGER_OOM_GITHUB      := $(CONFIG_PK_MEMMAP_TRIGGER_OOM_GITHUB)
PK_MEMMAP_TRIGGER_OOM_SUBNAME     :=
PK_MEMMAP_TRIGGER_OOM_BUILDARCH   := $(CONFIG_PK_MEMMAP_TRIGGER_OOM_BUILDARCH)
PK_MEMMAP_TRIGGER_OOM_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMMAP-TRIGGER-OOM
PK_MEMMAP_TRIGGER_OOM_PATCH       := $(PK_MEMMAP_TRIGGER_OOM_DIR)/patch
PK_MEMMAP_TRIGGER_OOM_BSBIT       := BiscuitOS-MEMMAP-TRIGGER-OOM-$(PK_MEMMAP_TRIGGER_OOM_VERSION).bsbit
PK_MEMMAP_TRIGGER_OOM_OUT         :=
PK_MEMMAP_TRIGGER_OOM_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMMAP_TRIGGER_OOM_TAR         := $(CONFIG_PK_MEMMAP_TRIGGER_OOM_TAR)
PK_MEMMAP_TRIGGER_OOM_PACKAGE     :=
PK_MEMMAP_TRIGGER_OOM_CONFIG      := $(CONFIG_PK_MEMMAP_TRIGGER_OOM_CONFIG)
SUB_TARGET                  += $(PK_MEMMAP_TRIGGER_OOM_DIR)/build

PK_MEMMAP_TRIGGER_OOM_sh = $(CONFIG_SHELL) $(PK_MEMMAP_TRIGGER_OOM_SCRIPTS) \
                 $1X \
                 $(PK_MEMMAP_TRIGGER_OOM_NAME)X \
                 $(PK_MEMMAP_TRIGGER_OOM_VERSION)X \
                 $(PK_MEMMAP_TRIGGER_OOM_SITE)X \
                 $(PK_MEMMAP_TRIGGER_OOM_GITHUB)X \
                 $(PK_MEMMAP_TRIGGER_OOM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMMAP_TRIGGER_OOM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMMAP_TRIGGER_OOM_SUBNAME)X \
                 $(CONFIG_PK_MEMMAP_TRIGGER_OOM_CONFIG)X \
                 $(CONFIG_PK_MEMMAP_TRIGGER_OOM_LIBPATH)X \
                 $(CONFIG_PK_MEMMAP_TRIGGER_OOM_CPPFLAGS)X \
                 $(CONFIG_PK_MEMMAP_TRIGGER_OOM_DPKCONFIG)X \
                 $(CONFIG_PK_MEMMAP_TRIGGER_OOM_CFLAGS)X \
                 $(CONFIG_PK_MEMMAP_TRIGGER_OOM_LDFLAGS)X \
                 $(CONFIG_PK_MEMMAP_TRIGGER_OOM_CXXFLAGS)X \
                 $(CONFIG_PK_MEMMAP_TRIGGER_OOM_ASFLAGS)X \
                 $(PK_MEMMAP_TRIGGER_OOM_BSBIT)X \
                 $(PK_MEMMAP_TRIGGER_OOM_BUILDARCH)X \
		 $(PK_MEMMAP_TRIGGER_OOM_CSRCFILE)X \
                 $(CONFIG_PK_MEMMAP_TRIGGER_OOM_SRCFILE)X \
                 $(CONFIG_PK_MEMMAP_TRIGGER_OOM_CSRCFILE)

$(PK_MEMMAP_TRIGGER_OOM_DIR)/build: FORCE
	$(Q)$(call PK_MEMMAP_TRIGGER_OOM_sh,$(srctree))
