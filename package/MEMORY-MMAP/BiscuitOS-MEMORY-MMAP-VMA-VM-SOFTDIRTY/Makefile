PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_NAME        := BiscuitOS-MEMORY-MMAP-VMA-VM-SOFTDIRTY
PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_VERSION     := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_VERSION)
PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_URL)
PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_GITHUB)
PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_SUBNAME     :=
PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_BUILDARCH)
PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMA-VM-SOFTDIRTY
PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_PATCH       := $(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_DIR)/patch
PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_BSBIT       := BiscuitOS-MEMORY-MMAP-VMA-VM-SOFTDIRTY-$(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_VERSION).bsbit
PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_OUT         :=
PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_TAR)
PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_PACKAGE     :=
PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_DIR)/build

PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_VERSION)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_SITE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_CSRCFILE)

$(PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMA_VM_SOFTDIRTY_sh,$(srctree))
