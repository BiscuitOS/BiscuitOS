PK_MEMORY_MMAP_MAP_NONBLOCK_NAME        := BiscuitOS-MEMORY-MMAP-MAP-NONBLOCK
PK_MEMORY_MMAP_MAP_NONBLOCK_VERSION     := $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_VERSION)
PK_MEMORY_MMAP_MAP_NONBLOCK_SITE        := $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_URL)
PK_MEMORY_MMAP_MAP_NONBLOCK_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_GITHUB)
PK_MEMORY_MMAP_MAP_NONBLOCK_SUBNAME     :=
PK_MEMORY_MMAP_MAP_NONBLOCK_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_BUILDARCH)
PK_MEMORY_MMAP_MAP_NONBLOCK_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-MAP-NONBLOCK
PK_MEMORY_MMAP_MAP_NONBLOCK_PATCH       := $(PK_MEMORY_MMAP_MAP_NONBLOCK_DIR)/patch
PK_MEMORY_MMAP_MAP_NONBLOCK_BSBIT       := BiscuitOS-MEMORY-MMAP-MAP-NONBLOCK-$(PK_MEMORY_MMAP_MAP_NONBLOCK_VERSION).bsbit
PK_MEMORY_MMAP_MAP_NONBLOCK_OUT         :=
PK_MEMORY_MMAP_MAP_NONBLOCK_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_MAP_NONBLOCK_TAR         := $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_TAR)
PK_MEMORY_MMAP_MAP_NONBLOCK_PACKAGE     :=
PK_MEMORY_MMAP_MAP_NONBLOCK_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_MAP_NONBLOCK_DIR)/build

PK_MEMORY_MMAP_MAP_NONBLOCK_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_MAP_NONBLOCK_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_MAP_NONBLOCK_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_NONBLOCK_VERSION)X \
                 $(PK_MEMORY_MMAP_MAP_NONBLOCK_SITE)X \
                 $(PK_MEMORY_MMAP_MAP_NONBLOCK_GITHUB)X \
                 $(PK_MEMORY_MMAP_MAP_NONBLOCK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_NONBLOCK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_MAP_NONBLOCK_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_MAP_NONBLOCK_BSBIT)X \
                 $(PK_MEMORY_MMAP_MAP_NONBLOCK_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_MAP_NONBLOCK_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_NONBLOCK_CSRCFILE)

$(PK_MEMORY_MMAP_MAP_NONBLOCK_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_MAP_NONBLOCK_sh,$(srctree))
