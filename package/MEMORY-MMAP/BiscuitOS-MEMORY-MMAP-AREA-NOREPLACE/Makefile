################################################
PK_MEMORY_MMAP_AREA_NOREPLACE_NAME        := BiscuitOS-MEMORY-MMAP-AREA-NOREPLACE
PK_MEMORY_MMAP_AREA_NOREPLACE_VERSION     := $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_VERSION)
PK_MEMORY_MMAP_AREA_NOREPLACE_SITE        := $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_URL)
PK_MEMORY_MMAP_AREA_NOREPLACE_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_GITHUB)
PK_MEMORY_MMAP_AREA_NOREPLACE_SUBNAME     :=
PK_MEMORY_MMAP_AREA_NOREPLACE_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_BUILDARCH)
PK_MEMORY_MMAP_AREA_NOREPLACE_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-AREA-NOREPLACE
PK_MEMORY_MMAP_AREA_NOREPLACE_PATCH       := $(PK_MEMORY_MMAP_AREA_NOREPLACE_DIR)/patch
PK_MEMORY_MMAP_AREA_NOREPLACE_BSBIT       := BiscuitOS-MEMORY-MMAP-AREA-NOREPLACE-$(PK_MEMORY_MMAP_AREA_NOREPLACE_VERSION).bsbit
PK_MEMORY_MMAP_AREA_NOREPLACE_OUT         :=
PK_MEMORY_MMAP_AREA_NOREPLACE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_AREA_NOREPLACE_TAR         := $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_TAR)
PK_MEMORY_MMAP_AREA_NOREPLACE_PACKAGE     :=
PK_MEMORY_MMAP_AREA_NOREPLACE_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_AREA_NOREPLACE_DIR)/build

PK_MEMORY_MMAP_AREA_NOREPLACE_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_AREA_NOREPLACE_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_AREA_NOREPLACE_NAME)X \
                 $(PK_MEMORY_MMAP_AREA_NOREPLACE_VERSION)X \
                 $(PK_MEMORY_MMAP_AREA_NOREPLACE_SITE)X \
                 $(PK_MEMORY_MMAP_AREA_NOREPLACE_GITHUB)X \
                 $(PK_MEMORY_MMAP_AREA_NOREPLACE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_AREA_NOREPLACE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_AREA_NOREPLACE_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_AREA_NOREPLACE_BSBIT)X \
                 $(PK_MEMORY_MMAP_AREA_NOREPLACE_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_AREA_NOREPLACE_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_AREA_NOREPLACE_CSRCFILE)

$(PK_MEMORY_MMAP_AREA_NOREPLACE_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_AREA_NOREPLACE_sh,$(srctree))
