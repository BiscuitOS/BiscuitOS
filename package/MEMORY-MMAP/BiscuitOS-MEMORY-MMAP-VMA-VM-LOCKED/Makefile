PK_MEMORY_MMAP_VMA_VM_LOCKED_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_LOCKED_GITHUB)
PK_MEMORY_MMAP_VMA_VM_LOCKED_SUBNAME     :=
PK_MEMORY_MMAP_VMA_VM_LOCKED_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_LOCKED_BUILDARCH)
PK_MEMORY_MMAP_VMA_VM_LOCKED_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMA-VM-LOCKED
PK_MEMORY_MMAP_VMA_VM_LOCKED_PATCH       := $(PK_MEMORY_MMAP_VMA_VM_LOCKED_DIR)/patch
PK_MEMORY_MMAP_VMA_VM_LOCKED_BSBIT       := BiscuitOS-MEMORY-MMAP-VMA-VM-LOCKED-$(PK_MEMORY_MMAP_VMA_VM_LOCKED_VERSION).bsbit
PK_MEMORY_MMAP_VMA_VM_LOCKED_OUT         :=
PK_MEMORY_MMAP_VMA_VM_LOCKED_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_VMA_VM_LOCKED_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_LOCKED_TAR)
PK_MEMORY_MMAP_VMA_VM_LOCKED_PACKAGE     :=
PK_MEMORY_MMAP_VMA_VM_LOCKED_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_LOCKED_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMA_VM_LOCKED_DIR)/build

PK_MEMORY_MMAP_VMA_VM_LOCKED_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMA_VM_LOCKED_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMA_VM_LOCKED_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_LOCKED_VERSION)X \
                 $(PK_MEMORY_MMAP_VMA_VM_LOCKED_SITE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_LOCKED_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMA_VM_LOCKED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_LOCKED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_LOCKED_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_LOCKED_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_LOCKED_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_LOCKED_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_LOCKED_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_LOCKED_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_LOCKED_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_LOCKED_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_LOCKED_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMA_VM_LOCKED_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMA_VM_LOCKED_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMA_VM_LOCKED_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_LOCKED_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_LOCKED_CSRCFILE)

$(PK_MEMORY_MMAP_VMA_VM_LOCKED_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMA_VM_LOCKED_sh,$(srctree))
