PK_MEMORY_MMAP_VMOPS_OPEN_NAME        := BiscuitOS-MEMORY-MMAP-VMOPS-OPEN
PK_MEMORY_MMAP_VMOPS_OPEN_VERSION     := $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_VERSION)
PK_MEMORY_MMAP_VMOPS_OPEN_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_URL)
PK_MEMORY_MMAP_VMOPS_OPEN_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_GITHUB)
PK_MEMORY_MMAP_VMOPS_OPEN_SUBNAME     :=
PK_MEMORY_MMAP_VMOPS_OPEN_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_BUILDARCH)
PK_MEMORY_MMAP_VMOPS_OPEN_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMOPS-OPEN
PK_MEMORY_MMAP_VMOPS_OPEN_PATCH       := $(PK_MEMORY_MMAP_VMOPS_OPEN_DIR)/patch
PK_MEMORY_MMAP_VMOPS_OPEN_BSBIT       := BiscuitOS-MEMORY-MMAP-VMOPS-OPEN-$(PK_MEMORY_MMAP_VMOPS_OPEN_VERSION).bsbit
PK_MEMORY_MMAP_VMOPS_OPEN_OUT         :=
PK_MEMORY_MMAP_VMOPS_OPEN_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMORY_MMAP_VMOPS_OPEN_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_TAR)
PK_MEMORY_MMAP_VMOPS_OPEN_PACKAGE     :=
PK_MEMORY_MMAP_VMOPS_OPEN_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMOPS_OPEN_DIR)/build

PK_MEMORY_MMAP_VMOPS_OPEN_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMOPS_OPEN_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMOPS_OPEN_NAME)X \
                 $(PK_MEMORY_MMAP_VMOPS_OPEN_VERSION)X \
                 $(PK_MEMORY_MMAP_VMOPS_OPEN_SITE)X \
                 $(PK_MEMORY_MMAP_VMOPS_OPEN_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMOPS_OPEN_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMOPS_OPEN_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMOPS_OPEN_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMOPS_OPEN_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMOPS_OPEN_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMOPS_OPEN_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_OPEN_CSRCFILE)

$(PK_MEMORY_MMAP_VMOPS_OPEN_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMOPS_OPEN_sh,$(srctree))
