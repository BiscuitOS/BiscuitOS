PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_NAME        := BiscuitOS-MEMORY-MMAP-MAP-SHARED-VALIDATE
PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_VERSION     := $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_VERSION)
PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_SITE        := $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_URL)
PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_GITHUB)
PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_SUBNAME     :=
PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_BUILDARCH)
PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-MAP-SHARED-VALIDATE
PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_PATCH       := $(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_DIR)/patch
PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_BSBIT       := BiscuitOS-MEMORY-MMAP-MAP-SHARED-VALIDATE-$(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_VERSION).bsbit
PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_OUT         :=
PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_TAR         := $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_TAR)
PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_PACKAGE     :=
PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_DIR)/build

PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_VERSION)X \
                 $(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_SITE)X \
                 $(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_GITHUB)X \
                 $(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_BSBIT)X \
                 $(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_CSRCFILE)

$(PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_MAP_SHARED_VALIDATE_sh,$(srctree))
