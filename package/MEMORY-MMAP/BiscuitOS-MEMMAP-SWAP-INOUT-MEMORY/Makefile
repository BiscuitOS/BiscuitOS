################################################
#
# BiscuitOS-MEMMAP-SWAP-INOUT-MEMORY
#
################################################
PK_MEMMAP_SWAP_MEMORY_NAME        := BiscuitOS-MEMMAP-SWAP-INOUT-MEMORY
PK_MEMMAP_SWAP_MEMORY_VERSION     := $(CONFIG_PK_MEMMAP_SWAP_MEMORY_VERSION)
PK_MEMMAP_SWAP_MEMORY_SITE        := $(CONFIG_PK_MEMMAP_SWAP_MEMORY_URL)
PK_MEMMAP_SWAP_MEMORY_GITHUB      := $(CONFIG_PK_MEMMAP_SWAP_MEMORY_GITHUB)
PK_MEMMAP_SWAP_MEMORY_SUBNAME     :=
PK_MEMMAP_SWAP_MEMORY_BUILDARCH   := $(CONFIG_PK_MEMMAP_SWAP_MEMORY_BUILDARCH)
PK_MEMMAP_SWAP_MEMORY_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMMAP-SWAP-INOUT-MEMORY
PK_MEMMAP_SWAP_MEMORY_PATCH       := $(PK_MEMMAP_SWAP_MEMORY_DIR)/patch
PK_MEMMAP_SWAP_MEMORY_BSBIT       := BiscuitOS-MEMMAP-SWAP-INOUT-MEMORY-$(PK_MEMMAP_SWAP_MEMORY_VERSION).bsbit
PK_MEMMAP_SWAP_MEMORY_OUT         :=
PK_MEMMAP_SWAP_MEMORY_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMMAP_SWAP_MEMORY_TAR         := $(CONFIG_PK_MEMMAP_SWAP_MEMORY_TAR)
PK_MEMMAP_SWAP_MEMORY_PACKAGE     :=
PK_MEMMAP_SWAP_MEMORY_CONFIG      := $(CONFIG_PK_MEMMAP_SWAP_MEMORY_CONFIG)
SUB_TARGET                  += $(PK_MEMMAP_SWAP_MEMORY_DIR)/build

PK_MEMMAP_SWAP_MEMORY_sh = $(CONFIG_SHELL) $(PK_MEMMAP_SWAP_MEMORY_SCRIPTS) \
                 $1X \
                 $(PK_MEMMAP_SWAP_MEMORY_NAME)X \
                 $(PK_MEMMAP_SWAP_MEMORY_VERSION)X \
                 $(PK_MEMMAP_SWAP_MEMORY_SITE)X \
                 $(PK_MEMMAP_SWAP_MEMORY_GITHUB)X \
                 $(PK_MEMMAP_SWAP_MEMORY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMMAP_SWAP_MEMORY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMMAP_SWAP_MEMORY_SUBNAME)X \
                 $(CONFIG_PK_MEMMAP_SWAP_MEMORY_CONFIG)X \
                 $(CONFIG_PK_MEMMAP_SWAP_MEMORY_LIBPATH)X \
                 $(CONFIG_PK_MEMMAP_SWAP_MEMORY_CPPFLAGS)X \
                 $(CONFIG_PK_MEMMAP_SWAP_MEMORY_DPKCONFIG)X \
                 $(CONFIG_PK_MEMMAP_SWAP_MEMORY_CFLAGS)X \
                 $(CONFIG_PK_MEMMAP_SWAP_MEMORY_LDFLAGS)X \
                 $(CONFIG_PK_MEMMAP_SWAP_MEMORY_CXXFLAGS)X \
                 $(CONFIG_PK_MEMMAP_SWAP_MEMORY_ASFLAGS)X \
                 $(PK_MEMMAP_SWAP_MEMORY_BSBIT)X \
                 $(PK_MEMMAP_SWAP_MEMORY_BUILDARCH)X \
		 $(PK_MEMMAP_SWAP_MEMORY_CSRCFILE)X \
                 $(CONFIG_PK_MEMMAP_SWAP_MEMORY_SRCFILE)X \
                 $(CONFIG_PK_MEMMAP_SWAP_MEMORY_CSRCFILE)

$(PK_MEMMAP_SWAP_MEMORY_DIR)/build: FORCE
	$(Q)$(call PK_MEMMAP_SWAP_MEMORY_sh,$(srctree))
