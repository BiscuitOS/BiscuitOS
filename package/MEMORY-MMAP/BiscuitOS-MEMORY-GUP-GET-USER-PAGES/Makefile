PK_MEMORY_GUP_GET_USER_PAGES_NAME        := BiscuitOS-MEMORY-GUP-GET-USER-PAGES
PK_MEMORY_GUP_GET_USER_PAGES_VERSION     := $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_VERSION)
PK_MEMORY_GUP_GET_USER_PAGES_SITE        := $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_URL)
PK_MEMORY_GUP_GET_USER_PAGES_GITHUB      := $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_GITHUB)
PK_MEMORY_GUP_GET_USER_PAGES_SUBNAME     :=
PK_MEMORY_GUP_GET_USER_PAGES_BUILDARCH   := $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_BUILDARCH)
PK_MEMORY_GUP_GET_USER_PAGES_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-GUP-GET-USER-PAGES
PK_MEMORY_GUP_GET_USER_PAGES_PATCH       := $(PK_MEMORY_GUP_GET_USER_PAGES_DIR)/patch
PK_MEMORY_GUP_GET_USER_PAGES_BSBIT       := BiscuitOS-MEMORY-GUP-GET-USER-PAGES-$(PK_MEMORY_GUP_GET_USER_PAGES_VERSION).bsbit
PK_MEMORY_GUP_GET_USER_PAGES_OUT         :=
PK_MEMORY_GUP_GET_USER_PAGES_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMORY_GUP_GET_USER_PAGES_TAR         := $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_TAR)
PK_MEMORY_GUP_GET_USER_PAGES_PACKAGE     :=
PK_MEMORY_GUP_GET_USER_PAGES_CONFIG      := $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_GUP_GET_USER_PAGES_DIR)/build

PK_MEMORY_GUP_GET_USER_PAGES_sh = $(CONFIG_SHELL) $(PK_MEMORY_GUP_GET_USER_PAGES_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_GUP_GET_USER_PAGES_NAME)X \
                 $(PK_MEMORY_GUP_GET_USER_PAGES_VERSION)X \
                 $(PK_MEMORY_GUP_GET_USER_PAGES_SITE)X \
                 $(PK_MEMORY_GUP_GET_USER_PAGES_GITHUB)X \
                 $(PK_MEMORY_GUP_GET_USER_PAGES_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_GUP_GET_USER_PAGES_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_GUP_GET_USER_PAGES_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_CONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_ASFLAGS)X \
                 $(PK_MEMORY_GUP_GET_USER_PAGES_BSBIT)X \
                 $(PK_MEMORY_GUP_GET_USER_PAGES_BUILDARCH)X \
		 $(PK_MEMORY_GUP_GET_USER_PAGES_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_GET_USER_PAGES_CSRCFILE)

$(PK_MEMORY_GUP_GET_USER_PAGES_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_GUP_GET_USER_PAGES_sh,$(srctree))
