PK_MEMORY_MMAP_VMA_VM_SHARED_NAME        := BiscuitOS-MEMORY-MMAP-VMA-VM-SHARED
PK_MEMORY_MMAP_VMA_VM_SHARED_VERSION     := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_VERSION)
PK_MEMORY_MMAP_VMA_VM_SHARED_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_URL)
PK_MEMORY_MMAP_VMA_VM_SHARED_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_GITHUB)
PK_MEMORY_MMAP_VMA_VM_SHARED_SUBNAME     :=
PK_MEMORY_MMAP_VMA_VM_SHARED_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_BUILDARCH)
PK_MEMORY_MMAP_VMA_VM_SHARED_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMA-VM-SHARED
PK_MEMORY_MMAP_VMA_VM_SHARED_PATCH       := $(PK_MEMORY_MMAP_VMA_VM_SHARED_DIR)/patch
PK_MEMORY_MMAP_VMA_VM_SHARED_BSBIT       := BiscuitOS-MEMORY-MMAP-VMA-VM-SHARED-$(PK_MEMORY_MMAP_VMA_VM_SHARED_VERSION).bsbit
PK_MEMORY_MMAP_VMA_VM_SHARED_OUT         :=
PK_MEMORY_MMAP_VMA_VM_SHARED_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_VMA_VM_SHARED_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_TAR)
PK_MEMORY_MMAP_VMA_VM_SHARED_PACKAGE     :=
PK_MEMORY_MMAP_VMA_VM_SHARED_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMA_VM_SHARED_DIR)/build

PK_MEMORY_MMAP_VMA_VM_SHARED_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMA_VM_SHARED_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMA_VM_SHARED_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SHARED_VERSION)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SHARED_SITE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SHARED_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SHARED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SHARED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SHARED_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SHARED_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMA_VM_SHARED_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMA_VM_SHARED_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_SHARED_CSRCFILE)

$(PK_MEMORY_MMAP_VMA_VM_SHARED_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMA_VM_SHARED_sh,$(srctree))
