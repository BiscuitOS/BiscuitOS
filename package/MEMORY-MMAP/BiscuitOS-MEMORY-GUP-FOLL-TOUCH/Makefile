PK_MEMORY_GUP_FOLL_TOUCH_NAME        := BiscuitOS-MEMORY-GUP-FOLL-TOUCH
PK_MEMORY_GUP_FOLL_TOUCH_VERSION     := $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_VERSION)
PK_MEMORY_GUP_FOLL_TOUCH_SITE        := $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_URL)
PK_MEMORY_GUP_FOLL_TOUCH_GITHUB      := $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_GITHUB)
PK_MEMORY_GUP_FOLL_TOUCH_SUBNAME     :=
PK_MEMORY_GUP_FOLL_TOUCH_BUILDARCH   := $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_BUILDARCH)
PK_MEMORY_GUP_FOLL_TOUCH_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-GUP-FOLL-TOUCH
PK_MEMORY_GUP_FOLL_TOUCH_PATCH       := $(PK_MEMORY_GUP_FOLL_TOUCH_DIR)/patch
PK_MEMORY_GUP_FOLL_TOUCH_BSBIT       := BiscuitOS-MEMORY-GUP-FOLL-TOUCH-$(PK_MEMORY_GUP_FOLL_TOUCH_VERSION).bsbit
PK_MEMORY_GUP_FOLL_TOUCH_OUT         :=
PK_MEMORY_GUP_FOLL_TOUCH_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMORY_GUP_FOLL_TOUCH_TAR         := $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_TAR)
PK_MEMORY_GUP_FOLL_TOUCH_PACKAGE     :=
PK_MEMORY_GUP_FOLL_TOUCH_CONFIG      := $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_GUP_FOLL_TOUCH_DIR)/build

PK_MEMORY_GUP_FOLL_TOUCH_sh = $(CONFIG_SHELL) $(PK_MEMORY_GUP_FOLL_TOUCH_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_GUP_FOLL_TOUCH_NAME)X \
                 $(PK_MEMORY_GUP_FOLL_TOUCH_VERSION)X \
                 $(PK_MEMORY_GUP_FOLL_TOUCH_SITE)X \
                 $(PK_MEMORY_GUP_FOLL_TOUCH_GITHUB)X \
                 $(PK_MEMORY_GUP_FOLL_TOUCH_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_GUP_FOLL_TOUCH_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_GUP_FOLL_TOUCH_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_CONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_ASFLAGS)X \
                 $(PK_MEMORY_GUP_FOLL_TOUCH_BSBIT)X \
                 $(PK_MEMORY_GUP_FOLL_TOUCH_BUILDARCH)X \
		 $(PK_MEMORY_GUP_FOLL_TOUCH_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_TOUCH_CSRCFILE)

$(PK_MEMORY_GUP_FOLL_TOUCH_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_GUP_FOLL_TOUCH_sh,$(srctree))
