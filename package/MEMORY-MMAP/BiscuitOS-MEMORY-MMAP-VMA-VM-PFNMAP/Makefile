PK_MEMORY_MMAP_VMA_VM_PFNMAP_NAME        := BiscuitOS-MEMORY-MMAP-VMA-VM-PFNMAP
PK_MEMORY_MMAP_VMA_VM_PFNMAP_VERSION     := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_VERSION)
PK_MEMORY_MMAP_VMA_VM_PFNMAP_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_URL)
PK_MEMORY_MMAP_VMA_VM_PFNMAP_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_GITHUB)
PK_MEMORY_MMAP_VMA_VM_PFNMAP_SUBNAME     :=
PK_MEMORY_MMAP_VMA_VM_PFNMAP_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_BUILDARCH)
PK_MEMORY_MMAP_VMA_VM_PFNMAP_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMA-VM-PFNMAP
PK_MEMORY_MMAP_VMA_VM_PFNMAP_PATCH       := $(PK_MEMORY_MMAP_VMA_VM_PFNMAP_DIR)/patch
PK_MEMORY_MMAP_VMA_VM_PFNMAP_BSBIT       := BiscuitOS-MEMORY-MMAP-VMA-VM-PFNMAP-$(PK_MEMORY_MMAP_VMA_VM_PFNMAP_VERSION).bsbit
PK_MEMORY_MMAP_VMA_VM_PFNMAP_OUT         :=
PK_MEMORY_MMAP_VMA_VM_PFNMAP_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_VMA_VM_PFNMAP_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_TAR)
PK_MEMORY_MMAP_VMA_VM_PFNMAP_PACKAGE     :=
PK_MEMORY_MMAP_VMA_VM_PFNMAP_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMA_VM_PFNMAP_DIR)/build

PK_MEMORY_MMAP_VMA_VM_PFNMAP_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMA_VM_PFNMAP_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMA_VM_PFNMAP_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_PFNMAP_VERSION)X \
                 $(PK_MEMORY_MMAP_VMA_VM_PFNMAP_SITE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_PFNMAP_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMA_VM_PFNMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_PFNMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_PFNMAP_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMA_VM_PFNMAP_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMA_VM_PFNMAP_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMA_VM_PFNMAP_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_PFNMAP_CSRCFILE)

$(PK_MEMORY_MMAP_VMA_VM_PFNMAP_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMA_VM_PFNMAP_sh,$(srctree))
