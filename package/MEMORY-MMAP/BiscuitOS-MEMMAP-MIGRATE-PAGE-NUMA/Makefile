                 $(PK_MEMMAP_MIGRATE_PAGE_NUMA_VERSION)X \
                 $(PK_MEMMAP_MIGRATE_PAGE_NUMA_SITE)X \
                 $(PK_MEMMAP_MIGRATE_PAGE_NUMA_GITHUB)X \
                 $(PK_MEMMAP_MIGRATE_PAGE_NUMA_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMMAP_MIGRATE_PAGE_NUMA_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMMAP_MIGRATE_PAGE_NUMA_SUBNAME)X \
                 $(CONFIG_PK_MEMMAP_MIGRATE_PAGE_NUMA_CONFIG)X \
                 $(CONFIG_PK_MEMMAP_MIGRATE_PAGE_NUMA_LIBPATH)X \
                 $(CONFIG_PK_MEMMAP_MIGRATE_PAGE_NUMA_CPPFLAGS)X \
                 $(CONFIG_PK_MEMMAP_MIGRATE_PAGE_NUMA_DPKCONFIG)X \
                 $(CONFIG_PK_MEMMAP_MIGRATE_PAGE_NUMA_CFLAGS)X \
                 $(CONFIG_PK_MEMMAP_MIGRATE_PAGE_NUMA_LDFLAGS)X \
                 $(CONFIG_PK_MEMMAP_MIGRATE_PAGE_NUMA_CXXFLAGS)X \
                 $(CONFIG_PK_MEMMAP_MIGRATE_PAGE_NUMA_ASFLAGS)X \
                 $(PK_MEMMAP_MIGRATE_PAGE_NUMA_BSBIT)X \
                 $(PK_MEMMAP_MIGRATE_PAGE_NUMA_BUILDARCH)X \
		 $(PK_MEMMAP_MIGRATE_PAGE_NUMA_CSRCFILE)X \
                 $(CONFIG_PK_MEMMAP_MIGRATE_PAGE_NUMA_SRCFILE)X \
