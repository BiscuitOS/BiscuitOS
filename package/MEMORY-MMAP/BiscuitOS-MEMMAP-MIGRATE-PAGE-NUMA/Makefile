PK_MEMMAP_MIGRATE_PAGE_NUMA_SITE        := $(CONFIG_PK_MEMMAP_MIGRATE_PAGE_NUMA_URL)
PK_MEMMAP_MIGRATE_PAGE_NUMA_GITHUB      := $(CONFIG_PK_MEMMAP_MIGRATE_PAGE_NUMA_GITHUB)
PK_MEMMAP_MIGRATE_PAGE_NUMA_SUBNAME     :=
PK_MEMMAP_MIGRATE_PAGE_NUMA_BUILDARCH   := $(CONFIG_PK_MEMMAP_MIGRATE_PAGE_NUMA_BUILDARCH)
PK_MEMMAP_MIGRATE_PAGE_NUMA_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMMAP-MIGRATE-PAGE-NUMA
PK_MEMMAP_MIGRATE_PAGE_NUMA_PATCH       := $(PK_MEMMAP_MIGRATE_PAGE_NUMA_DIR)/patch
PK_MEMMAP_MIGRATE_PAGE_NUMA_BSBIT       := BiscuitOS-MEMMAP-MIGRATE-PAGE-NUMA-$(PK_MEMMAP_MIGRATE_PAGE_NUMA_VERSION).bsbit
PK_MEMMAP_MIGRATE_PAGE_NUMA_OUT         :=
PK_MEMMAP_MIGRATE_PAGE_NUMA_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
