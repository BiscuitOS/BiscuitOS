PK_MEMMAP_MIGRATE_PAGE_NUMA_PACKAGE     :=
PK_MEMMAP_MIGRATE_PAGE_NUMA_CONFIG      := $(CONFIG_PK_MEMMAP_MIGRATE_PAGE_NUMA_CONFIG)
SUB_TARGET                  += $(PK_MEMMAP_MIGRATE_PAGE_NUMA_DIR)/build

PK_MEMMAP_MIGRATE_PAGE_NUMA_sh = $(CONFIG_SHELL) $(PK_MEMMAP_MIGRATE_PAGE_NUMA_SCRIPTS) \
                 $1X \
