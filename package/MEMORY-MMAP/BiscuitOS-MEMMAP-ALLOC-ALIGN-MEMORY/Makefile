PK_MEMMAP_ALLOC_ALIGN_MEM_NAME        := BiscuitOS-MEMMAP-ALLOC-ALIGN-MEMORY
PK_MEMMAP_ALLOC_ALIGN_MEM_VERSION     := $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_VERSION)
PK_MEMMAP_ALLOC_ALIGN_MEM_SITE        := $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_URL)
PK_MEMMAP_ALLOC_ALIGN_MEM_GITHUB      := $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_GITHUB)
PK_MEMMAP_ALLOC_ALIGN_MEM_SUBNAME     :=
PK_MEMMAP_ALLOC_ALIGN_MEM_BUILDARCH   := $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_BUILDARCH)
PK_MEMMAP_ALLOC_ALIGN_MEM_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMMAP-ALLOC-ALIGN-MEMORY
PK_MEMMAP_ALLOC_ALIGN_MEM_PATCH       := $(PK_MEMMAP_ALLOC_ALIGN_MEM_DIR)/patch
PK_MEMMAP_ALLOC_ALIGN_MEM_BSBIT       := BiscuitOS-MEMMAP-ALLOC-ALIGN-MEMORY-$(PK_MEMMAP_ALLOC_ALIGN_MEM_VERSION).bsbit
PK_MEMMAP_ALLOC_ALIGN_MEM_OUT         :=
PK_MEMMAP_ALLOC_ALIGN_MEM_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMMAP_ALLOC_ALIGN_MEM_TAR         := $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_TAR)
PK_MEMMAP_ALLOC_ALIGN_MEM_PACKAGE     :=
PK_MEMMAP_ALLOC_ALIGN_MEM_CONFIG      := $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_CONFIG)
SUB_TARGET                  += $(PK_MEMMAP_ALLOC_ALIGN_MEM_DIR)/build

PK_MEMMAP_ALLOC_ALIGN_MEM_sh = $(CONFIG_SHELL) $(PK_MEMMAP_ALLOC_ALIGN_MEM_SCRIPTS) \
                 $1X \
                 $(PK_MEMMAP_ALLOC_ALIGN_MEM_NAME)X \
                 $(PK_MEMMAP_ALLOC_ALIGN_MEM_VERSION)X \
                 $(PK_MEMMAP_ALLOC_ALIGN_MEM_SITE)X \
                 $(PK_MEMMAP_ALLOC_ALIGN_MEM_GITHUB)X \
                 $(PK_MEMMAP_ALLOC_ALIGN_MEM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMMAP_ALLOC_ALIGN_MEM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMMAP_ALLOC_ALIGN_MEM_SUBNAME)X \
                 $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_CONFIG)X \
                 $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_LIBPATH)X \
                 $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_CPPFLAGS)X \
                 $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_DPKCONFIG)X \
                 $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_CFLAGS)X \
                 $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_LDFLAGS)X \
                 $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_CXXFLAGS)X \
                 $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_ASFLAGS)X \
                 $(PK_MEMMAP_ALLOC_ALIGN_MEM_BSBIT)X \
                 $(PK_MEMMAP_ALLOC_ALIGN_MEM_BUILDARCH)X \
		 $(PK_MEMMAP_ALLOC_ALIGN_MEM_CSRCFILE)X \
                 $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_SRCFILE)X \
                 $(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM_CSRCFILE)

$(PK_MEMMAP_ALLOC_ALIGN_MEM_DIR)/build: FORCE
	$(Q)$(call PK_MEMMAP_ALLOC_ALIGN_MEM_sh,$(srctree))
