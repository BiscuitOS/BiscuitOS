PK_MEMORY_GUP_FAST_ONLY_NAME        := BiscuitOS-MEMORY-GUP-FAST-ONLY
PK_MEMORY_GUP_FAST_ONLY_VERSION     := $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_VERSION)
PK_MEMORY_GUP_FAST_ONLY_SITE        := $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_URL)
PK_MEMORY_GUP_FAST_ONLY_GITHUB      := $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_GITHUB)
PK_MEMORY_GUP_FAST_ONLY_SUBNAME     :=
PK_MEMORY_GUP_FAST_ONLY_BUILDARCH   := $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_BUILDARCH)
PK_MEMORY_GUP_FAST_ONLY_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-GUP-FAST-ONLY
PK_MEMORY_GUP_FAST_ONLY_PATCH       := $(PK_MEMORY_GUP_FAST_ONLY_DIR)/patch
PK_MEMORY_GUP_FAST_ONLY_BSBIT       := BiscuitOS-MEMORY-GUP-FAST-ONLY-$(PK_MEMORY_GUP_FAST_ONLY_VERSION).bsbit
PK_MEMORY_GUP_FAST_ONLY_OUT         :=
PK_MEMORY_GUP_FAST_ONLY_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMORY_GUP_FAST_ONLY_TAR         := $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_TAR)
PK_MEMORY_GUP_FAST_ONLY_PACKAGE     :=
PK_MEMORY_GUP_FAST_ONLY_CONFIG      := $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_GUP_FAST_ONLY_DIR)/build

PK_MEMORY_GUP_FAST_ONLY_sh = $(CONFIG_SHELL) $(PK_MEMORY_GUP_FAST_ONLY_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_GUP_FAST_ONLY_NAME)X \
                 $(PK_MEMORY_GUP_FAST_ONLY_VERSION)X \
                 $(PK_MEMORY_GUP_FAST_ONLY_SITE)X \
                 $(PK_MEMORY_GUP_FAST_ONLY_GITHUB)X \
                 $(PK_MEMORY_GUP_FAST_ONLY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_GUP_FAST_ONLY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_GUP_FAST_ONLY_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_CONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_ASFLAGS)X \
                 $(PK_MEMORY_GUP_FAST_ONLY_BSBIT)X \
                 $(PK_MEMORY_GUP_FAST_ONLY_BUILDARCH)X \
		 $(PK_MEMORY_GUP_FAST_ONLY_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_FAST_ONLY_CSRCFILE)

$(PK_MEMORY_GUP_FAST_ONLY_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_GUP_FAST_ONLY_sh,$(srctree))
