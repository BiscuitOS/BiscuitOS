PK_MEMORY_MMAP_VMA_VM_MAYEXEC_NAME        := BiscuitOS-MEMORY-MMAP-VMA-VM-MAYEXEC
PK_MEMORY_MMAP_VMA_VM_MAYEXEC_VERSION     := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_VERSION)
PK_MEMORY_MMAP_VMA_VM_MAYEXEC_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_URL)
PK_MEMORY_MMAP_VMA_VM_MAYEXEC_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_GITHUB)
PK_MEMORY_MMAP_VMA_VM_MAYEXEC_SUBNAME     :=
PK_MEMORY_MMAP_VMA_VM_MAYEXEC_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_BUILDARCH)
PK_MEMORY_MMAP_VMA_VM_MAYEXEC_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMA-VM-MAYEXEC
PK_MEMORY_MMAP_VMA_VM_MAYEXEC_PATCH       := $(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_DIR)/patch
PK_MEMORY_MMAP_VMA_VM_MAYEXEC_BSBIT       := BiscuitOS-MEMORY-MMAP-VMA-VM-MAYEXEC-$(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_VERSION).bsbit
PK_MEMORY_MMAP_VMA_VM_MAYEXEC_OUT         :=
PK_MEMORY_MMAP_VMA_VM_MAYEXEC_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_VMA_VM_MAYEXEC_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_TAR)
PK_MEMORY_MMAP_VMA_VM_MAYEXEC_PACKAGE     :=
PK_MEMORY_MMAP_VMA_VM_MAYEXEC_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_DIR)/build

PK_MEMORY_MMAP_VMA_VM_MAYEXEC_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_VERSION)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_SITE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYEXEC_CSRCFILE)

$(PK_MEMORY_MMAP_VMA_VM_MAYEXEC_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMA_VM_MAYEXEC_sh,$(srctree))
