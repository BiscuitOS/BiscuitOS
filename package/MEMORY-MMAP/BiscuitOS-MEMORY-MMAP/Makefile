PK_MEMORY_MMAP_DEFAULT_NAME        := BiscuitOS-MEMORY-MMAP
PK_MEMORY_MMAP_DEFAULT_VERSION     := $(CONFIG_PK_MEMORY_MMAP_DEFAULT_VERSION)
PK_MEMORY_MMAP_DEFAULT_SITE        := $(CONFIG_PK_MEMORY_MMAP_DEFAULT_URL)
PK_MEMORY_MMAP_DEFAULT_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_DEFAULT_GITHUB)
PK_MEMORY_MMAP_DEFAULT_SUBNAME     :=
PK_MEMORY_MMAP_DEFAULT_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_DEFAULT_BUILDARCH)
PK_MEMORY_MMAP_DEFAULT_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP
PK_MEMORY_MMAP_DEFAULT_PATCH       := $(PK_MEMORY_MMAP_DEFAULT_DIR)/patch
PK_MEMORY_MMAP_DEFAULT_BSBIT       := BiscuitOS-MEMORY-MMAP-$(PK_MEMORY_MMAP_DEFAULT_VERSION).bsbit
PK_MEMORY_MMAP_DEFAULT_OUT         :=
PK_MEMORY_MMAP_DEFAULT_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_DEFAULT_TAR         := $(CONFIG_PK_MEMORY_MMAP_DEFAULT_TAR)
PK_MEMORY_MMAP_DEFAULT_PACKAGE     :=
PK_MEMORY_MMAP_DEFAULT_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_DEFAULT_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_DEFAULT_DIR)/build

PK_MEMORY_MMAP_DEFAULT_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_DEFAULT_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_DEFAULT_NAME)X \
                 $(PK_MEMORY_MMAP_DEFAULT_VERSION)X \
                 $(PK_MEMORY_MMAP_DEFAULT_SITE)X \
                 $(PK_MEMORY_MMAP_DEFAULT_GITHUB)X \
                 $(PK_MEMORY_MMAP_DEFAULT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_DEFAULT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_DEFAULT_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_DEFAULT_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_DEFAULT_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_DEFAULT_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_DEFAULT_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_DEFAULT_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_DEFAULT_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_DEFAULT_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_DEFAULT_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_DEFAULT_BSBIT)X \
                 $(PK_MEMORY_MMAP_DEFAULT_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_DEFAULT_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_DEFAULT_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_DEFAULT_CSRCFILE)

$(PK_MEMORY_MMAP_DEFAULT_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_DEFAULT_sh,$(srctree))
