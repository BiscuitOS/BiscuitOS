################################################
#
# BiscuitOS-MEMORY-MMAP-FILE-REVERSE-TREE
#
################################################
PK_MEMORY_MMAP_FILE_REVERSE_TREE_NAME        := BiscuitOS-MEMORY-MMAP-FILE-REVERSE-TREE
PK_MEMORY_MMAP_FILE_REVERSE_TREE_VERSION     := $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_VERSION)
PK_MEMORY_MMAP_FILE_REVERSE_TREE_SITE        := $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_URL)
PK_MEMORY_MMAP_FILE_REVERSE_TREE_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_GITHUB)
PK_MEMORY_MMAP_FILE_REVERSE_TREE_SUBNAME     :=
PK_MEMORY_MMAP_FILE_REVERSE_TREE_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_BUILDARCH)
PK_MEMORY_MMAP_FILE_REVERSE_TREE_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-FILE-REVERSE-TREE
PK_MEMORY_MMAP_FILE_REVERSE_TREE_PATCH       := $(PK_MEMORY_MMAP_FILE_REVERSE_TREE_DIR)/patch
PK_MEMORY_MMAP_FILE_REVERSE_TREE_BSBIT       := BiscuitOS-MEMORY-MMAP-FILE-REVERSE-TREE-$(PK_MEMORY_MMAP_FILE_REVERSE_TREE_VERSION).bsbit
PK_MEMORY_MMAP_FILE_REVERSE_TREE_OUT         :=
PK_MEMORY_MMAP_FILE_REVERSE_TREE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMORY_MMAP_FILE_REVERSE_TREE_TAR         := $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_TAR)
PK_MEMORY_MMAP_FILE_REVERSE_TREE_PACKAGE     :=
PK_MEMORY_MMAP_FILE_REVERSE_TREE_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_FILE_REVERSE_TREE_DIR)/build

PK_MEMORY_MMAP_FILE_REVERSE_TREE_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_FILE_REVERSE_TREE_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_FILE_REVERSE_TREE_NAME)X \
                 $(PK_MEMORY_MMAP_FILE_REVERSE_TREE_VERSION)X \
                 $(PK_MEMORY_MMAP_FILE_REVERSE_TREE_SITE)X \
                 $(PK_MEMORY_MMAP_FILE_REVERSE_TREE_GITHUB)X \
                 $(PK_MEMORY_MMAP_FILE_REVERSE_TREE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_FILE_REVERSE_TREE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_FILE_REVERSE_TREE_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_FILE_REVERSE_TREE_BSBIT)X \
                 $(PK_MEMORY_MMAP_FILE_REVERSE_TREE_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_FILE_REVERSE_TREE_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_FILE_REVERSE_TREE_CSRCFILE)

$(PK_MEMORY_MMAP_FILE_REVERSE_TREE_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_FILE_REVERSE_TREE_sh,$(srctree))
