################################################
#
# BiscuitOS-MEMORY-MMAP-MAP-PRIVATE-FILE
#
################################################
PK_MEMORY_MMAP_MAP_PRIVATE_FILE_NAME        := BiscuitOS-MEMORY-MMAP-MAP-PRIVATE-FILE
PK_MEMORY_MMAP_MAP_PRIVATE_FILE_VERSION     := $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_VERSION)
PK_MEMORY_MMAP_MAP_PRIVATE_FILE_SITE        := $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_URL)
PK_MEMORY_MMAP_MAP_PRIVATE_FILE_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_GITHUB)
PK_MEMORY_MMAP_MAP_PRIVATE_FILE_SUBNAME     :=
PK_MEMORY_MMAP_MAP_PRIVATE_FILE_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_BUILDARCH)
PK_MEMORY_MMAP_MAP_PRIVATE_FILE_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-MAP-PRIVATE-FILE
PK_MEMORY_MMAP_MAP_PRIVATE_FILE_PATCH       := $(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_DIR)/patch
PK_MEMORY_MMAP_MAP_PRIVATE_FILE_BSBIT       := BiscuitOS-MEMORY-MMAP-MAP-PRIVATE-FILE-$(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_VERSION).bsbit
PK_MEMORY_MMAP_MAP_PRIVATE_FILE_OUT         :=
PK_MEMORY_MMAP_MAP_PRIVATE_FILE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_MAP_PRIVATE_FILE_TAR         := $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_TAR)
PK_MEMORY_MMAP_MAP_PRIVATE_FILE_PACKAGE     :=
PK_MEMORY_MMAP_MAP_PRIVATE_FILE_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_DIR)/build

PK_MEMORY_MMAP_MAP_PRIVATE_FILE_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_VERSION)X \
                 $(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_SITE)X \
                 $(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_GITHUB)X \
                 $(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_BSBIT)X \
                 $(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_PRIVATE_FILE_CSRCFILE)

$(PK_MEMORY_MMAP_MAP_PRIVATE_FILE_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_MAP_PRIVATE_FILE_sh,$(srctree))
