PK_MEMORY_MMAP_VMA_VM_READ_NAME        := BiscuitOS-MEMORY-MMAP-VMA-VM-READ
PK_MEMORY_MMAP_VMA_VM_READ_VERSION     := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_VERSION)
PK_MEMORY_MMAP_VMA_VM_READ_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_URL)
PK_MEMORY_MMAP_VMA_VM_READ_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_GITHUB)
PK_MEMORY_MMAP_VMA_VM_READ_SUBNAME     :=
PK_MEMORY_MMAP_VMA_VM_READ_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_BUILDARCH)
PK_MEMORY_MMAP_VMA_VM_READ_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMA-VM-READ
PK_MEMORY_MMAP_VMA_VM_READ_PATCH       := $(PK_MEMORY_MMAP_VMA_VM_READ_DIR)/patch
PK_MEMORY_MMAP_VMA_VM_READ_BSBIT       := BiscuitOS-MEMORY-MMAP-VMA-VM-READ-$(PK_MEMORY_MMAP_VMA_VM_READ_VERSION).bsbit
PK_MEMORY_MMAP_VMA_VM_READ_OUT         :=
PK_MEMORY_MMAP_VMA_VM_READ_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_VMA_VM_READ_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_TAR)
PK_MEMORY_MMAP_VMA_VM_READ_PACKAGE     :=
PK_MEMORY_MMAP_VMA_VM_READ_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMA_VM_READ_DIR)/build

PK_MEMORY_MMAP_VMA_VM_READ_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMA_VM_READ_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMA_VM_READ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_READ_VERSION)X \
                 $(PK_MEMORY_MMAP_VMA_VM_READ_SITE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_READ_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMA_VM_READ_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_READ_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_READ_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMA_VM_READ_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMA_VM_READ_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMA_VM_READ_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_READ_CSRCFILE)

$(PK_MEMORY_MMAP_VMA_VM_READ_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMA_VM_READ_sh,$(srctree))
