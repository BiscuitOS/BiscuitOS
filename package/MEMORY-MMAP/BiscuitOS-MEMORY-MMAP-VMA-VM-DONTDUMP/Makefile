PK_MEMORY_MMAP_VMA_VM_DONTDUMP_NAME        := BiscuitOS-MEMORY-MMAP-VMA-VM-DONTDUMP
PK_MEMORY_MMAP_VMA_VM_DONTDUMP_VERSION     := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_VERSION)
PK_MEMORY_MMAP_VMA_VM_DONTDUMP_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_URL)
PK_MEMORY_MMAP_VMA_VM_DONTDUMP_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_GITHUB)
PK_MEMORY_MMAP_VMA_VM_DONTDUMP_SUBNAME     :=
PK_MEMORY_MMAP_VMA_VM_DONTDUMP_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_BUILDARCH)
PK_MEMORY_MMAP_VMA_VM_DONTDUMP_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMA-VM-DONTDUMP
PK_MEMORY_MMAP_VMA_VM_DONTDUMP_PATCH       := $(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_DIR)/patch
PK_MEMORY_MMAP_VMA_VM_DONTDUMP_BSBIT       := BiscuitOS-MEMORY-MMAP-VMA-VM-DONTDUMP-$(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_VERSION).bsbit
PK_MEMORY_MMAP_VMA_VM_DONTDUMP_OUT         :=
PK_MEMORY_MMAP_VMA_VM_DONTDUMP_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_VMA_VM_DONTDUMP_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_TAR)
PK_MEMORY_MMAP_VMA_VM_DONTDUMP_PACKAGE     :=
PK_MEMORY_MMAP_VMA_VM_DONTDUMP_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_DIR)/build

PK_MEMORY_MMAP_VMA_VM_DONTDUMP_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_VERSION)X \
                 $(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_SITE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_DONTDUMP_CSRCFILE)

$(PK_MEMORY_MMAP_VMA_VM_DONTDUMP_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMA_VM_DONTDUMP_sh,$(srctree))
