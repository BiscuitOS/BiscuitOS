MEMMAP-$(CONFIG_PK_MEMMAP_ALLOC_ALIGN_MEM)			+= BiscuitOS-MEMMAP-ALLOC-ALIGN-MEMORY/
MEMMAP-$(CONFIG_PK_MEMMAP_INFO_PHYSMEM)				+= BiscuitOS-MEMMAP-INFO-PHYSMEM/
MEMMAP-$(CONFIG_PK_MEMMAP_INFO_NUMA_NODE)			+= BiscuitOS-MEMMAP-INFO-NUMA-NODE/
MEMMAP-$(CONFIG_PK_MEMMAP_SWAP_MEMORY)				+= BiscuitOS-MEMMAP-SWAP-INOUT-MEMORY/
MEMMAP-$(CONFIG_PK_MEMMAP_TRIGGER_OOM)				+= BiscuitOS-MEMMAP-TRIGGER-OOM/
MEMMAP-$(CONFIG_PK_MEMMAP_MIGRATE_PAGE_NUMA)			+= BiscuitOS-MEMMAP-MIGRATE-PAGE-NUMA/

PACKAGE_MAKEDIR := $(patsubst %/, $(srctree)/package/MEMORY-MMAP/%/Makefile, $(MEMMAP-y))

include $(PACKAGE_MAKEDIR)
