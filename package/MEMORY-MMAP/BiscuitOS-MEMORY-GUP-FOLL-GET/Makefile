PK_MEMORY_GUP_FOLL_GET_NAME        := BiscuitOS-MEMORY-GUP-FOLL-GET
PK_MEMORY_GUP_FOLL_GET_VERSION     := $(CONFIG_PK_MEMORY_GUP_FOLL_GET_VERSION)
PK_MEMORY_GUP_FOLL_GET_SITE        := $(CONFIG_PK_MEMORY_GUP_FOLL_GET_URL)
PK_MEMORY_GUP_FOLL_GET_GITHUB      := $(CONFIG_PK_MEMORY_GUP_FOLL_GET_GITHUB)
PK_MEMORY_GUP_FOLL_GET_SUBNAME     :=
PK_MEMORY_GUP_FOLL_GET_BUILDARCH   := $(CONFIG_PK_MEMORY_GUP_FOLL_GET_BUILDARCH)
PK_MEMORY_GUP_FOLL_GET_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-GUP-FOLL-GET
PK_MEMORY_GUP_FOLL_GET_PATCH       := $(PK_MEMORY_GUP_FOLL_GET_DIR)/patch
PK_MEMORY_GUP_FOLL_GET_BSBIT       := BiscuitOS-MEMORY-GUP-FOLL-GET-$(PK_MEMORY_GUP_FOLL_GET_VERSION).bsbit
PK_MEMORY_GUP_FOLL_GET_OUT         :=
PK_MEMORY_GUP_FOLL_GET_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMORY_GUP_FOLL_GET_TAR         := $(CONFIG_PK_MEMORY_GUP_FOLL_GET_TAR)
PK_MEMORY_GUP_FOLL_GET_PACKAGE     :=
PK_MEMORY_GUP_FOLL_GET_CONFIG      := $(CONFIG_PK_MEMORY_GUP_FOLL_GET_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_GUP_FOLL_GET_DIR)/build

PK_MEMORY_GUP_FOLL_GET_sh = $(CONFIG_SHELL) $(PK_MEMORY_GUP_FOLL_GET_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_GUP_FOLL_GET_NAME)X \
                 $(PK_MEMORY_GUP_FOLL_GET_VERSION)X \
                 $(PK_MEMORY_GUP_FOLL_GET_SITE)X \
                 $(PK_MEMORY_GUP_FOLL_GET_GITHUB)X \
                 $(PK_MEMORY_GUP_FOLL_GET_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_GUP_FOLL_GET_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_GUP_FOLL_GET_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_GET_CONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_GET_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_GET_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_GET_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_GET_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_GET_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_GET_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_GET_ASFLAGS)X \
                 $(PK_MEMORY_GUP_FOLL_GET_BSBIT)X \
                 $(PK_MEMORY_GUP_FOLL_GET_BUILDARCH)X \
		 $(PK_MEMORY_GUP_FOLL_GET_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_GET_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_FOLL_GET_CSRCFILE)

$(PK_MEMORY_GUP_FOLL_GET_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_GUP_FOLL_GET_sh,$(srctree))
