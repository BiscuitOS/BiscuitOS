PK_MEMORY_MMAP_VMOPS_FAULT_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMOPS_FAULT_URL)
PK_MEMORY_MMAP_VMOPS_FAULT_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMOPS_FAULT_GITHUB)
PK_MEMORY_MMAP_VMOPS_FAULT_SUBNAME     :=
PK_MEMORY_MMAP_VMOPS_FAULT_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMOPS_FAULT_BUILDARCH)
PK_MEMORY_MMAP_VMOPS_FAULT_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMOPS-FAULT
PK_MEMORY_MMAP_VMOPS_FAULT_PATCH       := $(PK_MEMORY_MMAP_VMOPS_FAULT_DIR)/patch
PK_MEMORY_MMAP_VMOPS_FAULT_BSBIT       := BiscuitOS-MEMORY-MMAP-VMOPS-FAULT-$(PK_MEMORY_MMAP_VMOPS_FAULT_VERSION).bsbit
