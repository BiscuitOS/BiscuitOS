PK_MEMORY_MMAP_VMOPS_FAULT_PACKAGE     :=
PK_MEMORY_MMAP_VMOPS_FAULT_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMOPS_FAULT_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMOPS_FAULT_DIR)/build

PK_MEMORY_MMAP_VMOPS_FAULT_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMOPS_FAULT_SCRIPTS) \
                 $1X \
