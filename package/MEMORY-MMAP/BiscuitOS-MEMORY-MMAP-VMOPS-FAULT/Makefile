                 $(PK_MEMORY_MMAP_VMOPS_FAULT_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMOPS_FAULT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMOPS_FAULT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMOPS_FAULT_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_FAULT_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_FAULT_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_FAULT_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_FAULT_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_FAULT_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_FAULT_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_FAULT_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_FAULT_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMOPS_FAULT_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMOPS_FAULT_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMOPS_FAULT_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_FAULT_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMOPS_FAULT_CSRCFILE)

$(PK_MEMORY_MMAP_VMOPS_FAULT_DIR)/build: FORCE
