PK_MEMORY_MMAP_VMA_VM_MAYSHARE_NAME        := BiscuitOS-MEMORY-MMAP-VMA-VM-MAYSHARE
PK_MEMORY_MMAP_VMA_VM_MAYSHARE_VERSION     := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_VERSION)
PK_MEMORY_MMAP_VMA_VM_MAYSHARE_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_URL)
PK_MEMORY_MMAP_VMA_VM_MAYSHARE_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_GITHUB)
PK_MEMORY_MMAP_VMA_VM_MAYSHARE_SUBNAME     :=
PK_MEMORY_MMAP_VMA_VM_MAYSHARE_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_BUILDARCH)
PK_MEMORY_MMAP_VMA_VM_MAYSHARE_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMA-VM-MAYSHARE
PK_MEMORY_MMAP_VMA_VM_MAYSHARE_PATCH       := $(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_DIR)/patch
PK_MEMORY_MMAP_VMA_VM_MAYSHARE_BSBIT       := BiscuitOS-MEMORY-MMAP-VMA-VM-MAYSHARE-$(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_VERSION).bsbit
PK_MEMORY_MMAP_VMA_VM_MAYSHARE_OUT         :=
PK_MEMORY_MMAP_VMA_VM_MAYSHARE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_VMA_VM_MAYSHARE_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_TAR)
PK_MEMORY_MMAP_VMA_VM_MAYSHARE_PACKAGE     :=
PK_MEMORY_MMAP_VMA_VM_MAYSHARE_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_DIR)/build

PK_MEMORY_MMAP_VMA_VM_MAYSHARE_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_VERSION)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_SITE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_MAYSHARE_CSRCFILE)

$(PK_MEMORY_MMAP_VMA_VM_MAYSHARE_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMA_VM_MAYSHARE_sh,$(srctree))
