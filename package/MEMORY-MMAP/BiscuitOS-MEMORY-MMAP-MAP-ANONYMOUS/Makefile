PK_MEMORY_MMAP_MAP_ANONYMOUS_NAME        := BiscuitOS-MEMORY-MMAP-MAP-ANONYMOUS
PK_MEMORY_MMAP_MAP_ANONYMOUS_VERSION     := $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_VERSION)
PK_MEMORY_MMAP_MAP_ANONYMOUS_SITE        := $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_URL)
PK_MEMORY_MMAP_MAP_ANONYMOUS_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_GITHUB)
PK_MEMORY_MMAP_MAP_ANONYMOUS_SUBNAME     :=
PK_MEMORY_MMAP_MAP_ANONYMOUS_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_BUILDARCH)
PK_MEMORY_MMAP_MAP_ANONYMOUS_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-MAP-ANONYMOUS
PK_MEMORY_MMAP_MAP_ANONYMOUS_PATCH       := $(PK_MEMORY_MMAP_MAP_ANONYMOUS_DIR)/patch
PK_MEMORY_MMAP_MAP_ANONYMOUS_BSBIT       := BiscuitOS-MEMORY-MMAP-MAP-ANONYMOUS-$(PK_MEMORY_MMAP_MAP_ANONYMOUS_VERSION).bsbit
PK_MEMORY_MMAP_MAP_ANONYMOUS_OUT         :=
PK_MEMORY_MMAP_MAP_ANONYMOUS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_MAP_ANONYMOUS_TAR         := $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_TAR)
PK_MEMORY_MMAP_MAP_ANONYMOUS_PACKAGE     :=
PK_MEMORY_MMAP_MAP_ANONYMOUS_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_MAP_ANONYMOUS_DIR)/build

PK_MEMORY_MMAP_MAP_ANONYMOUS_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_MAP_ANONYMOUS_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_MAP_ANONYMOUS_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_ANONYMOUS_VERSION)X \
                 $(PK_MEMORY_MMAP_MAP_ANONYMOUS_SITE)X \
                 $(PK_MEMORY_MMAP_MAP_ANONYMOUS_GITHUB)X \
                 $(PK_MEMORY_MMAP_MAP_ANONYMOUS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_MAP_ANONYMOUS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_MAP_ANONYMOUS_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_MAP_ANONYMOUS_BSBIT)X \
                 $(PK_MEMORY_MMAP_MAP_ANONYMOUS_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_MAP_ANONYMOUS_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_MAP_ANONYMOUS_CSRCFILE)

$(PK_MEMORY_MMAP_MAP_ANONYMOUS_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_MAP_ANONYMOUS_sh,$(srctree))
