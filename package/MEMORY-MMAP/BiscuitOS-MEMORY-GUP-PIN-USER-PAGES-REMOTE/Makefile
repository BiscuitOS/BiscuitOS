PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_NAME        := BiscuitOS-MEMORY-GUP-PIN-USER-PAGES-REMOTE
PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_VERSION     := $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_VERSION)
PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_SITE        := $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_URL)
PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_GITHUB      := $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_GITHUB)
PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_SUBNAME     :=
PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_BUILDARCH   := $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_BUILDARCH)
PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-GUP-PIN-USER-PAGES-REMOTE
PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_PATCH       := $(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_DIR)/patch
PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_BSBIT       := BiscuitOS-MEMORY-GUP-PIN-USER-PAGES-REMOTE-$(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_VERSION).bsbit
PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_OUT         :=
PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_TAR         := $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_TAR)
PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_PACKAGE     :=
PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_CONFIG      := $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_DIR)/build

PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_sh = $(CONFIG_SHELL) $(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_NAME)X \
                 $(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_VERSION)X \
                 $(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_SITE)X \
                 $(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_GITHUB)X \
                 $(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_CONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_ASFLAGS)X \
                 $(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_BSBIT)X \
                 $(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_BUILDARCH)X \
		 $(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_CSRCFILE)

$(PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_GUP_PIN_USER_PAGES_REMOTE_sh,$(srctree))
