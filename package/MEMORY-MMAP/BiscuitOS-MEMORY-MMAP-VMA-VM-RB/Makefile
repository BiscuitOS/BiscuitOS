PK_MEMORY_MMAP_VMA_VM_RB_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_RB_GITHUB)
PK_MEMORY_MMAP_VMA_VM_RB_SUBNAME     :=
PK_MEMORY_MMAP_VMA_VM_RB_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_RB_BUILDARCH)
PK_MEMORY_MMAP_VMA_VM_RB_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMA-VM-RB
PK_MEMORY_MMAP_VMA_VM_RB_PATCH       := $(PK_MEMORY_MMAP_VMA_VM_RB_DIR)/patch
PK_MEMORY_MMAP_VMA_VM_RB_BSBIT       := BiscuitOS-MEMORY-MMAP-VMA-VM-RB-$(PK_MEMORY_MMAP_VMA_VM_RB_VERSION).bsbit
PK_MEMORY_MMAP_VMA_VM_RB_OUT         :=
PK_MEMORY_MMAP_VMA_VM_RB_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMORY_MMAP_VMA_VM_RB_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_RB_TAR)
PK_MEMORY_MMAP_VMA_VM_RB_PACKAGE     :=
PK_MEMORY_MMAP_VMA_VM_RB_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_RB_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMA_VM_RB_DIR)/build

PK_MEMORY_MMAP_VMA_VM_RB_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMA_VM_RB_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMA_VM_RB_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_RB_VERSION)X \
                 $(PK_MEMORY_MMAP_VMA_VM_RB_SITE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_RB_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMA_VM_RB_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_RB_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_RB_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_RB_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_RB_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_RB_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_RB_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_RB_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_RB_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_RB_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_RB_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMA_VM_RB_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMA_VM_RB_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMA_VM_RB_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_RB_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_RB_CSRCFILE)

$(PK_MEMORY_MMAP_VMA_VM_RB_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMA_VM_RB_sh,$(srctree))
