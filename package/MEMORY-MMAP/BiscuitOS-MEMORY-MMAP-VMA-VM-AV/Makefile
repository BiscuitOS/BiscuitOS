PK_MEMORY_MMAP_VMA_VM_AV_NAME        := BiscuitOS-MEMORY-MMAP-VMA-VM-AV
PK_MEMORY_MMAP_VMA_VM_AV_VERSION     := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_VERSION)
PK_MEMORY_MMAP_VMA_VM_AV_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_URL)
PK_MEMORY_MMAP_VMA_VM_AV_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_GITHUB)
PK_MEMORY_MMAP_VMA_VM_AV_SUBNAME     :=
PK_MEMORY_MMAP_VMA_VM_AV_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_BUILDARCH)
PK_MEMORY_MMAP_VMA_VM_AV_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMA-VM-AV
PK_MEMORY_MMAP_VMA_VM_AV_PATCH       := $(PK_MEMORY_MMAP_VMA_VM_AV_DIR)/patch
PK_MEMORY_MMAP_VMA_VM_AV_BSBIT       := BiscuitOS-MEMORY-MMAP-VMA-VM-AV-$(PK_MEMORY_MMAP_VMA_VM_AV_VERSION).bsbit
PK_MEMORY_MMAP_VMA_VM_AV_OUT         :=
PK_MEMORY_MMAP_VMA_VM_AV_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_MEMORY_MMAP_VMA_VM_AV_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_TAR)
PK_MEMORY_MMAP_VMA_VM_AV_PACKAGE     :=
PK_MEMORY_MMAP_VMA_VM_AV_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMA_VM_AV_DIR)/build

PK_MEMORY_MMAP_VMA_VM_AV_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMA_VM_AV_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMA_VM_AV_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_AV_VERSION)X \
                 $(PK_MEMORY_MMAP_VMA_VM_AV_SITE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_AV_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMA_VM_AV_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_AV_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_AV_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMA_VM_AV_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMA_VM_AV_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMA_VM_AV_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_AV_CSRCFILE)

$(PK_MEMORY_MMAP_VMA_VM_AV_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMA_VM_AV_sh,$(srctree))
