PK_MEMORY_MMAP_VMA_VM_IO_NAME        := BiscuitOS-MEMORY-MMAP-VMA-VM-IO
PK_MEMORY_MMAP_VMA_VM_IO_VERSION     := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_VERSION)
PK_MEMORY_MMAP_VMA_VM_IO_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_URL)
PK_MEMORY_MMAP_VMA_VM_IO_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_GITHUB)
PK_MEMORY_MMAP_VMA_VM_IO_SUBNAME     :=
PK_MEMORY_MMAP_VMA_VM_IO_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_BUILDARCH)
PK_MEMORY_MMAP_VMA_VM_IO_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMA-VM-IO
PK_MEMORY_MMAP_VMA_VM_IO_PATCH       := $(PK_MEMORY_MMAP_VMA_VM_IO_DIR)/patch
PK_MEMORY_MMAP_VMA_VM_IO_BSBIT       := BiscuitOS-MEMORY-MMAP-VMA-VM-IO-$(PK_MEMORY_MMAP_VMA_VM_IO_VERSION).bsbit
PK_MEMORY_MMAP_VMA_VM_IO_OUT         :=
PK_MEMORY_MMAP_VMA_VM_IO_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_VMA_VM_IO_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_TAR)
PK_MEMORY_MMAP_VMA_VM_IO_PACKAGE     :=
PK_MEMORY_MMAP_VMA_VM_IO_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMA_VM_IO_DIR)/build

PK_MEMORY_MMAP_VMA_VM_IO_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMA_VM_IO_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMA_VM_IO_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_IO_VERSION)X \
                 $(PK_MEMORY_MMAP_VMA_VM_IO_SITE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_IO_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMA_VM_IO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_IO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_IO_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMA_VM_IO_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMA_VM_IO_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMA_VM_IO_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_IO_CSRCFILE)

$(PK_MEMORY_MMAP_VMA_VM_IO_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMA_VM_IO_sh,$(srctree))
