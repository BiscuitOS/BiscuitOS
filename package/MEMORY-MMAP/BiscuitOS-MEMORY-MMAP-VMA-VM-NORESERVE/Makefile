PK_MEMORY_MMAP_VMA_VM_NORESERVE_NAME        := BiscuitOS-MEMORY-MMAP-VMA-VM-NORESERVE
PK_MEMORY_MMAP_VMA_VM_NORESERVE_VERSION     := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_VERSION)
PK_MEMORY_MMAP_VMA_VM_NORESERVE_SITE        := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_URL)
PK_MEMORY_MMAP_VMA_VM_NORESERVE_GITHUB      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_GITHUB)
PK_MEMORY_MMAP_VMA_VM_NORESERVE_SUBNAME     :=
PK_MEMORY_MMAP_VMA_VM_NORESERVE_BUILDARCH   := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_BUILDARCH)
PK_MEMORY_MMAP_VMA_VM_NORESERVE_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-MMAP-VMA-VM-NORESERVE
PK_MEMORY_MMAP_VMA_VM_NORESERVE_PATCH       := $(PK_MEMORY_MMAP_VMA_VM_NORESERVE_DIR)/patch
PK_MEMORY_MMAP_VMA_VM_NORESERVE_BSBIT       := BiscuitOS-MEMORY-MMAP-VMA-VM-NORESERVE-$(PK_MEMORY_MMAP_VMA_VM_NORESERVE_VERSION).bsbit
PK_MEMORY_MMAP_VMA_VM_NORESERVE_OUT         :=
PK_MEMORY_MMAP_VMA_VM_NORESERVE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMORY_MMAP_VMA_VM_NORESERVE_TAR         := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_TAR)
PK_MEMORY_MMAP_VMA_VM_NORESERVE_PACKAGE     :=
PK_MEMORY_MMAP_VMA_VM_NORESERVE_CONFIG      := $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_MMAP_VMA_VM_NORESERVE_DIR)/build

PK_MEMORY_MMAP_VMA_VM_NORESERVE_sh = $(CONFIG_SHELL) $(PK_MEMORY_MMAP_VMA_VM_NORESERVE_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_MMAP_VMA_VM_NORESERVE_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NORESERVE_VERSION)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NORESERVE_SITE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NORESERVE_GITHUB)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NORESERVE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NORESERVE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NORESERVE_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_CONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_ASFLAGS)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NORESERVE_BSBIT)X \
                 $(PK_MEMORY_MMAP_VMA_VM_NORESERVE_BUILDARCH)X \
		 $(PK_MEMORY_MMAP_VMA_VM_NORESERVE_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_MMAP_VMA_VM_NORESERVE_CSRCFILE)

$(PK_MEMORY_MMAP_VMA_VM_NORESERVE_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_MMAP_VMA_VM_NORESERVE_sh,$(srctree))
