PK_MEMORY_GUP_API_FISW_NAME        := BiscuitOS-MEMORY-GUP-API-FISW
PK_MEMORY_GUP_API_FISW_VERSION     := $(CONFIG_PK_MEMORY_GUP_API_FISW_VERSION)
PK_MEMORY_GUP_API_FISW_SITE        := $(CONFIG_PK_MEMORY_GUP_API_FISW_URL)
PK_MEMORY_GUP_API_FISW_GITHUB      := $(CONFIG_PK_MEMORY_GUP_API_FISW_GITHUB)
PK_MEMORY_GUP_API_FISW_SUBNAME     :=
PK_MEMORY_GUP_API_FISW_BUILDARCH   := $(CONFIG_PK_MEMORY_GUP_API_FISW_BUILDARCH)
PK_MEMORY_GUP_API_FISW_DIR         := $(srctree)/package/MEMORY-MMAP/BiscuitOS-MEMORY-GUP-API-FISW
PK_MEMORY_GUP_API_FISW_PATCH       := $(PK_MEMORY_GUP_API_FISW_DIR)/patch
PK_MEMORY_GUP_API_FISW_BSBIT       := BiscuitOS-MEMORY-GUP-API-FISW-$(PK_MEMORY_GUP_API_FISW_VERSION).bsbit
PK_MEMORY_GUP_API_FISW_OUT         :=
PK_MEMORY_GUP_API_FISW_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMORY_GUP_API_FISW_TAR         := $(CONFIG_PK_MEMORY_GUP_API_FISW_TAR)
PK_MEMORY_GUP_API_FISW_PACKAGE     :=
PK_MEMORY_GUP_API_FISW_CONFIG      := $(CONFIG_PK_MEMORY_GUP_API_FISW_CONFIG)
SUB_TARGET                  += $(PK_MEMORY_GUP_API_FISW_DIR)/build

PK_MEMORY_GUP_API_FISW_sh = $(CONFIG_SHELL) $(PK_MEMORY_GUP_API_FISW_SCRIPTS) \
                 $1X \
                 $(PK_MEMORY_GUP_API_FISW_NAME)X \
                 $(PK_MEMORY_GUP_API_FISW_VERSION)X \
                 $(PK_MEMORY_GUP_API_FISW_SITE)X \
                 $(PK_MEMORY_GUP_API_FISW_GITHUB)X \
                 $(PK_MEMORY_GUP_API_FISW_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMORY_GUP_API_FISW_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMORY_GUP_API_FISW_SUBNAME)X \
                 $(CONFIG_PK_MEMORY_GUP_API_FISW_CONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_API_FISW_LIBPATH)X \
                 $(CONFIG_PK_MEMORY_GUP_API_FISW_CPPFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_API_FISW_DPKCONFIG)X \
                 $(CONFIG_PK_MEMORY_GUP_API_FISW_CFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_API_FISW_LDFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_API_FISW_CXXFLAGS)X \
                 $(CONFIG_PK_MEMORY_GUP_API_FISW_ASFLAGS)X \
                 $(PK_MEMORY_GUP_API_FISW_BSBIT)X \
                 $(PK_MEMORY_GUP_API_FISW_BUILDARCH)X \
		 $(PK_MEMORY_GUP_API_FISW_CSRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_API_FISW_SRCFILE)X \
                 $(CONFIG_PK_MEMORY_GUP_API_FISW_CSRCFILE)

$(PK_MEMORY_GUP_API_FISW_DIR)/build: FORCE
	$(Q)$(call PK_MEMORY_GUP_API_FISW_sh,$(srctree))
