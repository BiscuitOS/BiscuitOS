################################################
#
# BiscuitOS-select
#
################################################
PK_IO_BISCUITOS_SELECT_NAME        := BiscuitOS-select
PK_IO_BISCUITOS_SELECT_VERSION     := $(CONFIG_PK_IO_BISCUITOS_SELECT_VERSION)
PK_IO_BISCUITOS_SELECT_SITE        := $(CONFIG_PK_IO_BISCUITOS_SELECT_URL)
PK_IO_BISCUITOS_SELECT_GITHUB      := $(CONFIG_PK_IO_BISCUITOS_SELECT_GITHUB)
PK_IO_BISCUITOS_SELECT_SUBNAME     :=
PK_IO_BISCUITOS_SELECT_BUILDARCH   := $(CONFIG_PK_IO_BISCUITOS_SELECT_BUILDARCH)
PK_IO_BISCUITOS_SELECT_DIR         := $(srctree)/package/IO/BiscuitOS-select
PK_IO_BISCUITOS_SELECT_PATCH       := $(PK_IO_BISCUITOS_SELECT_DIR)/patch
PK_IO_BISCUITOS_SELECT_BSBIT       := BiscuitOS-select-$(PK_IO_BISCUITOS_SELECT_VERSION).bsbit
PK_IO_BISCUITOS_SELECT_OUT         :=
PK_IO_BISCUITOS_SELECT_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_IO_BISCUITOS_SELECT_TAR         := $(CONFIG_PK_IO_BISCUITOS_SELECT_TAR)
PK_IO_BISCUITOS_SELECT_PACKAGE     :=
PK_IO_BISCUITOS_SELECT_CONFIG      := $(CONFIG_PK_IO_BISCUITOS_SELECT_CONFIG)
SUB_TARGET                  += $(PK_IO_BISCUITOS_SELECT_DIR)/build

PK_IO_BISCUITOS_SELECT_sh = $(CONFIG_SHELL) $(PK_IO_BISCUITOS_SELECT_SCRIPTS) \
                 $1X \
                 $(PK_IO_BISCUITOS_SELECT_NAME)X \
                 $(PK_IO_BISCUITOS_SELECT_VERSION)X \
                 $(PK_IO_BISCUITOS_SELECT_SITE)X \
                 $(PK_IO_BISCUITOS_SELECT_GITHUB)X \
                 $(PK_IO_BISCUITOS_SELECT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_IO_BISCUITOS_SELECT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_IO_BISCUITOS_SELECT_SUBNAME)X \
                 $(CONFIG_PK_IO_BISCUITOS_SELECT_CONFIG)X \
                 $(CONFIG_PK_IO_BISCUITOS_SELECT_LIBPATH)X \
                 $(CONFIG_PK_IO_BISCUITOS_SELECT_CPPFLAGS)X \
                 $(CONFIG_PK_IO_BISCUITOS_SELECT_DPKCONFIG)X \
                 $(CONFIG_PK_IO_BISCUITOS_SELECT_CFLAGS)X \
                 $(CONFIG_PK_IO_BISCUITOS_SELECT_LDFLAGS)X \
                 $(CONFIG_PK_IO_BISCUITOS_SELECT_CXXFLAGS)X \
                 $(CONFIG_PK_IO_BISCUITOS_SELECT_ASFLAGS)X \
                 $(PK_IO_BISCUITOS_SELECT_BSBIT)X \
                 $(PK_IO_BISCUITOS_SELECT_BUILDARCH)X \
		 $(PK_IO_BISCUITOS_SELECT_CSRCFILE)X \
                 $(CONFIG_PK_IO_BISCUITOS_SELECT_SRCFILE)X \
                 $(CONFIG_PK_IO_BISCUITOS_SELECT_CSRCFILE)

$(PK_IO_BISCUITOS_SELECT_DIR)/build: FORCE
	$(Q)$(call PK_IO_BISCUITOS_SELECT_sh,$(srctree))
