################################################
#
# BiscuitOS-epoll
#
################################################
PK_IO_BISCUITOS_EPOLL_NAME        := BiscuitOS-epoll
PK_IO_BISCUITOS_EPOLL_VERSION     := $(CONFIG_PK_IO_BISCUITOS_EPOLL_VERSION)
PK_IO_BISCUITOS_EPOLL_SITE        := $(CONFIG_PK_IO_BISCUITOS_EPOLL_URL)
PK_IO_BISCUITOS_EPOLL_GITHUB      := $(CONFIG_PK_IO_BISCUITOS_EPOLL_GITHUB)
PK_IO_BISCUITOS_EPOLL_SUBNAME     :=
PK_IO_BISCUITOS_EPOLL_BUILDARCH   := $(CONFIG_PK_IO_BISCUITOS_EPOLL_BUILDARCH)
PK_IO_BISCUITOS_EPOLL_DIR         := $(srctree)/package/IO/BiscuitOS-epoll
PK_IO_BISCUITOS_EPOLL_PATCH       := $(PK_IO_BISCUITOS_EPOLL_DIR)/patch
PK_IO_BISCUITOS_EPOLL_BSBIT       := BiscuitOS-epoll-$(PK_IO_BISCUITOS_EPOLL_VERSION).bsbit
PK_IO_BISCUITOS_EPOLL_OUT         :=
PK_IO_BISCUITOS_EPOLL_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_IO_BISCUITOS_EPOLL_TAR         := $(CONFIG_PK_IO_BISCUITOS_EPOLL_TAR)
PK_IO_BISCUITOS_EPOLL_PACKAGE     :=
PK_IO_BISCUITOS_EPOLL_CONFIG      := $(CONFIG_PK_IO_BISCUITOS_EPOLL_CONFIG)
SUB_TARGET                  += $(PK_IO_BISCUITOS_EPOLL_DIR)/build

PK_IO_BISCUITOS_EPOLL_sh = $(CONFIG_SHELL) $(PK_IO_BISCUITOS_EPOLL_SCRIPTS) \
                 $1X \
                 $(PK_IO_BISCUITOS_EPOLL_NAME)X \
                 $(PK_IO_BISCUITOS_EPOLL_VERSION)X \
                 $(PK_IO_BISCUITOS_EPOLL_SITE)X \
                 $(PK_IO_BISCUITOS_EPOLL_GITHUB)X \
                 $(PK_IO_BISCUITOS_EPOLL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_IO_BISCUITOS_EPOLL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_IO_BISCUITOS_EPOLL_SUBNAME)X \
                 $(CONFIG_PK_IO_BISCUITOS_EPOLL_CONFIG)X \
                 $(CONFIG_PK_IO_BISCUITOS_EPOLL_LIBPATH)X \
                 $(CONFIG_PK_IO_BISCUITOS_EPOLL_CPPFLAGS)X \
                 $(CONFIG_PK_IO_BISCUITOS_EPOLL_DPKCONFIG)X \
                 $(CONFIG_PK_IO_BISCUITOS_EPOLL_CFLAGS)X \
                 $(CONFIG_PK_IO_BISCUITOS_EPOLL_LDFLAGS)X \
                 $(CONFIG_PK_IO_BISCUITOS_EPOLL_CXXFLAGS)X \
                 $(CONFIG_PK_IO_BISCUITOS_EPOLL_ASFLAGS)X \
                 $(PK_IO_BISCUITOS_EPOLL_BSBIT)X \
                 $(PK_IO_BISCUITOS_EPOLL_BUILDARCH)X \
		 $(PK_IO_BISCUITOS_EPOLL_CSRCFILE)X \
                 $(CONFIG_PK_IO_BISCUITOS_EPOLL_SRCFILE)X \
                 $(CONFIG_PK_IO_BISCUITOS_EPOLL_CSRCFILE)

$(PK_IO_BISCUITOS_EPOLL_DIR)/build: FORCE
	$(Q)$(call PK_IO_BISCUITOS_EPOLL_sh,$(srctree))
