################################################
#
# BiscuitOS-PAGE-userspace
#
################################################
PK_PAGE_ALLOC_USERSPACE_NAME        := BiscuitOS-PAGE-userspace
PK_PAGE_ALLOC_USERSPACE_VERSION     := $(CONFIG_PK_PAGE_ALLOC_USERSPACE_VERSION)
PK_PAGE_ALLOC_USERSPACE_SITE        := $(CONFIG_PK_PAGE_ALLOC_USERSPACE_URL)
PK_PAGE_ALLOC_USERSPACE_GITHUB      := $(CONFIG_PK_PAGE_ALLOC_USERSPACE_GITHUB)
PK_PAGE_ALLOC_USERSPACE_SUBNAME     :=
PK_PAGE_ALLOC_USERSPACE_BUILDARCH   := $(CONFIG_PK_PAGE_ALLOC_USERSPACE_BUILDARCH)
PK_PAGE_ALLOC_USERSPACE_DIR         := $(srctree)/package/PAGE/BiscuitOS-PAGE-userspace
PK_PAGE_ALLOC_USERSPACE_PATCH       := $(PK_PAGE_ALLOC_USERSPACE_DIR)/patch
PK_PAGE_ALLOC_USERSPACE_BSBIT       := BiscuitOS-PAGE-userspace-$(PK_PAGE_ALLOC_USERSPACE_VERSION).bsbit
PK_PAGE_ALLOC_USERSPACE_OUT         :=
PK_PAGE_ALLOC_USERSPACE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGE_ALLOC_USERSPACE_TAR         := $(CONFIG_PK_PAGE_ALLOC_USERSPACE_TAR)
PK_PAGE_ALLOC_USERSPACE_PACKAGE     :=
PK_PAGE_ALLOC_USERSPACE_CONFIG      := $(CONFIG_PK_PAGE_ALLOC_USERSPACE_CONFIG)
SUB_TARGET                  += $(PK_PAGE_ALLOC_USERSPACE_DIR)/build

PK_PAGE_ALLOC_USERSPACE_sh = $(CONFIG_SHELL) $(PK_PAGE_ALLOC_USERSPACE_SCRIPTS) \
                 $1X \
                 $(PK_PAGE_ALLOC_USERSPACE_NAME)X \
                 $(PK_PAGE_ALLOC_USERSPACE_VERSION)X \
                 $(PK_PAGE_ALLOC_USERSPACE_SITE)X \
                 $(PK_PAGE_ALLOC_USERSPACE_GITHUB)X \
                 $(PK_PAGE_ALLOC_USERSPACE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGE_ALLOC_USERSPACE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGE_ALLOC_USERSPACE_SUBNAME)X \
                 $(CONFIG_PK_PAGE_ALLOC_USERSPACE_CONFIG)X \
                 $(CONFIG_PK_PAGE_ALLOC_USERSPACE_LIBPATH)X \
                 $(CONFIG_PK_PAGE_ALLOC_USERSPACE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGE_ALLOC_USERSPACE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGE_ALLOC_USERSPACE_CFLAGS)X \
                 $(CONFIG_PK_PAGE_ALLOC_USERSPACE_LDFLAGS)X \
                 $(CONFIG_PK_PAGE_ALLOC_USERSPACE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGE_ALLOC_USERSPACE_ASFLAGS)X \
                 $(PK_PAGE_ALLOC_USERSPACE_BSBIT)X \
                 $(PK_PAGE_ALLOC_USERSPACE_BUILDARCH)X \
		 $(PK_PAGE_ALLOC_USERSPACE_CSRCFILE)X \
                 $(CONFIG_PK_PAGE_ALLOC_USERSPACE_SRCFILE)X \
                 $(CONFIG_PK_PAGE_ALLOC_USERSPACE_CSRCFILE)

$(PK_PAGE_ALLOC_USERSPACE_DIR)/build: FORCE
	$(Q)$(call PK_PAGE_ALLOC_USERSPACE_sh,$(srctree))
