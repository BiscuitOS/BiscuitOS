################################################
#
# BiscuitOS-THP-ShmemHugepage-file-shared
#
################################################
PK_THP_SHMEMHUGE_FILE_SHARED_NAME        := BiscuitOS-THP-ShmemHugepage-file-shared
PK_THP_SHMEMHUGE_FILE_SHARED_VERSION     := $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_VERSION)
PK_THP_SHMEMHUGE_FILE_SHARED_SITE        := $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_URL)
PK_THP_SHMEMHUGE_FILE_SHARED_GITHUB      := $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_GITHUB)
PK_THP_SHMEMHUGE_FILE_SHARED_SUBNAME     :=
PK_THP_SHMEMHUGE_FILE_SHARED_BUILDARCH   := $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_BUILDARCH)
PK_THP_SHMEMHUGE_FILE_SHARED_DIR         := $(srctree)/package/THP/BiscuitOS-THP-ShmemHugepage-file-shared
PK_THP_SHMEMHUGE_FILE_SHARED_PATCH       := $(PK_THP_SHMEMHUGE_FILE_SHARED_DIR)/patch
PK_THP_SHMEMHUGE_FILE_SHARED_BSBIT       := BiscuitOS-THP-ShmemHugepage-file-shared-$(PK_THP_SHMEMHUGE_FILE_SHARED_VERSION).bsbit
PK_THP_SHMEMHUGE_FILE_SHARED_OUT         :=
PK_THP_SHMEMHUGE_FILE_SHARED_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_THP_SHMEMHUGE_FILE_SHARED_TAR         := $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_TAR)
PK_THP_SHMEMHUGE_FILE_SHARED_PACKAGE     :=
PK_THP_SHMEMHUGE_FILE_SHARED_CONFIG      := $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_CONFIG)
SUB_TARGET                  += $(PK_THP_SHMEMHUGE_FILE_SHARED_DIR)/build

PK_THP_SHMEMHUGE_FILE_SHARED_sh = $(CONFIG_SHELL) $(PK_THP_SHMEMHUGE_FILE_SHARED_SCRIPTS) \
                 $1X \
                 $(PK_THP_SHMEMHUGE_FILE_SHARED_NAME)X \
                 $(PK_THP_SHMEMHUGE_FILE_SHARED_VERSION)X \
                 $(PK_THP_SHMEMHUGE_FILE_SHARED_SITE)X \
                 $(PK_THP_SHMEMHUGE_FILE_SHARED_GITHUB)X \
                 $(PK_THP_SHMEMHUGE_FILE_SHARED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_THP_SHMEMHUGE_FILE_SHARED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_THP_SHMEMHUGE_FILE_SHARED_SUBNAME)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_CONFIG)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_LIBPATH)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_CPPFLAGS)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_DPKCONFIG)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_CFLAGS)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_LDFLAGS)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_CXXFLAGS)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_ASFLAGS)X \
                 $(PK_THP_SHMEMHUGE_FILE_SHARED_BSBIT)X \
                 $(PK_THP_SHMEMHUGE_FILE_SHARED_BUILDARCH)X \
		 $(PK_THP_SHMEMHUGE_FILE_SHARED_CSRCFILE)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_SRCFILE)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_SHARED_CSRCFILE)

$(PK_THP_SHMEMHUGE_FILE_SHARED_DIR)/build: FORCE
	$(Q)$(call PK_THP_SHMEMHUGE_FILE_SHARED_sh,$(srctree))
