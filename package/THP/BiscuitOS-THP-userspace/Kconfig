	default "https://gitee.com/BiscuitOS_team/HardStack/raw/Gitee/Memory-Allocator/THP/BiscuitOS-THP-userspace"
	string "url"

config PK_THP_USERSPACE_GITHUB
	string "GitHub"

config PK_THP_USERSPACE_SRCFILE
	default "main.c Makefile RunBiscuitOS.sh README.md"
	string "Source code list"

config PK_THP_USERSPACE_CSRCFILE
	string "Compile source code list"

config PK_THP_USERSPACE_CONFIG
	string "CONFIG: Project Configure Options"

config PK_THP_USERSPACE_LIBPATH
	string "LIBS: Libraries to pass to the linker, e.g. -l<library>"

config PK_THP_USERSPACE_CPPFLAGS
	string "CPPFLAGS: C/C++ preprocessor flags, e.g. -I<include dir>"

config PK_THP_USERSPACE_DPKCONFIG
	string "PKG_CONFIG_PATH: Directories to add to pkg-config's search path"

config PK_THP_USERSPACE_CFLAGS
	string "CFLAGS: C compiler flags"

config PK_THP_USERSPACE_LDFLAGS
	string "LDFLAGS: Linker flags"

config PK_THP_USERSPACE_CXXFLAGS
	string "CXXFLAGS: C++ compiler flags"

config PK_THP_USERSPACE_ASFLAGS
	string "CCASFLAGS: assembler compiler flags (defaults to CFLAGS)"

config PK_THP_USERSPACE_BUILDARCH
	string "--host options on configure for Architecture"

endif # PK_THP_USERSPACE
