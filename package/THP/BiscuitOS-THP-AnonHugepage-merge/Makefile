PK_THP_ANONHUGE_MERGE_NAME        := BiscuitOS-THP-AnonHugepage-merge
PK_THP_ANONHUGE_MERGE_VERSION     := $(CONFIG_PK_THP_ANONHUGE_MERGE_VERSION)
PK_THP_ANONHUGE_MERGE_SITE        := $(CONFIG_PK_THP_ANONHUGE_MERGE_URL)
PK_THP_ANONHUGE_MERGE_GITHUB      := $(CONFIG_PK_THP_ANONHUGE_MERGE_GITHUB)
PK_THP_ANONHUGE_MERGE_SUBNAME     :=
PK_THP_ANONHUGE_MERGE_BUILDARCH   := $(CONFIG_PK_THP_ANONHUGE_MERGE_BUILDARCH)
PK_THP_ANONHUGE_MERGE_DIR         := $(srctree)/package/THP/BiscuitOS-THP-AnonHugepage-merge
PK_THP_ANONHUGE_MERGE_PATCH       := $(PK_THP_ANONHUGE_MERGE_DIR)/patch
PK_THP_ANONHUGE_MERGE_BSBIT       := BiscuitOS-THP-AnonHugepage-merge-$(PK_THP_ANONHUGE_MERGE_VERSION).bsbit
PK_THP_ANONHUGE_MERGE_OUT         :=
PK_THP_ANONHUGE_MERGE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_THP_ANONHUGE_MERGE_TAR         := $(CONFIG_PK_THP_ANONHUGE_MERGE_TAR)
PK_THP_ANONHUGE_MERGE_PACKAGE     :=
PK_THP_ANONHUGE_MERGE_CONFIG      := $(CONFIG_PK_THP_ANONHUGE_MERGE_CONFIG)
SUB_TARGET                  += $(PK_THP_ANONHUGE_MERGE_DIR)/build

PK_THP_ANONHUGE_MERGE_sh = $(CONFIG_SHELL) $(PK_THP_ANONHUGE_MERGE_SCRIPTS) \
                 $1X \
                 $(PK_THP_ANONHUGE_MERGE_NAME)X \
                 $(PK_THP_ANONHUGE_MERGE_VERSION)X \
                 $(PK_THP_ANONHUGE_MERGE_SITE)X \
                 $(PK_THP_ANONHUGE_MERGE_GITHUB)X \
                 $(PK_THP_ANONHUGE_MERGE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_THP_ANONHUGE_MERGE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_THP_ANONHUGE_MERGE_SUBNAME)X \
                 $(CONFIG_PK_THP_ANONHUGE_MERGE_CONFIG)X \
                 $(CONFIG_PK_THP_ANONHUGE_MERGE_LIBPATH)X \
                 $(CONFIG_PK_THP_ANONHUGE_MERGE_CPPFLAGS)X \
                 $(CONFIG_PK_THP_ANONHUGE_MERGE_DPKCONFIG)X \
                 $(CONFIG_PK_THP_ANONHUGE_MERGE_CFLAGS)X \
                 $(CONFIG_PK_THP_ANONHUGE_MERGE_LDFLAGS)X \
                 $(CONFIG_PK_THP_ANONHUGE_MERGE_CXXFLAGS)X \
                 $(CONFIG_PK_THP_ANONHUGE_MERGE_ASFLAGS)X \
                 $(PK_THP_ANONHUGE_MERGE_BSBIT)X \
                 $(PK_THP_ANONHUGE_MERGE_BUILDARCH)X \
		 $(PK_THP_ANONHUGE_MERGE_CSRCFILE)X \
                 $(CONFIG_PK_THP_ANONHUGE_MERGE_SRCFILE)X \
                 $(CONFIG_PK_THP_ANONHUGE_MERGE_CSRCFILE)

$(PK_THP_ANONHUGE_MERGE_DIR)/build: FORCE
	$(Q)$(call PK_THP_ANONHUGE_MERGE_sh,$(srctree))
