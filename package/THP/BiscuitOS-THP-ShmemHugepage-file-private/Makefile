################################################
#
# BiscuitOS-THP-ShmemHugepage-file-private
#
################################################
PK_THP_SHMEMHUGE_FILE_PRIVATE_NAME        := BiscuitOS-THP-ShmemHugepage-file-private
PK_THP_SHMEMHUGE_FILE_PRIVATE_VERSION     := $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_VERSION)
PK_THP_SHMEMHUGE_FILE_PRIVATE_SITE        := $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_URL)
PK_THP_SHMEMHUGE_FILE_PRIVATE_GITHUB      := $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_GITHUB)
PK_THP_SHMEMHUGE_FILE_PRIVATE_SUBNAME     :=
PK_THP_SHMEMHUGE_FILE_PRIVATE_BUILDARCH   := $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_BUILDARCH)
PK_THP_SHMEMHUGE_FILE_PRIVATE_DIR         := $(srctree)/package/THP/BiscuitOS-THP-ShmemHugepage-file-private
PK_THP_SHMEMHUGE_FILE_PRIVATE_PATCH       := $(PK_THP_SHMEMHUGE_FILE_PRIVATE_DIR)/patch
PK_THP_SHMEMHUGE_FILE_PRIVATE_BSBIT       := BiscuitOS-THP-ShmemHugepage-file-private-$(PK_THP_SHMEMHUGE_FILE_PRIVATE_VERSION).bsbit
PK_THP_SHMEMHUGE_FILE_PRIVATE_OUT         :=
PK_THP_SHMEMHUGE_FILE_PRIVATE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_THP_SHMEMHUGE_FILE_PRIVATE_TAR         := $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_TAR)
PK_THP_SHMEMHUGE_FILE_PRIVATE_PACKAGE     :=
PK_THP_SHMEMHUGE_FILE_PRIVATE_CONFIG      := $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_CONFIG)
SUB_TARGET                  += $(PK_THP_SHMEMHUGE_FILE_PRIVATE_DIR)/build

PK_THP_SHMEMHUGE_FILE_PRIVATE_sh = $(CONFIG_SHELL) $(PK_THP_SHMEMHUGE_FILE_PRIVATE_SCRIPTS) \
                 $1X \
                 $(PK_THP_SHMEMHUGE_FILE_PRIVATE_NAME)X \
                 $(PK_THP_SHMEMHUGE_FILE_PRIVATE_VERSION)X \
                 $(PK_THP_SHMEMHUGE_FILE_PRIVATE_SITE)X \
                 $(PK_THP_SHMEMHUGE_FILE_PRIVATE_GITHUB)X \
                 $(PK_THP_SHMEMHUGE_FILE_PRIVATE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_THP_SHMEMHUGE_FILE_PRIVATE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_THP_SHMEMHUGE_FILE_PRIVATE_SUBNAME)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_CONFIG)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_LIBPATH)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_CPPFLAGS)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_DPKCONFIG)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_CFLAGS)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_LDFLAGS)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_CXXFLAGS)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_ASFLAGS)X \
                 $(PK_THP_SHMEMHUGE_FILE_PRIVATE_BSBIT)X \
                 $(PK_THP_SHMEMHUGE_FILE_PRIVATE_BUILDARCH)X \
		 $(PK_THP_SHMEMHUGE_FILE_PRIVATE_CSRCFILE)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_SRCFILE)X \
                 $(CONFIG_PK_THP_SHMEMHUGE_FILE_PRIVATE_CSRCFILE)

$(PK_THP_SHMEMHUGE_FILE_PRIVATE_DIR)/build: FORCE
	$(Q)$(call PK_THP_SHMEMHUGE_FILE_PRIVATE_sh,$(srctree))
