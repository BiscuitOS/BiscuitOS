PK_THP_ANONHUGE_MADVISE_NAME        := BiscuitOS-THP-AnonHugepage-madvise
PK_THP_ANONHUGE_MADVISE_VERSION     := $(CONFIG_PK_THP_ANONHUGE_MADVISE_VERSION)
PK_THP_ANONHUGE_MADVISE_SITE        := $(CONFIG_PK_THP_ANONHUGE_MADVISE_URL)
PK_THP_ANONHUGE_MADVISE_GITHUB      := $(CONFIG_PK_THP_ANONHUGE_MADVISE_GITHUB)
PK_THP_ANONHUGE_MADVISE_SUBNAME     :=
PK_THP_ANONHUGE_MADVISE_BUILDARCH   := $(CONFIG_PK_THP_ANONHUGE_MADVISE_BUILDARCH)
PK_THP_ANONHUGE_MADVISE_DIR         := $(srctree)/package/THP/BiscuitOS-THP-AnonHugepage-madvise
PK_THP_ANONHUGE_MADVISE_PATCH       := $(PK_THP_ANONHUGE_MADVISE_DIR)/patch
PK_THP_ANONHUGE_MADVISE_BSBIT       := BiscuitOS-THP-AnonHugepage-madvise-$(PK_THP_ANONHUGE_MADVISE_VERSION).bsbit
PK_THP_ANONHUGE_MADVISE_OUT         :=
PK_THP_ANONHUGE_MADVISE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_THP_ANONHUGE_MADVISE_TAR         := $(CONFIG_PK_THP_ANONHUGE_MADVISE_TAR)
PK_THP_ANONHUGE_MADVISE_PACKAGE     :=
PK_THP_ANONHUGE_MADVISE_CONFIG      := $(CONFIG_PK_THP_ANONHUGE_MADVISE_CONFIG)
SUB_TARGET                  += $(PK_THP_ANONHUGE_MADVISE_DIR)/build

PK_THP_ANONHUGE_MADVISE_sh = $(CONFIG_SHELL) $(PK_THP_ANONHUGE_MADVISE_SCRIPTS) \
                 $1X \
                 $(PK_THP_ANONHUGE_MADVISE_NAME)X \
                 $(PK_THP_ANONHUGE_MADVISE_VERSION)X \
                 $(PK_THP_ANONHUGE_MADVISE_SITE)X \
                 $(PK_THP_ANONHUGE_MADVISE_GITHUB)X \
                 $(PK_THP_ANONHUGE_MADVISE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_THP_ANONHUGE_MADVISE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_THP_ANONHUGE_MADVISE_SUBNAME)X \
                 $(CONFIG_PK_THP_ANONHUGE_MADVISE_CONFIG)X \
                 $(CONFIG_PK_THP_ANONHUGE_MADVISE_LIBPATH)X \
                 $(CONFIG_PK_THP_ANONHUGE_MADVISE_CPPFLAGS)X \
                 $(CONFIG_PK_THP_ANONHUGE_MADVISE_DPKCONFIG)X \
                 $(CONFIG_PK_THP_ANONHUGE_MADVISE_CFLAGS)X \
                 $(CONFIG_PK_THP_ANONHUGE_MADVISE_LDFLAGS)X \
                 $(CONFIG_PK_THP_ANONHUGE_MADVISE_CXXFLAGS)X \
                 $(CONFIG_PK_THP_ANONHUGE_MADVISE_ASFLAGS)X \
                 $(PK_THP_ANONHUGE_MADVISE_BSBIT)X \
                 $(PK_THP_ANONHUGE_MADVISE_BUILDARCH)X \
		 $(PK_THP_ANONHUGE_MADVISE_CSRCFILE)X \
                 $(CONFIG_PK_THP_ANONHUGE_MADVISE_SRCFILE)X \
                 $(CONFIG_PK_THP_ANONHUGE_MADVISE_CSRCFILE)

$(PK_THP_ANONHUGE_MADVISE_DIR)/build: FORCE
	$(Q)$(call PK_THP_ANONHUGE_MADVISE_sh,$(srctree))
