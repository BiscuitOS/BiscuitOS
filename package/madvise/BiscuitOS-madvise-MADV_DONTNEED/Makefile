PK_MADVISE_MADV_DONTNEED_NAME        := BiscuitOS-madvise-MADV_DONTNEED
PK_MADVISE_MADV_DONTNEED_VERSION     := $(CONFIG_PK_MADVISE_MADV_DONTNEED_VERSION)
PK_MADVISE_MADV_DONTNEED_SITE        := $(CONFIG_PK_MADVISE_MADV_DONTNEED_URL)
PK_MADVISE_MADV_DONTNEED_GITHUB      := $(CONFIG_PK_MADVISE_MADV_DONTNEED_GITHUB)
PK_MADVISE_MADV_DONTNEED_SUBNAME     :=
PK_MADVISE_MADV_DONTNEED_BUILDARCH   := $(CONFIG_PK_MADVISE_MADV_DONTNEED_BUILDARCH)
PK_MADVISE_MADV_DONTNEED_DIR         := $(srctree)/package/madvise/BiscuitOS-madvise-MADV_DONTNEED
PK_MADVISE_MADV_DONTNEED_PATCH       := $(PK_MADVISE_MADV_DONTNEED_DIR)/patch
PK_MADVISE_MADV_DONTNEED_BSBIT       := BiscuitOS-madvise-MADV_DONTNEED-$(PK_MADVISE_MADV_DONTNEED_VERSION).bsbit
PK_MADVISE_MADV_DONTNEED_OUT         :=
PK_MADVISE_MADV_DONTNEED_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MADVISE_MADV_DONTNEED_TAR         := $(CONFIG_PK_MADVISE_MADV_DONTNEED_TAR)
PK_MADVISE_MADV_DONTNEED_PACKAGE     :=
PK_MADVISE_MADV_DONTNEED_CONFIG      := $(CONFIG_PK_MADVISE_MADV_DONTNEED_CONFIG)
SUB_TARGET                  += $(PK_MADVISE_MADV_DONTNEED_DIR)/build

PK_MADVISE_MADV_DONTNEED_sh = $(CONFIG_SHELL) $(PK_MADVISE_MADV_DONTNEED_SCRIPTS) \
                 $1X \
                 $(PK_MADVISE_MADV_DONTNEED_NAME)X \
                 $(PK_MADVISE_MADV_DONTNEED_VERSION)X \
                 $(PK_MADVISE_MADV_DONTNEED_SITE)X \
                 $(PK_MADVISE_MADV_DONTNEED_GITHUB)X \
                 $(PK_MADVISE_MADV_DONTNEED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MADVISE_MADV_DONTNEED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MADVISE_MADV_DONTNEED_SUBNAME)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTNEED_CONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTNEED_LIBPATH)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTNEED_CPPFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTNEED_DPKCONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTNEED_CFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTNEED_LDFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTNEED_CXXFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTNEED_ASFLAGS)X \
                 $(PK_MADVISE_MADV_DONTNEED_BSBIT)X \
                 $(PK_MADVISE_MADV_DONTNEED_BUILDARCH)X \
		 $(PK_MADVISE_MADV_DONTNEED_CSRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTNEED_SRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTNEED_CSRCFILE)

$(PK_MADVISE_MADV_DONTNEED_DIR)/build: FORCE
	$(Q)$(call PK_MADVISE_MADV_DONTNEED_sh,$(srctree))
