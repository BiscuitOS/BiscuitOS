PK_MADVISE_MADV_REMOVE_NAME        := BiscuitOS-madvise-MADV_REMOVE
PK_MADVISE_MADV_REMOVE_VERSION     := $(CONFIG_PK_MADVISE_MADV_REMOVE_VERSION)
PK_MADVISE_MADV_REMOVE_SITE        := $(CONFIG_PK_MADVISE_MADV_REMOVE_URL)
PK_MADVISE_MADV_REMOVE_GITHUB      := $(CONFIG_PK_MADVISE_MADV_REMOVE_GITHUB)
PK_MADVISE_MADV_REMOVE_SUBNAME     :=
PK_MADVISE_MADV_REMOVE_BUILDARCH   := $(CONFIG_PK_MADVISE_MADV_REMOVE_BUILDARCH)
PK_MADVISE_MADV_REMOVE_DIR         := $(srctree)/package/madvise/BiscuitOS-madvise-MADV_REMOVE
PK_MADVISE_MADV_REMOVE_PATCH       := $(PK_MADVISE_MADV_REMOVE_DIR)/patch
PK_MADVISE_MADV_REMOVE_BSBIT       := BiscuitOS-madvise-MADV_REMOVE-$(PK_MADVISE_MADV_REMOVE_VERSION).bsbit
PK_MADVISE_MADV_REMOVE_OUT         :=
PK_MADVISE_MADV_REMOVE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MADVISE_MADV_REMOVE_TAR         := $(CONFIG_PK_MADVISE_MADV_REMOVE_TAR)
PK_MADVISE_MADV_REMOVE_PACKAGE     :=
PK_MADVISE_MADV_REMOVE_CONFIG      := $(CONFIG_PK_MADVISE_MADV_REMOVE_CONFIG)
SUB_TARGET                  += $(PK_MADVISE_MADV_REMOVE_DIR)/build

PK_MADVISE_MADV_REMOVE_sh = $(CONFIG_SHELL) $(PK_MADVISE_MADV_REMOVE_SCRIPTS) \
                 $1X \
                 $(PK_MADVISE_MADV_REMOVE_NAME)X \
                 $(PK_MADVISE_MADV_REMOVE_VERSION)X \
                 $(PK_MADVISE_MADV_REMOVE_SITE)X \
                 $(PK_MADVISE_MADV_REMOVE_GITHUB)X \
                 $(PK_MADVISE_MADV_REMOVE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MADVISE_MADV_REMOVE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MADVISE_MADV_REMOVE_SUBNAME)X \
                 $(CONFIG_PK_MADVISE_MADV_REMOVE_CONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_REMOVE_LIBPATH)X \
                 $(CONFIG_PK_MADVISE_MADV_REMOVE_CPPFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_REMOVE_DPKCONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_REMOVE_CFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_REMOVE_LDFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_REMOVE_CXXFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_REMOVE_ASFLAGS)X \
                 $(PK_MADVISE_MADV_REMOVE_BSBIT)X \
                 $(PK_MADVISE_MADV_REMOVE_BUILDARCH)X \
		 $(PK_MADVISE_MADV_REMOVE_CSRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_REMOVE_SRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_REMOVE_CSRCFILE)

$(PK_MADVISE_MADV_REMOVE_DIR)/build: FORCE
	$(Q)$(call PK_MADVISE_MADV_REMOVE_sh,$(srctree))
