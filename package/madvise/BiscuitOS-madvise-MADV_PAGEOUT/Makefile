PK_MADVISE_MADV_PAGEOUT_NAME        := BiscuitOS-madvise-MADV_PAGEOUT
PK_MADVISE_MADV_PAGEOUT_VERSION     := $(CONFIG_PK_MADVISE_MADV_PAGEOUT_VERSION)
PK_MADVISE_MADV_PAGEOUT_SITE        := $(CONFIG_PK_MADVISE_MADV_PAGEOUT_URL)
PK_MADVISE_MADV_PAGEOUT_GITHUB      := $(CONFIG_PK_MADVISE_MADV_PAGEOUT_GITHUB)
PK_MADVISE_MADV_PAGEOUT_SUBNAME     :=
PK_MADVISE_MADV_PAGEOUT_BUILDARCH   := $(CONFIG_PK_MADVISE_MADV_PAGEOUT_BUILDARCH)
PK_MADVISE_MADV_PAGEOUT_DIR         := $(srctree)/package/madvise/BiscuitOS-madvise-MADV_PAGEOUT
PK_MADVISE_MADV_PAGEOUT_PATCH       := $(PK_MADVISE_MADV_PAGEOUT_DIR)/patch
PK_MADVISE_MADV_PAGEOUT_BSBIT       := BiscuitOS-madvise-MADV_PAGEOUT-$(PK_MADVISE_MADV_PAGEOUT_VERSION).bsbit
PK_MADVISE_MADV_PAGEOUT_OUT         :=
PK_MADVISE_MADV_PAGEOUT_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MADVISE_MADV_PAGEOUT_TAR         := $(CONFIG_PK_MADVISE_MADV_PAGEOUT_TAR)
PK_MADVISE_MADV_PAGEOUT_PACKAGE     :=
PK_MADVISE_MADV_PAGEOUT_CONFIG      := $(CONFIG_PK_MADVISE_MADV_PAGEOUT_CONFIG)
SUB_TARGET                  += $(PK_MADVISE_MADV_PAGEOUT_DIR)/build

PK_MADVISE_MADV_PAGEOUT_sh = $(CONFIG_SHELL) $(PK_MADVISE_MADV_PAGEOUT_SCRIPTS) \
                 $1X \
                 $(PK_MADVISE_MADV_PAGEOUT_NAME)X \
                 $(PK_MADVISE_MADV_PAGEOUT_VERSION)X \
                 $(PK_MADVISE_MADV_PAGEOUT_SITE)X \
                 $(PK_MADVISE_MADV_PAGEOUT_GITHUB)X \
                 $(PK_MADVISE_MADV_PAGEOUT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MADVISE_MADV_PAGEOUT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MADVISE_MADV_PAGEOUT_SUBNAME)X \
                 $(CONFIG_PK_MADVISE_MADV_PAGEOUT_CONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_PAGEOUT_LIBPATH)X \
                 $(CONFIG_PK_MADVISE_MADV_PAGEOUT_CPPFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_PAGEOUT_DPKCONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_PAGEOUT_CFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_PAGEOUT_LDFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_PAGEOUT_CXXFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_PAGEOUT_ASFLAGS)X \
                 $(PK_MADVISE_MADV_PAGEOUT_BSBIT)X \
                 $(PK_MADVISE_MADV_PAGEOUT_BUILDARCH)X \
		 $(PK_MADVISE_MADV_PAGEOUT_CSRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_PAGEOUT_SRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_PAGEOUT_CSRCFILE)

$(PK_MADVISE_MADV_PAGEOUT_DIR)/build: FORCE
	$(Q)$(call PK_MADVISE_MADV_PAGEOUT_sh,$(srctree))
