PK_MADVISE_MADV_DONTFORK_NAME        := BiscuitOS-madvise-MADV_DONTFORK
PK_MADVISE_MADV_DONTFORK_VERSION     := $(CONFIG_PK_MADVISE_MADV_DONTFORK_VERSION)
PK_MADVISE_MADV_DONTFORK_SITE        := $(CONFIG_PK_MADVISE_MADV_DONTFORK_URL)
PK_MADVISE_MADV_DONTFORK_GITHUB      := $(CONFIG_PK_MADVISE_MADV_DONTFORK_GITHUB)
PK_MADVISE_MADV_DONTFORK_SUBNAME     :=
PK_MADVISE_MADV_DONTFORK_BUILDARCH   := $(CONFIG_PK_MADVISE_MADV_DONTFORK_BUILDARCH)
PK_MADVISE_MADV_DONTFORK_DIR         := $(srctree)/package/madvise/BiscuitOS-madvise-MADV_DONTFORK
PK_MADVISE_MADV_DONTFORK_PATCH       := $(PK_MADVISE_MADV_DONTFORK_DIR)/patch
PK_MADVISE_MADV_DONTFORK_BSBIT       := BiscuitOS-madvise-MADV_DONTFORK-$(PK_MADVISE_MADV_DONTFORK_VERSION).bsbit
PK_MADVISE_MADV_DONTFORK_OUT         :=
PK_MADVISE_MADV_DONTFORK_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MADVISE_MADV_DONTFORK_TAR         := $(CONFIG_PK_MADVISE_MADV_DONTFORK_TAR)
PK_MADVISE_MADV_DONTFORK_PACKAGE     :=
PK_MADVISE_MADV_DONTFORK_CONFIG      := $(CONFIG_PK_MADVISE_MADV_DONTFORK_CONFIG)
SUB_TARGET                  += $(PK_MADVISE_MADV_DONTFORK_DIR)/build

PK_MADVISE_MADV_DONTFORK_sh = $(CONFIG_SHELL) $(PK_MADVISE_MADV_DONTFORK_SCRIPTS) \
                 $1X \
                 $(PK_MADVISE_MADV_DONTFORK_NAME)X \
                 $(PK_MADVISE_MADV_DONTFORK_VERSION)X \
                 $(PK_MADVISE_MADV_DONTFORK_SITE)X \
                 $(PK_MADVISE_MADV_DONTFORK_GITHUB)X \
                 $(PK_MADVISE_MADV_DONTFORK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MADVISE_MADV_DONTFORK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MADVISE_MADV_DONTFORK_SUBNAME)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTFORK_CONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTFORK_LIBPATH)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTFORK_CPPFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTFORK_DPKCONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTFORK_CFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTFORK_LDFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTFORK_CXXFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTFORK_ASFLAGS)X \
                 $(PK_MADVISE_MADV_DONTFORK_BSBIT)X \
                 $(PK_MADVISE_MADV_DONTFORK_BUILDARCH)X \
		 $(PK_MADVISE_MADV_DONTFORK_CSRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTFORK_SRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_DONTFORK_CSRCFILE)

$(PK_MADVISE_MADV_DONTFORK_DIR)/build: FORCE
	$(Q)$(call PK_MADVISE_MADV_DONTFORK_sh,$(srctree))
