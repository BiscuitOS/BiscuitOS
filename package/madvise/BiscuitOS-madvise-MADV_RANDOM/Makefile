PK_MADVISE_MADV_RANDOM_NAME        := BiscuitOS-madvise-MADV_RANDOM
PK_MADVISE_MADV_RANDOM_VERSION     := $(CONFIG_PK_MADVISE_MADV_RANDOM_VERSION)
PK_MADVISE_MADV_RANDOM_SITE        := $(CONFIG_PK_MADVISE_MADV_RANDOM_URL)
PK_MADVISE_MADV_RANDOM_GITHUB      := $(CONFIG_PK_MADVISE_MADV_RANDOM_GITHUB)
PK_MADVISE_MADV_RANDOM_SUBNAME     :=
PK_MADVISE_MADV_RANDOM_BUILDARCH   := $(CONFIG_PK_MADVISE_MADV_RANDOM_BUILDARCH)
PK_MADVISE_MADV_RANDOM_DIR         := $(srctree)/package/madvise/BiscuitOS-madvise-MADV_RANDOM
PK_MADVISE_MADV_RANDOM_PATCH       := $(PK_MADVISE_MADV_RANDOM_DIR)/patch
PK_MADVISE_MADV_RANDOM_BSBIT       := BiscuitOS-madvise-MADV_RANDOM-$(PK_MADVISE_MADV_RANDOM_VERSION).bsbit
PK_MADVISE_MADV_RANDOM_OUT         :=
PK_MADVISE_MADV_RANDOM_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MADVISE_MADV_RANDOM_TAR         := $(CONFIG_PK_MADVISE_MADV_RANDOM_TAR)
PK_MADVISE_MADV_RANDOM_PACKAGE     :=
PK_MADVISE_MADV_RANDOM_CONFIG      := $(CONFIG_PK_MADVISE_MADV_RANDOM_CONFIG)
SUB_TARGET                  += $(PK_MADVISE_MADV_RANDOM_DIR)/build

PK_MADVISE_MADV_RANDOM_sh = $(CONFIG_SHELL) $(PK_MADVISE_MADV_RANDOM_SCRIPTS) \
                 $1X \
                 $(PK_MADVISE_MADV_RANDOM_NAME)X \
                 $(PK_MADVISE_MADV_RANDOM_VERSION)X \
                 $(PK_MADVISE_MADV_RANDOM_SITE)X \
                 $(PK_MADVISE_MADV_RANDOM_GITHUB)X \
                 $(PK_MADVISE_MADV_RANDOM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MADVISE_MADV_RANDOM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MADVISE_MADV_RANDOM_SUBNAME)X \
                 $(CONFIG_PK_MADVISE_MADV_RANDOM_CONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_RANDOM_LIBPATH)X \
                 $(CONFIG_PK_MADVISE_MADV_RANDOM_CPPFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_RANDOM_DPKCONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_RANDOM_CFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_RANDOM_LDFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_RANDOM_CXXFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_RANDOM_ASFLAGS)X \
                 $(PK_MADVISE_MADV_RANDOM_BSBIT)X \
                 $(PK_MADVISE_MADV_RANDOM_BUILDARCH)X \
		 $(PK_MADVISE_MADV_RANDOM_CSRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_RANDOM_SRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_RANDOM_CSRCFILE)

$(PK_MADVISE_MADV_RANDOM_DIR)/build: FORCE
	$(Q)$(call PK_MADVISE_MADV_RANDOM_sh,$(srctree))
