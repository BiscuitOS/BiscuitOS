PK_MADVISE_MADV_FREE_NAME        := BiscuitOS-madvise-MADV_FREE
PK_MADVISE_MADV_FREE_VERSION     := $(CONFIG_PK_MADVISE_MADV_FREE_VERSION)
PK_MADVISE_MADV_FREE_SITE        := $(CONFIG_PK_MADVISE_MADV_FREE_URL)
PK_MADVISE_MADV_FREE_GITHUB      := $(CONFIG_PK_MADVISE_MADV_FREE_GITHUB)
PK_MADVISE_MADV_FREE_SUBNAME     :=
PK_MADVISE_MADV_FREE_BUILDARCH   := $(CONFIG_PK_MADVISE_MADV_FREE_BUILDARCH)
PK_MADVISE_MADV_FREE_DIR         := $(srctree)/package/madvise/BiscuitOS-madvise-MADV_FREE
PK_MADVISE_MADV_FREE_PATCH       := $(PK_MADVISE_MADV_FREE_DIR)/patch
PK_MADVISE_MADV_FREE_BSBIT       := BiscuitOS-madvise-MADV_FREE-$(PK_MADVISE_MADV_FREE_VERSION).bsbit
PK_MADVISE_MADV_FREE_OUT         :=
PK_MADVISE_MADV_FREE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MADVISE_MADV_FREE_TAR         := $(CONFIG_PK_MADVISE_MADV_FREE_TAR)
PK_MADVISE_MADV_FREE_PACKAGE     :=
PK_MADVISE_MADV_FREE_CONFIG      := $(CONFIG_PK_MADVISE_MADV_FREE_CONFIG)
SUB_TARGET                  += $(PK_MADVISE_MADV_FREE_DIR)/build

PK_MADVISE_MADV_FREE_sh = $(CONFIG_SHELL) $(PK_MADVISE_MADV_FREE_SCRIPTS) \
                 $1X \
                 $(PK_MADVISE_MADV_FREE_NAME)X \
                 $(PK_MADVISE_MADV_FREE_VERSION)X \
                 $(PK_MADVISE_MADV_FREE_SITE)X \
                 $(PK_MADVISE_MADV_FREE_GITHUB)X \
                 $(PK_MADVISE_MADV_FREE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MADVISE_MADV_FREE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MADVISE_MADV_FREE_SUBNAME)X \
                 $(CONFIG_PK_MADVISE_MADV_FREE_CONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_FREE_LIBPATH)X \
                 $(CONFIG_PK_MADVISE_MADV_FREE_CPPFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_FREE_DPKCONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_FREE_CFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_FREE_LDFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_FREE_CXXFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_FREE_ASFLAGS)X \
                 $(PK_MADVISE_MADV_FREE_BSBIT)X \
                 $(PK_MADVISE_MADV_FREE_BUILDARCH)X \
		 $(PK_MADVISE_MADV_FREE_CSRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_FREE_SRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_FREE_CSRCFILE)

$(PK_MADVISE_MADV_FREE_DIR)/build: FORCE
	$(Q)$(call PK_MADVISE_MADV_FREE_sh,$(srctree))
