PK_MADVISE_MADV_NORMAL_NAME        := BiscuitOS-madvise-MADV_NORMAL
PK_MADVISE_MADV_NORMAL_VERSION     := $(CONFIG_PK_MADVISE_MADV_NORMAL_VERSION)
PK_MADVISE_MADV_NORMAL_SITE        := $(CONFIG_PK_MADVISE_MADV_NORMAL_URL)
PK_MADVISE_MADV_NORMAL_GITHUB      := $(CONFIG_PK_MADVISE_MADV_NORMAL_GITHUB)
PK_MADVISE_MADV_NORMAL_SUBNAME     :=
PK_MADVISE_MADV_NORMAL_BUILDARCH   := $(CONFIG_PK_MADVISE_MADV_NORMAL_BUILDARCH)
PK_MADVISE_MADV_NORMAL_DIR         := $(srctree)/package/madvise/BiscuitOS-madvise-MADV_NORMAL
PK_MADVISE_MADV_NORMAL_PATCH       := $(PK_MADVISE_MADV_NORMAL_DIR)/patch
PK_MADVISE_MADV_NORMAL_BSBIT       := BiscuitOS-madvise-MADV_NORMAL-$(PK_MADVISE_MADV_NORMAL_VERSION).bsbit
PK_MADVISE_MADV_NORMAL_OUT         :=
PK_MADVISE_MADV_NORMAL_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MADVISE_MADV_NORMAL_TAR         := $(CONFIG_PK_MADVISE_MADV_NORMAL_TAR)
PK_MADVISE_MADV_NORMAL_PACKAGE     :=
PK_MADVISE_MADV_NORMAL_CONFIG      := $(CONFIG_PK_MADVISE_MADV_NORMAL_CONFIG)
SUB_TARGET                  += $(PK_MADVISE_MADV_NORMAL_DIR)/build

PK_MADVISE_MADV_NORMAL_sh = $(CONFIG_SHELL) $(PK_MADVISE_MADV_NORMAL_SCRIPTS) \
                 $1X \
                 $(PK_MADVISE_MADV_NORMAL_NAME)X \
                 $(PK_MADVISE_MADV_NORMAL_VERSION)X \
                 $(PK_MADVISE_MADV_NORMAL_SITE)X \
                 $(PK_MADVISE_MADV_NORMAL_GITHUB)X \
                 $(PK_MADVISE_MADV_NORMAL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MADVISE_MADV_NORMAL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MADVISE_MADV_NORMAL_SUBNAME)X \
                 $(CONFIG_PK_MADVISE_MADV_NORMAL_CONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_NORMAL_LIBPATH)X \
                 $(CONFIG_PK_MADVISE_MADV_NORMAL_CPPFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_NORMAL_DPKCONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_NORMAL_CFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_NORMAL_LDFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_NORMAL_CXXFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_NORMAL_ASFLAGS)X \
                 $(PK_MADVISE_MADV_NORMAL_BSBIT)X \
                 $(PK_MADVISE_MADV_NORMAL_BUILDARCH)X \
		 $(PK_MADVISE_MADV_NORMAL_CSRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_NORMAL_SRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_NORMAL_CSRCFILE)

$(PK_MADVISE_MADV_NORMAL_DIR)/build: FORCE
	$(Q)$(call PK_MADVISE_MADV_NORMAL_sh,$(srctree))
