PK_MADVISE_MADV_WILLNEED_CONFIG      := $(CONFIG_PK_MADVISE_MADV_WILLNEED_CONFIG)
SUB_TARGET                  += $(PK_MADVISE_MADV_WILLNEED_DIR)/build

PK_MADVISE_MADV_WILLNEED_sh = $(CONFIG_SHELL) $(PK_MADVISE_MADV_WILLNEED_SCRIPTS) \
                 $1X \
