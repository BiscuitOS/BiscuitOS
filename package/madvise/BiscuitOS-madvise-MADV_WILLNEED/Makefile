PK_MADVISE_MADV_WILLNEED_SITE        := $(CONFIG_PK_MADVISE_MADV_WILLNEED_URL)
PK_MADVISE_MADV_WILLNEED_GITHUB      := $(CONFIG_PK_MADVISE_MADV_WILLNEED_GITHUB)
PK_MADVISE_MADV_WILLNEED_SUBNAME     :=
