                 $(PK_MADVISE_MADV_WILLNEED_SITE)X \
                 $(PK_MADVISE_MADV_WILLNEED_GITHUB)X \
                 $(PK_MADVISE_MADV_WILLNEED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MADVISE_MADV_WILLNEED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MADVISE_MADV_WILLNEED_SUBNAME)X \
                 $(CONFIG_PK_MADVISE_MADV_WILLNEED_CONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_WILLNEED_LIBPATH)X \
                 $(CONFIG_PK_MADVISE_MADV_WILLNEED_CPPFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_WILLNEED_DPKCONFIG)X \
                 $(CONFIG_PK_MADVISE_MADV_WILLNEED_CFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_WILLNEED_LDFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_WILLNEED_CXXFLAGS)X \
                 $(CONFIG_PK_MADVISE_MADV_WILLNEED_ASFLAGS)X \
                 $(PK_MADVISE_MADV_WILLNEED_BSBIT)X \
                 $(PK_MADVISE_MADV_WILLNEED_BUILDARCH)X \
		 $(PK_MADVISE_MADV_WILLNEED_CSRCFILE)X \
                 $(CONFIG_PK_MADVISE_MADV_WILLNEED_SRCFILE)X \
