PK_MADVISE_MADV_WILLNEED_BSBIT       := BiscuitOS-madvise-MADV_WILLNEED-$(PK_MADVISE_MADV_WILLNEED_VERSION).bsbit
PK_MADVISE_MADV_WILLNEED_OUT         :=
