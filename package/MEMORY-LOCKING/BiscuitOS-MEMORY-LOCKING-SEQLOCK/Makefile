################################################
#
# BiscuitOS-MEMORY-LOCKING-SEQLOCK
#
################################################
PK_MEMLOCK_SEQLOCK_NAME        := BiscuitOS-MEMORY-LOCKING-SEQLOCK
PK_MEMLOCK_SEQLOCK_VERSION     := $(CONFIG_PK_MEMLOCK_SEQLOCK_VERSION)
PK_MEMLOCK_SEQLOCK_SITE        := $(CONFIG_PK_MEMLOCK_SEQLOCK_URL)
PK_MEMLOCK_SEQLOCK_GITHUB      := $(CONFIG_PK_MEMLOCK_SEQLOCK_GITHUB)
PK_MEMLOCK_SEQLOCK_SUBNAME     :=
PK_MEMLOCK_SEQLOCK_BUILDARCH   := $(CONFIG_PK_MEMLOCK_SEQLOCK_BUILDARCH)
PK_MEMLOCK_SEQLOCK_DIR         := $(srctree)/package/MEMORY-LOCKING/BiscuitOS-MEMORY-LOCKING-SEQLOCK
PK_MEMLOCK_SEQLOCK_PATCH       := $(PK_MEMLOCK_SEQLOCK_DIR)/patch
PK_MEMLOCK_SEQLOCK_BSBIT       := BiscuitOS-MEMORY-LOCKING-SEQLOCK-$(PK_MEMLOCK_SEQLOCK_VERSION).bsbit
PK_MEMLOCK_SEQLOCK_OUT         :=
PK_MEMLOCK_SEQLOCK_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMLOCK_SEQLOCK_TAR         := $(CONFIG_PK_MEMLOCK_SEQLOCK_TAR)
PK_MEMLOCK_SEQLOCK_PACKAGE     :=
PK_MEMLOCK_SEQLOCK_CONFIG      := $(CONFIG_PK_MEMLOCK_SEQLOCK_CONFIG)
SUB_TARGET                  += $(PK_MEMLOCK_SEQLOCK_DIR)/build

PK_MEMLOCK_SEQLOCK_sh = $(CONFIG_SHELL) $(PK_MEMLOCK_SEQLOCK_SCRIPTS) \
                 $1X \
                 $(PK_MEMLOCK_SEQLOCK_NAME)X \
                 $(PK_MEMLOCK_SEQLOCK_VERSION)X \
                 $(PK_MEMLOCK_SEQLOCK_SITE)X \
                 $(PK_MEMLOCK_SEQLOCK_GITHUB)X \
                 $(PK_MEMLOCK_SEQLOCK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMLOCK_SEQLOCK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMLOCK_SEQLOCK_SUBNAME)X \
                 $(CONFIG_PK_MEMLOCK_SEQLOCK_CONFIG)X \
                 $(CONFIG_PK_MEMLOCK_SEQLOCK_LIBPATH)X \
                 $(CONFIG_PK_MEMLOCK_SEQLOCK_CPPFLAGS)X \
                 $(CONFIG_PK_MEMLOCK_SEQLOCK_DPKCONFIG)X \
                 $(CONFIG_PK_MEMLOCK_SEQLOCK_CFLAGS)X \
                 $(CONFIG_PK_MEMLOCK_SEQLOCK_LDFLAGS)X \
                 $(CONFIG_PK_MEMLOCK_SEQLOCK_CXXFLAGS)X \
                 $(CONFIG_PK_MEMLOCK_SEQLOCK_ASFLAGS)X \
                 $(PK_MEMLOCK_SEQLOCK_BSBIT)X \
                 $(PK_MEMLOCK_SEQLOCK_BUILDARCH)X \
		 $(PK_MEMLOCK_SEQLOCK_CSRCFILE)X \
                 $(CONFIG_PK_MEMLOCK_SEQLOCK_SRCFILE)X \
                 $(CONFIG_PK_MEMLOCK_SEQLOCK_CSRCFILE)

$(PK_MEMLOCK_SEQLOCK_DIR)/build: FORCE
	$(Q)$(call PK_MEMLOCK_SEQLOCK_sh,$(srctree))
