MEMLOCKING-$(CONFIG_PK_MEMLOCK_SPINLOCK)			+= BiscuitOS-MEMORY-LOCKING-SPINLOCK/
MEMLOCKING-$(CONFIG_PK_MEMLOCK_SEMAPHORE)			+= BiscuitOS-MEMORY-LOCKING-SEMAPHORE/
MEMLOCKING-$(CONFIG_PK_MEMLOCK_MUTEX)				+= BiscuitOS-MEMORY-LOCKING-MUTEX/
MEMLOCKING-$(CONFIG_PK_MEMLOCK_RW_SEMAPHORE)			+= BiscuitOS-MEMORY-LOCKING-RW_SEMAPHORE/
MEMLOCKING-$(CONFIG_PK_MEMLOCK_RWLOCK)				+= BiscuitOS-MEMORY-LOCKING-RWLOCK/
MEMLOCKING-$(CONFIG_PK_MEMLOCK_RCU)				+= BiscuitOS-MEMORY-LOCKING-RCU/
MEMLOCKING-$(CONFIG_PK_MEMLOCK_SEQLOCK)				+= BiscuitOS-MEMORY-LOCKING-SEQLOCK/

PACKAGE_MAKEDIR := $(patsubst %/, $(srctree)/package/MEMORY-LOCKING/%/Makefile, $(MEMLOCKING-y))

include $(PACKAGE_MAKEDIR)
