################################################
#
# BiscuitOS-MEMORY-LOCKING-RWLOCK
#
################################################
PK_MEMLOCK_RWLOCK_NAME        := BiscuitOS-MEMORY-LOCKING-RWLOCK
PK_MEMLOCK_RWLOCK_VERSION     := $(CONFIG_PK_MEMLOCK_RWLOCK_VERSION)
PK_MEMLOCK_RWLOCK_SITE        := $(CONFIG_PK_MEMLOCK_RWLOCK_URL)
PK_MEMLOCK_RWLOCK_GITHUB      := $(CONFIG_PK_MEMLOCK_RWLOCK_GITHUB)
PK_MEMLOCK_RWLOCK_SUBNAME     :=
PK_MEMLOCK_RWLOCK_BUILDARCH   := $(CONFIG_PK_MEMLOCK_RWLOCK_BUILDARCH)
PK_MEMLOCK_RWLOCK_DIR         := $(srctree)/package/MEMORY-LOCKING/BiscuitOS-MEMORY-LOCKING-RWLOCK
PK_MEMLOCK_RWLOCK_PATCH       := $(PK_MEMLOCK_RWLOCK_DIR)/patch
PK_MEMLOCK_RWLOCK_BSBIT       := BiscuitOS-MEMORY-LOCKING-RWLOCK-$(PK_MEMLOCK_RWLOCK_VERSION).bsbit
PK_MEMLOCK_RWLOCK_OUT         :=
PK_MEMLOCK_RWLOCK_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMLOCK_RWLOCK_TAR         := $(CONFIG_PK_MEMLOCK_RWLOCK_TAR)
PK_MEMLOCK_RWLOCK_PACKAGE     :=
PK_MEMLOCK_RWLOCK_CONFIG      := $(CONFIG_PK_MEMLOCK_RWLOCK_CONFIG)
SUB_TARGET                  += $(PK_MEMLOCK_RWLOCK_DIR)/build

PK_MEMLOCK_RWLOCK_sh = $(CONFIG_SHELL) $(PK_MEMLOCK_RWLOCK_SCRIPTS) \
                 $1X \
                 $(PK_MEMLOCK_RWLOCK_NAME)X \
                 $(PK_MEMLOCK_RWLOCK_VERSION)X \
                 $(PK_MEMLOCK_RWLOCK_SITE)X \
                 $(PK_MEMLOCK_RWLOCK_GITHUB)X \
                 $(PK_MEMLOCK_RWLOCK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMLOCK_RWLOCK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMLOCK_RWLOCK_SUBNAME)X \
                 $(CONFIG_PK_MEMLOCK_RWLOCK_CONFIG)X \
                 $(CONFIG_PK_MEMLOCK_RWLOCK_LIBPATH)X \
                 $(CONFIG_PK_MEMLOCK_RWLOCK_CPPFLAGS)X \
                 $(CONFIG_PK_MEMLOCK_RWLOCK_DPKCONFIG)X \
                 $(CONFIG_PK_MEMLOCK_RWLOCK_CFLAGS)X \
                 $(CONFIG_PK_MEMLOCK_RWLOCK_LDFLAGS)X \
                 $(CONFIG_PK_MEMLOCK_RWLOCK_CXXFLAGS)X \
                 $(CONFIG_PK_MEMLOCK_RWLOCK_ASFLAGS)X \
                 $(PK_MEMLOCK_RWLOCK_BSBIT)X \
                 $(PK_MEMLOCK_RWLOCK_BUILDARCH)X \
		 $(PK_MEMLOCK_RWLOCK_CSRCFILE)X \
                 $(CONFIG_PK_MEMLOCK_RWLOCK_SRCFILE)X \
                 $(CONFIG_PK_MEMLOCK_RWLOCK_CSRCFILE)

$(PK_MEMLOCK_RWLOCK_DIR)/build: FORCE
	$(Q)$(call PK_MEMLOCK_RWLOCK_sh,$(srctree))
