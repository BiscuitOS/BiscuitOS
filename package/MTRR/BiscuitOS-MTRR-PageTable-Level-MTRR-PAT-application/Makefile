PK_MTRR_PAGETABLE_PAT_APP_NAME        := BiscuitOS-MTRR-PageTable-Level-MTRR-PAT-application
PK_MTRR_PAGETABLE_PAT_APP_VERSION     := $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_VERSION)
PK_MTRR_PAGETABLE_PAT_APP_SITE        := $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_URL)
PK_MTRR_PAGETABLE_PAT_APP_GITHUB      := $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_GITHUB)
PK_MTRR_PAGETABLE_PAT_APP_SUBNAME     :=
PK_MTRR_PAGETABLE_PAT_APP_BUILDARCH   := $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_BUILDARCH)
PK_MTRR_PAGETABLE_PAT_APP_DIR         := $(srctree)/package/MTRR/BiscuitOS-MTRR-PageTable-Level-MTRR-PAT-application
PK_MTRR_PAGETABLE_PAT_APP_PATCH       := $(PK_MTRR_PAGETABLE_PAT_APP_DIR)/patch
PK_MTRR_PAGETABLE_PAT_APP_BSBIT       := BiscuitOS-MTRR-PageTable-Level-MTRR-PAT-application-$(PK_MTRR_PAGETABLE_PAT_APP_VERSION).bsbit
PK_MTRR_PAGETABLE_PAT_APP_OUT         :=
PK_MTRR_PAGETABLE_PAT_APP_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MTRR_PAGETABLE_PAT_APP_TAR         := $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_TAR)
PK_MTRR_PAGETABLE_PAT_APP_PACKAGE     :=
PK_MTRR_PAGETABLE_PAT_APP_CONFIG      := $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_CONFIG)
SUB_TARGET                  += $(PK_MTRR_PAGETABLE_PAT_APP_DIR)/build

PK_MTRR_PAGETABLE_PAT_APP_sh = $(CONFIG_SHELL) $(PK_MTRR_PAGETABLE_PAT_APP_SCRIPTS) \
                 $1X \
                 $(PK_MTRR_PAGETABLE_PAT_APP_NAME)X \
                 $(PK_MTRR_PAGETABLE_PAT_APP_VERSION)X \
                 $(PK_MTRR_PAGETABLE_PAT_APP_SITE)X \
                 $(PK_MTRR_PAGETABLE_PAT_APP_GITHUB)X \
                 $(PK_MTRR_PAGETABLE_PAT_APP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MTRR_PAGETABLE_PAT_APP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MTRR_PAGETABLE_PAT_APP_SUBNAME)X \
                 $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_CONFIG)X \
                 $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_LIBPATH)X \
                 $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_CPPFLAGS)X \
                 $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_DPKCONFIG)X \
                 $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_CFLAGS)X \
                 $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_LDFLAGS)X \
                 $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_CXXFLAGS)X \
                 $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_ASFLAGS)X \
                 $(PK_MTRR_PAGETABLE_PAT_APP_BSBIT)X \
                 $(PK_MTRR_PAGETABLE_PAT_APP_BUILDARCH)X \
		 $(PK_MTRR_PAGETABLE_PAT_APP_CSRCFILE)X \
                 $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_SRCFILE)X \
                 $(CONFIG_PK_MTRR_PAGETABLE_PAT_APP_CSRCFILE)

$(PK_MTRR_PAGETABLE_PAT_APP_DIR)/build: FORCE
	$(Q)$(call PK_MTRR_PAGETABLE_PAT_APP_sh,$(srctree))
