PK_HMM_TIERED_MEMORY_NAME        := BiscuitOS-TIERED-MEMORY
PK_HMM_TIERED_MEMORY_VERSION     := $(CONFIG_PK_HMM_TIERED_MEMORY_VERSION)
PK_HMM_TIERED_MEMORY_SITE        := $(CONFIG_PK_HMM_TIERED_MEMORY_URL)
PK_HMM_TIERED_MEMORY_GITHUB      := $(CONFIG_PK_HMM_TIERED_MEMORY_GITHUB)
PK_HMM_TIERED_MEMORY_SUBNAME     :=
PK_HMM_TIERED_MEMORY_BUILDARCH   := $(CONFIG_PK_HMM_TIERED_MEMORY_BUILDARCH)
PK_HMM_TIERED_MEMORY_DIR         := $(srctree)/package/HMM-MEMORY/BiscuitOS-TIERED-MEMORY
PK_HMM_TIERED_MEMORY_PATCH       := $(PK_HMM_TIERED_MEMORY_DIR)/patch
PK_HMM_TIERED_MEMORY_BSBIT       := BiscuitOS-TIERED-MEMORY-$(PK_HMM_TIERED_MEMORY_VERSION).bsbit
PK_HMM_TIERED_MEMORY_OUT         :=
PK_HMM_TIERED_MEMORY_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_HMM_TIERED_MEMORY_TAR         := $(CONFIG_PK_HMM_TIERED_MEMORY_TAR)
PK_HMM_TIERED_MEMORY_PACKAGE     :=
PK_HMM_TIERED_MEMORY_CONFIG      := $(CONFIG_PK_HMM_TIERED_MEMORY_CONFIG)
SUB_TARGET                  += $(PK_HMM_TIERED_MEMORY_DIR)/build

PK_HMM_TIERED_MEMORY_sh = $(CONFIG_SHELL) $(PK_HMM_TIERED_MEMORY_SCRIPTS) \
                 $1X \
                 $(PK_HMM_TIERED_MEMORY_NAME)X \
                 $(PK_HMM_TIERED_MEMORY_VERSION)X \
                 $(PK_HMM_TIERED_MEMORY_SITE)X \
                 $(PK_HMM_TIERED_MEMORY_GITHUB)X \
                 $(PK_HMM_TIERED_MEMORY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_HMM_TIERED_MEMORY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_HMM_TIERED_MEMORY_SUBNAME)X \
                 $(CONFIG_PK_HMM_TIERED_MEMORY_CONFIG)X \
                 $(CONFIG_PK_HMM_TIERED_MEMORY_LIBPATH)X \
                 $(CONFIG_PK_HMM_TIERED_MEMORY_CPPFLAGS)X \
                 $(CONFIG_PK_HMM_TIERED_MEMORY_DPKCONFIG)X \
                 $(CONFIG_PK_HMM_TIERED_MEMORY_CFLAGS)X \
                 $(CONFIG_PK_HMM_TIERED_MEMORY_LDFLAGS)X \
                 $(CONFIG_PK_HMM_TIERED_MEMORY_CXXFLAGS)X \
                 $(CONFIG_PK_HMM_TIERED_MEMORY_ASFLAGS)X \
                 $(PK_HMM_TIERED_MEMORY_BSBIT)X \
                 $(PK_HMM_TIERED_MEMORY_BUILDARCH)X \
		 $(PK_HMM_TIERED_MEMORY_CSRCFILE)X \
                 $(CONFIG_PK_HMM_TIERED_MEMORY_SRCFILE)X \
                 $(CONFIG_PK_HMM_TIERED_MEMORY_CSRCFILE)

$(PK_HMM_TIERED_MEMORY_DIR)/build: FORCE
	$(Q)$(call PK_HMM_TIERED_MEMORY_sh,$(srctree))
