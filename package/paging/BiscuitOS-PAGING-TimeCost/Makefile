################################################
#
# BiscuitOS-PAGING-TimeCost
#
################################################
PK_PAGING_TIMECOST_NAME        := BiscuitOS-PAGING-TimeCost
PK_PAGING_TIMECOST_VERSION     := $(CONFIG_PK_PAGING_TIMECOST_VERSION)
PK_PAGING_TIMECOST_SITE        := $(CONFIG_PK_PAGING_TIMECOST_URL)
PK_PAGING_TIMECOST_GITHUB      := $(CONFIG_PK_PAGING_TIMECOST_GITHUB)
PK_PAGING_TIMECOST_SUBNAME     :=
PK_PAGING_TIMECOST_BUILDARCH   := $(CONFIG_PK_PAGING_TIMECOST_BUILDARCH)
PK_PAGING_TIMECOST_DIR         := $(srctree)/package/paging/BiscuitOS-PAGING-TimeCost
PK_PAGING_TIMECOST_PATCH       := $(PK_PAGING_TIMECOST_DIR)/patch
PK_PAGING_TIMECOST_BSBIT       := BiscuitOS-PAGING-TimeCost-$(PK_PAGING_TIMECOST_VERSION).bsbit
PK_PAGING_TIMECOST_OUT         :=
PK_PAGING_TIMECOST_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_TIMECOST_TAR         := $(CONFIG_PK_PAGING_TIMECOST_TAR)
PK_PAGING_TIMECOST_PACKAGE     :=
PK_PAGING_TIMECOST_CONFIG      := $(CONFIG_PK_PAGING_TIMECOST_CONFIG)
SUB_TARGET                  += $(PK_PAGING_TIMECOST_DIR)/build

PK_PAGING_TIMECOST_sh = $(CONFIG_SHELL) $(PK_PAGING_TIMECOST_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_TIMECOST_NAME)X \
                 $(PK_PAGING_TIMECOST_VERSION)X \
                 $(PK_PAGING_TIMECOST_SITE)X \
                 $(PK_PAGING_TIMECOST_GITHUB)X \
                 $(PK_PAGING_TIMECOST_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_TIMECOST_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_TIMECOST_SUBNAME)X \
                 $(CONFIG_PK_PAGING_TIMECOST_CONFIG)X \
                 $(CONFIG_PK_PAGING_TIMECOST_LIBPATH)X \
                 $(CONFIG_PK_PAGING_TIMECOST_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_TIMECOST_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_TIMECOST_CFLAGS)X \
                 $(CONFIG_PK_PAGING_TIMECOST_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_TIMECOST_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_TIMECOST_ASFLAGS)X \
                 $(PK_PAGING_TIMECOST_BSBIT)X \
                 $(PK_PAGING_TIMECOST_BUILDARCH)X \
		 $(PK_PAGING_TIMECOST_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_TIMECOST_SRCFILE)X \
                 $(CONFIG_PK_PAGING_TIMECOST_CSRCFILE)

$(PK_PAGING_TIMECOST_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_TIMECOST_sh,$(srctree))
