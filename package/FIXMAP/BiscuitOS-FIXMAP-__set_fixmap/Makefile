################################################
#
# BiscuitOS-FIXMAP-__set_fixmap
#
################################################
PK_FIXMAP__SET_FIXMAP_NAME        := BiscuitOS-FIXMAP-__set_fixmap
PK_FIXMAP__SET_FIXMAP_VERSION     := $(CONFIG_PK_FIXMAP__SET_FIXMAP_VERSION)
PK_FIXMAP__SET_FIXMAP_SITE        := $(CONFIG_PK_FIXMAP__SET_FIXMAP_URL)
PK_FIXMAP__SET_FIXMAP_GITHUB      := $(CONFIG_PK_FIXMAP__SET_FIXMAP_GITHUB)
PK_FIXMAP__SET_FIXMAP_SUBNAME     :=
PK_FIXMAP__SET_FIXMAP_BUILDARCH   := $(CONFIG_PK_FIXMAP__SET_FIXMAP_BUILDARCH)
PK_FIXMAP__SET_FIXMAP_DIR         := $(srctree)/package/FIXMAP/BiscuitOS-FIXMAP-__set_fixmap
PK_FIXMAP__SET_FIXMAP_PATCH       := $(PK_FIXMAP__SET_FIXMAP_DIR)/patch
PK_FIXMAP__SET_FIXMAP_BSBIT       := BiscuitOS-FIXMAP-__set_fixmap-$(PK_FIXMAP__SET_FIXMAP_VERSION).bsbit
PK_FIXMAP__SET_FIXMAP_OUT         :=
PK_FIXMAP__SET_FIXMAP_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_FIXMAP__SET_FIXMAP_TAR         := $(CONFIG_PK_FIXMAP__SET_FIXMAP_TAR)
PK_FIXMAP__SET_FIXMAP_PACKAGE     :=
PK_FIXMAP__SET_FIXMAP_CONFIG      := $(CONFIG_PK_FIXMAP__SET_FIXMAP_CONFIG)
SUB_TARGET                  += $(PK_FIXMAP__SET_FIXMAP_DIR)/build

PK_FIXMAP__SET_FIXMAP_sh = $(CONFIG_SHELL) $(PK_FIXMAP__SET_FIXMAP_SCRIPTS) \
                 $1X \
                 $(PK_FIXMAP__SET_FIXMAP_NAME)X \
                 $(PK_FIXMAP__SET_FIXMAP_VERSION)X \
                 $(PK_FIXMAP__SET_FIXMAP_SITE)X \
                 $(PK_FIXMAP__SET_FIXMAP_GITHUB)X \
                 $(PK_FIXMAP__SET_FIXMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_FIXMAP__SET_FIXMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_FIXMAP__SET_FIXMAP_SUBNAME)X \
                 $(CONFIG_PK_FIXMAP__SET_FIXMAP_CONFIG)X \
                 $(CONFIG_PK_FIXMAP__SET_FIXMAP_LIBPATH)X \
                 $(CONFIG_PK_FIXMAP__SET_FIXMAP_CPPFLAGS)X \
                 $(CONFIG_PK_FIXMAP__SET_FIXMAP_DPKCONFIG)X \
                 $(CONFIG_PK_FIXMAP__SET_FIXMAP_CFLAGS)X \
                 $(CONFIG_PK_FIXMAP__SET_FIXMAP_LDFLAGS)X \
                 $(CONFIG_PK_FIXMAP__SET_FIXMAP_CXXFLAGS)X \
                 $(CONFIG_PK_FIXMAP__SET_FIXMAP_ASFLAGS)X \
                 $(PK_FIXMAP__SET_FIXMAP_BSBIT)X \
                 $(PK_FIXMAP__SET_FIXMAP_BUILDARCH)X \
		 $(PK_FIXMAP__SET_FIXMAP_CSRCFILE)X \
                 $(CONFIG_PK_FIXMAP__SET_FIXMAP_SRCFILE)X \
                 $(CONFIG_PK_FIXMAP__SET_FIXMAP_CSRCFILE)

$(PK_FIXMAP__SET_FIXMAP_DIR)/build: FORCE
	$(Q)$(call PK_FIXMAP__SET_FIXMAP_sh,$(srctree))
