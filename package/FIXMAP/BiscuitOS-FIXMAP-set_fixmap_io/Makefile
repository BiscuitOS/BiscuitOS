################################################
#
# BiscuitOS-FIXMAP-set_fixmap_io
#
################################################
PK_FIXMAP_SET_FIXMAP_IO_NAME        := BiscuitOS-FIXMAP-set_fixmap_io
PK_FIXMAP_SET_FIXMAP_IO_VERSION     := $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_VERSION)
PK_FIXMAP_SET_FIXMAP_IO_SITE        := $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_URL)
PK_FIXMAP_SET_FIXMAP_IO_GITHUB      := $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_GITHUB)
PK_FIXMAP_SET_FIXMAP_IO_SUBNAME     :=
PK_FIXMAP_SET_FIXMAP_IO_BUILDARCH   := $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_BUILDARCH)
PK_FIXMAP_SET_FIXMAP_IO_DIR         := $(srctree)/package/FIXMAP/BiscuitOS-FIXMAP-set_fixmap_io
PK_FIXMAP_SET_FIXMAP_IO_PATCH       := $(PK_FIXMAP_SET_FIXMAP_IO_DIR)/patch
PK_FIXMAP_SET_FIXMAP_IO_BSBIT       := BiscuitOS-FIXMAP-set_fixmap_io-$(PK_FIXMAP_SET_FIXMAP_IO_VERSION).bsbit
PK_FIXMAP_SET_FIXMAP_IO_OUT         :=
PK_FIXMAP_SET_FIXMAP_IO_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_FIXMAP_SET_FIXMAP_IO_TAR         := $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_TAR)
PK_FIXMAP_SET_FIXMAP_IO_PACKAGE     :=
PK_FIXMAP_SET_FIXMAP_IO_CONFIG      := $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_CONFIG)
SUB_TARGET                  += $(PK_FIXMAP_SET_FIXMAP_IO_DIR)/build

PK_FIXMAP_SET_FIXMAP_IO_sh = $(CONFIG_SHELL) $(PK_FIXMAP_SET_FIXMAP_IO_SCRIPTS) \
                 $1X \
                 $(PK_FIXMAP_SET_FIXMAP_IO_NAME)X \
                 $(PK_FIXMAP_SET_FIXMAP_IO_VERSION)X \
                 $(PK_FIXMAP_SET_FIXMAP_IO_SITE)X \
                 $(PK_FIXMAP_SET_FIXMAP_IO_GITHUB)X \
                 $(PK_FIXMAP_SET_FIXMAP_IO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_FIXMAP_SET_FIXMAP_IO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_FIXMAP_SET_FIXMAP_IO_SUBNAME)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_CONFIG)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_LIBPATH)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_CPPFLAGS)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_DPKCONFIG)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_CFLAGS)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_LDFLAGS)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_CXXFLAGS)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_ASFLAGS)X \
                 $(PK_FIXMAP_SET_FIXMAP_IO_BSBIT)X \
                 $(PK_FIXMAP_SET_FIXMAP_IO_BUILDARCH)X \
		 $(PK_FIXMAP_SET_FIXMAP_IO_CSRCFILE)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_SRCFILE)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_IO_CSRCFILE)

$(PK_FIXMAP_SET_FIXMAP_IO_DIR)/build: FORCE
	$(Q)$(call PK_FIXMAP_SET_FIXMAP_IO_sh,$(srctree))
