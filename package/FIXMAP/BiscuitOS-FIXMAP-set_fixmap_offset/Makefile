################################################
#
# BiscuitOS-FIXMAP-set_fixmap_offset
#
################################################
PK_FIXMAP_SET_FIXMAP_OFFSET_NAME        := BiscuitOS-FIXMAP-set_fixmap_offset
PK_FIXMAP_SET_FIXMAP_OFFSET_VERSION     := $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_VERSION)
PK_FIXMAP_SET_FIXMAP_OFFSET_SITE        := $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_URL)
PK_FIXMAP_SET_FIXMAP_OFFSET_GITHUB      := $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_GITHUB)
PK_FIXMAP_SET_FIXMAP_OFFSET_SUBNAME     :=
PK_FIXMAP_SET_FIXMAP_OFFSET_BUILDARCH   := $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_BUILDARCH)
PK_FIXMAP_SET_FIXMAP_OFFSET_DIR         := $(srctree)/package/FIXMAP/BiscuitOS-FIXMAP-set_fixmap_offset
PK_FIXMAP_SET_FIXMAP_OFFSET_PATCH       := $(PK_FIXMAP_SET_FIXMAP_OFFSET_DIR)/patch
PK_FIXMAP_SET_FIXMAP_OFFSET_BSBIT       := BiscuitOS-FIXMAP-set_fixmap_offset-$(PK_FIXMAP_SET_FIXMAP_OFFSET_VERSION).bsbit
PK_FIXMAP_SET_FIXMAP_OFFSET_OUT         :=
PK_FIXMAP_SET_FIXMAP_OFFSET_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_FIXMAP_SET_FIXMAP_OFFSET_TAR         := $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_TAR)
PK_FIXMAP_SET_FIXMAP_OFFSET_PACKAGE     :=
PK_FIXMAP_SET_FIXMAP_OFFSET_CONFIG      := $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_CONFIG)
SUB_TARGET                  += $(PK_FIXMAP_SET_FIXMAP_OFFSET_DIR)/build

PK_FIXMAP_SET_FIXMAP_OFFSET_sh = $(CONFIG_SHELL) $(PK_FIXMAP_SET_FIXMAP_OFFSET_SCRIPTS) \
                 $1X \
                 $(PK_FIXMAP_SET_FIXMAP_OFFSET_NAME)X \
                 $(PK_FIXMAP_SET_FIXMAP_OFFSET_VERSION)X \
                 $(PK_FIXMAP_SET_FIXMAP_OFFSET_SITE)X \
                 $(PK_FIXMAP_SET_FIXMAP_OFFSET_GITHUB)X \
                 $(PK_FIXMAP_SET_FIXMAP_OFFSET_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_FIXMAP_SET_FIXMAP_OFFSET_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_FIXMAP_SET_FIXMAP_OFFSET_SUBNAME)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_CONFIG)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_LIBPATH)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_CPPFLAGS)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_DPKCONFIG)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_CFLAGS)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_LDFLAGS)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_CXXFLAGS)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_ASFLAGS)X \
                 $(PK_FIXMAP_SET_FIXMAP_OFFSET_BSBIT)X \
                 $(PK_FIXMAP_SET_FIXMAP_OFFSET_BUILDARCH)X \
		 $(PK_FIXMAP_SET_FIXMAP_OFFSET_CSRCFILE)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_SRCFILE)X \
                 $(CONFIG_PK_FIXMAP_SET_FIXMAP_OFFSET_CSRCFILE)

$(PK_FIXMAP_SET_FIXMAP_OFFSET_DIR)/build: FORCE
	$(Q)$(call PK_FIXMAP_SET_FIXMAP_OFFSET_sh,$(srctree))
