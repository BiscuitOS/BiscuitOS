# BiscuitOS-PageReserved-from-MEMBLOCK
#
################################################
PK_PAGERESERVED_MEMBLOCK_NAME        := BiscuitOS-PageReserved-from-MEMBLOCK
PK_PAGERESERVED_MEMBLOCK_VERSION     := $(CONFIG_PK_PAGERESERVED_MEMBLOCK_VERSION)
PK_PAGERESERVED_MEMBLOCK_SITE        := $(CONFIG_PK_PAGERESERVED_MEMBLOCK_URL)
PK_PAGERESERVED_MEMBLOCK_GITHUB      := $(CONFIG_PK_PAGERESERVED_MEMBLOCK_GITHUB)
PK_PAGERESERVED_MEMBLOCK_SUBNAME     :=
PK_PAGERESERVED_MEMBLOCK_BUILDARCH   := $(CONFIG_PK_PAGERESERVED_MEMBLOCK_BUILDARCH)
PK_PAGERESERVED_MEMBLOCK_DIR         := $(srctree)/package/page/PageReserved/BiscuitOS-PageReserved-from-MEMBLOCK
PK_PAGERESERVED_MEMBLOCK_PATCH       := $(PK_PAGERESERVED_MEMBLOCK_DIR)/patch
PK_PAGERESERVED_MEMBLOCK_BSBIT       := BiscuitOS-PageReserved-from-MEMBLOCK-$(PK_PAGERESERVED_MEMBLOCK_VERSION).bsbit
PK_PAGERESERVED_MEMBLOCK_OUT         :=
PK_PAGERESERVED_MEMBLOCK_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_PAGERESERVED_MEMBLOCK_TAR         := $(CONFIG_PK_PAGERESERVED_MEMBLOCK_TAR)
PK_PAGERESERVED_MEMBLOCK_PACKAGE     :=
PK_PAGERESERVED_MEMBLOCK_CONFIG      := $(CONFIG_PK_PAGERESERVED_MEMBLOCK_CONFIG)
SUB_TARGET                  += $(PK_PAGERESERVED_MEMBLOCK_DIR)/build

PK_PAGERESERVED_MEMBLOCK_sh = $(CONFIG_SHELL) $(PK_PAGERESERVED_MEMBLOCK_SCRIPTS) \
                 $1X \
                 $(PK_PAGERESERVED_MEMBLOCK_NAME)X \
                 $(PK_PAGERESERVED_MEMBLOCK_VERSION)X \
                 $(PK_PAGERESERVED_MEMBLOCK_SITE)X \
                 $(PK_PAGERESERVED_MEMBLOCK_GITHUB)X \
                 $(PK_PAGERESERVED_MEMBLOCK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGERESERVED_MEMBLOCK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGERESERVED_MEMBLOCK_SUBNAME)X \
                 $(CONFIG_PK_PAGERESERVED_MEMBLOCK_CONFIG)X \
                 $(CONFIG_PK_PAGERESERVED_MEMBLOCK_LIBPATH)X \
                 $(CONFIG_PK_PAGERESERVED_MEMBLOCK_CPPFLAGS)X \
                 $(CONFIG_PK_PAGERESERVED_MEMBLOCK_DPKCONFIG)X \
                 $(CONFIG_PK_PAGERESERVED_MEMBLOCK_CFLAGS)X \
                 $(CONFIG_PK_PAGERESERVED_MEMBLOCK_LDFLAGS)X \
                 $(CONFIG_PK_PAGERESERVED_MEMBLOCK_CXXFLAGS)X \
                 $(CONFIG_PK_PAGERESERVED_MEMBLOCK_ASFLAGS)X \
                 $(PK_PAGERESERVED_MEMBLOCK_BSBIT)X \
                 $(PK_PAGERESERVED_MEMBLOCK_BUILDARCH)X \
		 $(PK_PAGERESERVED_MEMBLOCK_CSRCFILE)X \
                 $(CONFIG_PK_PAGERESERVED_MEMBLOCK_SRCFILE)X \
                 $(CONFIG_PK_PAGERESERVED_MEMBLOCK_CSRCFILE)

$(PK_PAGERESERVED_MEMBLOCK_DIR)/build: FORCE
	$(Q)$(call PK_PAGERESERVED_MEMBLOCK_sh,$(srctree))
