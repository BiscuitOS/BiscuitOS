PK_MMAP_PREALLOC_VMALLOC32_NAME        := BiscuitOS-MMAP-PreAlloc-VMALLOC32
PK_MMAP_PREALLOC_VMALLOC32_VERSION     := $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_VERSION)
PK_MMAP_PREALLOC_VMALLOC32_SITE        := $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_URL)
PK_MMAP_PREALLOC_VMALLOC32_GITHUB      := $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_GITHUB)
PK_MMAP_PREALLOC_VMALLOC32_SUBNAME     :=
PK_MMAP_PREALLOC_VMALLOC32_BUILDARCH   := $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_BUILDARCH)
PK_MMAP_PREALLOC_VMALLOC32_DIR         := $(srctree)/package/memory-mapping/BiscuitOS-MMAP-PreAlloc-VMALLOC32
PK_MMAP_PREALLOC_VMALLOC32_PATCH       := $(PK_MMAP_PREALLOC_VMALLOC32_DIR)/patch
PK_MMAP_PREALLOC_VMALLOC32_BSBIT       := BiscuitOS-MMAP-PreAlloc-VMALLOC32-$(PK_MMAP_PREALLOC_VMALLOC32_VERSION).bsbit
PK_MMAP_PREALLOC_VMALLOC32_OUT         :=
PK_MMAP_PREALLOC_VMALLOC32_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MMAP_PREALLOC_VMALLOC32_TAR         := $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_TAR)
PK_MMAP_PREALLOC_VMALLOC32_PACKAGE     :=
PK_MMAP_PREALLOC_VMALLOC32_CONFIG      := $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_CONFIG)
SUB_TARGET                  += $(PK_MMAP_PREALLOC_VMALLOC32_DIR)/build

PK_MMAP_PREALLOC_VMALLOC32_sh = $(CONFIG_SHELL) $(PK_MMAP_PREALLOC_VMALLOC32_SCRIPTS) \
                 $1X \
                 $(PK_MMAP_PREALLOC_VMALLOC32_NAME)X \
                 $(PK_MMAP_PREALLOC_VMALLOC32_VERSION)X \
                 $(PK_MMAP_PREALLOC_VMALLOC32_SITE)X \
                 $(PK_MMAP_PREALLOC_VMALLOC32_GITHUB)X \
                 $(PK_MMAP_PREALLOC_VMALLOC32_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MMAP_PREALLOC_VMALLOC32_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MMAP_PREALLOC_VMALLOC32_SUBNAME)X \
                 $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_CONFIG)X \
                 $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_LIBPATH)X \
                 $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_CPPFLAGS)X \
                 $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_DPKCONFIG)X \
                 $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_CFLAGS)X \
                 $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_LDFLAGS)X \
                 $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_CXXFLAGS)X \
                 $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_ASFLAGS)X \
                 $(PK_MMAP_PREALLOC_VMALLOC32_BSBIT)X \
                 $(PK_MMAP_PREALLOC_VMALLOC32_BUILDARCH)X \
		 $(PK_MMAP_PREALLOC_VMALLOC32_CSRCFILE)X \
                 $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_SRCFILE)X \
                 $(CONFIG_PK_MMAP_PREALLOC_VMALLOC32_CSRCFILE)

$(PK_MMAP_PREALLOC_VMALLOC32_DIR)/build: FORCE
	$(Q)$(call PK_MMAP_PREALLOC_VMALLOC32_sh,$(srctree))
