PK_MMAP_PREALLOC_MAPLOCKED_NAME        := BiscuitOS-MMAP-PreAlloc-MAPLOCKED
PK_MMAP_PREALLOC_MAPLOCKED_VERSION     := $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_VERSION)
PK_MMAP_PREALLOC_MAPLOCKED_SITE        := $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_URL)
PK_MMAP_PREALLOC_MAPLOCKED_GITHUB      := $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_GITHUB)
PK_MMAP_PREALLOC_MAPLOCKED_SUBNAME     :=
PK_MMAP_PREALLOC_MAPLOCKED_BUILDARCH   := $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_BUILDARCH)
PK_MMAP_PREALLOC_MAPLOCKED_DIR         := $(srctree)/package/memory-mapping/BiscuitOS-MMAP-PreAlloc-MAPLOCKED
PK_MMAP_PREALLOC_MAPLOCKED_PATCH       := $(PK_MMAP_PREALLOC_MAPLOCKED_DIR)/patch
PK_MMAP_PREALLOC_MAPLOCKED_BSBIT       := BiscuitOS-MMAP-PreAlloc-MAPLOCKED-$(PK_MMAP_PREALLOC_MAPLOCKED_VERSION).bsbit
PK_MMAP_PREALLOC_MAPLOCKED_OUT         :=
PK_MMAP_PREALLOC_MAPLOCKED_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MMAP_PREALLOC_MAPLOCKED_TAR         := $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_TAR)
PK_MMAP_PREALLOC_MAPLOCKED_PACKAGE     :=
PK_MMAP_PREALLOC_MAPLOCKED_CONFIG      := $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_CONFIG)
SUB_TARGET                  += $(PK_MMAP_PREALLOC_MAPLOCKED_DIR)/build

PK_MMAP_PREALLOC_MAPLOCKED_sh = $(CONFIG_SHELL) $(PK_MMAP_PREALLOC_MAPLOCKED_SCRIPTS) \
                 $1X \
                 $(PK_MMAP_PREALLOC_MAPLOCKED_NAME)X \
                 $(PK_MMAP_PREALLOC_MAPLOCKED_VERSION)X \
                 $(PK_MMAP_PREALLOC_MAPLOCKED_SITE)X \
                 $(PK_MMAP_PREALLOC_MAPLOCKED_GITHUB)X \
                 $(PK_MMAP_PREALLOC_MAPLOCKED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MMAP_PREALLOC_MAPLOCKED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MMAP_PREALLOC_MAPLOCKED_SUBNAME)X \
                 $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_CONFIG)X \
                 $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_LIBPATH)X \
                 $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_CPPFLAGS)X \
                 $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_DPKCONFIG)X \
                 $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_CFLAGS)X \
                 $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_LDFLAGS)X \
                 $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_CXXFLAGS)X \
                 $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_ASFLAGS)X \
                 $(PK_MMAP_PREALLOC_MAPLOCKED_BSBIT)X \
                 $(PK_MMAP_PREALLOC_MAPLOCKED_BUILDARCH)X \
		 $(PK_MMAP_PREALLOC_MAPLOCKED_CSRCFILE)X \
                 $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_SRCFILE)X \
                 $(CONFIG_PK_MMAP_PREALLOC_MAPLOCKED_CSRCFILE)

$(PK_MMAP_PREALLOC_MAPLOCKED_DIR)/build: FORCE
	$(Q)$(call PK_MMAP_PREALLOC_MAPLOCKED_sh,$(srctree))
