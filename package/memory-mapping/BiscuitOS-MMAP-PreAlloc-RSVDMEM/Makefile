PK_MMAP_PREALLOC_RSVDMEM_NAME        := BiscuitOS-MMAP-PreAlloc-RSVDMEM
PK_MMAP_PREALLOC_RSVDMEM_VERSION     := $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_VERSION)
PK_MMAP_PREALLOC_RSVDMEM_SITE        := $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_URL)
PK_MMAP_PREALLOC_RSVDMEM_GITHUB      := $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_GITHUB)
PK_MMAP_PREALLOC_RSVDMEM_SUBNAME     :=
PK_MMAP_PREALLOC_RSVDMEM_BUILDARCH   := $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_BUILDARCH)
PK_MMAP_PREALLOC_RSVDMEM_DIR         := $(srctree)/package/memory-mapping/BiscuitOS-MMAP-PreAlloc-RSVDMEM
PK_MMAP_PREALLOC_RSVDMEM_PATCH       := $(PK_MMAP_PREALLOC_RSVDMEM_DIR)/patch
PK_MMAP_PREALLOC_RSVDMEM_BSBIT       := BiscuitOS-MMAP-PreAlloc-RSVDMEM-$(PK_MMAP_PREALLOC_RSVDMEM_VERSION).bsbit
PK_MMAP_PREALLOC_RSVDMEM_OUT         :=
PK_MMAP_PREALLOC_RSVDMEM_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MMAP_PREALLOC_RSVDMEM_TAR         := $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_TAR)
PK_MMAP_PREALLOC_RSVDMEM_PACKAGE     :=
PK_MMAP_PREALLOC_RSVDMEM_CONFIG      := $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_CONFIG)
SUB_TARGET                  += $(PK_MMAP_PREALLOC_RSVDMEM_DIR)/build

PK_MMAP_PREALLOC_RSVDMEM_sh = $(CONFIG_SHELL) $(PK_MMAP_PREALLOC_RSVDMEM_SCRIPTS) \
                 $1X \
                 $(PK_MMAP_PREALLOC_RSVDMEM_NAME)X \
                 $(PK_MMAP_PREALLOC_RSVDMEM_VERSION)X \
                 $(PK_MMAP_PREALLOC_RSVDMEM_SITE)X \
                 $(PK_MMAP_PREALLOC_RSVDMEM_GITHUB)X \
                 $(PK_MMAP_PREALLOC_RSVDMEM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MMAP_PREALLOC_RSVDMEM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MMAP_PREALLOC_RSVDMEM_SUBNAME)X \
                 $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_CONFIG)X \
                 $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_LIBPATH)X \
                 $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_CPPFLAGS)X \
                 $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_DPKCONFIG)X \
                 $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_CFLAGS)X \
                 $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_LDFLAGS)X \
                 $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_CXXFLAGS)X \
                 $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_ASFLAGS)X \
                 $(PK_MMAP_PREALLOC_RSVDMEM_BSBIT)X \
                 $(PK_MMAP_PREALLOC_RSVDMEM_BUILDARCH)X \
		 $(PK_MMAP_PREALLOC_RSVDMEM_CSRCFILE)X \
                 $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_SRCFILE)X \
                 $(CONFIG_PK_MMAP_PREALLOC_RSVDMEM_CSRCFILE)

$(PK_MMAP_PREALLOC_RSVDMEM_DIR)/build: FORCE
	$(Q)$(call PK_MMAP_PREALLOC_RSVDMEM_sh,$(srctree))
