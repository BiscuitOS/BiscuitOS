PK_VADDR_MECH_DETECT_PADDR_PACKAGE     :=
PK_VADDR_MECH_DETECT_PADDR_CONFIG      := $(CONFIG_PK_VADDR_MECH_DETECT_PADDR_CONFIG)
SUB_TARGET                  += $(PK_VADDR_MECH_DETECT_PADDR_DIR)/build

PK_VADDR_MECH_DETECT_PADDR_sh = $(CONFIG_SHELL) $(PK_VADDR_MECH_DETECT_PADDR_SCRIPTS) \
