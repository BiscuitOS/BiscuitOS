                 $(PK_VADDR_MECH_DETECT_PADDR_GITHUB)X \
                 $(PK_VADDR_MECH_DETECT_PADDR_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_VADDR_MECH_DETECT_PADDR_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_VADDR_MECH_DETECT_PADDR_SUBNAME)X \
                 $(CONFIG_PK_VADDR_MECH_DETECT_PADDR_CONFIG)X \
                 $(CONFIG_PK_VADDR_MECH_DETECT_PADDR_LIBPATH)X \
                 $(CONFIG_PK_VADDR_MECH_DETECT_PADDR_CPPFLAGS)X \
                 $(CONFIG_PK_VADDR_MECH_DETECT_PADDR_DPKCONFIG)X \
                 $(CONFIG_PK_VADDR_MECH_DETECT_PADDR_CFLAGS)X \
                 $(CONFIG_PK_VADDR_MECH_DETECT_PADDR_LDFLAGS)X \
                 $(CONFIG_PK_VADDR_MECH_DETECT_PADDR_CXXFLAGS)X \
                 $(CONFIG_PK_VADDR_MECH_DETECT_PADDR_ASFLAGS)X \
