                 $(PK_VADDR_MECH_DETECT_PADDR_BUILDARCH)X \
		 $(PK_VADDR_MECH_DETECT_PADDR_CSRCFILE)X \
                 $(CONFIG_PK_VADDR_MECH_DETECT_PADDR_SRCFILE)X \
