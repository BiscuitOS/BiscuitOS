PK_VADDR_MECH_DETECT_PADDR_GITHUB      := $(CONFIG_PK_VADDR_MECH_DETECT_PADDR_GITHUB)
PK_VADDR_MECH_DETECT_PADDR_SUBNAME     :=
PK_VADDR_MECH_DETECT_PADDR_BUILDARCH   := $(CONFIG_PK_VADDR_MECH_DETECT_PADDR_BUILDARCH)
PK_VADDR_MECH_DETECT_PADDR_DIR         := $(srctree)/package/Virtual-Address/BiscuitOS-VADDR-MECH-Detect-physcal-address
PK_VADDR_MECH_DETECT_PADDR_PATCH       := $(PK_VADDR_MECH_DETECT_PADDR_DIR)/patch
PK_VADDR_MECH_DETECT_PADDR_BSBIT       := BiscuitOS-VADDR-MECH-Detect-physcal-address-$(PK_VADDR_MECH_DETECT_PADDR_VERSION).bsbit
