PK_PCI_DMABUF_QEMU_DEV_NAME        := BiscuitOS-PCI-DMA-BUF-QEMU-DEVICE
PK_PCI_DMABUF_QEMU_DEV_VERSION     := $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_VERSION)
PK_PCI_DMABUF_QEMU_DEV_SITE        := $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_URL)
PK_PCI_DMABUF_QEMU_DEV_GITHUB      := $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_GITHUB)
PK_PCI_DMABUF_QEMU_DEV_SUBNAME     :=
PK_PCI_DMABUF_QEMU_DEV_BUILDARCH   := $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_BUILDARCH)
PK_PCI_DMABUF_QEMU_DEV_DIR         := $(srctree)/package/pci/BiscuitOS-PCI-DMA-BUF-QEMU-DEVICE
PK_PCI_DMABUF_QEMU_DEV_PATCH       := $(PK_PCI_DMABUF_QEMU_DEV_DIR)/patch
PK_PCI_DMABUF_QEMU_DEV_BSBIT       := BiscuitOS-PCI-DMA-BUF-QEMU-DEVICE-$(PK_PCI_DMABUF_QEMU_DEV_VERSION).bsbit
PK_PCI_DMABUF_QEMU_DEV_OUT         :=
PK_PCI_DMABUF_QEMU_DEV_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PCI_DMABUF_QEMU_DEV_TAR         := $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_TAR)
PK_PCI_DMABUF_QEMU_DEV_PACKAGE     :=
PK_PCI_DMABUF_QEMU_DEV_CONFIG      := $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_CONFIG)
SUB_TARGET                  += $(PK_PCI_DMABUF_QEMU_DEV_DIR)/build

PK_PCI_DMABUF_QEMU_DEV_sh = $(CONFIG_SHELL) $(PK_PCI_DMABUF_QEMU_DEV_SCRIPTS) \
                 $1X \
                 $(PK_PCI_DMABUF_QEMU_DEV_NAME)X \
                 $(PK_PCI_DMABUF_QEMU_DEV_VERSION)X \
                 $(PK_PCI_DMABUF_QEMU_DEV_SITE)X \
                 $(PK_PCI_DMABUF_QEMU_DEV_GITHUB)X \
                 $(PK_PCI_DMABUF_QEMU_DEV_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PCI_DMABUF_QEMU_DEV_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PCI_DMABUF_QEMU_DEV_SUBNAME)X \
                 $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_CONFIG)X \
                 $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_LIBPATH)X \
                 $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_CPPFLAGS)X \
                 $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_DPKCONFIG)X \
                 $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_CFLAGS)X \
                 $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_LDFLAGS)X \
                 $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_CXXFLAGS)X \
                 $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_ASFLAGS)X \
                 $(PK_PCI_DMABUF_QEMU_DEV_BSBIT)X \
                 $(PK_PCI_DMABUF_QEMU_DEV_BUILDARCH)X \
		 $(PK_PCI_DMABUF_QEMU_DEV_CSRCFILE)X \
                 $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_SRCFILE)X \
                 $(CONFIG_PK_PCI_DMABUF_QEMU_DEV_CSRCFILE)

$(PK_PCI_DMABUF_QEMU_DEV_DIR)/build: FORCE
	$(Q)$(call PK_PCI_DMABUF_QEMU_DEV_sh,$(srctree))
