# BiscuitOS-MOTION-MEMORY-ALLOCATOR-KDEMO
#
################################################
PK_MOTION_MEMORY_ALLOCATOR_KDEMO_NAME        := BiscuitOS-MOTION-MEMORY-ALLOCATOR-KDEMO
PK_MOTION_MEMORY_ALLOCATOR_KDEMO_VERSION     := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_VERSION)
PK_MOTION_MEMORY_ALLOCATOR_KDEMO_SITE        := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_URL)
PK_MOTION_MEMORY_ALLOCATOR_KDEMO_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_GITHUB)
PK_MOTION_MEMORY_ALLOCATOR_KDEMO_SUBNAME     :=
PK_MOTION_MEMORY_ALLOCATOR_KDEMO_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_BUILDARCH)
PK_MOTION_MEMORY_ALLOCATOR_KDEMO_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-ALLOCATOR-KDEMO
PK_MOTION_MEMORY_ALLOCATOR_KDEMO_PATCH       := $(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_DIR)/patch
PK_MOTION_MEMORY_ALLOCATOR_KDEMO_BSBIT       := BiscuitOS-MOTION-MEMORY-ALLOCATOR-KDEMO-$(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_VERSION).bsbit
PK_MOTION_MEMORY_ALLOCATOR_KDEMO_OUT         :=
PK_MOTION_MEMORY_ALLOCATOR_KDEMO_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_MOTION_MEMORY_ALLOCATOR_KDEMO_TAR         := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_TAR)
PK_MOTION_MEMORY_ALLOCATOR_KDEMO_PACKAGE     :=
PK_MOTION_MEMORY_ALLOCATOR_KDEMO_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_DIR)/build

PK_MOTION_MEMORY_ALLOCATOR_KDEMO_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_NAME)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_VERSION)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_SITE)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_GITHUB)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_BSBIT)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KDEMO_CSRCFILE)

$(PK_MOTION_MEMORY_ALLOCATOR_KDEMO_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_ALLOCATOR_KDEMO_sh,$(srctree))
