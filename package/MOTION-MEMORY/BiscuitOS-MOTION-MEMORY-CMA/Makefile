################################################
#
# BiscuitOS-MOTION-MEMORY-CMA
#
################################################
PK_MOTION_MEMORY_CMA_NAME        := BiscuitOS-MOTION-MEMORY-CMA
PK_MOTION_MEMORY_CMA_VERSION     := $(CONFIG_PK_MOTION_MEMORY_CMA_VERSION)
PK_MOTION_MEMORY_CMA_SITE        := $(CONFIG_PK_MOTION_MEMORY_CMA_URL)
PK_MOTION_MEMORY_CMA_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_CMA_GITHUB)
PK_MOTION_MEMORY_CMA_SUBNAME     :=
PK_MOTION_MEMORY_CMA_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_CMA_BUILDARCH)
PK_MOTION_MEMORY_CMA_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-CMA
PK_MOTION_MEMORY_CMA_PATCH       := $(PK_MOTION_MEMORY_CMA_DIR)/patch
PK_MOTION_MEMORY_CMA_BSBIT       := BiscuitOS-MOTION-MEMORY-CMA-$(PK_MOTION_MEMORY_CMA_VERSION).bsbit
PK_MOTION_MEMORY_CMA_OUT         :=
PK_MOTION_MEMORY_CMA_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MOTION_MEMORY_CMA_TAR         := $(CONFIG_PK_MOTION_MEMORY_CMA_TAR)
PK_MOTION_MEMORY_CMA_PACKAGE     :=
PK_MOTION_MEMORY_CMA_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_CMA_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_CMA_DIR)/build

PK_MOTION_MEMORY_CMA_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_CMA_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_CMA_NAME)X \
                 $(PK_MOTION_MEMORY_CMA_VERSION)X \
                 $(PK_MOTION_MEMORY_CMA_SITE)X \
                 $(PK_MOTION_MEMORY_CMA_GITHUB)X \
                 $(PK_MOTION_MEMORY_CMA_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_CMA_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_CMA_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_CMA_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_CMA_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_CMA_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_CMA_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_CMA_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_CMA_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_CMA_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_CMA_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_CMA_BSBIT)X \
                 $(PK_MOTION_MEMORY_CMA_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_CMA_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_CMA_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_CMA_CSRCFILE)

$(PK_MOTION_MEMORY_CMA_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_CMA_sh,$(srctree))
