################################################
#
# BiscuitOS-MOTION-MEMORY-COW
#
################################################
PK_MOTION_MEMORY_COW_NAME        := BiscuitOS-MOTION-MEMORY-COW
PK_MOTION_MEMORY_COW_VERSION     := $(CONFIG_PK_MOTION_MEMORY_COW_VERSION)
PK_MOTION_MEMORY_COW_SITE        := $(CONFIG_PK_MOTION_MEMORY_COW_URL)
PK_MOTION_MEMORY_COW_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_COW_GITHUB)
PK_MOTION_MEMORY_COW_SUBNAME     :=
PK_MOTION_MEMORY_COW_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_COW_BUILDARCH)
PK_MOTION_MEMORY_COW_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-COW
PK_MOTION_MEMORY_COW_PATCH       := $(PK_MOTION_MEMORY_COW_DIR)/patch
PK_MOTION_MEMORY_COW_BSBIT       := BiscuitOS-MOTION-MEMORY-COW-$(PK_MOTION_MEMORY_COW_VERSION).bsbit
PK_MOTION_MEMORY_COW_OUT         :=
PK_MOTION_MEMORY_COW_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MOTION_MEMORY_COW_TAR         := $(CONFIG_PK_MOTION_MEMORY_COW_TAR)
PK_MOTION_MEMORY_COW_PACKAGE     :=
PK_MOTION_MEMORY_COW_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_COW_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_COW_DIR)/build

PK_MOTION_MEMORY_COW_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_COW_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_COW_NAME)X \
                 $(PK_MOTION_MEMORY_COW_VERSION)X \
                 $(PK_MOTION_MEMORY_COW_SITE)X \
                 $(PK_MOTION_MEMORY_COW_GITHUB)X \
                 $(PK_MOTION_MEMORY_COW_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_COW_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_COW_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_COW_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_COW_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_COW_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_COW_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_COW_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_COW_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_COW_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_COW_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_COW_BSBIT)X \
                 $(PK_MOTION_MEMORY_COW_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_COW_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_COW_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_COW_CSRCFILE)

$(PK_MOTION_MEMORY_COW_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_COW_sh,$(srctree))
