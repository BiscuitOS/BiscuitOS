################################################
#
# BiscuitOS-MOTION-MEMORY-SWAP-SHARED
#
################################################
PK_MOTION_MEMORY_SWAP_SHARED_NAME        := BiscuitOS-MOTION-MEMORY-SWAP-SHARED
PK_MOTION_MEMORY_SWAP_SHARED_VERSION     := $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_VERSION)
PK_MOTION_MEMORY_SWAP_SHARED_SITE        := $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_URL)
PK_MOTION_MEMORY_SWAP_SHARED_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_GITHUB)
PK_MOTION_MEMORY_SWAP_SHARED_SUBNAME     :=
PK_MOTION_MEMORY_SWAP_SHARED_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_BUILDARCH)
PK_MOTION_MEMORY_SWAP_SHARED_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-SWAP-SHARED
PK_MOTION_MEMORY_SWAP_SHARED_PATCH       := $(PK_MOTION_MEMORY_SWAP_SHARED_DIR)/patch
PK_MOTION_MEMORY_SWAP_SHARED_BSBIT       := BiscuitOS-MOTION-MEMORY-SWAP-SHARED-$(PK_MOTION_MEMORY_SWAP_SHARED_VERSION).bsbit
PK_MOTION_MEMORY_SWAP_SHARED_OUT         :=
PK_MOTION_MEMORY_SWAP_SHARED_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MOTION_MEMORY_SWAP_SHARED_TAR         := $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_TAR)
PK_MOTION_MEMORY_SWAP_SHARED_PACKAGE     :=
PK_MOTION_MEMORY_SWAP_SHARED_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_SWAP_SHARED_DIR)/build

PK_MOTION_MEMORY_SWAP_SHARED_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_SWAP_SHARED_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_SWAP_SHARED_NAME)X \
                 $(PK_MOTION_MEMORY_SWAP_SHARED_VERSION)X \
                 $(PK_MOTION_MEMORY_SWAP_SHARED_SITE)X \
                 $(PK_MOTION_MEMORY_SWAP_SHARED_GITHUB)X \
                 $(PK_MOTION_MEMORY_SWAP_SHARED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_SWAP_SHARED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_SWAP_SHARED_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_SWAP_SHARED_BSBIT)X \
                 $(PK_MOTION_MEMORY_SWAP_SHARED_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_SWAP_SHARED_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_SWAP_SHARED_CSRCFILE)

$(PK_MOTION_MEMORY_SWAP_SHARED_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_SWAP_SHARED_sh,$(srctree))
