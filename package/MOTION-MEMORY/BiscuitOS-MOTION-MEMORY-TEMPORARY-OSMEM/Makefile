# BiscuitOS-MOTION-MEMORY-TEMPORARY-OSMEM
#
################################################
PK_MOTION_MEMORY_TEMPORARY_OSMEM_NAME        := BiscuitOS-MOTION-MEMORY-TEMPORARY-OSMEM
PK_MOTION_MEMORY_TEMPORARY_OSMEM_VERSION     := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_VERSION)
PK_MOTION_MEMORY_TEMPORARY_OSMEM_SITE        := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_URL)
PK_MOTION_MEMORY_TEMPORARY_OSMEM_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_GITHUB)
PK_MOTION_MEMORY_TEMPORARY_OSMEM_SUBNAME     :=
PK_MOTION_MEMORY_TEMPORARY_OSMEM_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_BUILDARCH)
PK_MOTION_MEMORY_TEMPORARY_OSMEM_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-TEMPORARY-OSMEM
PK_MOTION_MEMORY_TEMPORARY_OSMEM_PATCH       := $(PK_MOTION_MEMORY_TEMPORARY_OSMEM_DIR)/patch
PK_MOTION_MEMORY_TEMPORARY_OSMEM_BSBIT       := BiscuitOS-MOTION-MEMORY-TEMPORARY-OSMEM-$(PK_MOTION_MEMORY_TEMPORARY_OSMEM_VERSION).bsbit
PK_MOTION_MEMORY_TEMPORARY_OSMEM_OUT         :=
PK_MOTION_MEMORY_TEMPORARY_OSMEM_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MOTION_MEMORY_TEMPORARY_OSMEM_TAR         := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_TAR)
PK_MOTION_MEMORY_TEMPORARY_OSMEM_PACKAGE     :=
PK_MOTION_MEMORY_TEMPORARY_OSMEM_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_TEMPORARY_OSMEM_DIR)/build

PK_MOTION_MEMORY_TEMPORARY_OSMEM_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_TEMPORARY_OSMEM_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_TEMPORARY_OSMEM_NAME)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_OSMEM_VERSION)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_OSMEM_SITE)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_OSMEM_GITHUB)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_OSMEM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_OSMEM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_OSMEM_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_OSMEM_BSBIT)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_OSMEM_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_TEMPORARY_OSMEM_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_OSMEM_CSRCFILE)

$(PK_MOTION_MEMORY_TEMPORARY_OSMEM_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_TEMPORARY_OSMEM_sh,$(srctree))
