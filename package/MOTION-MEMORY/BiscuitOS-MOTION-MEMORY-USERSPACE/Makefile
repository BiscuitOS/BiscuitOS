# BiscuitOS-MOTION-MEMORY-USERSPACE
#
################################################
PK_MOTION_MEMORY_USERSPACE_NAME        := BiscuitOS-MOTION-MEMORY-USERSPACE
PK_MOTION_MEMORY_USERSPACE_VERSION     := $(CONFIG_PK_MOTION_MEMORY_USERSPACE_VERSION)
PK_MOTION_MEMORY_USERSPACE_SITE        := $(CONFIG_PK_MOTION_MEMORY_USERSPACE_URL)
PK_MOTION_MEMORY_USERSPACE_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_USERSPACE_GITHUB)
PK_MOTION_MEMORY_USERSPACE_SUBNAME     :=
PK_MOTION_MEMORY_USERSPACE_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_USERSPACE_BUILDARCH)
PK_MOTION_MEMORY_USERSPACE_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-USERSPACE
PK_MOTION_MEMORY_USERSPACE_PATCH       := $(PK_MOTION_MEMORY_USERSPACE_DIR)/patch
PK_MOTION_MEMORY_USERSPACE_BSBIT       := BiscuitOS-MOTION-MEMORY-USERSPACE-$(PK_MOTION_MEMORY_USERSPACE_VERSION).bsbit
PK_MOTION_MEMORY_USERSPACE_OUT         :=
PK_MOTION_MEMORY_USERSPACE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MOTION_MEMORY_USERSPACE_TAR         := $(CONFIG_PK_MOTION_MEMORY_USERSPACE_TAR)
PK_MOTION_MEMORY_USERSPACE_PACKAGE     :=
PK_MOTION_MEMORY_USERSPACE_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_USERSPACE_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_USERSPACE_DIR)/build

PK_MOTION_MEMORY_USERSPACE_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_USERSPACE_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_USERSPACE_NAME)X \
                 $(PK_MOTION_MEMORY_USERSPACE_VERSION)X \
                 $(PK_MOTION_MEMORY_USERSPACE_SITE)X \
                 $(PK_MOTION_MEMORY_USERSPACE_GITHUB)X \
                 $(PK_MOTION_MEMORY_USERSPACE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_USERSPACE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_USERSPACE_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_USERSPACE_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_USERSPACE_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_USERSPACE_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_USERSPACE_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_USERSPACE_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_USERSPACE_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_USERSPACE_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_USERSPACE_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_USERSPACE_BSBIT)X \
                 $(PK_MOTION_MEMORY_USERSPACE_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_USERSPACE_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_USERSPACE_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_USERSPACE_CSRCFILE)

$(PK_MOTION_MEMORY_USERSPACE_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_USERSPACE_sh,$(srctree))
