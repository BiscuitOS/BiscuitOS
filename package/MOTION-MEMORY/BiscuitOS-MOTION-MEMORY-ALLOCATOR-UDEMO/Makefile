################################################
PK_MOTION_MEMORY_ALLOCATOR_UDEMO_NAME        := BiscuitOS-MOTION-MEMORY-ALLOCATOR-UDEMO
PK_MOTION_MEMORY_ALLOCATOR_UDEMO_VERSION     := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_VERSION)
PK_MOTION_MEMORY_ALLOCATOR_UDEMO_SITE        := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_URL)
PK_MOTION_MEMORY_ALLOCATOR_UDEMO_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_GITHUB)
PK_MOTION_MEMORY_ALLOCATOR_UDEMO_SUBNAME     :=
PK_MOTION_MEMORY_ALLOCATOR_UDEMO_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_BUILDARCH)
PK_MOTION_MEMORY_ALLOCATOR_UDEMO_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-ALLOCATOR-UDEMO
PK_MOTION_MEMORY_ALLOCATOR_UDEMO_PATCH       := $(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_DIR)/patch
PK_MOTION_MEMORY_ALLOCATOR_UDEMO_BSBIT       := BiscuitOS-MOTION-MEMORY-ALLOCATOR-UDEMO-$(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_VERSION).bsbit
PK_MOTION_MEMORY_ALLOCATOR_UDEMO_OUT         :=
PK_MOTION_MEMORY_ALLOCATOR_UDEMO_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MOTION_MEMORY_ALLOCATOR_UDEMO_TAR         := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_TAR)
PK_MOTION_MEMORY_ALLOCATOR_UDEMO_PACKAGE     :=
PK_MOTION_MEMORY_ALLOCATOR_UDEMO_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_DIR)/build

PK_MOTION_MEMORY_ALLOCATOR_UDEMO_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_NAME)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_VERSION)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_SITE)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_GITHUB)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_BSBIT)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_UDEMO_CSRCFILE)

$(PK_MOTION_MEMORY_ALLOCATOR_UDEMO_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_ALLOCATOR_UDEMO_sh,$(srctree))
