################################################
PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_NAME        := BiscuitOS-MOTION-MEMORY-TEMPORARY-RSVDMEM
PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_VERSION     := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_VERSION)
PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_SITE        := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_URL)
PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_GITHUB)
PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_SUBNAME     :=
PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_BUILDARCH)
PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-TEMPORARY-RSVDMEM
PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_PATCH       := $(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_DIR)/patch
PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_BSBIT       := BiscuitOS-MOTION-MEMORY-TEMPORARY-RSVDMEM-$(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_VERSION).bsbit
PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_OUT         :=
PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_TAR         := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_TAR)
PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_PACKAGE     :=
PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_DIR)/build

PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_NAME)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_VERSION)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_SITE)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_GITHUB)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_BSBIT)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_CSRCFILE)

$(PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_TEMPORARY_RSVDMEM_sh,$(srctree))
