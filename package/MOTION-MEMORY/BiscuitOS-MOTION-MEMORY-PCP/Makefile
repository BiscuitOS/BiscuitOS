PK_MOTION_MEMORY_PCP_NAME        := BiscuitOS-MOTION-MEMORY-PCP
PK_MOTION_MEMORY_PCP_VERSION     := $(CONFIG_PK_MOTION_MEMORY_PCP_VERSION)
PK_MOTION_MEMORY_PCP_SITE        := $(CONFIG_PK_MOTION_MEMORY_PCP_URL)
PK_MOTION_MEMORY_PCP_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_PCP_GITHUB)
PK_MOTION_MEMORY_PCP_SUBNAME     :=
PK_MOTION_MEMORY_PCP_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_PCP_BUILDARCH)
PK_MOTION_MEMORY_PCP_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-PCP
PK_MOTION_MEMORY_PCP_PATCH       := $(PK_MOTION_MEMORY_PCP_DIR)/patch
PK_MOTION_MEMORY_PCP_BSBIT       := BiscuitOS-MOTION-MEMORY-PCP-$(PK_MOTION_MEMORY_PCP_VERSION).bsbit
PK_MOTION_MEMORY_PCP_OUT         :=
PK_MOTION_MEMORY_PCP_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MOTION_MEMORY_PCP_TAR         := $(CONFIG_PK_MOTION_MEMORY_PCP_TAR)
PK_MOTION_MEMORY_PCP_PACKAGE     :=
PK_MOTION_MEMORY_PCP_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_PCP_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_PCP_DIR)/build

PK_MOTION_MEMORY_PCP_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_PCP_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_PCP_NAME)X \
                 $(PK_MOTION_MEMORY_PCP_VERSION)X \
                 $(PK_MOTION_MEMORY_PCP_SITE)X \
                 $(PK_MOTION_MEMORY_PCP_GITHUB)X \
                 $(PK_MOTION_MEMORY_PCP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_PCP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_PCP_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_PCP_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_PCP_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_PCP_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_PCP_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_PCP_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_PCP_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_PCP_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_PCP_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_PCP_BSBIT)X \
                 $(PK_MOTION_MEMORY_PCP_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_PCP_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_PCP_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_PCP_CSRCFILE)

$(PK_MOTION_MEMORY_PCP_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_PCP_sh,$(srctree))
