################################################
#
# BiscuitOS-MOTION-MEMORY-ANONYMOUS-MEMORY
#
################################################
PK_MOTION_MEMORY_ANONYMOUS_MEMORY_NAME        := BiscuitOS-MOTION-MEMORY-ANONYMOUS-MEMORY
PK_MOTION_MEMORY_ANONYMOUS_MEMORY_VERSION     := $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_VERSION)
PK_MOTION_MEMORY_ANONYMOUS_MEMORY_SITE        := $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_URL)
PK_MOTION_MEMORY_ANONYMOUS_MEMORY_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_GITHUB)
PK_MOTION_MEMORY_ANONYMOUS_MEMORY_SUBNAME     :=
PK_MOTION_MEMORY_ANONYMOUS_MEMORY_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_BUILDARCH)
PK_MOTION_MEMORY_ANONYMOUS_MEMORY_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-ANONYMOUS-MEMORY
PK_MOTION_MEMORY_ANONYMOUS_MEMORY_PATCH       := $(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_DIR)/patch
PK_MOTION_MEMORY_ANONYMOUS_MEMORY_BSBIT       := BiscuitOS-MOTION-MEMORY-ANONYMOUS-MEMORY-$(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_VERSION).bsbit
PK_MOTION_MEMORY_ANONYMOUS_MEMORY_OUT         :=
PK_MOTION_MEMORY_ANONYMOUS_MEMORY_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MOTION_MEMORY_ANONYMOUS_MEMORY_TAR         := $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_TAR)
PK_MOTION_MEMORY_ANONYMOUS_MEMORY_PACKAGE     :=
PK_MOTION_MEMORY_ANONYMOUS_MEMORY_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_DIR)/build

PK_MOTION_MEMORY_ANONYMOUS_MEMORY_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_NAME)X \
                 $(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_VERSION)X \
                 $(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_SITE)X \
                 $(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_GITHUB)X \
                 $(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_BSBIT)X \
                 $(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_ANONYMOUS_MEMORY_CSRCFILE)

$(PK_MOTION_MEMORY_ANONYMOUS_MEMORY_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_ANONYMOUS_MEMORY_sh,$(srctree))
