################################################
#
# BiscuitOS-MOTION-MEMORY-USER
#
################################################
PK_MOTION_MEMORY_USER_NAME        := BiscuitOS-MOTION-MEMORY-USER
PK_MOTION_MEMORY_USER_VERSION     := $(CONFIG_PK_MOTION_MEMORY_USER_VERSION)
PK_MOTION_MEMORY_USER_SITE        := $(CONFIG_PK_MOTION_MEMORY_USER_URL)
PK_MOTION_MEMORY_USER_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_USER_GITHUB)
PK_MOTION_MEMORY_USER_SUBNAME     :=
PK_MOTION_MEMORY_USER_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_USER_BUILDARCH)
PK_MOTION_MEMORY_USER_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-USER
PK_MOTION_MEMORY_USER_PATCH       := $(PK_MOTION_MEMORY_USER_DIR)/patch
PK_MOTION_MEMORY_USER_BSBIT       := BiscuitOS-MOTION-MEMORY-USER-$(PK_MOTION_MEMORY_USER_VERSION).bsbit
PK_MOTION_MEMORY_USER_OUT         :=
PK_MOTION_MEMORY_USER_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MOTION_MEMORY_USER_TAR         := $(CONFIG_PK_MOTION_MEMORY_USER_TAR)
PK_MOTION_MEMORY_USER_PACKAGE     :=
PK_MOTION_MEMORY_USER_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_USER_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_USER_DIR)/build

PK_MOTION_MEMORY_USER_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_USER_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_USER_NAME)X \
                 $(PK_MOTION_MEMORY_USER_VERSION)X \
                 $(PK_MOTION_MEMORY_USER_SITE)X \
                 $(PK_MOTION_MEMORY_USER_GITHUB)X \
                 $(PK_MOTION_MEMORY_USER_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_USER_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_USER_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_USER_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_USER_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_USER_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_USER_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_USER_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_USER_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_USER_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_USER_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_USER_BSBIT)X \
                 $(PK_MOTION_MEMORY_USER_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_USER_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_USER_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_USER_CSRCFILE)

$(PK_MOTION_MEMORY_USER_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_USER_sh,$(srctree))
