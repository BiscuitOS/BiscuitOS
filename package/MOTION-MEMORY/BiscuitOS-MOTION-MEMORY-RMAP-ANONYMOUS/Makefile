#
# BiscuitOS-MOTION-MEMORY-RMAP-ANONYMOUS
#
################################################
PK_MOTION_MEMORY_RMAP_ANONYMOUS_NAME        := BiscuitOS-MOTION-MEMORY-RMAP-ANONYMOUS
PK_MOTION_MEMORY_RMAP_ANONYMOUS_VERSION     := $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_VERSION)
PK_MOTION_MEMORY_RMAP_ANONYMOUS_SITE        := $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_URL)
PK_MOTION_MEMORY_RMAP_ANONYMOUS_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_GITHUB)
PK_MOTION_MEMORY_RMAP_ANONYMOUS_SUBNAME     :=
PK_MOTION_MEMORY_RMAP_ANONYMOUS_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_BUILDARCH)
PK_MOTION_MEMORY_RMAP_ANONYMOUS_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-RMAP-ANONYMOUS
PK_MOTION_MEMORY_RMAP_ANONYMOUS_PATCH       := $(PK_MOTION_MEMORY_RMAP_ANONYMOUS_DIR)/patch
PK_MOTION_MEMORY_RMAP_ANONYMOUS_BSBIT       := BiscuitOS-MOTION-MEMORY-RMAP-ANONYMOUS-$(PK_MOTION_MEMORY_RMAP_ANONYMOUS_VERSION).bsbit
PK_MOTION_MEMORY_RMAP_ANONYMOUS_OUT         :=
PK_MOTION_MEMORY_RMAP_ANONYMOUS_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_MOTION_MEMORY_RMAP_ANONYMOUS_TAR         := $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_TAR)
PK_MOTION_MEMORY_RMAP_ANONYMOUS_PACKAGE     :=
PK_MOTION_MEMORY_RMAP_ANONYMOUS_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_RMAP_ANONYMOUS_DIR)/build

PK_MOTION_MEMORY_RMAP_ANONYMOUS_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_RMAP_ANONYMOUS_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_RMAP_ANONYMOUS_NAME)X \
                 $(PK_MOTION_MEMORY_RMAP_ANONYMOUS_VERSION)X \
                 $(PK_MOTION_MEMORY_RMAP_ANONYMOUS_SITE)X \
                 $(PK_MOTION_MEMORY_RMAP_ANONYMOUS_GITHUB)X \
                 $(PK_MOTION_MEMORY_RMAP_ANONYMOUS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_RMAP_ANONYMOUS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_RMAP_ANONYMOUS_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_RMAP_ANONYMOUS_BSBIT)X \
                 $(PK_MOTION_MEMORY_RMAP_ANONYMOUS_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_RMAP_ANONYMOUS_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_RMAP_ANONYMOUS_CSRCFILE)

$(PK_MOTION_MEMORY_RMAP_ANONYMOUS_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_RMAP_ANONYMOUS_sh,$(srctree))
