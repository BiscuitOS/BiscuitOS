PK_MOTION_MEMORY_FIXEDMAP_NAME        := BiscuitOS-MOTION-MEMORY-FIXEDMAP
PK_MOTION_MEMORY_FIXEDMAP_VERSION     := $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_VERSION)
PK_MOTION_MEMORY_FIXEDMAP_SITE        := $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_URL)
PK_MOTION_MEMORY_FIXEDMAP_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_GITHUB)
PK_MOTION_MEMORY_FIXEDMAP_SUBNAME     :=
PK_MOTION_MEMORY_FIXEDMAP_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_BUILDARCH)
PK_MOTION_MEMORY_FIXEDMAP_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-FIXEDMAP
PK_MOTION_MEMORY_FIXEDMAP_PATCH       := $(PK_MOTION_MEMORY_FIXEDMAP_DIR)/patch
PK_MOTION_MEMORY_FIXEDMAP_BSBIT       := BiscuitOS-MOTION-MEMORY-FIXEDMAP-$(PK_MOTION_MEMORY_FIXEDMAP_VERSION).bsbit
PK_MOTION_MEMORY_FIXEDMAP_OUT         :=
PK_MOTION_MEMORY_FIXEDMAP_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MOTION_MEMORY_FIXEDMAP_TAR         := $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_TAR)
PK_MOTION_MEMORY_FIXEDMAP_PACKAGE     :=
PK_MOTION_MEMORY_FIXEDMAP_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_FIXEDMAP_DIR)/build

PK_MOTION_MEMORY_FIXEDMAP_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_FIXEDMAP_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_FIXEDMAP_NAME)X \
                 $(PK_MOTION_MEMORY_FIXEDMAP_VERSION)X \
                 $(PK_MOTION_MEMORY_FIXEDMAP_SITE)X \
                 $(PK_MOTION_MEMORY_FIXEDMAP_GITHUB)X \
                 $(PK_MOTION_MEMORY_FIXEDMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_FIXEDMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_FIXEDMAP_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_FIXEDMAP_BSBIT)X \
                 $(PK_MOTION_MEMORY_FIXEDMAP_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_FIXEDMAP_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_FIXEDMAP_CSRCFILE)

$(PK_MOTION_MEMORY_FIXEDMAP_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_FIXEDMAP_sh,$(srctree))
