PK_MOTION_MEMORY_ALLOCATOR_KERNEL_NAME        := BiscuitOS-MOTION-MEMORY-ALLOCATOR-KERNEL
PK_MOTION_MEMORY_ALLOCATOR_KERNEL_VERSION     := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_VERSION)
PK_MOTION_MEMORY_ALLOCATOR_KERNEL_SITE        := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_URL)
PK_MOTION_MEMORY_ALLOCATOR_KERNEL_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_GITHUB)
PK_MOTION_MEMORY_ALLOCATOR_KERNEL_SUBNAME     :=
PK_MOTION_MEMORY_ALLOCATOR_KERNEL_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_BUILDARCH)
PK_MOTION_MEMORY_ALLOCATOR_KERNEL_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-ALLOCATOR-KERNEL
PK_MOTION_MEMORY_ALLOCATOR_KERNEL_PATCH       := $(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_DIR)/patch
PK_MOTION_MEMORY_ALLOCATOR_KERNEL_BSBIT       := BiscuitOS-MOTION-MEMORY-ALLOCATOR-KERNEL-$(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_VERSION).bsbit
PK_MOTION_MEMORY_ALLOCATOR_KERNEL_OUT         :=
PK_MOTION_MEMORY_ALLOCATOR_KERNEL_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_MOTION_MEMORY_ALLOCATOR_KERNEL_TAR         := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_TAR)
PK_MOTION_MEMORY_ALLOCATOR_KERNEL_PACKAGE     :=
PK_MOTION_MEMORY_ALLOCATOR_KERNEL_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_DIR)/build

PK_MOTION_MEMORY_ALLOCATOR_KERNEL_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_NAME)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_VERSION)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_SITE)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_GITHUB)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_BSBIT)X \
                 $(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_ALLOCATOR_KERNEL_CSRCFILE)

$(PK_MOTION_MEMORY_ALLOCATOR_KERNEL_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_ALLOCATOR_KERNEL_sh,$(srctree))
