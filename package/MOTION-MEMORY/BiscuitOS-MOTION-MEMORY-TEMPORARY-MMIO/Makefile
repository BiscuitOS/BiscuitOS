# BiscuitOS-MOTION-MEMORY-TEMPORARY-MMIO
#
################################################
PK_MOTION_MEMORY_TEMPORARY_MMIO_NAME        := BiscuitOS-MOTION-MEMORY-TEMPORARY-MMIO
PK_MOTION_MEMORY_TEMPORARY_MMIO_VERSION     := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_VERSION)
PK_MOTION_MEMORY_TEMPORARY_MMIO_SITE        := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_URL)
PK_MOTION_MEMORY_TEMPORARY_MMIO_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_GITHUB)
PK_MOTION_MEMORY_TEMPORARY_MMIO_SUBNAME     :=
PK_MOTION_MEMORY_TEMPORARY_MMIO_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_BUILDARCH)
PK_MOTION_MEMORY_TEMPORARY_MMIO_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-TEMPORARY-MMIO
PK_MOTION_MEMORY_TEMPORARY_MMIO_PATCH       := $(PK_MOTION_MEMORY_TEMPORARY_MMIO_DIR)/patch
PK_MOTION_MEMORY_TEMPORARY_MMIO_BSBIT       := BiscuitOS-MOTION-MEMORY-TEMPORARY-MMIO-$(PK_MOTION_MEMORY_TEMPORARY_MMIO_VERSION).bsbit
PK_MOTION_MEMORY_TEMPORARY_MMIO_OUT         :=
PK_MOTION_MEMORY_TEMPORARY_MMIO_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MOTION_MEMORY_TEMPORARY_MMIO_TAR         := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_TAR)
PK_MOTION_MEMORY_TEMPORARY_MMIO_PACKAGE     :=
PK_MOTION_MEMORY_TEMPORARY_MMIO_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_TEMPORARY_MMIO_DIR)/build

PK_MOTION_MEMORY_TEMPORARY_MMIO_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_TEMPORARY_MMIO_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_TEMPORARY_MMIO_NAME)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_MMIO_VERSION)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_MMIO_SITE)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_MMIO_GITHUB)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_MMIO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_MMIO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_MMIO_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_MMIO_BSBIT)X \
                 $(PK_MOTION_MEMORY_TEMPORARY_MMIO_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_TEMPORARY_MMIO_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_TEMPORARY_MMIO_CSRCFILE)

$(PK_MOTION_MEMORY_TEMPORARY_MMIO_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_TEMPORARY_MMIO_sh,$(srctree))
