# BiscuitOS-MOTION-MEMORY-VMALLOC
#
################################################
PK_MOTION_MEMORY_VMALLOC_NAME        := BiscuitOS-MOTION-MEMORY-VMALLOC
PK_MOTION_MEMORY_VMALLOC_VERSION     := $(CONFIG_PK_MOTION_MEMORY_VMALLOC_VERSION)
PK_MOTION_MEMORY_VMALLOC_SITE        := $(CONFIG_PK_MOTION_MEMORY_VMALLOC_URL)
PK_MOTION_MEMORY_VMALLOC_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_VMALLOC_GITHUB)
PK_MOTION_MEMORY_VMALLOC_SUBNAME     :=
PK_MOTION_MEMORY_VMALLOC_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_VMALLOC_BUILDARCH)
PK_MOTION_MEMORY_VMALLOC_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-VMALLOC
PK_MOTION_MEMORY_VMALLOC_PATCH       := $(PK_MOTION_MEMORY_VMALLOC_DIR)/patch
PK_MOTION_MEMORY_VMALLOC_BSBIT       := BiscuitOS-MOTION-MEMORY-VMALLOC-$(PK_MOTION_MEMORY_VMALLOC_VERSION).bsbit
PK_MOTION_MEMORY_VMALLOC_OUT         :=
PK_MOTION_MEMORY_VMALLOC_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MOTION_MEMORY_VMALLOC_TAR         := $(CONFIG_PK_MOTION_MEMORY_VMALLOC_TAR)
PK_MOTION_MEMORY_VMALLOC_PACKAGE     :=
PK_MOTION_MEMORY_VMALLOC_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_VMALLOC_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_VMALLOC_DIR)/build

PK_MOTION_MEMORY_VMALLOC_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_VMALLOC_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_VMALLOC_NAME)X \
                 $(PK_MOTION_MEMORY_VMALLOC_VERSION)X \
                 $(PK_MOTION_MEMORY_VMALLOC_SITE)X \
                 $(PK_MOTION_MEMORY_VMALLOC_GITHUB)X \
                 $(PK_MOTION_MEMORY_VMALLOC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_VMALLOC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_VMALLOC_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_VMALLOC_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_VMALLOC_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_VMALLOC_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_VMALLOC_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_VMALLOC_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_VMALLOC_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_VMALLOC_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_VMALLOC_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_VMALLOC_BSBIT)X \
                 $(PK_MOTION_MEMORY_VMALLOC_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_VMALLOC_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_VMALLOC_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_VMALLOC_CSRCFILE)

$(PK_MOTION_MEMORY_VMALLOC_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_VMALLOC_sh,$(srctree))
