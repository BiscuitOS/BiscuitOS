################################################
#
# BiscuitOS-MOTION-MEMORY-SLAB
#
################################################
PK_MOTION_MEMORY_SLAB_NAME        := BiscuitOS-MOTION-MEMORY-SLAB
PK_MOTION_MEMORY_SLAB_VERSION     := $(CONFIG_PK_MOTION_MEMORY_SLAB_VERSION)
PK_MOTION_MEMORY_SLAB_SITE        := $(CONFIG_PK_MOTION_MEMORY_SLAB_URL)
PK_MOTION_MEMORY_SLAB_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_SLAB_GITHUB)
PK_MOTION_MEMORY_SLAB_SUBNAME     :=
PK_MOTION_MEMORY_SLAB_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_SLAB_BUILDARCH)
PK_MOTION_MEMORY_SLAB_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-SLAB
PK_MOTION_MEMORY_SLAB_PATCH       := $(PK_MOTION_MEMORY_SLAB_DIR)/patch
PK_MOTION_MEMORY_SLAB_BSBIT       := BiscuitOS-MOTION-MEMORY-SLAB-$(PK_MOTION_MEMORY_SLAB_VERSION).bsbit
PK_MOTION_MEMORY_SLAB_OUT         :=
PK_MOTION_MEMORY_SLAB_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MOTION_MEMORY_SLAB_TAR         := $(CONFIG_PK_MOTION_MEMORY_SLAB_TAR)
PK_MOTION_MEMORY_SLAB_PACKAGE     :=
PK_MOTION_MEMORY_SLAB_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_SLAB_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_SLAB_DIR)/build

PK_MOTION_MEMORY_SLAB_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_SLAB_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_SLAB_NAME)X \
                 $(PK_MOTION_MEMORY_SLAB_VERSION)X \
                 $(PK_MOTION_MEMORY_SLAB_SITE)X \
                 $(PK_MOTION_MEMORY_SLAB_GITHUB)X \
                 $(PK_MOTION_MEMORY_SLAB_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_SLAB_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_SLAB_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_SLAB_BSBIT)X \
                 $(PK_MOTION_MEMORY_SLAB_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_SLAB_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_CSRCFILE)

$(PK_MOTION_MEMORY_SLAB_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_SLAB_sh,$(srctree))
