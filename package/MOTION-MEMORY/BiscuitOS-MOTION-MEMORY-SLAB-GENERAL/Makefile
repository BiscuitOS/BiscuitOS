PK_MOTION_MEMORY_SLAB_GENERAL_NAME        := BiscuitOS-MOTION-MEMORY-SLAB-GENERAL
PK_MOTION_MEMORY_SLAB_GENERAL_VERSION     := $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_VERSION)
PK_MOTION_MEMORY_SLAB_GENERAL_SITE        := $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_URL)
PK_MOTION_MEMORY_SLAB_GENERAL_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_GITHUB)
PK_MOTION_MEMORY_SLAB_GENERAL_SUBNAME     :=
PK_MOTION_MEMORY_SLAB_GENERAL_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_BUILDARCH)
PK_MOTION_MEMORY_SLAB_GENERAL_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-SLAB-GENERAL
PK_MOTION_MEMORY_SLAB_GENERAL_PATCH       := $(PK_MOTION_MEMORY_SLAB_GENERAL_DIR)/patch
PK_MOTION_MEMORY_SLAB_GENERAL_BSBIT       := BiscuitOS-MOTION-MEMORY-SLAB-GENERAL-$(PK_MOTION_MEMORY_SLAB_GENERAL_VERSION).bsbit
PK_MOTION_MEMORY_SLAB_GENERAL_OUT         :=
PK_MOTION_MEMORY_SLAB_GENERAL_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MOTION_MEMORY_SLAB_GENERAL_TAR         := $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_TAR)
PK_MOTION_MEMORY_SLAB_GENERAL_PACKAGE     :=
PK_MOTION_MEMORY_SLAB_GENERAL_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_SLAB_GENERAL_DIR)/build

PK_MOTION_MEMORY_SLAB_GENERAL_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_SLAB_GENERAL_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_SLAB_GENERAL_NAME)X \
                 $(PK_MOTION_MEMORY_SLAB_GENERAL_VERSION)X \
                 $(PK_MOTION_MEMORY_SLAB_GENERAL_SITE)X \
                 $(PK_MOTION_MEMORY_SLAB_GENERAL_GITHUB)X \
                 $(PK_MOTION_MEMORY_SLAB_GENERAL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_SLAB_GENERAL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_SLAB_GENERAL_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_SLAB_GENERAL_BSBIT)X \
                 $(PK_MOTION_MEMORY_SLAB_GENERAL_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_SLAB_GENERAL_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_SLAB_GENERAL_CSRCFILE)

$(PK_MOTION_MEMORY_SLAB_GENERAL_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_SLAB_GENERAL_sh,$(srctree))
