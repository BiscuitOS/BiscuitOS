PK_MOTION_MEMORY_ASAN_NAME        := BiscuitOS-MOTION-MEMORY-ASAN
PK_MOTION_MEMORY_ASAN_VERSION     := $(CONFIG_PK_MOTION_MEMORY_ASAN_VERSION)
PK_MOTION_MEMORY_ASAN_SITE        := $(CONFIG_PK_MOTION_MEMORY_ASAN_URL)
PK_MOTION_MEMORY_ASAN_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_ASAN_GITHUB)
PK_MOTION_MEMORY_ASAN_SUBNAME     :=
PK_MOTION_MEMORY_ASAN_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_ASAN_BUILDARCH)
PK_MOTION_MEMORY_ASAN_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-ASAN
PK_MOTION_MEMORY_ASAN_PATCH       := $(PK_MOTION_MEMORY_ASAN_DIR)/patch
PK_MOTION_MEMORY_ASAN_BSBIT       := BiscuitOS-MOTION-MEMORY-ASAN-$(PK_MOTION_MEMORY_ASAN_VERSION).bsbit
PK_MOTION_MEMORY_ASAN_OUT         :=
PK_MOTION_MEMORY_ASAN_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MOTION_MEMORY_ASAN_TAR         := $(CONFIG_PK_MOTION_MEMORY_ASAN_TAR)
PK_MOTION_MEMORY_ASAN_PACKAGE     :=
PK_MOTION_MEMORY_ASAN_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_ASAN_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_ASAN_DIR)/build

PK_MOTION_MEMORY_ASAN_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_ASAN_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_ASAN_NAME)X \
                 $(PK_MOTION_MEMORY_ASAN_VERSION)X \
                 $(PK_MOTION_MEMORY_ASAN_SITE)X \
                 $(PK_MOTION_MEMORY_ASAN_GITHUB)X \
                 $(PK_MOTION_MEMORY_ASAN_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_ASAN_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_ASAN_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_ASAN_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_ASAN_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_ASAN_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ASAN_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_ASAN_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ASAN_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ASAN_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_ASAN_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_ASAN_BSBIT)X \
                 $(PK_MOTION_MEMORY_ASAN_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_ASAN_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_ASAN_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_ASAN_CSRCFILE)

$(PK_MOTION_MEMORY_ASAN_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_ASAN_sh,$(srctree))
