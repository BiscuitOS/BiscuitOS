################################################
#
# BiscuitOS-MOTION-MEMORY-PREALLOC
#
################################################
PK_MOTION_MEMORY_PREALLOC_NAME        := BiscuitOS-MOTION-MEMORY-PREALLOC
PK_MOTION_MEMORY_PREALLOC_VERSION     := $(CONFIG_PK_MOTION_MEMORY_PREALLOC_VERSION)
PK_MOTION_MEMORY_PREALLOC_SITE        := $(CONFIG_PK_MOTION_MEMORY_PREALLOC_URL)
PK_MOTION_MEMORY_PREALLOC_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_PREALLOC_GITHUB)
PK_MOTION_MEMORY_PREALLOC_SUBNAME     :=
PK_MOTION_MEMORY_PREALLOC_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_PREALLOC_BUILDARCH)
PK_MOTION_MEMORY_PREALLOC_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-PREALLOC
PK_MOTION_MEMORY_PREALLOC_PATCH       := $(PK_MOTION_MEMORY_PREALLOC_DIR)/patch
PK_MOTION_MEMORY_PREALLOC_BSBIT       := BiscuitOS-MOTION-MEMORY-PREALLOC-$(PK_MOTION_MEMORY_PREALLOC_VERSION).bsbit
PK_MOTION_MEMORY_PREALLOC_OUT         :=
PK_MOTION_MEMORY_PREALLOC_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MOTION_MEMORY_PREALLOC_TAR         := $(CONFIG_PK_MOTION_MEMORY_PREALLOC_TAR)
PK_MOTION_MEMORY_PREALLOC_PACKAGE     :=
PK_MOTION_MEMORY_PREALLOC_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_PREALLOC_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_PREALLOC_DIR)/build

PK_MOTION_MEMORY_PREALLOC_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_PREALLOC_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_PREALLOC_NAME)X \
                 $(PK_MOTION_MEMORY_PREALLOC_VERSION)X \
                 $(PK_MOTION_MEMORY_PREALLOC_SITE)X \
                 $(PK_MOTION_MEMORY_PREALLOC_GITHUB)X \
                 $(PK_MOTION_MEMORY_PREALLOC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_PREALLOC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_PREALLOC_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_PREALLOC_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_PREALLOC_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_PREALLOC_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_PREALLOC_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_PREALLOC_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_PREALLOC_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_PREALLOC_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_PREALLOC_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_PREALLOC_BSBIT)X \
                 $(PK_MOTION_MEMORY_PREALLOC_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_PREALLOC_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_PREALLOC_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_PREALLOC_CSRCFILE)

$(PK_MOTION_MEMORY_PREALLOC_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_PREALLOC_sh,$(srctree))
