PK_MOTION_MEMORY_GDBCORE_NAME        := BiscuitOS-MOTION-MEMORY-GDBCORE
PK_MOTION_MEMORY_GDBCORE_VERSION     := $(CONFIG_PK_MOTION_MEMORY_GDBCORE_VERSION)
PK_MOTION_MEMORY_GDBCORE_SITE        := $(CONFIG_PK_MOTION_MEMORY_GDBCORE_URL)
PK_MOTION_MEMORY_GDBCORE_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_GDBCORE_GITHUB)
PK_MOTION_MEMORY_GDBCORE_SUBNAME     :=
PK_MOTION_MEMORY_GDBCORE_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_GDBCORE_BUILDARCH)
PK_MOTION_MEMORY_GDBCORE_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-GDBCORE
PK_MOTION_MEMORY_GDBCORE_PATCH       := $(PK_MOTION_MEMORY_GDBCORE_DIR)/patch
PK_MOTION_MEMORY_GDBCORE_BSBIT       := BiscuitOS-MOTION-MEMORY-GDBCORE-$(PK_MOTION_MEMORY_GDBCORE_VERSION).bsbit
PK_MOTION_MEMORY_GDBCORE_OUT         :=
PK_MOTION_MEMORY_GDBCORE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MOTION_MEMORY_GDBCORE_TAR         := $(CONFIG_PK_MOTION_MEMORY_GDBCORE_TAR)
PK_MOTION_MEMORY_GDBCORE_PACKAGE     :=
PK_MOTION_MEMORY_GDBCORE_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_GDBCORE_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_GDBCORE_DIR)/build

PK_MOTION_MEMORY_GDBCORE_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_GDBCORE_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_GDBCORE_NAME)X \
                 $(PK_MOTION_MEMORY_GDBCORE_VERSION)X \
                 $(PK_MOTION_MEMORY_GDBCORE_SITE)X \
                 $(PK_MOTION_MEMORY_GDBCORE_GITHUB)X \
                 $(PK_MOTION_MEMORY_GDBCORE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_GDBCORE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_GDBCORE_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_GDBCORE_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_GDBCORE_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_GDBCORE_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_GDBCORE_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_GDBCORE_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_GDBCORE_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_GDBCORE_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_GDBCORE_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_GDBCORE_BSBIT)X \
                 $(PK_MOTION_MEMORY_GDBCORE_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_GDBCORE_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_GDBCORE_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_GDBCORE_CSRCFILE)

$(PK_MOTION_MEMORY_GDBCORE_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_GDBCORE_sh,$(srctree))
