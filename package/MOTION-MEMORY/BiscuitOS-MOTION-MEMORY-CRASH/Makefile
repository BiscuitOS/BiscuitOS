################################################
#
# BiscuitOS-MOTION-MEMORY-CRASH
#
################################################
PK_MOTION_MEMORY_CRASH_NAME        := BiscuitOS-MOTION-MEMORY-CRASH
PK_MOTION_MEMORY_CRASH_VERSION     := $(CONFIG_PK_MOTION_MEMORY_CRASH_VERSION)
PK_MOTION_MEMORY_CRASH_SITE        := $(CONFIG_PK_MOTION_MEMORY_CRASH_URL)
PK_MOTION_MEMORY_CRASH_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_CRASH_GITHUB)
PK_MOTION_MEMORY_CRASH_SUBNAME     :=
PK_MOTION_MEMORY_CRASH_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_CRASH_BUILDARCH)
PK_MOTION_MEMORY_CRASH_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-CRASH
PK_MOTION_MEMORY_CRASH_PATCH       := $(PK_MOTION_MEMORY_CRASH_DIR)/patch
PK_MOTION_MEMORY_CRASH_BSBIT       := BiscuitOS-MOTION-MEMORY-CRASH-$(PK_MOTION_MEMORY_CRASH_VERSION).bsbit
PK_MOTION_MEMORY_CRASH_OUT         :=
PK_MOTION_MEMORY_CRASH_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MOTION_MEMORY_CRASH_TAR         := $(CONFIG_PK_MOTION_MEMORY_CRASH_TAR)
PK_MOTION_MEMORY_CRASH_PACKAGE     :=
PK_MOTION_MEMORY_CRASH_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_CRASH_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_CRASH_DIR)/build

PK_MOTION_MEMORY_CRASH_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_CRASH_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_CRASH_NAME)X \
                 $(PK_MOTION_MEMORY_CRASH_VERSION)X \
                 $(PK_MOTION_MEMORY_CRASH_SITE)X \
                 $(PK_MOTION_MEMORY_CRASH_GITHUB)X \
                 $(PK_MOTION_MEMORY_CRASH_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_CRASH_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_CRASH_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_CRASH_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_CRASH_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_CRASH_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_CRASH_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_CRASH_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_CRASH_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_CRASH_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_CRASH_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_CRASH_BSBIT)X \
                 $(PK_MOTION_MEMORY_CRASH_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_CRASH_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_CRASH_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_CRASH_CSRCFILE)

$(PK_MOTION_MEMORY_CRASH_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_CRASH_sh,$(srctree))
