PK_MOTION_MEMORY_HUGETLB_NAME        := BiscuitOS-MOTION-MEMORY-HUGETLB
PK_MOTION_MEMORY_HUGETLB_VERSION     := $(CONFIG_PK_MOTION_MEMORY_HUGETLB_VERSION)
PK_MOTION_MEMORY_HUGETLB_SITE        := $(CONFIG_PK_MOTION_MEMORY_HUGETLB_URL)
PK_MOTION_MEMORY_HUGETLB_GITHUB      := $(CONFIG_PK_MOTION_MEMORY_HUGETLB_GITHUB)
PK_MOTION_MEMORY_HUGETLB_SUBNAME     :=
PK_MOTION_MEMORY_HUGETLB_BUILDARCH   := $(CONFIG_PK_MOTION_MEMORY_HUGETLB_BUILDARCH)
PK_MOTION_MEMORY_HUGETLB_DIR         := $(srctree)/package/MOTION-MEMORY/BiscuitOS-MOTION-MEMORY-HUGETLB
PK_MOTION_MEMORY_HUGETLB_PATCH       := $(PK_MOTION_MEMORY_HUGETLB_DIR)/patch
PK_MOTION_MEMORY_HUGETLB_BSBIT       := BiscuitOS-MOTION-MEMORY-HUGETLB-$(PK_MOTION_MEMORY_HUGETLB_VERSION).bsbit
PK_MOTION_MEMORY_HUGETLB_OUT         :=
PK_MOTION_MEMORY_HUGETLB_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MOTION_MEMORY_HUGETLB_TAR         := $(CONFIG_PK_MOTION_MEMORY_HUGETLB_TAR)
PK_MOTION_MEMORY_HUGETLB_PACKAGE     :=
PK_MOTION_MEMORY_HUGETLB_CONFIG      := $(CONFIG_PK_MOTION_MEMORY_HUGETLB_CONFIG)
SUB_TARGET                  += $(PK_MOTION_MEMORY_HUGETLB_DIR)/build

PK_MOTION_MEMORY_HUGETLB_sh = $(CONFIG_SHELL) $(PK_MOTION_MEMORY_HUGETLB_SCRIPTS) \
                 $1X \
                 $(PK_MOTION_MEMORY_HUGETLB_NAME)X \
                 $(PK_MOTION_MEMORY_HUGETLB_VERSION)X \
                 $(PK_MOTION_MEMORY_HUGETLB_SITE)X \
                 $(PK_MOTION_MEMORY_HUGETLB_GITHUB)X \
                 $(PK_MOTION_MEMORY_HUGETLB_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MOTION_MEMORY_HUGETLB_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MOTION_MEMORY_HUGETLB_SUBNAME)X \
                 $(CONFIG_PK_MOTION_MEMORY_HUGETLB_CONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_HUGETLB_LIBPATH)X \
                 $(CONFIG_PK_MOTION_MEMORY_HUGETLB_CPPFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_HUGETLB_DPKCONFIG)X \
                 $(CONFIG_PK_MOTION_MEMORY_HUGETLB_CFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_HUGETLB_LDFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_HUGETLB_CXXFLAGS)X \
                 $(CONFIG_PK_MOTION_MEMORY_HUGETLB_ASFLAGS)X \
                 $(PK_MOTION_MEMORY_HUGETLB_BSBIT)X \
                 $(PK_MOTION_MEMORY_HUGETLB_BUILDARCH)X \
		 $(PK_MOTION_MEMORY_HUGETLB_CSRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_HUGETLB_SRCFILE)X \
                 $(CONFIG_PK_MOTION_MEMORY_HUGETLB_CSRCFILE)

$(PK_MOTION_MEMORY_HUGETLB_DIR)/build: FORCE
	$(Q)$(call PK_MOTION_MEMORY_HUGETLB_sh,$(srctree))
