                 $(PK_MCE_MADV_SOFT_OFFLINE_PAGE_SITE)X \
                 $(PK_MCE_MADV_SOFT_OFFLINE_PAGE_GITHUB)X \
                 $(PK_MCE_MADV_SOFT_OFFLINE_PAGE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MCE_MADV_SOFT_OFFLINE_PAGE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MCE_MADV_SOFT_OFFLINE_PAGE_SUBNAME)X \
                 $(CONFIG_PK_MCE_MADV_SOFT_OFFLINE_PAGE_CONFIG)X \
                 $(CONFIG_PK_MCE_MADV_SOFT_OFFLINE_PAGE_LIBPATH)X \
                 $(CONFIG_PK_MCE_MADV_SOFT_OFFLINE_PAGE_CPPFLAGS)X \
                 $(CONFIG_PK_MCE_MADV_SOFT_OFFLINE_PAGE_DPKCONFIG)X \
                 $(CONFIG_PK_MCE_MADV_SOFT_OFFLINE_PAGE_CFLAGS)X \
                 $(CONFIG_PK_MCE_MADV_SOFT_OFFLINE_PAGE_LDFLAGS)X \
                 $(CONFIG_PK_MCE_MADV_SOFT_OFFLINE_PAGE_CXXFLAGS)X \
                 $(CONFIG_PK_MCE_MADV_SOFT_OFFLINE_PAGE_ASFLAGS)X \
                 $(PK_MCE_MADV_SOFT_OFFLINE_PAGE_BSBIT)X \
                 $(PK_MCE_MADV_SOFT_OFFLINE_PAGE_BUILDARCH)X \
		 $(PK_MCE_MADV_SOFT_OFFLINE_PAGE_CSRCFILE)X \
                 $(CONFIG_PK_MCE_MADV_SOFT_OFFLINE_PAGE_SRCFILE)X \
