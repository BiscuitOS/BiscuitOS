PK_MCE_MADV_SOFT_OFFLINE_PAGE_PACKAGE     :=
PK_MCE_MADV_SOFT_OFFLINE_PAGE_CONFIG      := $(CONFIG_PK_MCE_MADV_SOFT_OFFLINE_PAGE_CONFIG)
SUB_TARGET                  += $(PK_MCE_MADV_SOFT_OFFLINE_PAGE_DIR)/build

PK_MCE_MADV_SOFT_OFFLINE_PAGE_sh = $(CONFIG_SHELL) $(PK_MCE_MADV_SOFT_OFFLINE_PAGE_SCRIPTS) \
                 $1X \
