PK_MCE_MADV_SOFT_OFFLINE_PAGE_GITHUB      := $(CONFIG_PK_MCE_MADV_SOFT_OFFLINE_PAGE_GITHUB)
PK_MCE_MADV_SOFT_OFFLINE_PAGE_SUBNAME     :=
PK_MCE_MADV_SOFT_OFFLINE_PAGE_BUILDARCH   := $(CONFIG_PK_MCE_MADV_SOFT_OFFLINE_PAGE_BUILDARCH)
PK_MCE_MADV_SOFT_OFFLINE_PAGE_DIR         := $(srctree)/package/MCE/BiscuitOS-MCE-madvise-software-offline-poison-page
PK_MCE_MADV_SOFT_OFFLINE_PAGE_PATCH       := $(PK_MCE_MADV_SOFT_OFFLINE_PAGE_DIR)/patch
PK_MCE_MADV_SOFT_OFFLINE_PAGE_BSBIT       := BiscuitOS-MCE-madvise-software-offline-poison-page-$(PK_MCE_MADV_SOFT_OFFLINE_PAGE_VERSION).bsbit
PK_MCE_MADV_SOFT_OFFLINE_PAGE_OUT         :=
