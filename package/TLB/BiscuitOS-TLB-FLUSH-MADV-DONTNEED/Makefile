PK_TLB_FLUSH_MADVDONTNEED_NAME        := BiscuitOS-TLB-FLUSH-MADV-DONTNEED
PK_TLB_FLUSH_MADVDONTNEED_VERSION     := $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_VERSION)
PK_TLB_FLUSH_MADVDONTNEED_SITE        := $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_URL)
PK_TLB_FLUSH_MADVDONTNEED_GITHUB      := $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_GITHUB)
PK_TLB_FLUSH_MADVDONTNEED_SUBNAME     :=
PK_TLB_FLUSH_MADVDONTNEED_BUILDARCH   := $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_BUILDARCH)
PK_TLB_FLUSH_MADVDONTNEED_DIR         := $(srctree)/package/TLB/BiscuitOS-TLB-FLUSH-MADV-DONTNEED
PK_TLB_FLUSH_MADVDONTNEED_PATCH       := $(PK_TLB_FLUSH_MADVDONTNEED_DIR)/patch
PK_TLB_FLUSH_MADVDONTNEED_BSBIT       := BiscuitOS-TLB-FLUSH-MADV-DONTNEED-$(PK_TLB_FLUSH_MADVDONTNEED_VERSION).bsbit
PK_TLB_FLUSH_MADVDONTNEED_OUT         :=
PK_TLB_FLUSH_MADVDONTNEED_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_TLB_FLUSH_MADVDONTNEED_TAR         := $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_TAR)
PK_TLB_FLUSH_MADVDONTNEED_PACKAGE     :=
PK_TLB_FLUSH_MADVDONTNEED_CONFIG      := $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_CONFIG)
SUB_TARGET                  += $(PK_TLB_FLUSH_MADVDONTNEED_DIR)/build

PK_TLB_FLUSH_MADVDONTNEED_sh = $(CONFIG_SHELL) $(PK_TLB_FLUSH_MADVDONTNEED_SCRIPTS) \
                 $1X \
                 $(PK_TLB_FLUSH_MADVDONTNEED_NAME)X \
                 $(PK_TLB_FLUSH_MADVDONTNEED_VERSION)X \
                 $(PK_TLB_FLUSH_MADVDONTNEED_SITE)X \
                 $(PK_TLB_FLUSH_MADVDONTNEED_GITHUB)X \
                 $(PK_TLB_FLUSH_MADVDONTNEED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_TLB_FLUSH_MADVDONTNEED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_TLB_FLUSH_MADVDONTNEED_SUBNAME)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_CONFIG)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_LIBPATH)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_CPPFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_DPKCONFIG)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_CFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_LDFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_CXXFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_ASFLAGS)X \
                 $(PK_TLB_FLUSH_MADVDONTNEED_BSBIT)X \
                 $(PK_TLB_FLUSH_MADVDONTNEED_BUILDARCH)X \
		 $(PK_TLB_FLUSH_MADVDONTNEED_CSRCFILE)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_SRCFILE)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVDONTNEED_CSRCFILE)

$(PK_TLB_FLUSH_MADVDONTNEED_DIR)/build: FORCE
	$(Q)$(call PK_TLB_FLUSH_MADVDONTNEED_sh,$(srctree))
