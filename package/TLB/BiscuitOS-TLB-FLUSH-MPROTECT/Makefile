PK_TLB_FLUSH_MPROTECT_NAME        := BiscuitOS-TLB-FLUSH-MPROTECT
PK_TLB_FLUSH_MPROTECT_VERSION     := $(CONFIG_PK_TLB_FLUSH_MPROTECT_VERSION)
PK_TLB_FLUSH_MPROTECT_SITE        := $(CONFIG_PK_TLB_FLUSH_MPROTECT_URL)
PK_TLB_FLUSH_MPROTECT_GITHUB      := $(CONFIG_PK_TLB_FLUSH_MPROTECT_GITHUB)
PK_TLB_FLUSH_MPROTECT_SUBNAME     :=
PK_TLB_FLUSH_MPROTECT_BUILDARCH   := $(CONFIG_PK_TLB_FLUSH_MPROTECT_BUILDARCH)
PK_TLB_FLUSH_MPROTECT_DIR         := $(srctree)/package/TLB/BiscuitOS-TLB-FLUSH-MPROTECT
PK_TLB_FLUSH_MPROTECT_PATCH       := $(PK_TLB_FLUSH_MPROTECT_DIR)/patch
PK_TLB_FLUSH_MPROTECT_BSBIT       := BiscuitOS-TLB-FLUSH-MPROTECT-$(PK_TLB_FLUSH_MPROTECT_VERSION).bsbit
PK_TLB_FLUSH_MPROTECT_OUT         :=
PK_TLB_FLUSH_MPROTECT_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_TLB_FLUSH_MPROTECT_TAR         := $(CONFIG_PK_TLB_FLUSH_MPROTECT_TAR)
PK_TLB_FLUSH_MPROTECT_PACKAGE     :=
PK_TLB_FLUSH_MPROTECT_CONFIG      := $(CONFIG_PK_TLB_FLUSH_MPROTECT_CONFIG)
SUB_TARGET                  += $(PK_TLB_FLUSH_MPROTECT_DIR)/build

PK_TLB_FLUSH_MPROTECT_sh = $(CONFIG_SHELL) $(PK_TLB_FLUSH_MPROTECT_SCRIPTS) \
                 $1X \
                 $(PK_TLB_FLUSH_MPROTECT_NAME)X \
                 $(PK_TLB_FLUSH_MPROTECT_VERSION)X \
                 $(PK_TLB_FLUSH_MPROTECT_SITE)X \
                 $(PK_TLB_FLUSH_MPROTECT_GITHUB)X \
                 $(PK_TLB_FLUSH_MPROTECT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_TLB_FLUSH_MPROTECT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_TLB_FLUSH_MPROTECT_SUBNAME)X \
                 $(CONFIG_PK_TLB_FLUSH_MPROTECT_CONFIG)X \
                 $(CONFIG_PK_TLB_FLUSH_MPROTECT_LIBPATH)X \
                 $(CONFIG_PK_TLB_FLUSH_MPROTECT_CPPFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MPROTECT_DPKCONFIG)X \
                 $(CONFIG_PK_TLB_FLUSH_MPROTECT_CFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MPROTECT_LDFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MPROTECT_CXXFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MPROTECT_ASFLAGS)X \
                 $(PK_TLB_FLUSH_MPROTECT_BSBIT)X \
                 $(PK_TLB_FLUSH_MPROTECT_BUILDARCH)X \
		 $(PK_TLB_FLUSH_MPROTECT_CSRCFILE)X \
                 $(CONFIG_PK_TLB_FLUSH_MPROTECT_SRCFILE)X \
                 $(CONFIG_PK_TLB_FLUSH_MPROTECT_CSRCFILE)

$(PK_TLB_FLUSH_MPROTECT_DIR)/build: FORCE
	$(Q)$(call PK_TLB_FLUSH_MPROTECT_sh,$(srctree))
