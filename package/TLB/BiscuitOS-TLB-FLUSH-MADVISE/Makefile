PK_TLB_FLUSH_MADVISE_NAME        := BiscuitOS-TLB-FLUSH-MADVISE
PK_TLB_FLUSH_MADVISE_VERSION     := $(CONFIG_PK_TLB_FLUSH_MADVISE_VERSION)
PK_TLB_FLUSH_MADVISE_SITE        := $(CONFIG_PK_TLB_FLUSH_MADVISE_URL)
PK_TLB_FLUSH_MADVISE_GITHUB      := $(CONFIG_PK_TLB_FLUSH_MADVISE_GITHUB)
PK_TLB_FLUSH_MADVISE_SUBNAME     :=
PK_TLB_FLUSH_MADVISE_BUILDARCH   := $(CONFIG_PK_TLB_FLUSH_MADVISE_BUILDARCH)
PK_TLB_FLUSH_MADVISE_DIR         := $(srctree)/package/TLB/BiscuitOS-TLB-FLUSH-MADVISE
PK_TLB_FLUSH_MADVISE_PATCH       := $(PK_TLB_FLUSH_MADVISE_DIR)/patch
PK_TLB_FLUSH_MADVISE_BSBIT       := BiscuitOS-TLB-FLUSH-MADVISE-$(PK_TLB_FLUSH_MADVISE_VERSION).bsbit
PK_TLB_FLUSH_MADVISE_OUT         :=
PK_TLB_FLUSH_MADVISE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_TLB_FLUSH_MADVISE_TAR         := $(CONFIG_PK_TLB_FLUSH_MADVISE_TAR)
PK_TLB_FLUSH_MADVISE_PACKAGE     :=
PK_TLB_FLUSH_MADVISE_CONFIG      := $(CONFIG_PK_TLB_FLUSH_MADVISE_CONFIG)
SUB_TARGET                  += $(PK_TLB_FLUSH_MADVISE_DIR)/build

PK_TLB_FLUSH_MADVISE_sh = $(CONFIG_SHELL) $(PK_TLB_FLUSH_MADVISE_SCRIPTS) \
                 $1X \
                 $(PK_TLB_FLUSH_MADVISE_NAME)X \
                 $(PK_TLB_FLUSH_MADVISE_VERSION)X \
                 $(PK_TLB_FLUSH_MADVISE_SITE)X \
                 $(PK_TLB_FLUSH_MADVISE_GITHUB)X \
                 $(PK_TLB_FLUSH_MADVISE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_TLB_FLUSH_MADVISE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_TLB_FLUSH_MADVISE_SUBNAME)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVISE_CONFIG)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVISE_LIBPATH)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVISE_CPPFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVISE_DPKCONFIG)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVISE_CFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVISE_LDFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVISE_CXXFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVISE_ASFLAGS)X \
                 $(PK_TLB_FLUSH_MADVISE_BSBIT)X \
                 $(PK_TLB_FLUSH_MADVISE_BUILDARCH)X \
		 $(PK_TLB_FLUSH_MADVISE_CSRCFILE)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVISE_SRCFILE)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVISE_CSRCFILE)

$(PK_TLB_FLUSH_MADVISE_DIR)/build: FORCE
	$(Q)$(call PK_TLB_FLUSH_MADVISE_sh,$(srctree))
