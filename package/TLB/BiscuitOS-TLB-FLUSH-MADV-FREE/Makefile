PK_TLB_FLUSH_MADVFREE_NAME        := BiscuitOS-TLB-FLUSH-MADV-FREE
PK_TLB_FLUSH_MADVFREE_VERSION     := $(CONFIG_PK_TLB_FLUSH_MADVFREE_VERSION)
PK_TLB_FLUSH_MADVFREE_SITE        := $(CONFIG_PK_TLB_FLUSH_MADVFREE_URL)
PK_TLB_FLUSH_MADVFREE_GITHUB      := $(CONFIG_PK_TLB_FLUSH_MADVFREE_GITHUB)
PK_TLB_FLUSH_MADVFREE_SUBNAME     :=
PK_TLB_FLUSH_MADVFREE_BUILDARCH   := $(CONFIG_PK_TLB_FLUSH_MADVFREE_BUILDARCH)
PK_TLB_FLUSH_MADVFREE_DIR         := $(srctree)/package/TLB/BiscuitOS-TLB-FLUSH-MADV-FREE
PK_TLB_FLUSH_MADVFREE_PATCH       := $(PK_TLB_FLUSH_MADVFREE_DIR)/patch
PK_TLB_FLUSH_MADVFREE_BSBIT       := BiscuitOS-TLB-FLUSH-MADV-FREE-$(PK_TLB_FLUSH_MADVFREE_VERSION).bsbit
PK_TLB_FLUSH_MADVFREE_OUT         :=
PK_TLB_FLUSH_MADVFREE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_TLB_FLUSH_MADVFREE_TAR         := $(CONFIG_PK_TLB_FLUSH_MADVFREE_TAR)
PK_TLB_FLUSH_MADVFREE_PACKAGE     :=
PK_TLB_FLUSH_MADVFREE_CONFIG      := $(CONFIG_PK_TLB_FLUSH_MADVFREE_CONFIG)
SUB_TARGET                  += $(PK_TLB_FLUSH_MADVFREE_DIR)/build

PK_TLB_FLUSH_MADVFREE_sh = $(CONFIG_SHELL) $(PK_TLB_FLUSH_MADVFREE_SCRIPTS) \
                 $1X \
                 $(PK_TLB_FLUSH_MADVFREE_NAME)X \
                 $(PK_TLB_FLUSH_MADVFREE_VERSION)X \
                 $(PK_TLB_FLUSH_MADVFREE_SITE)X \
                 $(PK_TLB_FLUSH_MADVFREE_GITHUB)X \
                 $(PK_TLB_FLUSH_MADVFREE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_TLB_FLUSH_MADVFREE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_TLB_FLUSH_MADVFREE_SUBNAME)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVFREE_CONFIG)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVFREE_LIBPATH)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVFREE_CPPFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVFREE_DPKCONFIG)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVFREE_CFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVFREE_LDFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVFREE_CXXFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVFREE_ASFLAGS)X \
                 $(PK_TLB_FLUSH_MADVFREE_BSBIT)X \
                 $(PK_TLB_FLUSH_MADVFREE_BUILDARCH)X \
		 $(PK_TLB_FLUSH_MADVFREE_CSRCFILE)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVFREE_SRCFILE)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVFREE_CSRCFILE)

$(PK_TLB_FLUSH_MADVFREE_DIR)/build: FORCE
	$(Q)$(call PK_TLB_FLUSH_MADVFREE_sh,$(srctree))
