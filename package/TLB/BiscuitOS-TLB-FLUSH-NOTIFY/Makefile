################################################
#
# BiscuitOS-TLB-FLUSH-NOTIFY
#
################################################
PK_TLB_FLUSH_NOTIFY_NAME        := BiscuitOS-TLB-FLUSH-NOTIFY
PK_TLB_FLUSH_NOTIFY_VERSION     := $(CONFIG_PK_TLB_FLUSH_NOTIFY_VERSION)
PK_TLB_FLUSH_NOTIFY_SITE        := $(CONFIG_PK_TLB_FLUSH_NOTIFY_URL)
PK_TLB_FLUSH_NOTIFY_GITHUB      := $(CONFIG_PK_TLB_FLUSH_NOTIFY_GITHUB)
PK_TLB_FLUSH_NOTIFY_SUBNAME     :=
PK_TLB_FLUSH_NOTIFY_BUILDARCH   := $(CONFIG_PK_TLB_FLUSH_NOTIFY_BUILDARCH)
PK_TLB_FLUSH_NOTIFY_DIR         := $(srctree)/package/TLB/BiscuitOS-TLB-FLUSH-NOTIFY
PK_TLB_FLUSH_NOTIFY_PATCH       := $(PK_TLB_FLUSH_NOTIFY_DIR)/patch
PK_TLB_FLUSH_NOTIFY_BSBIT       := BiscuitOS-TLB-FLUSH-NOTIFY-$(PK_TLB_FLUSH_NOTIFY_VERSION).bsbit
PK_TLB_FLUSH_NOTIFY_OUT         :=
PK_TLB_FLUSH_NOTIFY_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_TLB_FLUSH_NOTIFY_TAR         := $(CONFIG_PK_TLB_FLUSH_NOTIFY_TAR)
PK_TLB_FLUSH_NOTIFY_PACKAGE     :=
PK_TLB_FLUSH_NOTIFY_CONFIG      := $(CONFIG_PK_TLB_FLUSH_NOTIFY_CONFIG)
SUB_TARGET                  += $(PK_TLB_FLUSH_NOTIFY_DIR)/build

PK_TLB_FLUSH_NOTIFY_sh = $(CONFIG_SHELL) $(PK_TLB_FLUSH_NOTIFY_SCRIPTS) \
                 $1X \
                 $(PK_TLB_FLUSH_NOTIFY_NAME)X \
                 $(PK_TLB_FLUSH_NOTIFY_VERSION)X \
                 $(PK_TLB_FLUSH_NOTIFY_SITE)X \
                 $(PK_TLB_FLUSH_NOTIFY_GITHUB)X \
                 $(PK_TLB_FLUSH_NOTIFY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_TLB_FLUSH_NOTIFY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_TLB_FLUSH_NOTIFY_SUBNAME)X \
                 $(CONFIG_PK_TLB_FLUSH_NOTIFY_CONFIG)X \
                 $(CONFIG_PK_TLB_FLUSH_NOTIFY_LIBPATH)X \
                 $(CONFIG_PK_TLB_FLUSH_NOTIFY_CPPFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_NOTIFY_DPKCONFIG)X \
                 $(CONFIG_PK_TLB_FLUSH_NOTIFY_CFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_NOTIFY_LDFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_NOTIFY_CXXFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_NOTIFY_ASFLAGS)X \
                 $(PK_TLB_FLUSH_NOTIFY_BSBIT)X \
                 $(PK_TLB_FLUSH_NOTIFY_BUILDARCH)X \
		 $(PK_TLB_FLUSH_NOTIFY_CSRCFILE)X \
                 $(CONFIG_PK_TLB_FLUSH_NOTIFY_SRCFILE)X \
                 $(CONFIG_PK_TLB_FLUSH_NOTIFY_CSRCFILE)

$(PK_TLB_FLUSH_NOTIFY_DIR)/build: FORCE
	$(Q)$(call PK_TLB_FLUSH_NOTIFY_sh,$(srctree))
