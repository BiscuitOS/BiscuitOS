################################################
#
# BiscuitOS-TLB-FLUSH-MADV-PAGEOUT
#
################################################
PK_TLB_FLUSH_MADVPAGEOUT_NAME        := BiscuitOS-TLB-FLUSH-MADV-PAGEOUT
PK_TLB_FLUSH_MADVPAGEOUT_VERSION     := $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_VERSION)
PK_TLB_FLUSH_MADVPAGEOUT_SITE        := $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_URL)
PK_TLB_FLUSH_MADVPAGEOUT_GITHUB      := $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_GITHUB)
PK_TLB_FLUSH_MADVPAGEOUT_SUBNAME     :=
PK_TLB_FLUSH_MADVPAGEOUT_BUILDARCH   := $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_BUILDARCH)
PK_TLB_FLUSH_MADVPAGEOUT_DIR         := $(srctree)/package/TLB/BiscuitOS-TLB-FLUSH-MADV-PAGEOUT
PK_TLB_FLUSH_MADVPAGEOUT_PATCH       := $(PK_TLB_FLUSH_MADVPAGEOUT_DIR)/patch
PK_TLB_FLUSH_MADVPAGEOUT_BSBIT       := BiscuitOS-TLB-FLUSH-MADV-PAGEOUT-$(PK_TLB_FLUSH_MADVPAGEOUT_VERSION).bsbit
PK_TLB_FLUSH_MADVPAGEOUT_OUT         :=
PK_TLB_FLUSH_MADVPAGEOUT_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_TLB_FLUSH_MADVPAGEOUT_TAR         := $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_TAR)
PK_TLB_FLUSH_MADVPAGEOUT_PACKAGE     :=
PK_TLB_FLUSH_MADVPAGEOUT_CONFIG      := $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_CONFIG)
SUB_TARGET                  += $(PK_TLB_FLUSH_MADVPAGEOUT_DIR)/build

PK_TLB_FLUSH_MADVPAGEOUT_sh = $(CONFIG_SHELL) $(PK_TLB_FLUSH_MADVPAGEOUT_SCRIPTS) \
                 $1X \
                 $(PK_TLB_FLUSH_MADVPAGEOUT_NAME)X \
                 $(PK_TLB_FLUSH_MADVPAGEOUT_VERSION)X \
                 $(PK_TLB_FLUSH_MADVPAGEOUT_SITE)X \
                 $(PK_TLB_FLUSH_MADVPAGEOUT_GITHUB)X \
                 $(PK_TLB_FLUSH_MADVPAGEOUT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_TLB_FLUSH_MADVPAGEOUT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_TLB_FLUSH_MADVPAGEOUT_SUBNAME)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_CONFIG)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_LIBPATH)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_CPPFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_DPKCONFIG)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_CFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_LDFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_CXXFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_ASFLAGS)X \
                 $(PK_TLB_FLUSH_MADVPAGEOUT_BSBIT)X \
                 $(PK_TLB_FLUSH_MADVPAGEOUT_BUILDARCH)X \
		 $(PK_TLB_FLUSH_MADVPAGEOUT_CSRCFILE)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_SRCFILE)X \
                 $(CONFIG_PK_TLB_FLUSH_MADVPAGEOUT_CSRCFILE)

$(PK_TLB_FLUSH_MADVPAGEOUT_DIR)/build: FORCE
	$(Q)$(call PK_TLB_FLUSH_MADVPAGEOUT_sh,$(srctree))
