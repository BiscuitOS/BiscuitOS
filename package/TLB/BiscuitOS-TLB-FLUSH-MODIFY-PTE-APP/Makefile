PK_TLB_FLUSH_MDPTE_APP_NAME        := BiscuitOS-TLB-FLUSH-MODIFY-PTE-APP
PK_TLB_FLUSH_MDPTE_APP_VERSION     := $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_VERSION)
PK_TLB_FLUSH_MDPTE_APP_SITE        := $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_URL)
PK_TLB_FLUSH_MDPTE_APP_GITHUB      := $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_GITHUB)
PK_TLB_FLUSH_MDPTE_APP_SUBNAME     :=
PK_TLB_FLUSH_MDPTE_APP_BUILDARCH   := $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_BUILDARCH)
PK_TLB_FLUSH_MDPTE_APP_DIR         := $(srctree)/package/TLB/BiscuitOS-TLB-FLUSH-MODIFY-PTE-APP
PK_TLB_FLUSH_MDPTE_APP_PATCH       := $(PK_TLB_FLUSH_MDPTE_APP_DIR)/patch
PK_TLB_FLUSH_MDPTE_APP_BSBIT       := BiscuitOS-TLB-FLUSH-MODIFY-PTE-APP-$(PK_TLB_FLUSH_MDPTE_APP_VERSION).bsbit
PK_TLB_FLUSH_MDPTE_APP_OUT         :=
PK_TLB_FLUSH_MDPTE_APP_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_TLB_FLUSH_MDPTE_APP_TAR         := $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_TAR)
PK_TLB_FLUSH_MDPTE_APP_PACKAGE     :=
PK_TLB_FLUSH_MDPTE_APP_CONFIG      := $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_CONFIG)
SUB_TARGET                  += $(PK_TLB_FLUSH_MDPTE_APP_DIR)/build

PK_TLB_FLUSH_MDPTE_APP_sh = $(CONFIG_SHELL) $(PK_TLB_FLUSH_MDPTE_APP_SCRIPTS) \
                 $1X \
                 $(PK_TLB_FLUSH_MDPTE_APP_NAME)X \
                 $(PK_TLB_FLUSH_MDPTE_APP_VERSION)X \
                 $(PK_TLB_FLUSH_MDPTE_APP_SITE)X \
                 $(PK_TLB_FLUSH_MDPTE_APP_GITHUB)X \
                 $(PK_TLB_FLUSH_MDPTE_APP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_TLB_FLUSH_MDPTE_APP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_TLB_FLUSH_MDPTE_APP_SUBNAME)X \
                 $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_CONFIG)X \
                 $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_LIBPATH)X \
                 $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_CPPFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_DPKCONFIG)X \
                 $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_CFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_LDFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_CXXFLAGS)X \
                 $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_ASFLAGS)X \
                 $(PK_TLB_FLUSH_MDPTE_APP_BSBIT)X \
                 $(PK_TLB_FLUSH_MDPTE_APP_BUILDARCH)X \
		 $(PK_TLB_FLUSH_MDPTE_APP_CSRCFILE)X \
                 $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_SRCFILE)X \
                 $(CONFIG_PK_TLB_FLUSH_MDPTE_APP_CSRCFILE)

$(PK_TLB_FLUSH_MDPTE_APP_DIR)/build: FORCE
	$(Q)$(call PK_TLB_FLUSH_MDPTE_APP_sh,$(srctree))
