PK_BUDDY_GFP_HIGHMEM_NAME        := BiscuitOS-BUDDY-GFP-HIGHMEM
PK_BUDDY_GFP_HIGHMEM_VERSION     := $(CONFIG_PK_BUDDY_GFP_HIGHMEM_VERSION)
PK_BUDDY_GFP_HIGHMEM_SITE        := $(CONFIG_PK_BUDDY_GFP_HIGHMEM_URL)
PK_BUDDY_GFP_HIGHMEM_GITHUB      := $(CONFIG_PK_BUDDY_GFP_HIGHMEM_GITHUB)
PK_BUDDY_GFP_HIGHMEM_SUBNAME     :=
PK_BUDDY_GFP_HIGHMEM_BUILDARCH   := $(CONFIG_PK_BUDDY_GFP_HIGHMEM_BUILDARCH)
PK_BUDDY_GFP_HIGHMEM_DIR         := $(srctree)/package/BUDDY-ALLOCATOR/BiscuitOS-BUDDY-GFP-HIGHMEM
PK_BUDDY_GFP_HIGHMEM_PATCH       := $(PK_BUDDY_GFP_HIGHMEM_DIR)/patch
PK_BUDDY_GFP_HIGHMEM_BSBIT       := BiscuitOS-BUDDY-GFP-HIGHMEM-$(PK_BUDDY_GFP_HIGHMEM_VERSION).bsbit
PK_BUDDY_GFP_HIGHMEM_OUT         :=
PK_BUDDY_GFP_HIGHMEM_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_BUDDY_GFP_HIGHMEM_TAR         := $(CONFIG_PK_BUDDY_GFP_HIGHMEM_TAR)
PK_BUDDY_GFP_HIGHMEM_PACKAGE     :=
PK_BUDDY_GFP_HIGHMEM_CONFIG      := $(CONFIG_PK_BUDDY_GFP_HIGHMEM_CONFIG)
SUB_TARGET                  += $(PK_BUDDY_GFP_HIGHMEM_DIR)/build

PK_BUDDY_GFP_HIGHMEM_sh = $(CONFIG_SHELL) $(PK_BUDDY_GFP_HIGHMEM_SCRIPTS) \
                 $1X \
                 $(PK_BUDDY_GFP_HIGHMEM_NAME)X \
                 $(PK_BUDDY_GFP_HIGHMEM_VERSION)X \
                 $(PK_BUDDY_GFP_HIGHMEM_SITE)X \
                 $(PK_BUDDY_GFP_HIGHMEM_GITHUB)X \
                 $(PK_BUDDY_GFP_HIGHMEM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BUDDY_GFP_HIGHMEM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BUDDY_GFP_HIGHMEM_SUBNAME)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHMEM_CONFIG)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHMEM_LIBPATH)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHMEM_CPPFLAGS)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHMEM_DPKCONFIG)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHMEM_CFLAGS)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHMEM_LDFLAGS)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHMEM_CXXFLAGS)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHMEM_ASFLAGS)X \
                 $(PK_BUDDY_GFP_HIGHMEM_BSBIT)X \
                 $(PK_BUDDY_GFP_HIGHMEM_BUILDARCH)X \
		 $(PK_BUDDY_GFP_HIGHMEM_CSRCFILE)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHMEM_SRCFILE)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHMEM_CSRCFILE)

$(PK_BUDDY_GFP_HIGHMEM_DIR)/build: FORCE
	$(Q)$(call PK_BUDDY_GFP_HIGHMEM_sh,$(srctree))
