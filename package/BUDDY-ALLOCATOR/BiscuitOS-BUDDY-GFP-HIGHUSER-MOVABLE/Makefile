PK_BUDDY_GFP_HIGHUSER_MOVABLE_NAME        := BiscuitOS-BUDDY-GFP-HIGHUSER-MOVABLE
PK_BUDDY_GFP_HIGHUSER_MOVABLE_VERSION     := $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_VERSION)
PK_BUDDY_GFP_HIGHUSER_MOVABLE_SITE        := $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_URL)
PK_BUDDY_GFP_HIGHUSER_MOVABLE_GITHUB      := $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_GITHUB)
PK_BUDDY_GFP_HIGHUSER_MOVABLE_SUBNAME     :=
PK_BUDDY_GFP_HIGHUSER_MOVABLE_BUILDARCH   := $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_BUILDARCH)
PK_BUDDY_GFP_HIGHUSER_MOVABLE_DIR         := $(srctree)/package/BUDDY-ALLOCATOR/BiscuitOS-BUDDY-GFP-HIGHUSER-MOVABLE
PK_BUDDY_GFP_HIGHUSER_MOVABLE_PATCH       := $(PK_BUDDY_GFP_HIGHUSER_MOVABLE_DIR)/patch
PK_BUDDY_GFP_HIGHUSER_MOVABLE_BSBIT       := BiscuitOS-BUDDY-GFP-HIGHUSER-MOVABLE-$(PK_BUDDY_GFP_HIGHUSER_MOVABLE_VERSION).bsbit
PK_BUDDY_GFP_HIGHUSER_MOVABLE_OUT         :=
PK_BUDDY_GFP_HIGHUSER_MOVABLE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_BUDDY_GFP_HIGHUSER_MOVABLE_TAR         := $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_TAR)
PK_BUDDY_GFP_HIGHUSER_MOVABLE_PACKAGE     :=
PK_BUDDY_GFP_HIGHUSER_MOVABLE_CONFIG      := $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_CONFIG)
SUB_TARGET                  += $(PK_BUDDY_GFP_HIGHUSER_MOVABLE_DIR)/build

PK_BUDDY_GFP_HIGHUSER_MOVABLE_sh = $(CONFIG_SHELL) $(PK_BUDDY_GFP_HIGHUSER_MOVABLE_SCRIPTS) \
                 $1X \
                 $(PK_BUDDY_GFP_HIGHUSER_MOVABLE_NAME)X \
                 $(PK_BUDDY_GFP_HIGHUSER_MOVABLE_VERSION)X \
                 $(PK_BUDDY_GFP_HIGHUSER_MOVABLE_SITE)X \
                 $(PK_BUDDY_GFP_HIGHUSER_MOVABLE_GITHUB)X \
                 $(PK_BUDDY_GFP_HIGHUSER_MOVABLE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BUDDY_GFP_HIGHUSER_MOVABLE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BUDDY_GFP_HIGHUSER_MOVABLE_SUBNAME)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_CONFIG)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_LIBPATH)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_CPPFLAGS)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_DPKCONFIG)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_CFLAGS)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_LDFLAGS)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_CXXFLAGS)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_ASFLAGS)X \
                 $(PK_BUDDY_GFP_HIGHUSER_MOVABLE_BSBIT)X \
                 $(PK_BUDDY_GFP_HIGHUSER_MOVABLE_BUILDARCH)X \
		 $(PK_BUDDY_GFP_HIGHUSER_MOVABLE_CSRCFILE)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_SRCFILE)X \
                 $(CONFIG_PK_BUDDY_GFP_HIGHUSER_MOVABLE_CSRCFILE)

$(PK_BUDDY_GFP_HIGHUSER_MOVABLE_DIR)/build: FORCE
	$(Q)$(call PK_BUDDY_GFP_HIGHUSER_MOVABLE_sh,$(srctree))
