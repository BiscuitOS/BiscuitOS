PK_ZONE_ALLOC_ZONE_DMA_NAME        := BiscuitOS-ZONE-alloc-page-ZONE_DMA
PK_ZONE_ALLOC_ZONE_DMA_VERSION     := $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_VERSION)
PK_ZONE_ALLOC_ZONE_DMA_SITE        := $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_URL)
PK_ZONE_ALLOC_ZONE_DMA_GITHUB      := $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_GITHUB)
PK_ZONE_ALLOC_ZONE_DMA_SUBNAME     :=
PK_ZONE_ALLOC_ZONE_DMA_BUILDARCH   := $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_BUILDARCH)
PK_ZONE_ALLOC_ZONE_DMA_DIR         := $(srctree)/package/ZONE/BiscuitOS-ZONE-alloc-page-ZONE_DMA
PK_ZONE_ALLOC_ZONE_DMA_PATCH       := $(PK_ZONE_ALLOC_ZONE_DMA_DIR)/patch
PK_ZONE_ALLOC_ZONE_DMA_BSBIT       := BiscuitOS-ZONE-alloc-page-ZONE_DMA-$(PK_ZONE_ALLOC_ZONE_DMA_VERSION).bsbit
PK_ZONE_ALLOC_ZONE_DMA_OUT         :=
PK_ZONE_ALLOC_ZONE_DMA_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_ZONE_ALLOC_ZONE_DMA_TAR         := $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_TAR)
PK_ZONE_ALLOC_ZONE_DMA_PACKAGE     :=
PK_ZONE_ALLOC_ZONE_DMA_CONFIG      := $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_CONFIG)
SUB_TARGET                  += $(PK_ZONE_ALLOC_ZONE_DMA_DIR)/build

PK_ZONE_ALLOC_ZONE_DMA_sh = $(CONFIG_SHELL) $(PK_ZONE_ALLOC_ZONE_DMA_SCRIPTS) \
                 $1X \
                 $(PK_ZONE_ALLOC_ZONE_DMA_NAME)X \
                 $(PK_ZONE_ALLOC_ZONE_DMA_VERSION)X \
                 $(PK_ZONE_ALLOC_ZONE_DMA_SITE)X \
                 $(PK_ZONE_ALLOC_ZONE_DMA_GITHUB)X \
                 $(PK_ZONE_ALLOC_ZONE_DMA_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_ZONE_ALLOC_ZONE_DMA_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_ZONE_ALLOC_ZONE_DMA_SUBNAME)X \
                 $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_CONFIG)X \
                 $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_LIBPATH)X \
                 $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_CPPFLAGS)X \
                 $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_DPKCONFIG)X \
                 $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_CFLAGS)X \
                 $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_LDFLAGS)X \
                 $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_CXXFLAGS)X \
                 $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_ASFLAGS)X \
                 $(PK_ZONE_ALLOC_ZONE_DMA_BSBIT)X \
                 $(PK_ZONE_ALLOC_ZONE_DMA_BUILDARCH)X \
		 $(PK_ZONE_ALLOC_ZONE_DMA_CSRCFILE)X \
                 $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_SRCFILE)X \
                 $(CONFIG_PK_ZONE_ALLOC_ZONE_DMA_CSRCFILE)

$(PK_ZONE_ALLOC_ZONE_DMA_DIR)/build: FORCE
	$(Q)$(call PK_ZONE_ALLOC_ZONE_DMA_sh,$(srctree))
