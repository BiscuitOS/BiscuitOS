PK_MEMBLOCK_MERGE_ADJACENT_NAME        := BiscuitOS-MEMBLOCK-MERGE-Adjacent
PK_MEMBLOCK_MERGE_ADJACENT_VERSION     := $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_VERSION)
PK_MEMBLOCK_MERGE_ADJACENT_SITE        := $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_URL)
PK_MEMBLOCK_MERGE_ADJACENT_GITHUB      := $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_GITHUB)
PK_MEMBLOCK_MERGE_ADJACENT_SUBNAME     :=
PK_MEMBLOCK_MERGE_ADJACENT_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_BUILDARCH)
PK_MEMBLOCK_MERGE_ADJACENT_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-MERGE-Adjacent
PK_MEMBLOCK_MERGE_ADJACENT_PATCH       := $(PK_MEMBLOCK_MERGE_ADJACENT_DIR)/patch
PK_MEMBLOCK_MERGE_ADJACENT_BSBIT       := BiscuitOS-MEMBLOCK-MERGE-Adjacent-$(PK_MEMBLOCK_MERGE_ADJACENT_VERSION).bsbit
PK_MEMBLOCK_MERGE_ADJACENT_OUT         :=
PK_MEMBLOCK_MERGE_ADJACENT_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MERGE_ADJACENT_TAR         := $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_TAR)
PK_MEMBLOCK_MERGE_ADJACENT_PACKAGE     :=
PK_MEMBLOCK_MERGE_ADJACENT_CONFIG      := $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MERGE_ADJACENT_DIR)/build

PK_MEMBLOCK_MERGE_ADJACENT_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MERGE_ADJACENT_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_NAME)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_VERSION)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_SITE)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_GITHUB)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_ASFLAGS)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_BSBIT)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_BUILDARCH)X \
		 $(PK_MEMBLOCK_MERGE_ADJACENT_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_CSRCFILE)

$(PK_MEMBLOCK_MERGE_ADJACENT_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MERGE_ADJACENT_sh,$(srctree))
