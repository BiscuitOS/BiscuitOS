PK_MEMBLOCK_NOMAP_IOREMAP_NAME        := BiscuitOS-MEMBLOCK-NOMAP-IOREMAP
PK_MEMBLOCK_NOMAP_IOREMAP_VERSION     := $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_VERSION)
PK_MEMBLOCK_NOMAP_IOREMAP_SITE        := $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_URL)
PK_MEMBLOCK_NOMAP_IOREMAP_GITHUB      := $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_GITHUB)
PK_MEMBLOCK_NOMAP_IOREMAP_SUBNAME     :=
PK_MEMBLOCK_NOMAP_IOREMAP_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_BUILDARCH)
PK_MEMBLOCK_NOMAP_IOREMAP_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-NOMAP-IOREMAP
PK_MEMBLOCK_NOMAP_IOREMAP_PATCH       := $(PK_MEMBLOCK_NOMAP_IOREMAP_DIR)/patch
PK_MEMBLOCK_NOMAP_IOREMAP_BSBIT       := BiscuitOS-MEMBLOCK-NOMAP-IOREMAP-$(PK_MEMBLOCK_NOMAP_IOREMAP_VERSION).bsbit
PK_MEMBLOCK_NOMAP_IOREMAP_OUT         :=
PK_MEMBLOCK_NOMAP_IOREMAP_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_MEMBLOCK_NOMAP_IOREMAP_TAR         := $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_TAR)
PK_MEMBLOCK_NOMAP_IOREMAP_PACKAGE     :=
PK_MEMBLOCK_NOMAP_IOREMAP_CONFIG      := $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_NOMAP_IOREMAP_DIR)/build

PK_MEMBLOCK_NOMAP_IOREMAP_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_NOMAP_IOREMAP_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_NOMAP_IOREMAP_NAME)X \
                 $(PK_MEMBLOCK_NOMAP_IOREMAP_VERSION)X \
                 $(PK_MEMBLOCK_NOMAP_IOREMAP_SITE)X \
                 $(PK_MEMBLOCK_NOMAP_IOREMAP_GITHUB)X \
                 $(PK_MEMBLOCK_NOMAP_IOREMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_NOMAP_IOREMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_NOMAP_IOREMAP_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_ASFLAGS)X \
                 $(PK_MEMBLOCK_NOMAP_IOREMAP_BSBIT)X \
                 $(PK_MEMBLOCK_NOMAP_IOREMAP_BUILDARCH)X \
		 $(PK_MEMBLOCK_NOMAP_IOREMAP_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_IOREMAP_CSRCFILE)

$(PK_MEMBLOCK_NOMAP_IOREMAP_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_NOMAP_IOREMAP_sh,$(srctree))
