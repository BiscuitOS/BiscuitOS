################################################
PK_MEMBLOCK_MEMBLOCK_REMOVE_NAME        := BiscuitOS-MEMBLOCK-memblock_remove
PK_MEMBLOCK_MEMBLOCK_REMOVE_VERSION     := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_VERSION)
PK_MEMBLOCK_MEMBLOCK_REMOVE_SITE        := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_URL)
PK_MEMBLOCK_MEMBLOCK_REMOVE_GITHUB      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_GITHUB)
PK_MEMBLOCK_MEMBLOCK_REMOVE_SUBNAME     :=
PK_MEMBLOCK_MEMBLOCK_REMOVE_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_BUILDARCH)
PK_MEMBLOCK_MEMBLOCK_REMOVE_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-memblock_remove
PK_MEMBLOCK_MEMBLOCK_REMOVE_PATCH       := $(PK_MEMBLOCK_MEMBLOCK_REMOVE_DIR)/patch
PK_MEMBLOCK_MEMBLOCK_REMOVE_BSBIT       := BiscuitOS-MEMBLOCK-memblock_remove-$(PK_MEMBLOCK_MEMBLOCK_REMOVE_VERSION).bsbit
PK_MEMBLOCK_MEMBLOCK_REMOVE_OUT         :=
PK_MEMBLOCK_MEMBLOCK_REMOVE_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MEMBLOCK_REMOVE_TAR         := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_TAR)
PK_MEMBLOCK_MEMBLOCK_REMOVE_PACKAGE     :=
PK_MEMBLOCK_MEMBLOCK_REMOVE_CONFIG      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MEMBLOCK_REMOVE_DIR)/build

PK_MEMBLOCK_MEMBLOCK_REMOVE_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MEMBLOCK_REMOVE_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MEMBLOCK_REMOVE_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_REMOVE_VERSION)X \
                 $(PK_MEMBLOCK_MEMBLOCK_REMOVE_SITE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_REMOVE_GITHUB)X \
                 $(PK_MEMBLOCK_MEMBLOCK_REMOVE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_REMOVE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_REMOVE_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_ASFLAGS)X \
                 $(PK_MEMBLOCK_MEMBLOCK_REMOVE_BSBIT)X \
                 $(PK_MEMBLOCK_MEMBLOCK_REMOVE_BUILDARCH)X \
		 $(PK_MEMBLOCK_MEMBLOCK_REMOVE_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_REMOVE_CSRCFILE)

$(PK_MEMBLOCK_MEMBLOCK_REMOVE_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MEMBLOCK_REMOVE_sh,$(srctree))
