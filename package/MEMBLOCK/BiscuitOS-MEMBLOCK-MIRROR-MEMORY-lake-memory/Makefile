PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_NAME        := BiscuitOS-MEMBLOCK-MIRROR-MEMORY-lake-memory
PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_VERSION     := $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_VERSION)
PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_SITE        := $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_URL)
PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_GITHUB      := $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_GITHUB)
PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_SUBNAME     :=
PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_BUILDARCH)
PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-MIRROR-MEMORY-lake-memory
PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_PATCH       := $(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_DIR)/patch
PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_BSBIT       := BiscuitOS-MEMBLOCK-MIRROR-MEMORY-lake-memory-$(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_VERSION).bsbit
PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_OUT         :=
PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_TAR         := $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_TAR)
PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_PACKAGE     :=
PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_CONFIG      := $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_DIR)/build

PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_NAME)X \
                 $(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_VERSION)X \
                 $(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_SITE)X \
                 $(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_GITHUB)X \
                 $(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_ASFLAGS)X \
                 $(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_BSBIT)X \
                 $(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_BUILDARCH)X \
		 $(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_CSRCFILE)

$(PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MIRROR_MEMORY_LAKE_MEM_sh,$(srctree))
