PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_SITE        := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_URL)
PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_GITHUB      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_GITHUB)
PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_SUBNAME     :=
PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_BUILDARCH)
PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-memblock_is_reserved
PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_PATCH       := $(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_DIR)/patch
PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_BSBIT       := BiscuitOS-MEMBLOCK-memblock_is_reserved-$(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_VERSION).bsbit
PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_OUT         :=
PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_TAR         := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_TAR)
PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_PACKAGE     :=
PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_CONFIG      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_DIR)/build

PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_VERSION)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_SITE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_GITHUB)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_ASFLAGS)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_BSBIT)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_BUILDARCH)X \
		 $(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_CSRCFILE)

$(PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MEMBLOCK_IS_RESERVED_sh,$(srctree))
