PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_NAME        := BiscuitOS-MEMBLOCK-for_each_free_mem_range_reverse
PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_VERSION     := $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_VERSION)
PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_SITE        := $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_URL)
PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_GITHUB      := $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_GITHUB)
PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_SUBNAME     :=
PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_BUILDARCH)
PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-for_each_free_mem_range_reverse
PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_PATCH       := $(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_DIR)/patch
PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_BSBIT       := BiscuitOS-MEMBLOCK-for_each_free_mem_range_reverse-$(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_VERSION).bsbit
PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_OUT         :=
PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_TAR         := $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_TAR)
PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_PACKAGE     :=
PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_CONFIG      := $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_DIR)/build

PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_NAME)X \
                 $(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_VERSION)X \
                 $(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_SITE)X \
                 $(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_GITHUB)X \
                 $(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_ASFLAGS)X \
                 $(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_BSBIT)X \
                 $(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_BUILDARCH)X \
		 $(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_CSRCFILE)

$(PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_FOR_EACH_FREE_MEM_RANGE_REVERSE_sh,$(srctree))
