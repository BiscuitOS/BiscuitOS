PK_MEMBLOCK_MERGE_INTERSECTION_NAME        := BiscuitOS-MEMBLOCK-MERGE-Intersection
PK_MEMBLOCK_MERGE_INTERSECTION_VERSION     := $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_VERSION)
PK_MEMBLOCK_MERGE_INTERSECTION_SITE        := $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_URL)
PK_MEMBLOCK_MERGE_INTERSECTION_GITHUB      := $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_GITHUB)
PK_MEMBLOCK_MERGE_INTERSECTION_SUBNAME     :=
PK_MEMBLOCK_MERGE_INTERSECTION_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_BUILDARCH)
PK_MEMBLOCK_MERGE_INTERSECTION_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-MERGE-Intersection
PK_MEMBLOCK_MERGE_INTERSECTION_PATCH       := $(PK_MEMBLOCK_MERGE_INTERSECTION_DIR)/patch
PK_MEMBLOCK_MERGE_INTERSECTION_BSBIT       := BiscuitOS-MEMBLOCK-MERGE-Intersection-$(PK_MEMBLOCK_MERGE_INTERSECTION_VERSION).bsbit
PK_MEMBLOCK_MERGE_INTERSECTION_OUT         :=
PK_MEMBLOCK_MERGE_INTERSECTION_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MERGE_INTERSECTION_TAR         := $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_TAR)
PK_MEMBLOCK_MERGE_INTERSECTION_PACKAGE     :=
PK_MEMBLOCK_MERGE_INTERSECTION_CONFIG      := $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MERGE_INTERSECTION_DIR)/build

PK_MEMBLOCK_MERGE_INTERSECTION_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MERGE_INTERSECTION_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MERGE_INTERSECTION_NAME)X \
                 $(PK_MEMBLOCK_MERGE_INTERSECTION_VERSION)X \
                 $(PK_MEMBLOCK_MERGE_INTERSECTION_SITE)X \
                 $(PK_MEMBLOCK_MERGE_INTERSECTION_GITHUB)X \
                 $(PK_MEMBLOCK_MERGE_INTERSECTION_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MERGE_INTERSECTION_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MERGE_INTERSECTION_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_ASFLAGS)X \
                 $(PK_MEMBLOCK_MERGE_INTERSECTION_BSBIT)X \
                 $(PK_MEMBLOCK_MERGE_INTERSECTION_BUILDARCH)X \
		 $(PK_MEMBLOCK_MERGE_INTERSECTION_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_INTERSECTION_CSRCFILE)

$(PK_MEMBLOCK_MERGE_INTERSECTION_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MERGE_INTERSECTION_sh,$(srctree))
