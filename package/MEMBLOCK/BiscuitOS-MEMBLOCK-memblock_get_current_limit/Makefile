PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_NAME        := BiscuitOS-MEMBLOCK-memblock_get_current_limit
PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_VERSION     := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_VERSION)
PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_SITE        := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_URL)
PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_GITHUB      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_GITHUB)
PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_SUBNAME     :=
PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_BUILDARCH)
PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-memblock_get_current_limit
PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_PATCH       := $(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_DIR)/patch
PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_BSBIT       := BiscuitOS-MEMBLOCK-memblock_get_current_limit-$(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_VERSION).bsbit
PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_OUT         :=
PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_TAR         := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_TAR)
PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_PACKAGE     :=
PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_CONFIG      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_DIR)/build

PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_VERSION)X \
                 $(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_SITE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_GITHUB)X \
                 $(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_ASFLAGS)X \
                 $(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_BSBIT)X \
                 $(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_BUILDARCH)X \
		 $(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_CSRCFILE)

$(PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MEMBLOCK_GET_CURRENT_LIMIT_sh,$(srctree))
