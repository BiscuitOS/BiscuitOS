PK_MEMBLOCK_DRIVER_MANAGED_NAME        := BiscuitOS-MEMBLOCK-DRIVER-MANAGED
PK_MEMBLOCK_DRIVER_MANAGED_VERSION     := $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_VERSION)
PK_MEMBLOCK_DRIVER_MANAGED_SITE        := $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_URL)
PK_MEMBLOCK_DRIVER_MANAGED_GITHUB      := $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_GITHUB)
PK_MEMBLOCK_DRIVER_MANAGED_SUBNAME     :=
PK_MEMBLOCK_DRIVER_MANAGED_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_BUILDARCH)
PK_MEMBLOCK_DRIVER_MANAGED_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-DRIVER-MANAGED
PK_MEMBLOCK_DRIVER_MANAGED_PATCH       := $(PK_MEMBLOCK_DRIVER_MANAGED_DIR)/patch
PK_MEMBLOCK_DRIVER_MANAGED_BSBIT       := BiscuitOS-MEMBLOCK-DRIVER-MANAGED-$(PK_MEMBLOCK_DRIVER_MANAGED_VERSION).bsbit
PK_MEMBLOCK_DRIVER_MANAGED_OUT         :=
PK_MEMBLOCK_DRIVER_MANAGED_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_MEMBLOCK_DRIVER_MANAGED_TAR         := $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_TAR)
PK_MEMBLOCK_DRIVER_MANAGED_PACKAGE     :=
PK_MEMBLOCK_DRIVER_MANAGED_CONFIG      := $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_DRIVER_MANAGED_DIR)/build

PK_MEMBLOCK_DRIVER_MANAGED_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_DRIVER_MANAGED_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_DRIVER_MANAGED_NAME)X \
                 $(PK_MEMBLOCK_DRIVER_MANAGED_VERSION)X \
                 $(PK_MEMBLOCK_DRIVER_MANAGED_SITE)X \
                 $(PK_MEMBLOCK_DRIVER_MANAGED_GITHUB)X \
                 $(PK_MEMBLOCK_DRIVER_MANAGED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_DRIVER_MANAGED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_DRIVER_MANAGED_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_ASFLAGS)X \
                 $(PK_MEMBLOCK_DRIVER_MANAGED_BSBIT)X \
                 $(PK_MEMBLOCK_DRIVER_MANAGED_BUILDARCH)X \
		 $(PK_MEMBLOCK_DRIVER_MANAGED_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_DRIVER_MANAGED_CSRCFILE)

$(PK_MEMBLOCK_DRIVER_MANAGED_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_DRIVER_MANAGED_sh,$(srctree))
