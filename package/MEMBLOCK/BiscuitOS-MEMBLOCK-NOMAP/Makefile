################################################
#
# BiscuitOS-MEMBLOCK-NOMAP
#
################################################
PK_MEMBLOCK_NOMAP_NAME        := BiscuitOS-MEMBLOCK-NOMAP
PK_MEMBLOCK_NOMAP_VERSION     := $(CONFIG_PK_MEMBLOCK_NOMAP_VERSION)
PK_MEMBLOCK_NOMAP_SITE        := $(CONFIG_PK_MEMBLOCK_NOMAP_URL)
PK_MEMBLOCK_NOMAP_GITHUB      := $(CONFIG_PK_MEMBLOCK_NOMAP_GITHUB)
PK_MEMBLOCK_NOMAP_SUBNAME     :=
PK_MEMBLOCK_NOMAP_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_NOMAP_BUILDARCH)
PK_MEMBLOCK_NOMAP_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-NOMAP
PK_MEMBLOCK_NOMAP_PATCH       := $(PK_MEMBLOCK_NOMAP_DIR)/patch
PK_MEMBLOCK_NOMAP_BSBIT       := BiscuitOS-MEMBLOCK-NOMAP-$(PK_MEMBLOCK_NOMAP_VERSION).bsbit
PK_MEMBLOCK_NOMAP_OUT         :=
PK_MEMBLOCK_NOMAP_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_MEMBLOCK_NOMAP_TAR         := $(CONFIG_PK_MEMBLOCK_NOMAP_TAR)
PK_MEMBLOCK_NOMAP_PACKAGE     :=
PK_MEMBLOCK_NOMAP_CONFIG      := $(CONFIG_PK_MEMBLOCK_NOMAP_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_NOMAP_DIR)/build

PK_MEMBLOCK_NOMAP_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_NOMAP_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_NOMAP_NAME)X \
                 $(PK_MEMBLOCK_NOMAP_VERSION)X \
                 $(PK_MEMBLOCK_NOMAP_SITE)X \
                 $(PK_MEMBLOCK_NOMAP_GITHUB)X \
                 $(PK_MEMBLOCK_NOMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_NOMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_NOMAP_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_ASFLAGS)X \
                 $(PK_MEMBLOCK_NOMAP_BSBIT)X \
                 $(PK_MEMBLOCK_NOMAP_BUILDARCH)X \
		 $(PK_MEMBLOCK_NOMAP_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_NOMAP_CSRCFILE)

$(PK_MEMBLOCK_NOMAP_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_NOMAP_sh,$(srctree))
