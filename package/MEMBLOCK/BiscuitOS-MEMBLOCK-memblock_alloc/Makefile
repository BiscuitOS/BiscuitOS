PK_MEMBLOCK_MEMBLOCK_ALLOC_NAME        := BiscuitOS-MEMBLOCK-memblock_alloc
PK_MEMBLOCK_MEMBLOCK_ALLOC_VERSION     := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_VERSION)
PK_MEMBLOCK_MEMBLOCK_ALLOC_SITE        := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_URL)
PK_MEMBLOCK_MEMBLOCK_ALLOC_GITHUB      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_GITHUB)
PK_MEMBLOCK_MEMBLOCK_ALLOC_SUBNAME     :=
PK_MEMBLOCK_MEMBLOCK_ALLOC_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_BUILDARCH)
PK_MEMBLOCK_MEMBLOCK_ALLOC_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-memblock_alloc
PK_MEMBLOCK_MEMBLOCK_ALLOC_PATCH       := $(PK_MEMBLOCK_MEMBLOCK_ALLOC_DIR)/patch
PK_MEMBLOCK_MEMBLOCK_ALLOC_BSBIT       := BiscuitOS-MEMBLOCK-memblock_alloc-$(PK_MEMBLOCK_MEMBLOCK_ALLOC_VERSION).bsbit
PK_MEMBLOCK_MEMBLOCK_ALLOC_OUT         :=
PK_MEMBLOCK_MEMBLOCK_ALLOC_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MEMBLOCK_ALLOC_TAR         := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TAR)
PK_MEMBLOCK_MEMBLOCK_ALLOC_PACKAGE     :=
PK_MEMBLOCK_MEMBLOCK_ALLOC_CONFIG      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MEMBLOCK_ALLOC_DIR)/build

PK_MEMBLOCK_MEMBLOCK_ALLOC_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MEMBLOCK_ALLOC_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_VERSION)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_SITE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_GITHUB)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_ASFLAGS)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_BSBIT)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_BUILDARCH)X \
		 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_CSRCFILE)

$(PK_MEMBLOCK_MEMBLOCK_ALLOC_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MEMBLOCK_ALLOC_sh,$(srctree))
