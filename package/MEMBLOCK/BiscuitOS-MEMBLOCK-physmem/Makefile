################################################
#
# BiscuitOS-MEMBLOCK-physmem
#
################################################
PK_MEMBLOCK_PHYSMEM_NAME        := BiscuitOS-MEMBLOCK-physmem
PK_MEMBLOCK_PHYSMEM_VERSION     := $(CONFIG_PK_MEMBLOCK_PHYSMEM_VERSION)
PK_MEMBLOCK_PHYSMEM_SITE        := $(CONFIG_PK_MEMBLOCK_PHYSMEM_URL)
PK_MEMBLOCK_PHYSMEM_GITHUB      := $(CONFIG_PK_MEMBLOCK_PHYSMEM_GITHUB)
PK_MEMBLOCK_PHYSMEM_SUBNAME     :=
PK_MEMBLOCK_PHYSMEM_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_PHYSMEM_BUILDARCH)
PK_MEMBLOCK_PHYSMEM_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-physmem
PK_MEMBLOCK_PHYSMEM_PATCH       := $(PK_MEMBLOCK_PHYSMEM_DIR)/patch
PK_MEMBLOCK_PHYSMEM_BSBIT       := BiscuitOS-MEMBLOCK-physmem-$(PK_MEMBLOCK_PHYSMEM_VERSION).bsbit
PK_MEMBLOCK_PHYSMEM_OUT         :=
PK_MEMBLOCK_PHYSMEM_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_PHYSMEM_TAR         := $(CONFIG_PK_MEMBLOCK_PHYSMEM_TAR)
PK_MEMBLOCK_PHYSMEM_PACKAGE     :=
PK_MEMBLOCK_PHYSMEM_CONFIG      := $(CONFIG_PK_MEMBLOCK_PHYSMEM_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_PHYSMEM_DIR)/build

PK_MEMBLOCK_PHYSMEM_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_PHYSMEM_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_PHYSMEM_NAME)X \
                 $(PK_MEMBLOCK_PHYSMEM_VERSION)X \
                 $(PK_MEMBLOCK_PHYSMEM_SITE)X \
                 $(PK_MEMBLOCK_PHYSMEM_GITHUB)X \
                 $(PK_MEMBLOCK_PHYSMEM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_PHYSMEM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_PHYSMEM_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_PHYSMEM_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_PHYSMEM_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_PHYSMEM_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_PHYSMEM_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_PHYSMEM_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_PHYSMEM_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_PHYSMEM_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_PHYSMEM_ASFLAGS)X \
                 $(PK_MEMBLOCK_PHYSMEM_BSBIT)X \
                 $(PK_MEMBLOCK_PHYSMEM_BUILDARCH)X \
		 $(PK_MEMBLOCK_PHYSMEM_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_PHYSMEM_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_PHYSMEM_CSRCFILE)

$(PK_MEMBLOCK_PHYSMEM_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_PHYSMEM_sh,$(srctree))
