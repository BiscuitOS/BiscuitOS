PK_MEMBLOCK_MEMBLOCK_FREE_NAME        := BiscuitOS-MEMBLOCK-memblock_free
PK_MEMBLOCK_MEMBLOCK_FREE_VERSION     := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_VERSION)
PK_MEMBLOCK_MEMBLOCK_FREE_SITE        := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_URL)
PK_MEMBLOCK_MEMBLOCK_FREE_GITHUB      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_GITHUB)
PK_MEMBLOCK_MEMBLOCK_FREE_SUBNAME     :=
PK_MEMBLOCK_MEMBLOCK_FREE_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_BUILDARCH)
PK_MEMBLOCK_MEMBLOCK_FREE_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-memblock_free
PK_MEMBLOCK_MEMBLOCK_FREE_PATCH       := $(PK_MEMBLOCK_MEMBLOCK_FREE_DIR)/patch
PK_MEMBLOCK_MEMBLOCK_FREE_BSBIT       := BiscuitOS-MEMBLOCK-memblock_free-$(PK_MEMBLOCK_MEMBLOCK_FREE_VERSION).bsbit
PK_MEMBLOCK_MEMBLOCK_FREE_OUT         :=
PK_MEMBLOCK_MEMBLOCK_FREE_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MEMBLOCK_FREE_TAR         := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_TAR)
PK_MEMBLOCK_MEMBLOCK_FREE_PACKAGE     :=
PK_MEMBLOCK_MEMBLOCK_FREE_CONFIG      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MEMBLOCK_FREE_DIR)/build

PK_MEMBLOCK_MEMBLOCK_FREE_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MEMBLOCK_FREE_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MEMBLOCK_FREE_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_FREE_VERSION)X \
                 $(PK_MEMBLOCK_MEMBLOCK_FREE_SITE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_FREE_GITHUB)X \
                 $(PK_MEMBLOCK_MEMBLOCK_FREE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_FREE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_FREE_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_ASFLAGS)X \
                 $(PK_MEMBLOCK_MEMBLOCK_FREE_BSBIT)X \
                 $(PK_MEMBLOCK_MEMBLOCK_FREE_BUILDARCH)X \
		 $(PK_MEMBLOCK_MEMBLOCK_FREE_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_FREE_CSRCFILE)

$(PK_MEMBLOCK_MEMBLOCK_FREE_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MEMBLOCK_FREE_sh,$(srctree))
