PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_NAME        := BiscuitOS-MEMBLOCK-memblock_is_memory
PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_VERSION     := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_VERSION)
PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_SITE        := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_URL)
PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_GITHUB      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_GITHUB)
PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_SUBNAME     :=
PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_BUILDARCH)
PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-memblock_is_memory
PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_PATCH       := $(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_DIR)/patch
PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_BSBIT       := BiscuitOS-MEMBLOCK-memblock_is_memory-$(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_VERSION).bsbit
PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_OUT         :=
PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_TAR         := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_TAR)
PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_PACKAGE     :=
PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_CONFIG      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_DIR)/build

PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_VERSION)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_SITE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_GITHUB)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_ASFLAGS)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_BSBIT)X \
                 $(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_BUILDARCH)X \
		 $(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_CSRCFILE)

$(PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MEMBLOCK_IS_MEMORY_sh,$(srctree))
