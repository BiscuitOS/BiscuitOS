PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_SITE        := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_URL)
PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_GITHUB      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_GITHUB)
PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_SUBNAME     :=
PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_BUILDARCH)
PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-memblock_phys_mem_size
PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_PATCH       := $(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_DIR)/patch
PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_BSBIT       := BiscuitOS-MEMBLOCK-memblock_phys_mem_size-$(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_VERSION).bsbit
PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_OUT         :=
PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_TAR         := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_TAR)
PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_PACKAGE     :=
PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_CONFIG      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_DIR)/build

PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_VERSION)X \
                 $(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_SITE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_GITHUB)X \
                 $(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_ASFLAGS)X \
                 $(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_BSBIT)X \
                 $(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_BUILDARCH)X \
		 $(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_CSRCFILE)

$(PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MEMBLOCK_PHYS_MEM_SIZE_sh,$(srctree))
