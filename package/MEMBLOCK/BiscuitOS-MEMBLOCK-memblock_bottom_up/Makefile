PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_SITE        := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_URL)
PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_GITHUB      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_GITHUB)
PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_SUBNAME     :=
PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_BUILDARCH)
PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-memblock_bottom_up
PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_PATCH       := $(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_DIR)/patch
PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_BSBIT       := BiscuitOS-MEMBLOCK-memblock_bottom_up-$(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_VERSION).bsbit
PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_OUT         :=
PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_TAR         := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_TAR)
PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_PACKAGE     :=
PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_CONFIG      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_DIR)/build

PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_VERSION)X \
                 $(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_SITE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_GITHUB)X \
                 $(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_ASFLAGS)X \
                 $(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_BSBIT)X \
                 $(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_BUILDARCH)X \
		 $(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_CSRCFILE)

$(PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MEMBLOCK_BOTTOM_UP_sh,$(srctree))
