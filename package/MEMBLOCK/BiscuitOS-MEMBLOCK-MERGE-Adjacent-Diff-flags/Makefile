PK_MEMBLOCK_MERGE_ADJACENT_DF_NAME        := BiscuitOS-MEMBLOCK-MERGE-Adjacent-Diff-flags
PK_MEMBLOCK_MERGE_ADJACENT_DF_VERSION     := $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_VERSION)
PK_MEMBLOCK_MERGE_ADJACENT_DF_SITE        := $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_URL)
PK_MEMBLOCK_MERGE_ADJACENT_DF_GITHUB      := $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_GITHUB)
PK_MEMBLOCK_MERGE_ADJACENT_DF_SUBNAME     :=
PK_MEMBLOCK_MERGE_ADJACENT_DF_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_BUILDARCH)
PK_MEMBLOCK_MERGE_ADJACENT_DF_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-MERGE-Adjacent-Diff-flags
PK_MEMBLOCK_MERGE_ADJACENT_DF_PATCH       := $(PK_MEMBLOCK_MERGE_ADJACENT_DF_DIR)/patch
PK_MEMBLOCK_MERGE_ADJACENT_DF_BSBIT       := BiscuitOS-MEMBLOCK-MERGE-Adjacent-Diff-flags-$(PK_MEMBLOCK_MERGE_ADJACENT_DF_VERSION).bsbit
PK_MEMBLOCK_MERGE_ADJACENT_DF_OUT         :=
PK_MEMBLOCK_MERGE_ADJACENT_DF_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MERGE_ADJACENT_DF_TAR         := $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_TAR)
PK_MEMBLOCK_MERGE_ADJACENT_DF_PACKAGE     :=
PK_MEMBLOCK_MERGE_ADJACENT_DF_CONFIG      := $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MERGE_ADJACENT_DF_DIR)/build

PK_MEMBLOCK_MERGE_ADJACENT_DF_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MERGE_ADJACENT_DF_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_DF_NAME)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_DF_VERSION)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_DF_SITE)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_DF_GITHUB)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_DF_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_DF_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_DF_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_ASFLAGS)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_DF_BSBIT)X \
                 $(PK_MEMBLOCK_MERGE_ADJACENT_DF_BUILDARCH)X \
		 $(PK_MEMBLOCK_MERGE_ADJACENT_DF_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_ADJACENT_DF_CSRCFILE)

$(PK_MEMBLOCK_MERGE_ADJACENT_DF_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MERGE_ADJACENT_DF_sh,$(srctree))
