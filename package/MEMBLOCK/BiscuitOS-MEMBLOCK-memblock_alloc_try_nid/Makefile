PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_NAME        := BiscuitOS-MEMBLOCK-memblock_alloc_try_nid
PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_VERSION     := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_VERSION)
PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_SITE        := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_URL)
PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_GITHUB      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_GITHUB)
PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_SUBNAME     :=
PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_BUILDARCH)
PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-memblock_alloc_try_nid
PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_PATCH       := $(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_DIR)/patch
PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_BSBIT       := BiscuitOS-MEMBLOCK-memblock_alloc_try_nid-$(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_VERSION).bsbit
PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_OUT         :=
PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_TAR         := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_TAR)
PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_PACKAGE     :=
PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_CONFIG      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_DIR)/build

PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_VERSION)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_SITE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_GITHUB)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_ASFLAGS)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_BSBIT)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_BUILDARCH)X \
		 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_CSRCFILE)

$(PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MEMBLOCK_ALLOC_TRY_NID_sh,$(srctree))
