                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_ASFLAGS)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_BSBIT)X \
                 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_BUILDARCH)X \
		 $(PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_CSRCFILE)X \
