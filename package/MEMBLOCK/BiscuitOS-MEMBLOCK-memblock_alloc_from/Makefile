PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_TAR         := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_TAR)
PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_PACKAGE     :=
PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_CONFIG      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_ALLOC_FROM_CONFIG)
