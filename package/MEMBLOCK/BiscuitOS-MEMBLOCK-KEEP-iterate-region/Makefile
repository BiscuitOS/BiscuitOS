PK_MEMBLOCK_KEEP_WORK_NAME        := BiscuitOS-MEMBLOCK-KEEP-iterate-region
PK_MEMBLOCK_KEEP_WORK_VERSION     := $(CONFIG_PK_MEMBLOCK_KEEP_WORK_VERSION)
PK_MEMBLOCK_KEEP_WORK_SITE        := $(CONFIG_PK_MEMBLOCK_KEEP_WORK_URL)
PK_MEMBLOCK_KEEP_WORK_GITHUB      := $(CONFIG_PK_MEMBLOCK_KEEP_WORK_GITHUB)
PK_MEMBLOCK_KEEP_WORK_SUBNAME     :=
PK_MEMBLOCK_KEEP_WORK_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_KEEP_WORK_BUILDARCH)
PK_MEMBLOCK_KEEP_WORK_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-KEEP-iterate-region
PK_MEMBLOCK_KEEP_WORK_PATCH       := $(PK_MEMBLOCK_KEEP_WORK_DIR)/patch
PK_MEMBLOCK_KEEP_WORK_BSBIT       := BiscuitOS-MEMBLOCK-KEEP-iterate-region-$(PK_MEMBLOCK_KEEP_WORK_VERSION).bsbit
PK_MEMBLOCK_KEEP_WORK_OUT         :=
PK_MEMBLOCK_KEEP_WORK_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_MEMBLOCK_KEEP_WORK_TAR         := $(CONFIG_PK_MEMBLOCK_KEEP_WORK_TAR)
PK_MEMBLOCK_KEEP_WORK_PACKAGE     :=
PK_MEMBLOCK_KEEP_WORK_CONFIG      := $(CONFIG_PK_MEMBLOCK_KEEP_WORK_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_KEEP_WORK_DIR)/build

PK_MEMBLOCK_KEEP_WORK_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_KEEP_WORK_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_KEEP_WORK_NAME)X \
                 $(PK_MEMBLOCK_KEEP_WORK_VERSION)X \
                 $(PK_MEMBLOCK_KEEP_WORK_SITE)X \
                 $(PK_MEMBLOCK_KEEP_WORK_GITHUB)X \
                 $(PK_MEMBLOCK_KEEP_WORK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_KEEP_WORK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_KEEP_WORK_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_KEEP_WORK_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_KEEP_WORK_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_KEEP_WORK_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_KEEP_WORK_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_KEEP_WORK_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_KEEP_WORK_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_KEEP_WORK_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_KEEP_WORK_ASFLAGS)X \
                 $(PK_MEMBLOCK_KEEP_WORK_BSBIT)X \
                 $(PK_MEMBLOCK_KEEP_WORK_BUILDARCH)X \
		 $(PK_MEMBLOCK_KEEP_WORK_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_KEEP_WORK_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_KEEP_WORK_CSRCFILE)

$(PK_MEMBLOCK_KEEP_WORK_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_KEEP_WORK_sh,$(srctree))
