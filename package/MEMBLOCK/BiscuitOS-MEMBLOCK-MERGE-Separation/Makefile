PK_MEMBLOCK_MERGE_SEPARATION_NAME        := BiscuitOS-MEMBLOCK-MERGE-Separation
PK_MEMBLOCK_MERGE_SEPARATION_VERSION     := $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_VERSION)
PK_MEMBLOCK_MERGE_SEPARATION_SITE        := $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_URL)
PK_MEMBLOCK_MERGE_SEPARATION_GITHUB      := $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_GITHUB)
PK_MEMBLOCK_MERGE_SEPARATION_SUBNAME     :=
PK_MEMBLOCK_MERGE_SEPARATION_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_BUILDARCH)
PK_MEMBLOCK_MERGE_SEPARATION_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-MERGE-Separation
PK_MEMBLOCK_MERGE_SEPARATION_PATCH       := $(PK_MEMBLOCK_MERGE_SEPARATION_DIR)/patch
PK_MEMBLOCK_MERGE_SEPARATION_BSBIT       := BiscuitOS-MEMBLOCK-MERGE-Separation-$(PK_MEMBLOCK_MERGE_SEPARATION_VERSION).bsbit
PK_MEMBLOCK_MERGE_SEPARATION_OUT         :=
PK_MEMBLOCK_MERGE_SEPARATION_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MERGE_SEPARATION_TAR         := $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_TAR)
PK_MEMBLOCK_MERGE_SEPARATION_PACKAGE     :=
PK_MEMBLOCK_MERGE_SEPARATION_CONFIG      := $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MERGE_SEPARATION_DIR)/build

PK_MEMBLOCK_MERGE_SEPARATION_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MERGE_SEPARATION_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MERGE_SEPARATION_NAME)X \
                 $(PK_MEMBLOCK_MERGE_SEPARATION_VERSION)X \
                 $(PK_MEMBLOCK_MERGE_SEPARATION_SITE)X \
                 $(PK_MEMBLOCK_MERGE_SEPARATION_GITHUB)X \
                 $(PK_MEMBLOCK_MERGE_SEPARATION_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MERGE_SEPARATION_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MERGE_SEPARATION_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_ASFLAGS)X \
                 $(PK_MEMBLOCK_MERGE_SEPARATION_BSBIT)X \
                 $(PK_MEMBLOCK_MERGE_SEPARATION_BUILDARCH)X \
		 $(PK_MEMBLOCK_MERGE_SEPARATION_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MERGE_SEPARATION_CSRCFILE)

$(PK_MEMBLOCK_MERGE_SEPARATION_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MERGE_SEPARATION_sh,$(srctree))
