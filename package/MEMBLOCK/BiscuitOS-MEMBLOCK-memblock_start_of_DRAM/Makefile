PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-memblock_start_of_DRAM
PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_PATCH       := $(PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_DIR)/patch
PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_BSBIT       := BiscuitOS-MEMBLOCK-memblock_start_of_DRAM-$(PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_VERSION).bsbit
