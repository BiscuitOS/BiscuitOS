PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_PACKAGE     :=
PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_CONFIG      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_DIR)/build

PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_SCRIPTS) \
                 $1X \
