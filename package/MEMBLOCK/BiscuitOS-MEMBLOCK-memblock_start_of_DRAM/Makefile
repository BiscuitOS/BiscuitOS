                 $(PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_ASFLAGS)X \
                 $(PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_BSBIT)X \
                 $(PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_BUILDARCH)X \
		 $(PK_MEMBLOCK_MEMBLOCK_START_OF_DRAM_CSRCFILE)X \
