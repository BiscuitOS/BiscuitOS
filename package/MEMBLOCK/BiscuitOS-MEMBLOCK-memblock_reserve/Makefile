PK_MEMBLOCK_MEMBLOCK_RESERVE_NAME        := BiscuitOS-MEMBLOCK-memblock_reserve
PK_MEMBLOCK_MEMBLOCK_RESERVE_VERSION     := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_VERSION)
PK_MEMBLOCK_MEMBLOCK_RESERVE_SITE        := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_URL)
PK_MEMBLOCK_MEMBLOCK_RESERVE_GITHUB      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_GITHUB)
PK_MEMBLOCK_MEMBLOCK_RESERVE_SUBNAME     :=
PK_MEMBLOCK_MEMBLOCK_RESERVE_BUILDARCH   := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_BUILDARCH)
PK_MEMBLOCK_MEMBLOCK_RESERVE_DIR         := $(srctree)/package/MEMBLOCK/BiscuitOS-MEMBLOCK-memblock_reserve
PK_MEMBLOCK_MEMBLOCK_RESERVE_PATCH       := $(PK_MEMBLOCK_MEMBLOCK_RESERVE_DIR)/patch
PK_MEMBLOCK_MEMBLOCK_RESERVE_BSBIT       := BiscuitOS-MEMBLOCK-memblock_reserve-$(PK_MEMBLOCK_MEMBLOCK_RESERVE_VERSION).bsbit
PK_MEMBLOCK_MEMBLOCK_RESERVE_OUT         :=
PK_MEMBLOCK_MEMBLOCK_RESERVE_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_MEMBLOCK_MEMBLOCK_RESERVE_TAR         := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_TAR)
PK_MEMBLOCK_MEMBLOCK_RESERVE_PACKAGE     :=
PK_MEMBLOCK_MEMBLOCK_RESERVE_CONFIG      := $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_CONFIG)
SUB_TARGET                  += $(PK_MEMBLOCK_MEMBLOCK_RESERVE_DIR)/build

PK_MEMBLOCK_MEMBLOCK_RESERVE_sh = $(CONFIG_SHELL) $(PK_MEMBLOCK_MEMBLOCK_RESERVE_SCRIPTS) \
                 $1X \
                 $(PK_MEMBLOCK_MEMBLOCK_RESERVE_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_RESERVE_VERSION)X \
                 $(PK_MEMBLOCK_MEMBLOCK_RESERVE_SITE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_RESERVE_GITHUB)X \
                 $(PK_MEMBLOCK_MEMBLOCK_RESERVE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBLOCK_MEMBLOCK_RESERVE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBLOCK_MEMBLOCK_RESERVE_SUBNAME)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_CONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_LIBPATH)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_CFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_LDFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_ASFLAGS)X \
                 $(PK_MEMBLOCK_MEMBLOCK_RESERVE_BSBIT)X \
                 $(PK_MEMBLOCK_MEMBLOCK_RESERVE_BUILDARCH)X \
		 $(PK_MEMBLOCK_MEMBLOCK_RESERVE_CSRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_SRCFILE)X \
                 $(CONFIG_PK_MEMBLOCK_MEMBLOCK_RESERVE_CSRCFILE)

$(PK_MEMBLOCK_MEMBLOCK_RESERVE_DIR)/build: FORCE
	$(Q)$(call PK_MEMBLOCK_MEMBLOCK_RESERVE_sh,$(srctree))
