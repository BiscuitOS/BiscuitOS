################################################
#
# BiscuitOS-SCHED-INTERRUPT
#
################################################
PK_SCHED_INTERRUPT_NAME        := BiscuitOS-SCHED-INTERRUPT
PK_SCHED_INTERRUPT_VERSION     := $(CONFIG_PK_SCHED_INTERRUPT_VERSION)
PK_SCHED_INTERRUPT_SITE        := $(CONFIG_PK_SCHED_INTERRUPT_URL)
PK_SCHED_INTERRUPT_GITHUB      := $(CONFIG_PK_SCHED_INTERRUPT_GITHUB)
PK_SCHED_INTERRUPT_SUBNAME     :=
PK_SCHED_INTERRUPT_BUILDARCH   := $(CONFIG_PK_SCHED_INTERRUPT_BUILDARCH)
PK_SCHED_INTERRUPT_DIR         := $(srctree)/package/SCHEDULE/BiscuitOS-SCHED-INTERRUPT
PK_SCHED_INTERRUPT_PATCH       := $(PK_SCHED_INTERRUPT_DIR)/patch
PK_SCHED_INTERRUPT_BSBIT       := BiscuitOS-SCHED-INTERRUPT-$(PK_SCHED_INTERRUPT_VERSION).bsbit
PK_SCHED_INTERRUPT_OUT         :=
PK_SCHED_INTERRUPT_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_SCHED_INTERRUPT_TAR         := $(CONFIG_PK_SCHED_INTERRUPT_TAR)
PK_SCHED_INTERRUPT_PACKAGE     :=
PK_SCHED_INTERRUPT_CONFIG      := $(CONFIG_PK_SCHED_INTERRUPT_CONFIG)
SUB_TARGET                  += $(PK_SCHED_INTERRUPT_DIR)/build

PK_SCHED_INTERRUPT_sh = $(CONFIG_SHELL) $(PK_SCHED_INTERRUPT_SCRIPTS) \
                 $1X \
                 $(PK_SCHED_INTERRUPT_NAME)X \
                 $(PK_SCHED_INTERRUPT_VERSION)X \
                 $(PK_SCHED_INTERRUPT_SITE)X \
                 $(PK_SCHED_INTERRUPT_GITHUB)X \
                 $(PK_SCHED_INTERRUPT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SCHED_INTERRUPT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SCHED_INTERRUPT_SUBNAME)X \
                 $(CONFIG_PK_SCHED_INTERRUPT_CONFIG)X \
                 $(CONFIG_PK_SCHED_INTERRUPT_LIBPATH)X \
                 $(CONFIG_PK_SCHED_INTERRUPT_CPPFLAGS)X \
                 $(CONFIG_PK_SCHED_INTERRUPT_DPKCONFIG)X \
                 $(CONFIG_PK_SCHED_INTERRUPT_CFLAGS)X \
                 $(CONFIG_PK_SCHED_INTERRUPT_LDFLAGS)X \
                 $(CONFIG_PK_SCHED_INTERRUPT_CXXFLAGS)X \
                 $(CONFIG_PK_SCHED_INTERRUPT_ASFLAGS)X \
                 $(PK_SCHED_INTERRUPT_BSBIT)X \
                 $(PK_SCHED_INTERRUPT_BUILDARCH)X \
		 $(PK_SCHED_INTERRUPT_CSRCFILE)X \
                 $(CONFIG_PK_SCHED_INTERRUPT_SRCFILE)X \
                 $(CONFIG_PK_SCHED_INTERRUPT_CSRCFILE)

$(PK_SCHED_INTERRUPT_DIR)/build: FORCE
	$(Q)$(call PK_SCHED_INTERRUPT_sh,$(srctree))
