################################################
#
# BiscuitOS-SCHED-APP-KERNEL-MODE
#
################################################
PK_SCHED_APP_KERNEL_NAME        := BiscuitOS-SCHED-APP-KERNEL-MODE
PK_SCHED_APP_KERNEL_VERSION     := $(CONFIG_PK_SCHED_APP_KERNEL_VERSION)
PK_SCHED_APP_KERNEL_SITE        := $(CONFIG_PK_SCHED_APP_KERNEL_URL)
PK_SCHED_APP_KERNEL_GITHUB      := $(CONFIG_PK_SCHED_APP_KERNEL_GITHUB)
PK_SCHED_APP_KERNEL_SUBNAME     :=
PK_SCHED_APP_KERNEL_BUILDARCH   := $(CONFIG_PK_SCHED_APP_KERNEL_BUILDARCH)
PK_SCHED_APP_KERNEL_DIR         := $(srctree)/package/SCHEDULE/BiscuitOS-SCHED-APP-KERNEL-MODE
PK_SCHED_APP_KERNEL_PATCH       := $(PK_SCHED_APP_KERNEL_DIR)/patch
PK_SCHED_APP_KERNEL_BSBIT       := BiscuitOS-SCHED-APP-KERNEL-MODE-$(PK_SCHED_APP_KERNEL_VERSION).bsbit
PK_SCHED_APP_KERNEL_OUT         :=
PK_SCHED_APP_KERNEL_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_SCHED_APP_KERNEL_TAR         := $(CONFIG_PK_SCHED_APP_KERNEL_TAR)
PK_SCHED_APP_KERNEL_PACKAGE     :=
PK_SCHED_APP_KERNEL_CONFIG      := $(CONFIG_PK_SCHED_APP_KERNEL_CONFIG)
SUB_TARGET                  += $(PK_SCHED_APP_KERNEL_DIR)/build

PK_SCHED_APP_KERNEL_sh = $(CONFIG_SHELL) $(PK_SCHED_APP_KERNEL_SCRIPTS) \
                 $1X \
                 $(PK_SCHED_APP_KERNEL_NAME)X \
                 $(PK_SCHED_APP_KERNEL_VERSION)X \
                 $(PK_SCHED_APP_KERNEL_SITE)X \
                 $(PK_SCHED_APP_KERNEL_GITHUB)X \
                 $(PK_SCHED_APP_KERNEL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SCHED_APP_KERNEL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SCHED_APP_KERNEL_SUBNAME)X \
                 $(CONFIG_PK_SCHED_APP_KERNEL_CONFIG)X \
                 $(CONFIG_PK_SCHED_APP_KERNEL_LIBPATH)X \
                 $(CONFIG_PK_SCHED_APP_KERNEL_CPPFLAGS)X \
                 $(CONFIG_PK_SCHED_APP_KERNEL_DPKCONFIG)X \
                 $(CONFIG_PK_SCHED_APP_KERNEL_CFLAGS)X \
                 $(CONFIG_PK_SCHED_APP_KERNEL_LDFLAGS)X \
                 $(CONFIG_PK_SCHED_APP_KERNEL_CXXFLAGS)X \
                 $(CONFIG_PK_SCHED_APP_KERNEL_ASFLAGS)X \
                 $(PK_SCHED_APP_KERNEL_BSBIT)X \
                 $(PK_SCHED_APP_KERNEL_BUILDARCH)X \
		 $(PK_SCHED_APP_KERNEL_CSRCFILE)X \
                 $(CONFIG_PK_SCHED_APP_KERNEL_SRCFILE)X \
                 $(CONFIG_PK_SCHED_APP_KERNEL_CSRCFILE)

$(PK_SCHED_APP_KERNEL_DIR)/build: FORCE
	$(Q)$(call PK_SCHED_APP_KERNEL_sh,$(srctree))
