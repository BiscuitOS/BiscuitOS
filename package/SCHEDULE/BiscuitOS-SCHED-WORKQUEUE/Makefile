################################################
#
# BiscuitOS-SCHED-WORKQUEUE
#
################################################
PK_SCHED_WORKQUEUE_NAME        := BiscuitOS-SCHED-WORKQUEUE
PK_SCHED_WORKQUEUE_VERSION     := $(CONFIG_PK_SCHED_WORKQUEUE_VERSION)
PK_SCHED_WORKQUEUE_SITE        := $(CONFIG_PK_SCHED_WORKQUEUE_URL)
PK_SCHED_WORKQUEUE_GITHUB      := $(CONFIG_PK_SCHED_WORKQUEUE_GITHUB)
PK_SCHED_WORKQUEUE_SUBNAME     :=
PK_SCHED_WORKQUEUE_BUILDARCH   := $(CONFIG_PK_SCHED_WORKQUEUE_BUILDARCH)
PK_SCHED_WORKQUEUE_DIR         := $(srctree)/package/SCHEDULE/BiscuitOS-SCHED-WORKQUEUE
PK_SCHED_WORKQUEUE_PATCH       := $(PK_SCHED_WORKQUEUE_DIR)/patch
PK_SCHED_WORKQUEUE_BSBIT       := BiscuitOS-SCHED-WORKQUEUE-$(PK_SCHED_WORKQUEUE_VERSION).bsbit
PK_SCHED_WORKQUEUE_OUT         :=
PK_SCHED_WORKQUEUE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_SCHED_WORKQUEUE_TAR         := $(CONFIG_PK_SCHED_WORKQUEUE_TAR)
PK_SCHED_WORKQUEUE_PACKAGE     :=
PK_SCHED_WORKQUEUE_CONFIG      := $(CONFIG_PK_SCHED_WORKQUEUE_CONFIG)
SUB_TARGET                  += $(PK_SCHED_WORKQUEUE_DIR)/build

PK_SCHED_WORKQUEUE_sh = $(CONFIG_SHELL) $(PK_SCHED_WORKQUEUE_SCRIPTS) \
                 $1X \
                 $(PK_SCHED_WORKQUEUE_NAME)X \
                 $(PK_SCHED_WORKQUEUE_VERSION)X \
                 $(PK_SCHED_WORKQUEUE_SITE)X \
                 $(PK_SCHED_WORKQUEUE_GITHUB)X \
                 $(PK_SCHED_WORKQUEUE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SCHED_WORKQUEUE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SCHED_WORKQUEUE_SUBNAME)X \
                 $(CONFIG_PK_SCHED_WORKQUEUE_CONFIG)X \
                 $(CONFIG_PK_SCHED_WORKQUEUE_LIBPATH)X \
                 $(CONFIG_PK_SCHED_WORKQUEUE_CPPFLAGS)X \
                 $(CONFIG_PK_SCHED_WORKQUEUE_DPKCONFIG)X \
                 $(CONFIG_PK_SCHED_WORKQUEUE_CFLAGS)X \
                 $(CONFIG_PK_SCHED_WORKQUEUE_LDFLAGS)X \
                 $(CONFIG_PK_SCHED_WORKQUEUE_CXXFLAGS)X \
                 $(CONFIG_PK_SCHED_WORKQUEUE_ASFLAGS)X \
                 $(PK_SCHED_WORKQUEUE_BSBIT)X \
                 $(PK_SCHED_WORKQUEUE_BUILDARCH)X \
		 $(PK_SCHED_WORKQUEUE_CSRCFILE)X \
                 $(CONFIG_PK_SCHED_WORKQUEUE_SRCFILE)X \
                 $(CONFIG_PK_SCHED_WORKQUEUE_CSRCFILE)

$(PK_SCHED_WORKQUEUE_DIR)/build: FORCE
	$(Q)$(call PK_SCHED_WORKQUEUE_sh,$(srctree))
