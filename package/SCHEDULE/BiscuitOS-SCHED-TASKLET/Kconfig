	default "https://gitee.com/BiscuitOS_team/HardStack/raw/Gitee/SCHEDULE-SUBSYSTEM/BiscuitOS-SCHED-TASKLET"
	string "url"

config PK_SCHED_TASKLET_GITHUB
	string "GitHub"

config PK_SCHED_TASKLET_SRCFILE
	default "Kconfig  Makefile  Makefile.host  RunBiscuitOS.sh  main.c"
	string "Source code list"

config PK_SCHED_TASKLET_CSRCFILE
	string "Compile source code list"

config PK_SCHED_TASKLET_CONFIG
	string "CONFIG: Project Configure Options"

config PK_SCHED_TASKLET_LIBPATH
	string "LIBS: Libraries to pass to the linker, e.g. -l<library>"

config PK_SCHED_TASKLET_CPPFLAGS
	string "CPPFLAGS: C/C++ preprocessor flags, e.g. -I<include dir>"

config PK_SCHED_TASKLET_DPKCONFIG
	string "PKG_CONFIG_PATH: Directories to add to pkg-config's search path"

config PK_SCHED_TASKLET_CFLAGS
	string "CFLAGS: C compiler flags"

config PK_SCHED_TASKLET_LDFLAGS
	string "LDFLAGS: Linker flags"

config PK_SCHED_TASKLET_CXXFLAGS
	string "CXXFLAGS: C++ compiler flags"

config PK_SCHED_TASKLET_ASFLAGS
	string "CCASFLAGS: assembler compiler flags (defaults to CFLAGS)"

config PK_SCHED_TASKLET_BUILDARCH
	string "--host options on configure for Architecture"

endif # PK_SCHED_TASKLET
