################################################
#
# BiscuitOS-SCHED-WAITQUEUE
#
################################################
PK_SCHED_WAITQUEUE_NAME        := BiscuitOS-SCHED-WAITQUEUE
PK_SCHED_WAITQUEUE_VERSION     := $(CONFIG_PK_SCHED_WAITQUEUE_VERSION)
PK_SCHED_WAITQUEUE_SITE        := $(CONFIG_PK_SCHED_WAITQUEUE_URL)
PK_SCHED_WAITQUEUE_GITHUB      := $(CONFIG_PK_SCHED_WAITQUEUE_GITHUB)
PK_SCHED_WAITQUEUE_SUBNAME     :=
PK_SCHED_WAITQUEUE_BUILDARCH   := $(CONFIG_PK_SCHED_WAITQUEUE_BUILDARCH)
PK_SCHED_WAITQUEUE_DIR         := $(srctree)/package/SCHEDULE/BiscuitOS-SCHED-WAITQUEUE
PK_SCHED_WAITQUEUE_PATCH       := $(PK_SCHED_WAITQUEUE_DIR)/patch
PK_SCHED_WAITQUEUE_BSBIT       := BiscuitOS-SCHED-WAITQUEUE-$(PK_SCHED_WAITQUEUE_VERSION).bsbit
PK_SCHED_WAITQUEUE_OUT         :=
PK_SCHED_WAITQUEUE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_SCHED_WAITQUEUE_TAR         := $(CONFIG_PK_SCHED_WAITQUEUE_TAR)
PK_SCHED_WAITQUEUE_PACKAGE     :=
PK_SCHED_WAITQUEUE_CONFIG      := $(CONFIG_PK_SCHED_WAITQUEUE_CONFIG)
SUB_TARGET                  += $(PK_SCHED_WAITQUEUE_DIR)/build

PK_SCHED_WAITQUEUE_sh = $(CONFIG_SHELL) $(PK_SCHED_WAITQUEUE_SCRIPTS) \
                 $1X \
                 $(PK_SCHED_WAITQUEUE_NAME)X \
                 $(PK_SCHED_WAITQUEUE_VERSION)X \
                 $(PK_SCHED_WAITQUEUE_SITE)X \
                 $(PK_SCHED_WAITQUEUE_GITHUB)X \
                 $(PK_SCHED_WAITQUEUE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SCHED_WAITQUEUE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SCHED_WAITQUEUE_SUBNAME)X \
                 $(CONFIG_PK_SCHED_WAITQUEUE_CONFIG)X \
                 $(CONFIG_PK_SCHED_WAITQUEUE_LIBPATH)X \
                 $(CONFIG_PK_SCHED_WAITQUEUE_CPPFLAGS)X \
                 $(CONFIG_PK_SCHED_WAITQUEUE_DPKCONFIG)X \
                 $(CONFIG_PK_SCHED_WAITQUEUE_CFLAGS)X \
                 $(CONFIG_PK_SCHED_WAITQUEUE_LDFLAGS)X \
                 $(CONFIG_PK_SCHED_WAITQUEUE_CXXFLAGS)X \
                 $(CONFIG_PK_SCHED_WAITQUEUE_ASFLAGS)X \
                 $(PK_SCHED_WAITQUEUE_BSBIT)X \
                 $(PK_SCHED_WAITQUEUE_BUILDARCH)X \
		 $(PK_SCHED_WAITQUEUE_CSRCFILE)X \
                 $(CONFIG_PK_SCHED_WAITQUEUE_SRCFILE)X \
                 $(CONFIG_PK_SCHED_WAITQUEUE_CSRCFILE)

$(PK_SCHED_WAITQUEUE_DIR)/build: FORCE
	$(Q)$(call PK_SCHED_WAITQUEUE_sh,$(srctree))
