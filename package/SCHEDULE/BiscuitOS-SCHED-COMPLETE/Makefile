################################################
#
# BiscuitOS-SCHED-COMPLETE
#
################################################
PK_SCHED_COMPLETE_NAME        := BiscuitOS-SCHED-COMPLETE
PK_SCHED_COMPLETE_VERSION     := $(CONFIG_PK_SCHED_COMPLETE_VERSION)
PK_SCHED_COMPLETE_SITE        := $(CONFIG_PK_SCHED_COMPLETE_URL)
PK_SCHED_COMPLETE_GITHUB      := $(CONFIG_PK_SCHED_COMPLETE_GITHUB)
PK_SCHED_COMPLETE_SUBNAME     :=
PK_SCHED_COMPLETE_BUILDARCH   := $(CONFIG_PK_SCHED_COMPLETE_BUILDARCH)
PK_SCHED_COMPLETE_DIR         := $(srctree)/package/SCHEDULE/BiscuitOS-SCHED-COMPLETE
PK_SCHED_COMPLETE_PATCH       := $(PK_SCHED_COMPLETE_DIR)/patch
PK_SCHED_COMPLETE_BSBIT       := BiscuitOS-SCHED-COMPLETE-$(PK_SCHED_COMPLETE_VERSION).bsbit
PK_SCHED_COMPLETE_OUT         :=
PK_SCHED_COMPLETE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_SCHED_COMPLETE_TAR         := $(CONFIG_PK_SCHED_COMPLETE_TAR)
PK_SCHED_COMPLETE_PACKAGE     :=
PK_SCHED_COMPLETE_CONFIG      := $(CONFIG_PK_SCHED_COMPLETE_CONFIG)
SUB_TARGET                  += $(PK_SCHED_COMPLETE_DIR)/build

PK_SCHED_COMPLETE_sh = $(CONFIG_SHELL) $(PK_SCHED_COMPLETE_SCRIPTS) \
                 $1X \
                 $(PK_SCHED_COMPLETE_NAME)X \
                 $(PK_SCHED_COMPLETE_VERSION)X \
                 $(PK_SCHED_COMPLETE_SITE)X \
                 $(PK_SCHED_COMPLETE_GITHUB)X \
                 $(PK_SCHED_COMPLETE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SCHED_COMPLETE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SCHED_COMPLETE_SUBNAME)X \
                 $(CONFIG_PK_SCHED_COMPLETE_CONFIG)X \
                 $(CONFIG_PK_SCHED_COMPLETE_LIBPATH)X \
                 $(CONFIG_PK_SCHED_COMPLETE_CPPFLAGS)X \
                 $(CONFIG_PK_SCHED_COMPLETE_DPKCONFIG)X \
                 $(CONFIG_PK_SCHED_COMPLETE_CFLAGS)X \
                 $(CONFIG_PK_SCHED_COMPLETE_LDFLAGS)X \
                 $(CONFIG_PK_SCHED_COMPLETE_CXXFLAGS)X \
                 $(CONFIG_PK_SCHED_COMPLETE_ASFLAGS)X \
                 $(PK_SCHED_COMPLETE_BSBIT)X \
                 $(PK_SCHED_COMPLETE_BUILDARCH)X \
		 $(PK_SCHED_COMPLETE_CSRCFILE)X \
                 $(CONFIG_PK_SCHED_COMPLETE_SRCFILE)X \
                 $(CONFIG_PK_SCHED_COMPLETE_CSRCFILE)

$(PK_SCHED_COMPLETE_DIR)/build: FORCE
	$(Q)$(call PK_SCHED_COMPLETE_sh,$(srctree))
