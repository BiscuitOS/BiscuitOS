PK_PAGING_PF_SHMEM_FORK_NAME        := BiscuitOS-PAGING-PF-SHMEM-FORK
PK_PAGING_PF_SHMEM_FORK_VERSION     := $(CONFIG_PK_PAGING_PF_SHMEM_FORK_VERSION)
PK_PAGING_PF_SHMEM_FORK_SITE        := $(CONFIG_PK_PAGING_PF_SHMEM_FORK_URL)
PK_PAGING_PF_SHMEM_FORK_GITHUB      := $(CONFIG_PK_PAGING_PF_SHMEM_FORK_GITHUB)
PK_PAGING_PF_SHMEM_FORK_SUBNAME     :=
PK_PAGING_PF_SHMEM_FORK_BUILDARCH   := $(CONFIG_PK_PAGING_PF_SHMEM_FORK_BUILDARCH)
PK_PAGING_PF_SHMEM_FORK_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-SHMEM-FORK
PK_PAGING_PF_SHMEM_FORK_PATCH       := $(PK_PAGING_PF_SHMEM_FORK_DIR)/patch
PK_PAGING_PF_SHMEM_FORK_BSBIT       := BiscuitOS-PAGING-PF-SHMEM-FORK-$(PK_PAGING_PF_SHMEM_FORK_VERSION).bsbit
PK_PAGING_PF_SHMEM_FORK_OUT         :=
PK_PAGING_PF_SHMEM_FORK_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_SHMEM_FORK_TAR         := $(CONFIG_PK_PAGING_PF_SHMEM_FORK_TAR)
PK_PAGING_PF_SHMEM_FORK_PACKAGE     :=
PK_PAGING_PF_SHMEM_FORK_CONFIG      := $(CONFIG_PK_PAGING_PF_SHMEM_FORK_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_SHMEM_FORK_DIR)/build

PK_PAGING_PF_SHMEM_FORK_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_SHMEM_FORK_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_SHMEM_FORK_NAME)X \
                 $(PK_PAGING_PF_SHMEM_FORK_VERSION)X \
                 $(PK_PAGING_PF_SHMEM_FORK_SITE)X \
                 $(PK_PAGING_PF_SHMEM_FORK_GITHUB)X \
                 $(PK_PAGING_PF_SHMEM_FORK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_SHMEM_FORK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_SHMEM_FORK_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_FORK_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_FORK_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_FORK_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_FORK_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_FORK_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_FORK_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_FORK_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_FORK_ASFLAGS)X \
                 $(PK_PAGING_PF_SHMEM_FORK_BSBIT)X \
                 $(PK_PAGING_PF_SHMEM_FORK_BUILDARCH)X \
		 $(PK_PAGING_PF_SHMEM_FORK_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_FORK_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_FORK_CSRCFILE)

$(PK_PAGING_PF_SHMEM_FORK_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_SHMEM_FORK_sh,$(srctree))
