PK_PAGING_ATPR_CONSULT_NAME        := BiscuitOS-PAGING-ATPR-CONSULT
PK_PAGING_ATPR_CONSULT_VERSION     := $(CONFIG_PK_PAGING_ATPR_CONSULT_VERSION)
PK_PAGING_ATPR_CONSULT_SITE        := $(CONFIG_PK_PAGING_ATPR_CONSULT_URL)
PK_PAGING_ATPR_CONSULT_GITHUB      := $(CONFIG_PK_PAGING_ATPR_CONSULT_GITHUB)
PK_PAGING_ATPR_CONSULT_SUBNAME     :=
PK_PAGING_ATPR_CONSULT_BUILDARCH   := $(CONFIG_PK_PAGING_ATPR_CONSULT_BUILDARCH)
PK_PAGING_ATPR_CONSULT_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-ATPR-CONSULT
PK_PAGING_ATPR_CONSULT_PATCH       := $(PK_PAGING_ATPR_CONSULT_DIR)/patch
PK_PAGING_ATPR_CONSULT_BSBIT       := BiscuitOS-PAGING-ATPR-CONSULT-$(PK_PAGING_ATPR_CONSULT_VERSION).bsbit
PK_PAGING_ATPR_CONSULT_OUT         :=
PK_PAGING_ATPR_CONSULT_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_ATPR_CONSULT_TAR         := $(CONFIG_PK_PAGING_ATPR_CONSULT_TAR)
PK_PAGING_ATPR_CONSULT_PACKAGE     :=
PK_PAGING_ATPR_CONSULT_CONFIG      := $(CONFIG_PK_PAGING_ATPR_CONSULT_CONFIG)
SUB_TARGET                  += $(PK_PAGING_ATPR_CONSULT_DIR)/build

PK_PAGING_ATPR_CONSULT_sh = $(CONFIG_SHELL) $(PK_PAGING_ATPR_CONSULT_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_ATPR_CONSULT_NAME)X \
                 $(PK_PAGING_ATPR_CONSULT_VERSION)X \
                 $(PK_PAGING_ATPR_CONSULT_SITE)X \
                 $(PK_PAGING_ATPR_CONSULT_GITHUB)X \
                 $(PK_PAGING_ATPR_CONSULT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_ATPR_CONSULT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_ATPR_CONSULT_SUBNAME)X \
                 $(CONFIG_PK_PAGING_ATPR_CONSULT_CONFIG)X \
                 $(CONFIG_PK_PAGING_ATPR_CONSULT_LIBPATH)X \
                 $(CONFIG_PK_PAGING_ATPR_CONSULT_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_ATPR_CONSULT_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_ATPR_CONSULT_CFLAGS)X \
                 $(CONFIG_PK_PAGING_ATPR_CONSULT_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_ATPR_CONSULT_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_ATPR_CONSULT_ASFLAGS)X \
                 $(PK_PAGING_ATPR_CONSULT_BSBIT)X \
                 $(PK_PAGING_ATPR_CONSULT_BUILDARCH)X \
		 $(PK_PAGING_ATPR_CONSULT_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_ATPR_CONSULT_SRCFILE)X \
                 $(CONFIG_PK_PAGING_ATPR_CONSULT_CSRCFILE)

$(PK_PAGING_ATPR_CONSULT_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_ATPR_CONSULT_sh,$(srctree))
