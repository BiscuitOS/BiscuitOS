PK_PAGING_VM_ZERO_COPY_DMA_NAME        := BiscuitOS-PAGING-VM-ZERO-COPY-DMA
PK_PAGING_VM_ZERO_COPY_DMA_VERSION     := $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_VERSION)
PK_PAGING_VM_ZERO_COPY_DMA_SITE        := $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_URL)
PK_PAGING_VM_ZERO_COPY_DMA_GITHUB      := $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_GITHUB)
PK_PAGING_VM_ZERO_COPY_DMA_SUBNAME     :=
PK_PAGING_VM_ZERO_COPY_DMA_BUILDARCH   := $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_BUILDARCH)
PK_PAGING_VM_ZERO_COPY_DMA_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-VM-ZERO-COPY-DMA
PK_PAGING_VM_ZERO_COPY_DMA_PATCH       := $(PK_PAGING_VM_ZERO_COPY_DMA_DIR)/patch
PK_PAGING_VM_ZERO_COPY_DMA_BSBIT       := BiscuitOS-PAGING-VM-ZERO-COPY-DMA-$(PK_PAGING_VM_ZERO_COPY_DMA_VERSION).bsbit
PK_PAGING_VM_ZERO_COPY_DMA_OUT         :=
PK_PAGING_VM_ZERO_COPY_DMA_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_VM_ZERO_COPY_DMA_TAR         := $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_TAR)
PK_PAGING_VM_ZERO_COPY_DMA_PACKAGE     :=
PK_PAGING_VM_ZERO_COPY_DMA_CONFIG      := $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_CONFIG)
SUB_TARGET                  += $(PK_PAGING_VM_ZERO_COPY_DMA_DIR)/build

PK_PAGING_VM_ZERO_COPY_DMA_sh = $(CONFIG_SHELL) $(PK_PAGING_VM_ZERO_COPY_DMA_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_VM_ZERO_COPY_DMA_NAME)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMA_VERSION)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMA_SITE)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMA_GITHUB)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMA_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMA_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMA_SUBNAME)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_CONFIG)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_LIBPATH)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_CFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_ASFLAGS)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMA_BSBIT)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMA_BUILDARCH)X \
		 $(PK_PAGING_VM_ZERO_COPY_DMA_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_SRCFILE)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMA_CSRCFILE)

$(PK_PAGING_VM_ZERO_COPY_DMA_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_VM_ZERO_COPY_DMA_sh,$(srctree))
