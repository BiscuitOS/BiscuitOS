PK_PAGING_PF_ANON_KSM_NAME        := BiscuitOS-PAGING-PF-ANON-KSM
PK_PAGING_PF_ANON_KSM_VERSION     := $(CONFIG_PK_PAGING_PF_ANON_KSM_VERSION)
PK_PAGING_PF_ANON_KSM_SITE        := $(CONFIG_PK_PAGING_PF_ANON_KSM_URL)
PK_PAGING_PF_ANON_KSM_GITHUB      := $(CONFIG_PK_PAGING_PF_ANON_KSM_GITHUB)
PK_PAGING_PF_ANON_KSM_SUBNAME     :=
PK_PAGING_PF_ANON_KSM_BUILDARCH   := $(CONFIG_PK_PAGING_PF_ANON_KSM_BUILDARCH)
PK_PAGING_PF_ANON_KSM_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-ANON-KSM
PK_PAGING_PF_ANON_KSM_PATCH       := $(PK_PAGING_PF_ANON_KSM_DIR)/patch
PK_PAGING_PF_ANON_KSM_BSBIT       := BiscuitOS-PAGING-PF-ANON-KSM-$(PK_PAGING_PF_ANON_KSM_VERSION).bsbit
PK_PAGING_PF_ANON_KSM_OUT         :=
PK_PAGING_PF_ANON_KSM_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_ANON_KSM_TAR         := $(CONFIG_PK_PAGING_PF_ANON_KSM_TAR)
PK_PAGING_PF_ANON_KSM_PACKAGE     :=
PK_PAGING_PF_ANON_KSM_CONFIG      := $(CONFIG_PK_PAGING_PF_ANON_KSM_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_ANON_KSM_DIR)/build

PK_PAGING_PF_ANON_KSM_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_ANON_KSM_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_ANON_KSM_NAME)X \
                 $(PK_PAGING_PF_ANON_KSM_VERSION)X \
                 $(PK_PAGING_PF_ANON_KSM_SITE)X \
                 $(PK_PAGING_PF_ANON_KSM_GITHUB)X \
                 $(PK_PAGING_PF_ANON_KSM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_ANON_KSM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_ANON_KSM_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_ANON_KSM_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ANON_KSM_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_ANON_KSM_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_KSM_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ANON_KSM_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_KSM_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_KSM_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_KSM_ASFLAGS)X \
                 $(PK_PAGING_PF_ANON_KSM_BSBIT)X \
                 $(PK_PAGING_PF_ANON_KSM_BUILDARCH)X \
		 $(PK_PAGING_PF_ANON_KSM_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ANON_KSM_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ANON_KSM_CSRCFILE)

$(PK_PAGING_PF_ANON_KSM_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_ANON_KSM_sh,$(srctree))
