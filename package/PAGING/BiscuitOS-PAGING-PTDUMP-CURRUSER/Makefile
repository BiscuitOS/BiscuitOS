PK_PAGING_PTDUMP_CURRUSER_NAME        := BiscuitOS-PAGING-PTDUMP-CURRUSER
PK_PAGING_PTDUMP_CURRUSER_VERSION     := $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_VERSION)
PK_PAGING_PTDUMP_CURRUSER_SITE        := $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_URL)
PK_PAGING_PTDUMP_CURRUSER_GITHUB      := $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_GITHUB)
PK_PAGING_PTDUMP_CURRUSER_SUBNAME     :=
PK_PAGING_PTDUMP_CURRUSER_BUILDARCH   := $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_BUILDARCH)
PK_PAGING_PTDUMP_CURRUSER_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PTDUMP-CURRUSER
PK_PAGING_PTDUMP_CURRUSER_PATCH       := $(PK_PAGING_PTDUMP_CURRUSER_DIR)/patch
PK_PAGING_PTDUMP_CURRUSER_BSBIT       := BiscuitOS-PAGING-PTDUMP-CURRUSER-$(PK_PAGING_PTDUMP_CURRUSER_VERSION).bsbit
PK_PAGING_PTDUMP_CURRUSER_OUT         :=
PK_PAGING_PTDUMP_CURRUSER_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PTDUMP_CURRUSER_TAR         := $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_TAR)
PK_PAGING_PTDUMP_CURRUSER_PACKAGE     :=
PK_PAGING_PTDUMP_CURRUSER_CONFIG      := $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PTDUMP_CURRUSER_DIR)/build

PK_PAGING_PTDUMP_CURRUSER_sh = $(CONFIG_SHELL) $(PK_PAGING_PTDUMP_CURRUSER_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PTDUMP_CURRUSER_NAME)X \
                 $(PK_PAGING_PTDUMP_CURRUSER_VERSION)X \
                 $(PK_PAGING_PTDUMP_CURRUSER_SITE)X \
                 $(PK_PAGING_PTDUMP_CURRUSER_GITHUB)X \
                 $(PK_PAGING_PTDUMP_CURRUSER_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PTDUMP_CURRUSER_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PTDUMP_CURRUSER_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_CONFIG)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_ASFLAGS)X \
                 $(PK_PAGING_PTDUMP_CURRUSER_BSBIT)X \
                 $(PK_PAGING_PTDUMP_CURRUSER_BUILDARCH)X \
		 $(PK_PAGING_PTDUMP_CURRUSER_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRUSER_CSRCFILE)

$(PK_PAGING_PTDUMP_CURRUSER_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PTDUMP_CURRUSER_sh,$(srctree))
