PK_PAGING_RMAP_ANON_NAME        := BiscuitOS-PAGING-RMAP-ANON
PK_PAGING_RMAP_ANON_VERSION     := $(CONFIG_PK_PAGING_RMAP_ANON_VERSION)
PK_PAGING_RMAP_ANON_SITE        := $(CONFIG_PK_PAGING_RMAP_ANON_URL)
PK_PAGING_RMAP_ANON_GITHUB      := $(CONFIG_PK_PAGING_RMAP_ANON_GITHUB)
PK_PAGING_RMAP_ANON_SUBNAME     :=
PK_PAGING_RMAP_ANON_BUILDARCH   := $(CONFIG_PK_PAGING_RMAP_ANON_BUILDARCH)
PK_PAGING_RMAP_ANON_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-RMAP-ANON
PK_PAGING_RMAP_ANON_PATCH       := $(PK_PAGING_RMAP_ANON_DIR)/patch
PK_PAGING_RMAP_ANON_BSBIT       := BiscuitOS-PAGING-RMAP-ANON-$(PK_PAGING_RMAP_ANON_VERSION).bsbit
PK_PAGING_RMAP_ANON_OUT         :=
PK_PAGING_RMAP_ANON_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_RMAP_ANON_TAR         := $(CONFIG_PK_PAGING_RMAP_ANON_TAR)
PK_PAGING_RMAP_ANON_PACKAGE     :=
PK_PAGING_RMAP_ANON_CONFIG      := $(CONFIG_PK_PAGING_RMAP_ANON_CONFIG)
SUB_TARGET                  += $(PK_PAGING_RMAP_ANON_DIR)/build

PK_PAGING_RMAP_ANON_sh = $(CONFIG_SHELL) $(PK_PAGING_RMAP_ANON_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_RMAP_ANON_NAME)X \
                 $(PK_PAGING_RMAP_ANON_VERSION)X \
                 $(PK_PAGING_RMAP_ANON_SITE)X \
                 $(PK_PAGING_RMAP_ANON_GITHUB)X \
                 $(PK_PAGING_RMAP_ANON_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_RMAP_ANON_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_RMAP_ANON_SUBNAME)X \
                 $(CONFIG_PK_PAGING_RMAP_ANON_CONFIG)X \
                 $(CONFIG_PK_PAGING_RMAP_ANON_LIBPATH)X \
                 $(CONFIG_PK_PAGING_RMAP_ANON_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_RMAP_ANON_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_RMAP_ANON_CFLAGS)X \
                 $(CONFIG_PK_PAGING_RMAP_ANON_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_RMAP_ANON_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_RMAP_ANON_ASFLAGS)X \
                 $(PK_PAGING_RMAP_ANON_BSBIT)X \
                 $(PK_PAGING_RMAP_ANON_BUILDARCH)X \
		 $(PK_PAGING_RMAP_ANON_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_RMAP_ANON_SRCFILE)X \
                 $(CONFIG_PK_PAGING_RMAP_ANON_CSRCFILE)

$(PK_PAGING_RMAP_ANON_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_RMAP_ANON_sh,$(srctree))
