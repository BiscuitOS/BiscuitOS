PK_PAGING_PF_SHMEM_USOCK_SITE        := $(CONFIG_PK_PAGING_PF_SHMEM_USOCK_URL)
PK_PAGING_PF_SHMEM_USOCK_GITHUB      := $(CONFIG_PK_PAGING_PF_SHMEM_USOCK_GITHUB)
PK_PAGING_PF_SHMEM_USOCK_SUBNAME     :=
PK_PAGING_PF_SHMEM_USOCK_BUILDARCH   := $(CONFIG_PK_PAGING_PF_SHMEM_USOCK_BUILDARCH)
PK_PAGING_PF_SHMEM_USOCK_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-SHMEM-USOCK
PK_PAGING_PF_SHMEM_USOCK_PATCH       := $(PK_PAGING_PF_SHMEM_USOCK_DIR)/patch
PK_PAGING_PF_SHMEM_USOCK_BSBIT       := BiscuitOS-PAGING-PF-SHMEM-USOCK-$(PK_PAGING_PF_SHMEM_USOCK_VERSION).bsbit
PK_PAGING_PF_SHMEM_USOCK_OUT         :=
PK_PAGING_PF_SHMEM_USOCK_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_SHMEM_USOCK_TAR         := $(CONFIG_PK_PAGING_PF_SHMEM_USOCK_TAR)
PK_PAGING_PF_SHMEM_USOCK_PACKAGE     :=
PK_PAGING_PF_SHMEM_USOCK_CONFIG      := $(CONFIG_PK_PAGING_PF_SHMEM_USOCK_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_SHMEM_USOCK_DIR)/build

PK_PAGING_PF_SHMEM_USOCK_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_SHMEM_USOCK_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_SHMEM_USOCK_NAME)X \
                 $(PK_PAGING_PF_SHMEM_USOCK_VERSION)X \
                 $(PK_PAGING_PF_SHMEM_USOCK_SITE)X \
                 $(PK_PAGING_PF_SHMEM_USOCK_GITHUB)X \
                 $(PK_PAGING_PF_SHMEM_USOCK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_SHMEM_USOCK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_SHMEM_USOCK_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_USOCK_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_USOCK_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_USOCK_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_USOCK_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_USOCK_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_USOCK_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_USOCK_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_USOCK_ASFLAGS)X \
                 $(PK_PAGING_PF_SHMEM_USOCK_BSBIT)X \
                 $(PK_PAGING_PF_SHMEM_USOCK_BUILDARCH)X \
		 $(PK_PAGING_PF_SHMEM_USOCK_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_USOCK_SRCFILE)X \
