PK_PAGING_ATPR_PREALLOC_NAME        := BiscuitOS-PAGING-ATPR-PREALLOC
PK_PAGING_ATPR_PREALLOC_VERSION     := $(CONFIG_PK_PAGING_ATPR_PREALLOC_VERSION)
PK_PAGING_ATPR_PREALLOC_SITE        := $(CONFIG_PK_PAGING_ATPR_PREALLOC_URL)
PK_PAGING_ATPR_PREALLOC_GITHUB      := $(CONFIG_PK_PAGING_ATPR_PREALLOC_GITHUB)
PK_PAGING_ATPR_PREALLOC_SUBNAME     :=
PK_PAGING_ATPR_PREALLOC_BUILDARCH   := $(CONFIG_PK_PAGING_ATPR_PREALLOC_BUILDARCH)
PK_PAGING_ATPR_PREALLOC_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-ATPR-PREALLOC
PK_PAGING_ATPR_PREALLOC_PATCH       := $(PK_PAGING_ATPR_PREALLOC_DIR)/patch
PK_PAGING_ATPR_PREALLOC_BSBIT       := BiscuitOS-PAGING-ATPR-PREALLOC-$(PK_PAGING_ATPR_PREALLOC_VERSION).bsbit
PK_PAGING_ATPR_PREALLOC_OUT         :=
PK_PAGING_ATPR_PREALLOC_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_ATPR_PREALLOC_TAR         := $(CONFIG_PK_PAGING_ATPR_PREALLOC_TAR)
PK_PAGING_ATPR_PREALLOC_PACKAGE     :=
PK_PAGING_ATPR_PREALLOC_CONFIG      := $(CONFIG_PK_PAGING_ATPR_PREALLOC_CONFIG)
SUB_TARGET                  += $(PK_PAGING_ATPR_PREALLOC_DIR)/build

PK_PAGING_ATPR_PREALLOC_sh = $(CONFIG_SHELL) $(PK_PAGING_ATPR_PREALLOC_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_ATPR_PREALLOC_NAME)X \
                 $(PK_PAGING_ATPR_PREALLOC_VERSION)X \
                 $(PK_PAGING_ATPR_PREALLOC_SITE)X \
                 $(PK_PAGING_ATPR_PREALLOC_GITHUB)X \
                 $(PK_PAGING_ATPR_PREALLOC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_ATPR_PREALLOC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_ATPR_PREALLOC_SUBNAME)X \
                 $(CONFIG_PK_PAGING_ATPR_PREALLOC_CONFIG)X \
                 $(CONFIG_PK_PAGING_ATPR_PREALLOC_LIBPATH)X \
                 $(CONFIG_PK_PAGING_ATPR_PREALLOC_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_ATPR_PREALLOC_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_ATPR_PREALLOC_CFLAGS)X \
                 $(CONFIG_PK_PAGING_ATPR_PREALLOC_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_ATPR_PREALLOC_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_ATPR_PREALLOC_ASFLAGS)X \
                 $(PK_PAGING_ATPR_PREALLOC_BSBIT)X \
                 $(PK_PAGING_ATPR_PREALLOC_BUILDARCH)X \
		 $(PK_PAGING_ATPR_PREALLOC_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_ATPR_PREALLOC_SRCFILE)X \
                 $(CONFIG_PK_PAGING_ATPR_PREALLOC_CSRCFILE)

$(PK_PAGING_ATPR_PREALLOC_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_ATPR_PREALLOC_sh,$(srctree))
