PK_PAGING_PAGEWALK_ADD_NAME        := BiscuitOS-PAGING-PageWalk-ADD
PK_PAGING_PAGEWALK_ADD_VERSION     := $(CONFIG_PK_PAGING_PAGEWALK_ADD_VERSION)
PK_PAGING_PAGEWALK_ADD_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_ADD_URL)
PK_PAGING_PAGEWALK_ADD_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_ADD_GITHUB)
PK_PAGING_PAGEWALK_ADD_SUBNAME     :=
PK_PAGING_PAGEWALK_ADD_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_ADD_BUILDARCH)
PK_PAGING_PAGEWALK_ADD_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-ADD
PK_PAGING_PAGEWALK_ADD_PATCH       := $(PK_PAGING_PAGEWALK_ADD_DIR)/patch
PK_PAGING_PAGEWALK_ADD_BSBIT       := BiscuitOS-PAGING-PageWalk-ADD-$(PK_PAGING_PAGEWALK_ADD_VERSION).bsbit
PK_PAGING_PAGEWALK_ADD_OUT         :=
PK_PAGING_PAGEWALK_ADD_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_ADD_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_ADD_TAR)
PK_PAGING_PAGEWALK_ADD_PACKAGE     :=
PK_PAGING_PAGEWALK_ADD_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_ADD_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_ADD_DIR)/build

PK_PAGING_PAGEWALK_ADD_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_ADD_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_ADD_NAME)X \
                 $(PK_PAGING_PAGEWALK_ADD_VERSION)X \
                 $(PK_PAGING_PAGEWALK_ADD_SITE)X \
                 $(PK_PAGING_PAGEWALK_ADD_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_ADD_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_ADD_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_ADD_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_ADD_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_ADD_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_ADD_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_ADD_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_ADD_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_ADD_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_ADD_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_ADD_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_ADD_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_ADD_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_ADD_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_ADD_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_ADD_CSRCFILE)

$(PK_PAGING_PAGEWALK_ADD_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_ADD_sh,$(srctree))
