PK_PAGING_PT_U2PT_PAGE_USER_GITHUB      := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_USER_GITHUB)
PK_PAGING_PT_U2PT_PAGE_USER_SUBNAME     :=
PK_PAGING_PT_U2PT_PAGE_USER_BUILDARCH   := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_USER_BUILDARCH)
PK_PAGING_PT_U2PT_PAGE_USER_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PT-U2PT-PAGE-USER
PK_PAGING_PT_U2PT_PAGE_USER_PATCH       := $(PK_PAGING_PT_U2PT_PAGE_USER_DIR)/patch
PK_PAGING_PT_U2PT_PAGE_USER_BSBIT       := BiscuitOS-PAGING-PT-U2PT-PAGE-USER-$(PK_PAGING_PT_U2PT_PAGE_USER_VERSION).bsbit
PK_PAGING_PT_U2PT_PAGE_USER_OUT         :=
