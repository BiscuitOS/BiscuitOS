                 $(PK_PAGING_PT_U2PT_PAGE_USER_SITE)X \
                 $(PK_PAGING_PT_U2PT_PAGE_USER_GITHUB)X \
                 $(PK_PAGING_PT_U2PT_PAGE_USER_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_U2PT_PAGE_USER_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_U2PT_PAGE_USER_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_USER_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_USER_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_USER_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_USER_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_USER_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_USER_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_USER_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_USER_ASFLAGS)X \
                 $(PK_PAGING_PT_U2PT_PAGE_USER_BSBIT)X \
                 $(PK_PAGING_PT_U2PT_PAGE_USER_BUILDARCH)X \
		 $(PK_PAGING_PT_U2PT_PAGE_USER_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_USER_SRCFILE)X \
