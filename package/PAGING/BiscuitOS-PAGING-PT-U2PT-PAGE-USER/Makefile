PK_PAGING_PT_U2PT_PAGE_USER_TAR         := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_USER_TAR)
PK_PAGING_PT_U2PT_PAGE_USER_PACKAGE     :=
PK_PAGING_PT_U2PT_PAGE_USER_CONFIG      := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_USER_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_U2PT_PAGE_USER_DIR)/build

PK_PAGING_PT_U2PT_PAGE_USER_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_U2PT_PAGE_USER_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PT_U2PT_PAGE_USER_NAME)X \
