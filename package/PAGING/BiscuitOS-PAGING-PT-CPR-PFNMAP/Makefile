PK_PAGING_PT_CPR_PFNMAP_NAME        := BiscuitOS-PAGING-PT-CPR-PFNMAP
PK_PAGING_PT_CPR_PFNMAP_VERSION     := $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_VERSION)
PK_PAGING_PT_CPR_PFNMAP_SITE        := $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_URL)
PK_PAGING_PT_CPR_PFNMAP_GITHUB      := $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_GITHUB)
PK_PAGING_PT_CPR_PFNMAP_SUBNAME     :=
PK_PAGING_PT_CPR_PFNMAP_BUILDARCH   := $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_BUILDARCH)
PK_PAGING_PT_CPR_PFNMAP_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PT-CPR-PFNMAP
PK_PAGING_PT_CPR_PFNMAP_PATCH       := $(PK_PAGING_PT_CPR_PFNMAP_DIR)/patch
PK_PAGING_PT_CPR_PFNMAP_BSBIT       := BiscuitOS-PAGING-PT-CPR-PFNMAP-$(PK_PAGING_PT_CPR_PFNMAP_VERSION).bsbit
PK_PAGING_PT_CPR_PFNMAP_OUT         :=
PK_PAGING_PT_CPR_PFNMAP_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_PT_CPR_PFNMAP_TAR         := $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_TAR)
PK_PAGING_PT_CPR_PFNMAP_PACKAGE     :=
PK_PAGING_PT_CPR_PFNMAP_CONFIG      := $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_CPR_PFNMAP_DIR)/build

PK_PAGING_PT_CPR_PFNMAP_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_CPR_PFNMAP_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PT_CPR_PFNMAP_NAME)X \
                 $(PK_PAGING_PT_CPR_PFNMAP_VERSION)X \
                 $(PK_PAGING_PT_CPR_PFNMAP_SITE)X \
                 $(PK_PAGING_PT_CPR_PFNMAP_GITHUB)X \
                 $(PK_PAGING_PT_CPR_PFNMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_CPR_PFNMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_CPR_PFNMAP_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_ASFLAGS)X \
                 $(PK_PAGING_PT_CPR_PFNMAP_BSBIT)X \
                 $(PK_PAGING_PT_CPR_PFNMAP_BUILDARCH)X \
		 $(PK_PAGING_PT_CPR_PFNMAP_CSRCPFNMAP)X \
                 $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_SRCPFNMAP)X \
                 $(CONFIG_PK_PAGING_PT_CPR_PFNMAP_CSRCPFNMAP)

$(PK_PAGING_PT_CPR_PFNMAP_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PT_CPR_PFNMAP_sh,$(srctree))
