PK_PAGING_PT_U2PT_PAGE_PRESENT_NAME        := BiscuitOS-PAGING-PT-U2PT-PAGE-PRESENT
PK_PAGING_PT_U2PT_PAGE_PRESENT_VERSION     := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_VERSION)
PK_PAGING_PT_U2PT_PAGE_PRESENT_SITE        := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_URL)
PK_PAGING_PT_U2PT_PAGE_PRESENT_GITHUB      := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_GITHUB)
PK_PAGING_PT_U2PT_PAGE_PRESENT_SUBNAME     :=
PK_PAGING_PT_U2PT_PAGE_PRESENT_BUILDARCH   := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_BUILDARCH)
PK_PAGING_PT_U2PT_PAGE_PRESENT_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PT-U2PT-PAGE-PRESENT
PK_PAGING_PT_U2PT_PAGE_PRESENT_PATCH       := $(PK_PAGING_PT_U2PT_PAGE_PRESENT_DIR)/patch
PK_PAGING_PT_U2PT_PAGE_PRESENT_BSBIT       := BiscuitOS-PAGING-PT-U2PT-PAGE-PRESENT-$(PK_PAGING_PT_U2PT_PAGE_PRESENT_VERSION).bsbit
PK_PAGING_PT_U2PT_PAGE_PRESENT_OUT         :=
PK_PAGING_PT_U2PT_PAGE_PRESENT_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PT_U2PT_PAGE_PRESENT_TAR         := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_TAR)
PK_PAGING_PT_U2PT_PAGE_PRESENT_PACKAGE     :=
PK_PAGING_PT_U2PT_PAGE_PRESENT_CONFIG      := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_U2PT_PAGE_PRESENT_DIR)/build

PK_PAGING_PT_U2PT_PAGE_PRESENT_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_U2PT_PAGE_PRESENT_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PT_U2PT_PAGE_PRESENT_NAME)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PRESENT_VERSION)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PRESENT_SITE)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PRESENT_GITHUB)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PRESENT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PRESENT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PRESENT_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_ASFLAGS)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PRESENT_BSBIT)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PRESENT_BUILDARCH)X \
		 $(PK_PAGING_PT_U2PT_PAGE_PRESENT_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PRESENT_CSRCFILE)

$(PK_PAGING_PT_U2PT_PAGE_PRESENT_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PT_U2PT_PAGE_PRESENT_sh,$(srctree))
