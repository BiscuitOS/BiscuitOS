# BiscuitOS-PAGING-LOOKUP-ADDR-EARLY
#
################################################
PK_PAGING_LOOKUP_ADDR_EARLY_NAME        := BiscuitOS-PAGING-LOOKUP-ADDR-EARLY
PK_PAGING_LOOKUP_ADDR_EARLY_VERSION     := $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_VERSION)
PK_PAGING_LOOKUP_ADDR_EARLY_SITE        := $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_URL)
PK_PAGING_LOOKUP_ADDR_EARLY_GITHUB      := $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_GITHUB)
PK_PAGING_LOOKUP_ADDR_EARLY_SUBNAME     :=
PK_PAGING_LOOKUP_ADDR_EARLY_BUILDARCH   := $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_BUILDARCH)
PK_PAGING_LOOKUP_ADDR_EARLY_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-LOOKUP-ADDR-EARLY
PK_PAGING_LOOKUP_ADDR_EARLY_PATCH       := $(PK_PAGING_LOOKUP_ADDR_EARLY_DIR)/patch
PK_PAGING_LOOKUP_ADDR_EARLY_BSBIT       := BiscuitOS-PAGING-LOOKUP-ADDR-EARLY-$(PK_PAGING_LOOKUP_ADDR_EARLY_VERSION).bsbit
PK_PAGING_LOOKUP_ADDR_EARLY_OUT         :=
PK_PAGING_LOOKUP_ADDR_EARLY_SCRIPTS     := $(srctree)/scripts/package/package-kernel-any.sh
PK_PAGING_LOOKUP_ADDR_EARLY_TAR         := $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_TAR)
PK_PAGING_LOOKUP_ADDR_EARLY_PACKAGE     :=
PK_PAGING_LOOKUP_ADDR_EARLY_CONFIG      := $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_CONFIG)
SUB_TARGET                  += $(PK_PAGING_LOOKUP_ADDR_EARLY_DIR)/build

PK_PAGING_LOOKUP_ADDR_EARLY_sh = $(CONFIG_SHELL) $(PK_PAGING_LOOKUP_ADDR_EARLY_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_LOOKUP_ADDR_EARLY_NAME)X \
                 $(PK_PAGING_LOOKUP_ADDR_EARLY_VERSION)X \
                 $(PK_PAGING_LOOKUP_ADDR_EARLY_SITE)X \
                 $(PK_PAGING_LOOKUP_ADDR_EARLY_GITHUB)X \
                 $(PK_PAGING_LOOKUP_ADDR_EARLY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_LOOKUP_ADDR_EARLY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_LOOKUP_ADDR_EARLY_SUBNAME)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_CONFIG)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_LIBPATH)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_CFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_ASFLAGS)X \
                 $(PK_PAGING_LOOKUP_ADDR_EARLY_BSBIT)X \
                 $(PK_PAGING_LOOKUP_ADDR_EARLY_BUILDARCH)X \
		 $(PK_PAGING_LOOKUP_ADDR_EARLY_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_SRCFILE)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_EARLY_CSRCFILE)

$(PK_PAGING_LOOKUP_ADDR_EARLY_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_LOOKUP_ADDR_EARLY_sh,$(srctree))
