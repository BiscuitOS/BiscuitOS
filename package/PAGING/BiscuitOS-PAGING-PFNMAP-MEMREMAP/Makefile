# BiscuitOS-PAGING-PFNMAP-MEMREMAP
#
################################################
PK_PAGING_PFNMAP_MEMREMAP_NAME        := BiscuitOS-PAGING-PFNMAP-MEMREMAP
PK_PAGING_PFNMAP_MEMREMAP_VERSION     := $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_VERSION)
PK_PAGING_PFNMAP_MEMREMAP_SITE        := $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_URL)
PK_PAGING_PFNMAP_MEMREMAP_GITHUB      := $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_GITHUB)
PK_PAGING_PFNMAP_MEMREMAP_SUBNAME     :=
PK_PAGING_PFNMAP_MEMREMAP_BUILDARCH   := $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_BUILDARCH)
PK_PAGING_PFNMAP_MEMREMAP_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PFNMAP-MEMREMAP
PK_PAGING_PFNMAP_MEMREMAP_PATCH       := $(PK_PAGING_PFNMAP_MEMREMAP_DIR)/patch
PK_PAGING_PFNMAP_MEMREMAP_BSBIT       := BiscuitOS-PAGING-PFNMAP-MEMREMAP-$(PK_PAGING_PFNMAP_MEMREMAP_VERSION).bsbit
PK_PAGING_PFNMAP_MEMREMAP_OUT         :=
PK_PAGING_PFNMAP_MEMREMAP_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_PFNMAP_MEMREMAP_TAR         := $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_TAR)
PK_PAGING_PFNMAP_MEMREMAP_PACKAGE     :=
PK_PAGING_PFNMAP_MEMREMAP_CONFIG      := $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PFNMAP_MEMREMAP_DIR)/build

PK_PAGING_PFNMAP_MEMREMAP_sh = $(CONFIG_SHELL) $(PK_PAGING_PFNMAP_MEMREMAP_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PFNMAP_MEMREMAP_NAME)X \
                 $(PK_PAGING_PFNMAP_MEMREMAP_VERSION)X \
                 $(PK_PAGING_PFNMAP_MEMREMAP_SITE)X \
                 $(PK_PAGING_PFNMAP_MEMREMAP_GITHUB)X \
                 $(PK_PAGING_PFNMAP_MEMREMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PFNMAP_MEMREMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PFNMAP_MEMREMAP_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_CONFIG)X \
                 $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_ASFLAGS)X \
                 $(PK_PAGING_PFNMAP_MEMREMAP_BSBIT)X \
                 $(PK_PAGING_PFNMAP_MEMREMAP_BUILDARCH)X \
		 $(PK_PAGING_PFNMAP_MEMREMAP_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PFNMAP_MEMREMAP_CSRCFILE)

$(PK_PAGING_PFNMAP_MEMREMAP_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PFNMAP_MEMREMAP_sh,$(srctree))
