PK_PAGING_PF_ANON_HEAP_NAME        := BiscuitOS-PAGING-PF-ANON-HEAP
PK_PAGING_PF_ANON_HEAP_VERSION     := $(CONFIG_PK_PAGING_PF_ANON_HEAP_VERSION)
PK_PAGING_PF_ANON_HEAP_SITE        := $(CONFIG_PK_PAGING_PF_ANON_HEAP_URL)
PK_PAGING_PF_ANON_HEAP_GITHUB      := $(CONFIG_PK_PAGING_PF_ANON_HEAP_GITHUB)
PK_PAGING_PF_ANON_HEAP_SUBNAME     :=
PK_PAGING_PF_ANON_HEAP_BUILDARCH   := $(CONFIG_PK_PAGING_PF_ANON_HEAP_BUILDARCH)
PK_PAGING_PF_ANON_HEAP_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-ANON-HEAP
PK_PAGING_PF_ANON_HEAP_PATCH       := $(PK_PAGING_PF_ANON_HEAP_DIR)/patch
PK_PAGING_PF_ANON_HEAP_BSBIT       := BiscuitOS-PAGING-PF-ANON-HEAP-$(PK_PAGING_PF_ANON_HEAP_VERSION).bsbit
PK_PAGING_PF_ANON_HEAP_OUT         :=
PK_PAGING_PF_ANON_HEAP_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_ANON_HEAP_TAR         := $(CONFIG_PK_PAGING_PF_ANON_HEAP_TAR)
PK_PAGING_PF_ANON_HEAP_PACKAGE     :=
PK_PAGING_PF_ANON_HEAP_CONFIG      := $(CONFIG_PK_PAGING_PF_ANON_HEAP_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_ANON_HEAP_DIR)/build

PK_PAGING_PF_ANON_HEAP_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_ANON_HEAP_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_ANON_HEAP_NAME)X \
                 $(PK_PAGING_PF_ANON_HEAP_VERSION)X \
                 $(PK_PAGING_PF_ANON_HEAP_SITE)X \
                 $(PK_PAGING_PF_ANON_HEAP_GITHUB)X \
                 $(PK_PAGING_PF_ANON_HEAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_ANON_HEAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_ANON_HEAP_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_ANON_HEAP_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ANON_HEAP_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_ANON_HEAP_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_HEAP_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ANON_HEAP_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_HEAP_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_HEAP_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_HEAP_ASFLAGS)X \
                 $(PK_PAGING_PF_ANON_HEAP_BSBIT)X \
                 $(PK_PAGING_PF_ANON_HEAP_BUILDARCH)X \
		 $(PK_PAGING_PF_ANON_HEAP_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ANON_HEAP_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ANON_HEAP_CSRCFILE)

$(PK_PAGING_PF_ANON_HEAP_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_ANON_HEAP_sh,$(srctree))
