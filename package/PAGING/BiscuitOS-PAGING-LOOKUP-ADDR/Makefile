# BiscuitOS-PAGING-LOOKUP-ADDR
#
################################################
PK_PAGING_LOOKUP_ADDR_NAME        := BiscuitOS-PAGING-LOOKUP-ADDR
PK_PAGING_LOOKUP_ADDR_VERSION     := $(CONFIG_PK_PAGING_LOOKUP_ADDR_VERSION)
PK_PAGING_LOOKUP_ADDR_SITE        := $(CONFIG_PK_PAGING_LOOKUP_ADDR_URL)
PK_PAGING_LOOKUP_ADDR_GITHUB      := $(CONFIG_PK_PAGING_LOOKUP_ADDR_GITHUB)
PK_PAGING_LOOKUP_ADDR_SUBNAME     :=
PK_PAGING_LOOKUP_ADDR_BUILDARCH   := $(CONFIG_PK_PAGING_LOOKUP_ADDR_BUILDARCH)
PK_PAGING_LOOKUP_ADDR_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-LOOKUP-ADDR
PK_PAGING_LOOKUP_ADDR_PATCH       := $(PK_PAGING_LOOKUP_ADDR_DIR)/patch
PK_PAGING_LOOKUP_ADDR_BSBIT       := BiscuitOS-PAGING-LOOKUP-ADDR-$(PK_PAGING_LOOKUP_ADDR_VERSION).bsbit
PK_PAGING_LOOKUP_ADDR_OUT         :=
PK_PAGING_LOOKUP_ADDR_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_LOOKUP_ADDR_TAR         := $(CONFIG_PK_PAGING_LOOKUP_ADDR_TAR)
PK_PAGING_LOOKUP_ADDR_PACKAGE     :=
PK_PAGING_LOOKUP_ADDR_CONFIG      := $(CONFIG_PK_PAGING_LOOKUP_ADDR_CONFIG)
SUB_TARGET                  += $(PK_PAGING_LOOKUP_ADDR_DIR)/build

PK_PAGING_LOOKUP_ADDR_sh = $(CONFIG_SHELL) $(PK_PAGING_LOOKUP_ADDR_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_LOOKUP_ADDR_NAME)X \
                 $(PK_PAGING_LOOKUP_ADDR_VERSION)X \
                 $(PK_PAGING_LOOKUP_ADDR_SITE)X \
                 $(PK_PAGING_LOOKUP_ADDR_GITHUB)X \
                 $(PK_PAGING_LOOKUP_ADDR_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_LOOKUP_ADDR_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_LOOKUP_ADDR_SUBNAME)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_CONFIG)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_LIBPATH)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_CFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_ASFLAGS)X \
                 $(PK_PAGING_LOOKUP_ADDR_BSBIT)X \
                 $(PK_PAGING_LOOKUP_ADDR_BUILDARCH)X \
		 $(PK_PAGING_LOOKUP_ADDR_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_SRCFILE)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_CSRCFILE)

$(PK_PAGING_LOOKUP_ADDR_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_LOOKUP_ADDR_sh,$(srctree))
