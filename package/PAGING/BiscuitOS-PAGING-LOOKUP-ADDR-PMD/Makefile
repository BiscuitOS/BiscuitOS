# BiscuitOS-PAGING-LOOKUP-ADDR-PMD
#
################################################
PK_PAGING_LOOKUP_ADDR_PMD_NAME        := BiscuitOS-PAGING-LOOKUP-ADDR-PMD
PK_PAGING_LOOKUP_ADDR_PMD_VERSION     := $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_VERSION)
PK_PAGING_LOOKUP_ADDR_PMD_SITE        := $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_URL)
PK_PAGING_LOOKUP_ADDR_PMD_GITHUB      := $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_GITHUB)
PK_PAGING_LOOKUP_ADDR_PMD_SUBNAME     :=
PK_PAGING_LOOKUP_ADDR_PMD_BUILDARCH   := $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_BUILDARCH)
PK_PAGING_LOOKUP_ADDR_PMD_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-LOOKUP-ADDR-PMD
PK_PAGING_LOOKUP_ADDR_PMD_PATCH       := $(PK_PAGING_LOOKUP_ADDR_PMD_DIR)/patch
PK_PAGING_LOOKUP_ADDR_PMD_BSBIT       := BiscuitOS-PAGING-LOOKUP-ADDR-PMD-$(PK_PAGING_LOOKUP_ADDR_PMD_VERSION).bsbit
PK_PAGING_LOOKUP_ADDR_PMD_OUT         :=
PK_PAGING_LOOKUP_ADDR_PMD_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_LOOKUP_ADDR_PMD_TAR         := $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_TAR)
PK_PAGING_LOOKUP_ADDR_PMD_PACKAGE     :=
PK_PAGING_LOOKUP_ADDR_PMD_CONFIG      := $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_CONFIG)
SUB_TARGET                  += $(PK_PAGING_LOOKUP_ADDR_PMD_DIR)/build

PK_PAGING_LOOKUP_ADDR_PMD_sh = $(CONFIG_SHELL) $(PK_PAGING_LOOKUP_ADDR_PMD_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_LOOKUP_ADDR_PMD_NAME)X \
                 $(PK_PAGING_LOOKUP_ADDR_PMD_VERSION)X \
                 $(PK_PAGING_LOOKUP_ADDR_PMD_SITE)X \
                 $(PK_PAGING_LOOKUP_ADDR_PMD_GITHUB)X \
                 $(PK_PAGING_LOOKUP_ADDR_PMD_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_LOOKUP_ADDR_PMD_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_LOOKUP_ADDR_PMD_SUBNAME)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_CONFIG)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_LIBPATH)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_CFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_ASFLAGS)X \
                 $(PK_PAGING_LOOKUP_ADDR_PMD_BSBIT)X \
                 $(PK_PAGING_LOOKUP_ADDR_PMD_BUILDARCH)X \
		 $(PK_PAGING_LOOKUP_ADDR_PMD_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_SRCFILE)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_PMD_CSRCFILE)

$(PK_PAGING_LOOKUP_ADDR_PMD_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_LOOKUP_ADDR_PMD_sh,$(srctree))
