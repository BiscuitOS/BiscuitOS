PK_PAGING_PAGEWALK_RANGE_NAME        := BiscuitOS-PAGING-PageWalk-RANGE
PK_PAGING_PAGEWALK_RANGE_VERSION     := $(CONFIG_PK_PAGING_PAGEWALK_RANGE_VERSION)
PK_PAGING_PAGEWALK_RANGE_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_RANGE_URL)
PK_PAGING_PAGEWALK_RANGE_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_RANGE_GITHUB)
PK_PAGING_PAGEWALK_RANGE_SUBNAME     :=
PK_PAGING_PAGEWALK_RANGE_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_RANGE_BUILDARCH)
PK_PAGING_PAGEWALK_RANGE_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-RANGE
PK_PAGING_PAGEWALK_RANGE_PATCH       := $(PK_PAGING_PAGEWALK_RANGE_DIR)/patch
PK_PAGING_PAGEWALK_RANGE_BSBIT       := BiscuitOS-PAGING-PageWalk-RANGE-$(PK_PAGING_PAGEWALK_RANGE_VERSION).bsbit
PK_PAGING_PAGEWALK_RANGE_OUT         :=
PK_PAGING_PAGEWALK_RANGE_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_RANGE_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_RANGE_TAR)
PK_PAGING_PAGEWALK_RANGE_PACKAGE     :=
PK_PAGING_PAGEWALK_RANGE_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_RANGE_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_RANGE_DIR)/build

PK_PAGING_PAGEWALK_RANGE_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_RANGE_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_RANGE_NAME)X \
                 $(PK_PAGING_PAGEWALK_RANGE_VERSION)X \
                 $(PK_PAGING_PAGEWALK_RANGE_SITE)X \
                 $(PK_PAGING_PAGEWALK_RANGE_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_RANGE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_RANGE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_RANGE_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_RANGE_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_RANGE_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_RANGE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_RANGE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_RANGE_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_RANGE_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_RANGE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_RANGE_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_RANGE_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_RANGE_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_RANGE_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_RANGE_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_RANGE_CSRCFILE)

$(PK_PAGING_PAGEWALK_RANGE_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_RANGE_sh,$(srctree))
