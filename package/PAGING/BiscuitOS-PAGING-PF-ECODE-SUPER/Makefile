PK_PAGING_PF_ECODE_SUPER_NAME        := BiscuitOS-PAGING-PF-ECODE-SUPER
PK_PAGING_PF_ECODE_SUPER_VERSION     := $(CONFIG_PK_PAGING_PF_ECODE_SUPER_VERSION)
PK_PAGING_PF_ECODE_SUPER_SITE        := $(CONFIG_PK_PAGING_PF_ECODE_SUPER_URL)
PK_PAGING_PF_ECODE_SUPER_GITHUB      := $(CONFIG_PK_PAGING_PF_ECODE_SUPER_GITHUB)
PK_PAGING_PF_ECODE_SUPER_SUBNAME     :=
PK_PAGING_PF_ECODE_SUPER_BUILDARCH   := $(CONFIG_PK_PAGING_PF_ECODE_SUPER_BUILDARCH)
PK_PAGING_PF_ECODE_SUPER_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-ECODE-SUPER
PK_PAGING_PF_ECODE_SUPER_PATCH       := $(PK_PAGING_PF_ECODE_SUPER_DIR)/patch
PK_PAGING_PF_ECODE_SUPER_BSBIT       := BiscuitOS-PAGING-PF-ECODE-SUPER-$(PK_PAGING_PF_ECODE_SUPER_VERSION).bsbit
PK_PAGING_PF_ECODE_SUPER_OUT         :=
PK_PAGING_PF_ECODE_SUPER_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PF_ECODE_SUPER_TAR         := $(CONFIG_PK_PAGING_PF_ECODE_SUPER_TAR)
PK_PAGING_PF_ECODE_SUPER_PACKAGE     :=
PK_PAGING_PF_ECODE_SUPER_CONFIG      := $(CONFIG_PK_PAGING_PF_ECODE_SUPER_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_ECODE_SUPER_DIR)/build

PK_PAGING_PF_ECODE_SUPER_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_ECODE_SUPER_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_ECODE_SUPER_NAME)X \
                 $(PK_PAGING_PF_ECODE_SUPER_VERSION)X \
                 $(PK_PAGING_PF_ECODE_SUPER_SITE)X \
                 $(PK_PAGING_PF_ECODE_SUPER_GITHUB)X \
                 $(PK_PAGING_PF_ECODE_SUPER_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_ECODE_SUPER_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_ECODE_SUPER_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_SUPER_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_SUPER_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_SUPER_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_SUPER_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_SUPER_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_SUPER_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_SUPER_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_SUPER_ASFLAGS)X \
                 $(PK_PAGING_PF_ECODE_SUPER_BSBIT)X \
                 $(PK_PAGING_PF_ECODE_SUPER_BUILDARCH)X \
		 $(PK_PAGING_PF_ECODE_SUPER_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_SUPER_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_SUPER_CSRCFILE)

$(PK_PAGING_PF_ECODE_SUPER_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_ECODE_SUPER_sh,$(srctree))
