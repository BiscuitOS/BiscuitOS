PK_PAGING_PAGEWALK_MADV_COLD_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_URL)
PK_PAGING_PAGEWALK_MADV_COLD_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_GITHUB)
PK_PAGING_PAGEWALK_MADV_COLD_SUBNAME     :=
PK_PAGING_PAGEWALK_MADV_COLD_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_BUILDARCH)
PK_PAGING_PAGEWALK_MADV_COLD_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-MADV_COLD
PK_PAGING_PAGEWALK_MADV_COLD_PATCH       := $(PK_PAGING_PAGEWALK_MADV_COLD_DIR)/patch
PK_PAGING_PAGEWALK_MADV_COLD_BSBIT       := BiscuitOS-PAGING-PageWalk-MADV_COLD-$(PK_PAGING_PAGEWALK_MADV_COLD_VERSION).bsbit
PK_PAGING_PAGEWALK_MADV_COLD_OUT         :=
PK_PAGING_PAGEWALK_MADV_COLD_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PAGEWALK_MADV_COLD_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_TAR)
PK_PAGING_PAGEWALK_MADV_COLD_PACKAGE     :=
PK_PAGING_PAGEWALK_MADV_COLD_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_MADV_COLD_DIR)/build

PK_PAGING_PAGEWALK_MADV_COLD_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_MADV_COLD_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_MADV_COLD_NAME)X \
                 $(PK_PAGING_PAGEWALK_MADV_COLD_VERSION)X \
                 $(PK_PAGING_PAGEWALK_MADV_COLD_SITE)X \
                 $(PK_PAGING_PAGEWALK_MADV_COLD_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_MADV_COLD_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_MADV_COLD_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_MADV_COLD_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_MADV_COLD_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_MADV_COLD_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_MADV_COLD_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MADV_COLD_CSRCFILE)

$(PK_PAGING_PAGEWALK_MADV_COLD_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_MADV_COLD_sh,$(srctree))
