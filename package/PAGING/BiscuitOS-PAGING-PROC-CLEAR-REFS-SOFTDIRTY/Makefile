PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_NAME        := BiscuitOS-PAGING-PROC-CLEAR-REFS-SOFTDIRTY
PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_VERSION     := $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_VERSION)
PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_SITE        := $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_URL)
PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_GITHUB      := $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_GITHUB)
PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_SUBNAME     :=
PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_BUILDARCH   := $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_BUILDARCH)
PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PROC-CLEAR-REFS-SOFTDIRTY
PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_PATCH       := $(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_DIR)/patch
PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_BSBIT       := BiscuitOS-PAGING-PROC-CLEAR-REFS-SOFTDIRTY-$(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_VERSION).bsbit
PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_OUT         :=
PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_TAR         := $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_TAR)
PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_PACKAGE     :=
PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_CONFIG      := $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_DIR)/build

PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_sh = $(CONFIG_SHELL) $(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_NAME)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_VERSION)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_SITE)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_GITHUB)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_CONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_ASFLAGS)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_BSBIT)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_BUILDARCH)X \
		 $(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_CSRCFILE)

$(PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PROC_CLEAR_REFS_SOFTDIRTY_sh,$(srctree))
