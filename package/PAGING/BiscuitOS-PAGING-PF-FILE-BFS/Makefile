PK_PAGING_PF_FILE_BFS_NAME        := BiscuitOS-PAGING-PF-FILE-BFS
PK_PAGING_PF_FILE_BFS_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_BFS_VERSION)
PK_PAGING_PF_FILE_BFS_SITE        := $(CONFIG_PK_PAGING_PF_FILE_BFS_URL)
PK_PAGING_PF_FILE_BFS_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_BFS_GITHUB)
PK_PAGING_PF_FILE_BFS_SUBNAME     :=
PK_PAGING_PF_FILE_BFS_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_BFS_BUILDARCH)
PK_PAGING_PF_FILE_BFS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-BFS
PK_PAGING_PF_FILE_BFS_PATCH       := $(PK_PAGING_PF_FILE_BFS_DIR)/patch
PK_PAGING_PF_FILE_BFS_BSBIT       := BiscuitOS-PAGING-PF-FILE-BFS-$(PK_PAGING_PF_FILE_BFS_VERSION).bsbit
PK_PAGING_PF_FILE_BFS_OUT         :=
PK_PAGING_PF_FILE_BFS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_BFS_TAR         := $(CONFIG_PK_PAGING_PF_FILE_BFS_TAR)
PK_PAGING_PF_FILE_BFS_PACKAGE     :=
PK_PAGING_PF_FILE_BFS_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_BFS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_BFS_DIR)/build

PK_PAGING_PF_FILE_BFS_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_BFS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_BFS_NAME)X \
                 $(PK_PAGING_PF_FILE_BFS_VERSION)X \
                 $(PK_PAGING_PF_FILE_BFS_SITE)X \
                 $(PK_PAGING_PF_FILE_BFS_GITHUB)X \
                 $(PK_PAGING_PF_FILE_BFS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_BFS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_BFS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BFS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BFS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BFS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BFS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BFS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BFS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BFS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BFS_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_BFS_BSBIT)X \
                 $(PK_PAGING_PF_FILE_BFS_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_BFS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BFS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BFS_CSRCFILE)

$(PK_PAGING_PF_FILE_BFS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_BFS_sh,$(srctree))
