# BiscuitOS-PAGING-VM-ONDEMAND-MLOCK
#
################################################
PK_PAGING_VM_ONDEMAND_MLOCK_NAME        := BiscuitOS-PAGING-VM-ONDEMAND-MLOCK
PK_PAGING_VM_ONDEMAND_MLOCK_VERSION     := $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_VERSION)
PK_PAGING_VM_ONDEMAND_MLOCK_SITE        := $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_URL)
PK_PAGING_VM_ONDEMAND_MLOCK_GITHUB      := $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_GITHUB)
PK_PAGING_VM_ONDEMAND_MLOCK_SUBNAME     :=
PK_PAGING_VM_ONDEMAND_MLOCK_BUILDARCH   := $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_BUILDARCH)
PK_PAGING_VM_ONDEMAND_MLOCK_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-VM-ONDEMAND-MLOCK
PK_PAGING_VM_ONDEMAND_MLOCK_PATCH       := $(PK_PAGING_VM_ONDEMAND_MLOCK_DIR)/patch
PK_PAGING_VM_ONDEMAND_MLOCK_BSBIT       := BiscuitOS-PAGING-VM-ONDEMAND-MLOCK-$(PK_PAGING_VM_ONDEMAND_MLOCK_VERSION).bsbit
PK_PAGING_VM_ONDEMAND_MLOCK_OUT         :=
PK_PAGING_VM_ONDEMAND_MLOCK_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_VM_ONDEMAND_MLOCK_TAR         := $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_TAR)
PK_PAGING_VM_ONDEMAND_MLOCK_PACKAGE     :=
PK_PAGING_VM_ONDEMAND_MLOCK_CONFIG      := $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_CONFIG)
SUB_TARGET                  += $(PK_PAGING_VM_ONDEMAND_MLOCK_DIR)/build

PK_PAGING_VM_ONDEMAND_MLOCK_sh = $(CONFIG_SHELL) $(PK_PAGING_VM_ONDEMAND_MLOCK_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_VM_ONDEMAND_MLOCK_NAME)X \
                 $(PK_PAGING_VM_ONDEMAND_MLOCK_VERSION)X \
                 $(PK_PAGING_VM_ONDEMAND_MLOCK_SITE)X \
                 $(PK_PAGING_VM_ONDEMAND_MLOCK_GITHUB)X \
                 $(PK_PAGING_VM_ONDEMAND_MLOCK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_VM_ONDEMAND_MLOCK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_VM_ONDEMAND_MLOCK_SUBNAME)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_CONFIG)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_LIBPATH)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_CFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_ASFLAGS)X \
                 $(PK_PAGING_VM_ONDEMAND_MLOCK_BSBIT)X \
                 $(PK_PAGING_VM_ONDEMAND_MLOCK_BUILDARCH)X \
		 $(PK_PAGING_VM_ONDEMAND_MLOCK_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_SRCFILE)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_MLOCK_CSRCFILE)

$(PK_PAGING_VM_ONDEMAND_MLOCK_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_VM_ONDEMAND_MLOCK_sh,$(srctree))
