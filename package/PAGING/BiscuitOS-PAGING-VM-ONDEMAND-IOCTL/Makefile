################################################
#
# BiscuitOS-PAGING-VM-ONDEMAND-IOCTL
#
################################################
PK_PAGING_VM_ONDEMAND_IOCTL_NAME        := BiscuitOS-PAGING-VM-ONDEMAND-IOCTL
PK_PAGING_VM_ONDEMAND_IOCTL_VERSION     := $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_VERSION)
PK_PAGING_VM_ONDEMAND_IOCTL_SITE        := $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_URL)
PK_PAGING_VM_ONDEMAND_IOCTL_GITHUB      := $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_GITHUB)
PK_PAGING_VM_ONDEMAND_IOCTL_SUBNAME     :=
PK_PAGING_VM_ONDEMAND_IOCTL_BUILDARCH   := $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_BUILDARCH)
PK_PAGING_VM_ONDEMAND_IOCTL_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-VM-ONDEMAND-IOCTL
PK_PAGING_VM_ONDEMAND_IOCTL_PATCH       := $(PK_PAGING_VM_ONDEMAND_IOCTL_DIR)/patch
PK_PAGING_VM_ONDEMAND_IOCTL_BSBIT       := BiscuitOS-PAGING-VM-ONDEMAND-IOCTL-$(PK_PAGING_VM_ONDEMAND_IOCTL_VERSION).bsbit
PK_PAGING_VM_ONDEMAND_IOCTL_OUT         :=
PK_PAGING_VM_ONDEMAND_IOCTL_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_VM_ONDEMAND_IOCTL_TAR         := $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_TAR)
PK_PAGING_VM_ONDEMAND_IOCTL_PACKAGE     :=
PK_PAGING_VM_ONDEMAND_IOCTL_CONFIG      := $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_CONFIG)
SUB_TARGET                  += $(PK_PAGING_VM_ONDEMAND_IOCTL_DIR)/build

PK_PAGING_VM_ONDEMAND_IOCTL_sh = $(CONFIG_SHELL) $(PK_PAGING_VM_ONDEMAND_IOCTL_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_VM_ONDEMAND_IOCTL_NAME)X \
                 $(PK_PAGING_VM_ONDEMAND_IOCTL_VERSION)X \
                 $(PK_PAGING_VM_ONDEMAND_IOCTL_SITE)X \
                 $(PK_PAGING_VM_ONDEMAND_IOCTL_GITHUB)X \
                 $(PK_PAGING_VM_ONDEMAND_IOCTL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_VM_ONDEMAND_IOCTL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_VM_ONDEMAND_IOCTL_SUBNAME)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_CONFIG)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_LIBPATH)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_CFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_ASFLAGS)X \
                 $(PK_PAGING_VM_ONDEMAND_IOCTL_BSBIT)X \
                 $(PK_PAGING_VM_ONDEMAND_IOCTL_BUILDARCH)X \
		 $(PK_PAGING_VM_ONDEMAND_IOCTL_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_SRCFILE)X \
                 $(CONFIG_PK_PAGING_VM_ONDEMAND_IOCTL_CSRCFILE)

$(PK_PAGING_VM_ONDEMAND_IOCTL_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_VM_ONDEMAND_IOCTL_sh,$(srctree))
