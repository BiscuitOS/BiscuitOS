PK_PAGING_PF_FILE_MSDOS_NAME        := BiscuitOS-PAGING-PF-FILE-MSDOS
PK_PAGING_PF_FILE_MSDOS_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_MSDOS_VERSION)
PK_PAGING_PF_FILE_MSDOS_SITE        := $(CONFIG_PK_PAGING_PF_FILE_MSDOS_URL)
PK_PAGING_PF_FILE_MSDOS_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_MSDOS_GITHUB)
PK_PAGING_PF_FILE_MSDOS_SUBNAME     :=
PK_PAGING_PF_FILE_MSDOS_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_MSDOS_BUILDARCH)
PK_PAGING_PF_FILE_MSDOS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-MSDOS
PK_PAGING_PF_FILE_MSDOS_PATCH       := $(PK_PAGING_PF_FILE_MSDOS_DIR)/patch
PK_PAGING_PF_FILE_MSDOS_BSBIT       := BiscuitOS-PAGING-PF-FILE-MSDOS-$(PK_PAGING_PF_FILE_MSDOS_VERSION).bsbit
PK_PAGING_PF_FILE_MSDOS_OUT         :=
PK_PAGING_PF_FILE_MSDOS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_MSDOS_TAR         := $(CONFIG_PK_PAGING_PF_FILE_MSDOS_TAR)
PK_PAGING_PF_FILE_MSDOS_PACKAGE     :=
PK_PAGING_PF_FILE_MSDOS_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_MSDOS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_MSDOS_DIR)/build

PK_PAGING_PF_FILE_MSDOS_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_MSDOS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_MSDOS_NAME)X \
                 $(PK_PAGING_PF_FILE_MSDOS_VERSION)X \
                 $(PK_PAGING_PF_FILE_MSDOS_SITE)X \
                 $(PK_PAGING_PF_FILE_MSDOS_GITHUB)X \
                 $(PK_PAGING_PF_FILE_MSDOS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_MSDOS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_MSDOS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MSDOS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MSDOS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MSDOS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MSDOS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MSDOS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MSDOS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MSDOS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MSDOS_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_MSDOS_BSBIT)X \
                 $(PK_PAGING_PF_FILE_MSDOS_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_MSDOS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MSDOS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MSDOS_CSRCFILE)

$(PK_PAGING_PF_FILE_MSDOS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_MSDOS_sh,$(srctree))
