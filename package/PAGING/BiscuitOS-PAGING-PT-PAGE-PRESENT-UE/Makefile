                 $(PK_PAGING_PT_PAGE_PRESENT_UE_VERSION)X \
                 $(PK_PAGING_PT_PAGE_PRESENT_UE_SITE)X \
                 $(PK_PAGING_PT_PAGE_PRESENT_UE_GITHUB)X \
                 $(PK_PAGING_PT_PAGE_PRESENT_UE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_PAGE_PRESENT_UE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_PAGE_PRESENT_UE_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_PAGE_PRESENT_UE_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_PAGE_PRESENT_UE_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_PAGE_PRESENT_UE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_PAGE_PRESENT_UE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_PAGE_PRESENT_UE_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_PAGE_PRESENT_UE_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_PAGE_PRESENT_UE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_PAGE_PRESENT_UE_ASFLAGS)X \
                 $(PK_PAGING_PT_PAGE_PRESENT_UE_BSBIT)X \
                 $(PK_PAGING_PT_PAGE_PRESENT_UE_BUILDARCH)X \
		 $(PK_PAGING_PT_PAGE_PRESENT_UE_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_PAGE_PRESENT_UE_SRCFILE)X \
