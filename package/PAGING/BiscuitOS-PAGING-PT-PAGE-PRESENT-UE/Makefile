PK_PAGING_PT_PAGE_PRESENT_UE_PACKAGE     :=
PK_PAGING_PT_PAGE_PRESENT_UE_CONFIG      := $(CONFIG_PK_PAGING_PT_PAGE_PRESENT_UE_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_PAGE_PRESENT_UE_DIR)/build

PK_PAGING_PT_PAGE_PRESENT_UE_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_PAGE_PRESENT_UE_SCRIPTS) \
                 $1X \
