PK_PAGING_PF_HUGETLB_MCE_NAME        := BiscuitOS-PAGING-PF-HUGETLB-MCE
PK_PAGING_PF_HUGETLB_MCE_VERSION     := $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_VERSION)
PK_PAGING_PF_HUGETLB_MCE_SITE        := $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_URL)
PK_PAGING_PF_HUGETLB_MCE_GITHUB      := $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_GITHUB)
PK_PAGING_PF_HUGETLB_MCE_SUBNAME     :=
PK_PAGING_PF_HUGETLB_MCE_BUILDARCH   := $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_BUILDARCH)
PK_PAGING_PF_HUGETLB_MCE_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-HUGETLB-MCE
PK_PAGING_PF_HUGETLB_MCE_PATCH       := $(PK_PAGING_PF_HUGETLB_MCE_DIR)/patch
PK_PAGING_PF_HUGETLB_MCE_BSBIT       := BiscuitOS-PAGING-PF-HUGETLB-MCE-$(PK_PAGING_PF_HUGETLB_MCE_VERSION).bsbit
PK_PAGING_PF_HUGETLB_MCE_OUT         :=
PK_PAGING_PF_HUGETLB_MCE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_HUGETLB_MCE_TAR         := $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_TAR)
PK_PAGING_PF_HUGETLB_MCE_PACKAGE     :=
PK_PAGING_PF_HUGETLB_MCE_CONFIG      := $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_HUGETLB_MCE_DIR)/build

PK_PAGING_PF_HUGETLB_MCE_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_HUGETLB_MCE_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_HUGETLB_MCE_NAME)X \
                 $(PK_PAGING_PF_HUGETLB_MCE_VERSION)X \
                 $(PK_PAGING_PF_HUGETLB_MCE_SITE)X \
                 $(PK_PAGING_PF_HUGETLB_MCE_GITHUB)X \
                 $(PK_PAGING_PF_HUGETLB_MCE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_HUGETLB_MCE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_HUGETLB_MCE_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_ASFLAGS)X \
                 $(PK_PAGING_PF_HUGETLB_MCE_BSBIT)X \
                 $(PK_PAGING_PF_HUGETLB_MCE_BUILDARCH)X \
		 $(PK_PAGING_PF_HUGETLB_MCE_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_MCE_CSRCFILE)

$(PK_PAGING_PF_HUGETLB_MCE_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_HUGETLB_MCE_sh,$(srctree))
