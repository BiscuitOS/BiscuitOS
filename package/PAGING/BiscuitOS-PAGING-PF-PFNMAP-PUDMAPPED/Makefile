PK_PAGING_PF_PFNMAP_PUDMAPPED_NAME        := BiscuitOS-PAGING-PF-PFNMAP-PUDMAPPED
PK_PAGING_PF_PFNMAP_PUDMAPPED_VERSION     := $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_VERSION)
PK_PAGING_PF_PFNMAP_PUDMAPPED_SITE        := $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_URL)
PK_PAGING_PF_PFNMAP_PUDMAPPED_GITHUB      := $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_GITHUB)
PK_PAGING_PF_PFNMAP_PUDMAPPED_SUBNAME     :=
PK_PAGING_PF_PFNMAP_PUDMAPPED_BUILDARCH   := $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_BUILDARCH)
PK_PAGING_PF_PFNMAP_PUDMAPPED_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-PFNMAP-PUDMAPPED
PK_PAGING_PF_PFNMAP_PUDMAPPED_PATCH       := $(PK_PAGING_PF_PFNMAP_PUDMAPPED_DIR)/patch
PK_PAGING_PF_PFNMAP_PUDMAPPED_BSBIT       := BiscuitOS-PAGING-PF-PFNMAP-PUDMAPPED-$(PK_PAGING_PF_PFNMAP_PUDMAPPED_VERSION).bsbit
PK_PAGING_PF_PFNMAP_PUDMAPPED_OUT         :=
PK_PAGING_PF_PFNMAP_PUDMAPPED_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_PF_PFNMAP_PUDMAPPED_TAR         := $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_TAR)
PK_PAGING_PF_PFNMAP_PUDMAPPED_PACKAGE     :=
PK_PAGING_PF_PFNMAP_PUDMAPPED_CONFIG      := $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_PFNMAP_PUDMAPPED_DIR)/build

PK_PAGING_PF_PFNMAP_PUDMAPPED_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_PFNMAP_PUDMAPPED_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_PFNMAP_PUDMAPPED_NAME)X \
                 $(PK_PAGING_PF_PFNMAP_PUDMAPPED_VERSION)X \
                 $(PK_PAGING_PF_PFNMAP_PUDMAPPED_SITE)X \
                 $(PK_PAGING_PF_PFNMAP_PUDMAPPED_GITHUB)X \
                 $(PK_PAGING_PF_PFNMAP_PUDMAPPED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_PFNMAP_PUDMAPPED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_PFNMAP_PUDMAPPED_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_ASFLAGS)X \
                 $(PK_PAGING_PF_PFNMAP_PUDMAPPED_BSBIT)X \
                 $(PK_PAGING_PF_PFNMAP_PUDMAPPED_BUILDARCH)X \
		 $(PK_PAGING_PF_PFNMAP_PUDMAPPED_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_PFNMAP_PUDMAPPED_CSRCFILE)

$(PK_PAGING_PF_PFNMAP_PUDMAPPED_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_PFNMAP_PUDMAPPED_sh,$(srctree))
