PK_PAGING_PF_SIGBUS_NAME        := BiscuitOS-PAGING-PF-SIGBUS
PK_PAGING_PF_SIGBUS_VERSION     := $(CONFIG_PK_PAGING_PF_SIGBUS_VERSION)
PK_PAGING_PF_SIGBUS_SITE        := $(CONFIG_PK_PAGING_PF_SIGBUS_URL)
PK_PAGING_PF_SIGBUS_GITHUB      := $(CONFIG_PK_PAGING_PF_SIGBUS_GITHUB)
PK_PAGING_PF_SIGBUS_SUBNAME     :=
PK_PAGING_PF_SIGBUS_BUILDARCH   := $(CONFIG_PK_PAGING_PF_SIGBUS_BUILDARCH)
PK_PAGING_PF_SIGBUS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-SIGBUS
PK_PAGING_PF_SIGBUS_PATCH       := $(PK_PAGING_PF_SIGBUS_DIR)/patch
PK_PAGING_PF_SIGBUS_BSBIT       := BiscuitOS-PAGING-PF-SIGBUS-$(PK_PAGING_PF_SIGBUS_VERSION).bsbit
PK_PAGING_PF_SIGBUS_OUT         :=
PK_PAGING_PF_SIGBUS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_SIGBUS_TAR         := $(CONFIG_PK_PAGING_PF_SIGBUS_TAR)
PK_PAGING_PF_SIGBUS_PACKAGE     :=
PK_PAGING_PF_SIGBUS_CONFIG      := $(CONFIG_PK_PAGING_PF_SIGBUS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_SIGBUS_DIR)/build

PK_PAGING_PF_SIGBUS_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_SIGBUS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_SIGBUS_NAME)X \
                 $(PK_PAGING_PF_SIGBUS_VERSION)X \
                 $(PK_PAGING_PF_SIGBUS_SITE)X \
                 $(PK_PAGING_PF_SIGBUS_GITHUB)X \
                 $(PK_PAGING_PF_SIGBUS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_SIGBUS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_SIGBUS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_SIGBUS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SIGBUS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_SIGBUS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SIGBUS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SIGBUS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SIGBUS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SIGBUS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SIGBUS_ASFLAGS)X \
                 $(PK_PAGING_PF_SIGBUS_BSBIT)X \
                 $(PK_PAGING_PF_SIGBUS_BUILDARCH)X \
		 $(PK_PAGING_PF_SIGBUS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SIGBUS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SIGBUS_CSRCFILE)

$(PK_PAGING_PF_SIGBUS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_SIGBUS_sh,$(srctree))
