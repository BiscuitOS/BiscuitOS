PK_PAGING_PF_FILE_EXT3_FR_SITE        := $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_URL)
PK_PAGING_PF_FILE_EXT3_FR_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_GITHUB)
PK_PAGING_PF_FILE_EXT3_FR_SUBNAME     :=
PK_PAGING_PF_FILE_EXT3_FR_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_BUILDARCH)
PK_PAGING_PF_FILE_EXT3_FR_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-EXT3-FR
PK_PAGING_PF_FILE_EXT3_FR_PATCH       := $(PK_PAGING_PF_FILE_EXT3_FR_DIR)/patch
PK_PAGING_PF_FILE_EXT3_FR_BSBIT       := BiscuitOS-PAGING-PF-FILE-EXT3-FR-$(PK_PAGING_PF_FILE_EXT3_FR_VERSION).bsbit
PK_PAGING_PF_FILE_EXT3_FR_OUT         :=
PK_PAGING_PF_FILE_EXT3_FR_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_EXT3_FR_TAR         := $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_TAR)
PK_PAGING_PF_FILE_EXT3_FR_PACKAGE     :=
PK_PAGING_PF_FILE_EXT3_FR_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_EXT3_FR_DIR)/build

PK_PAGING_PF_FILE_EXT3_FR_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_EXT3_FR_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_EXT3_FR_NAME)X \
                 $(PK_PAGING_PF_FILE_EXT3_FR_VERSION)X \
                 $(PK_PAGING_PF_FILE_EXT3_FR_SITE)X \
                 $(PK_PAGING_PF_FILE_EXT3_FR_GITHUB)X \
                 $(PK_PAGING_PF_FILE_EXT3_FR_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_EXT3_FR_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_EXT3_FR_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_EXT3_FR_BSBIT)X \
                 $(PK_PAGING_PF_FILE_EXT3_FR_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_EXT3_FR_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_FR_CSRCFILE)

$(PK_PAGING_PF_FILE_EXT3_FR_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_EXT3_FR_sh,$(srctree))
