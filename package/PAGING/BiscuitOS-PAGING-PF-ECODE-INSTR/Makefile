PK_PAGING_PF_ECODE_INSTR_NAME        := BiscuitOS-PAGING-PF-ECODE-INSTR
PK_PAGING_PF_ECODE_INSTR_VERSION     := $(CONFIG_PK_PAGING_PF_ECODE_INSTR_VERSION)
PK_PAGING_PF_ECODE_INSTR_SITE        := $(CONFIG_PK_PAGING_PF_ECODE_INSTR_URL)
PK_PAGING_PF_ECODE_INSTR_GITHUB      := $(CONFIG_PK_PAGING_PF_ECODE_INSTR_GITHUB)
PK_PAGING_PF_ECODE_INSTR_SUBNAME     :=
PK_PAGING_PF_ECODE_INSTR_BUILDARCH   := $(CONFIG_PK_PAGING_PF_ECODE_INSTR_BUILDARCH)
PK_PAGING_PF_ECODE_INSTR_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-ECODE-INSTR
PK_PAGING_PF_ECODE_INSTR_PATCH       := $(PK_PAGING_PF_ECODE_INSTR_DIR)/patch
PK_PAGING_PF_ECODE_INSTR_BSBIT       := BiscuitOS-PAGING-PF-ECODE-INSTR-$(PK_PAGING_PF_ECODE_INSTR_VERSION).bsbit
PK_PAGING_PF_ECODE_INSTR_OUT         :=
PK_PAGING_PF_ECODE_INSTR_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_ECODE_INSTR_TAR         := $(CONFIG_PK_PAGING_PF_ECODE_INSTR_TAR)
PK_PAGING_PF_ECODE_INSTR_PACKAGE     :=
PK_PAGING_PF_ECODE_INSTR_CONFIG      := $(CONFIG_PK_PAGING_PF_ECODE_INSTR_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_ECODE_INSTR_DIR)/build

PK_PAGING_PF_ECODE_INSTR_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_ECODE_INSTR_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_ECODE_INSTR_NAME)X \
                 $(PK_PAGING_PF_ECODE_INSTR_VERSION)X \
                 $(PK_PAGING_PF_ECODE_INSTR_SITE)X \
                 $(PK_PAGING_PF_ECODE_INSTR_GITHUB)X \
                 $(PK_PAGING_PF_ECODE_INSTR_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_ECODE_INSTR_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_ECODE_INSTR_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_INSTR_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_INSTR_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_INSTR_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_INSTR_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_INSTR_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_INSTR_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_INSTR_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_INSTR_ASFLAGS)X \
                 $(PK_PAGING_PF_ECODE_INSTR_BSBIT)X \
                 $(PK_PAGING_PF_ECODE_INSTR_BUILDARCH)X \
		 $(PK_PAGING_PF_ECODE_INSTR_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_INSTR_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_INSTR_CSRCFILE)

$(PK_PAGING_PF_ECODE_INSTR_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_ECODE_INSTR_sh,$(srctree))
