PK_PAGING_VM_ZERO_PAGE_ONDEMAND_NAME        := BiscuitOS-PAGING-VM-ZERO-PAGE-ONDEMAND
PK_PAGING_VM_ZERO_PAGE_ONDEMAND_VERSION     := $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_VERSION)
PK_PAGING_VM_ZERO_PAGE_ONDEMAND_SITE        := $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_URL)
PK_PAGING_VM_ZERO_PAGE_ONDEMAND_GITHUB      := $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_GITHUB)
PK_PAGING_VM_ZERO_PAGE_ONDEMAND_SUBNAME     :=
PK_PAGING_VM_ZERO_PAGE_ONDEMAND_BUILDARCH   := $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_BUILDARCH)
PK_PAGING_VM_ZERO_PAGE_ONDEMAND_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-VM-ZERO-PAGE-ONDEMAND
PK_PAGING_VM_ZERO_PAGE_ONDEMAND_PATCH       := $(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_DIR)/patch
PK_PAGING_VM_ZERO_PAGE_ONDEMAND_BSBIT       := BiscuitOS-PAGING-VM-ZERO-PAGE-ONDEMAND-$(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_VERSION).bsbit
PK_PAGING_VM_ZERO_PAGE_ONDEMAND_OUT         :=
PK_PAGING_VM_ZERO_PAGE_ONDEMAND_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_VM_ZERO_PAGE_ONDEMAND_TAR         := $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_TAR)
PK_PAGING_VM_ZERO_PAGE_ONDEMAND_PACKAGE     :=
PK_PAGING_VM_ZERO_PAGE_ONDEMAND_CONFIG      := $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_CONFIG)
SUB_TARGET                  += $(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_DIR)/build

PK_PAGING_VM_ZERO_PAGE_ONDEMAND_sh = $(CONFIG_SHELL) $(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_NAME)X \
                 $(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_VERSION)X \
                 $(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_SITE)X \
                 $(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_GITHUB)X \
                 $(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_SUBNAME)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_CONFIG)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_LIBPATH)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_CFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_ASFLAGS)X \
                 $(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_BSBIT)X \
                 $(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_BUILDARCH)X \
		 $(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_SRCFILE)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_PAGE_ONDEMAND_CSRCFILE)

$(PK_PAGING_VM_ZERO_PAGE_ONDEMAND_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_VM_ZERO_PAGE_ONDEMAND_sh,$(srctree))
