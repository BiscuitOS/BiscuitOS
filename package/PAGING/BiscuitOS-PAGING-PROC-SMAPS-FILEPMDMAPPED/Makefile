PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_NAME        := BiscuitOS-PAGING-PROC-SMAPS-FILEPMDMAPPED
PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_VERSION     := $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_VERSION)
PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_SITE        := $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_URL)
PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_GITHUB      := $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_GITHUB)
PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_SUBNAME     :=
PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_BUILDARCH   := $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_BUILDARCH)
PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PROC-SMAPS-FILEPMDMAPPED
PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_PATCH       := $(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_DIR)/patch
PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_BSBIT       := BiscuitOS-PAGING-PROC-SMAPS-FILEPMDMAPPED-$(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_VERSION).bsbit
PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_OUT         :=
PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_TAR         := $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_TAR)
PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_PACKAGE     :=
PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_CONFIG      := $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_DIR)/build

PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_sh = $(CONFIG_SHELL) $(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_VERSION)X \
                 $(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_SITE)X \
                 $(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_GITHUB)X \
                 $(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_CONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_ASFLAGS)X \
                 $(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_BSBIT)X \
                 $(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_BUILDARCH)X \
		 $(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_CSRCFILE)

$(PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PROC_SMAPS_FILEPMDMAPPED_sh,$(srctree))
