PK_PAGING_PF_FILE_F2FS_NAME        := BiscuitOS-PAGING-PF-FILE-F2FS
PK_PAGING_PF_FILE_F2FS_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_F2FS_VERSION)
PK_PAGING_PF_FILE_F2FS_SITE        := $(CONFIG_PK_PAGING_PF_FILE_F2FS_URL)
PK_PAGING_PF_FILE_F2FS_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_F2FS_GITHUB)
PK_PAGING_PF_FILE_F2FS_SUBNAME     :=
PK_PAGING_PF_FILE_F2FS_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_F2FS_BUILDARCH)
PK_PAGING_PF_FILE_F2FS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-F2FS
PK_PAGING_PF_FILE_F2FS_PATCH       := $(PK_PAGING_PF_FILE_F2FS_DIR)/patch
PK_PAGING_PF_FILE_F2FS_BSBIT       := BiscuitOS-PAGING-PF-FILE-F2FS-$(PK_PAGING_PF_FILE_F2FS_VERSION).bsbit
PK_PAGING_PF_FILE_F2FS_OUT         :=
PK_PAGING_PF_FILE_F2FS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_F2FS_TAR         := $(CONFIG_PK_PAGING_PF_FILE_F2FS_TAR)
PK_PAGING_PF_FILE_F2FS_PACKAGE     :=
PK_PAGING_PF_FILE_F2FS_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_F2FS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_F2FS_DIR)/build

PK_PAGING_PF_FILE_F2FS_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_F2FS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_F2FS_NAME)X \
                 $(PK_PAGING_PF_FILE_F2FS_VERSION)X \
                 $(PK_PAGING_PF_FILE_F2FS_SITE)X \
                 $(PK_PAGING_PF_FILE_F2FS_GITHUB)X \
                 $(PK_PAGING_PF_FILE_F2FS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_F2FS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_F2FS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_F2FS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_F2FS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_F2FS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_F2FS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_F2FS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_F2FS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_F2FS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_F2FS_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_F2FS_BSBIT)X \
                 $(PK_PAGING_PF_FILE_F2FS_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_F2FS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_F2FS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_F2FS_CSRCFILE)

$(PK_PAGING_PF_FILE_F2FS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_F2FS_sh,$(srctree))
