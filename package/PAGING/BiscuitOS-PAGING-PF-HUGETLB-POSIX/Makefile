PK_PAGING_PF_HUGETLB_POSIX_NAME        := BiscuitOS-PAGING-PF-HUGETLB-POSIX
PK_PAGING_PF_HUGETLB_POSIX_VERSION     := $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_VERSION)
PK_PAGING_PF_HUGETLB_POSIX_SITE        := $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_URL)
PK_PAGING_PF_HUGETLB_POSIX_GITHUB      := $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_GITHUB)
PK_PAGING_PF_HUGETLB_POSIX_SUBNAME     :=
PK_PAGING_PF_HUGETLB_POSIX_BUILDARCH   := $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_BUILDARCH)
PK_PAGING_PF_HUGETLB_POSIX_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-HUGETLB-POSIX
PK_PAGING_PF_HUGETLB_POSIX_PATCH       := $(PK_PAGING_PF_HUGETLB_POSIX_DIR)/patch
PK_PAGING_PF_HUGETLB_POSIX_BSBIT       := BiscuitOS-PAGING-PF-HUGETLB-POSIX-$(PK_PAGING_PF_HUGETLB_POSIX_VERSION).bsbit
PK_PAGING_PF_HUGETLB_POSIX_OUT         :=
PK_PAGING_PF_HUGETLB_POSIX_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_HUGETLB_POSIX_TAR         := $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_TAR)
PK_PAGING_PF_HUGETLB_POSIX_PACKAGE     :=
PK_PAGING_PF_HUGETLB_POSIX_CONFIG      := $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_HUGETLB_POSIX_DIR)/build

PK_PAGING_PF_HUGETLB_POSIX_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_HUGETLB_POSIX_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_HUGETLB_POSIX_NAME)X \
                 $(PK_PAGING_PF_HUGETLB_POSIX_VERSION)X \
                 $(PK_PAGING_PF_HUGETLB_POSIX_SITE)X \
                 $(PK_PAGING_PF_HUGETLB_POSIX_GITHUB)X \
                 $(PK_PAGING_PF_HUGETLB_POSIX_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_HUGETLB_POSIX_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_HUGETLB_POSIX_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_ASFLAGS)X \
                 $(PK_PAGING_PF_HUGETLB_POSIX_BSBIT)X \
                 $(PK_PAGING_PF_HUGETLB_POSIX_BUILDARCH)X \
		 $(PK_PAGING_PF_HUGETLB_POSIX_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_POSIX_CSRCFILE)

$(PK_PAGING_PF_HUGETLB_POSIX_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_HUGETLB_POSIX_sh,$(srctree))
