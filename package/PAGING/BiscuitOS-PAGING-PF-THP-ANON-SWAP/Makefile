PK_PAGING_PF_THP_ANON_SWAP_SITE        := $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_URL)
PK_PAGING_PF_THP_ANON_SWAP_GITHUB      := $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_GITHUB)
PK_PAGING_PF_THP_ANON_SWAP_SUBNAME     :=
PK_PAGING_PF_THP_ANON_SWAP_BUILDARCH   := $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_BUILDARCH)
PK_PAGING_PF_THP_ANON_SWAP_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-THP-ANON-SWAP
PK_PAGING_PF_THP_ANON_SWAP_PATCH       := $(PK_PAGING_PF_THP_ANON_SWAP_DIR)/patch
PK_PAGING_PF_THP_ANON_SWAP_BSBIT       := BiscuitOS-PAGING-PF-THP-ANON-SWAP-$(PK_PAGING_PF_THP_ANON_SWAP_VERSION).bsbit
PK_PAGING_PF_THP_ANON_SWAP_OUT         :=
PK_PAGING_PF_THP_ANON_SWAP_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_THP_ANON_SWAP_TAR         := $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_TAR)
PK_PAGING_PF_THP_ANON_SWAP_PACKAGE     :=
PK_PAGING_PF_THP_ANON_SWAP_CONFIG      := $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_THP_ANON_SWAP_DIR)/build

PK_PAGING_PF_THP_ANON_SWAP_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_THP_ANON_SWAP_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_THP_ANON_SWAP_NAME)X \
                 $(PK_PAGING_PF_THP_ANON_SWAP_VERSION)X \
                 $(PK_PAGING_PF_THP_ANON_SWAP_SITE)X \
                 $(PK_PAGING_PF_THP_ANON_SWAP_GITHUB)X \
                 $(PK_PAGING_PF_THP_ANON_SWAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_THP_ANON_SWAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_THP_ANON_SWAP_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_ASFLAGS)X \
                 $(PK_PAGING_PF_THP_ANON_SWAP_BSBIT)X \
                 $(PK_PAGING_PF_THP_ANON_SWAP_BUILDARCH)X \
		 $(PK_PAGING_PF_THP_ANON_SWAP_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_THP_ANON_SWAP_CSRCFILE)

$(PK_PAGING_PF_THP_ANON_SWAP_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_THP_ANON_SWAP_sh,$(srctree))
