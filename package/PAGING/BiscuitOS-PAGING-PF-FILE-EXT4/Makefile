PK_PAGING_PF_FILE_EXT4_NAME        := BiscuitOS-PAGING-PF-FILE-EXT4
PK_PAGING_PF_FILE_EXT4_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_EXT4_VERSION)
PK_PAGING_PF_FILE_EXT4_SITE        := $(CONFIG_PK_PAGING_PF_FILE_EXT4_URL)
PK_PAGING_PF_FILE_EXT4_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_EXT4_GITHUB)
PK_PAGING_PF_FILE_EXT4_SUBNAME     :=
PK_PAGING_PF_FILE_EXT4_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_EXT4_BUILDARCH)
PK_PAGING_PF_FILE_EXT4_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-EXT4
PK_PAGING_PF_FILE_EXT4_PATCH       := $(PK_PAGING_PF_FILE_EXT4_DIR)/patch
PK_PAGING_PF_FILE_EXT4_BSBIT       := BiscuitOS-PAGING-PF-FILE-EXT4-$(PK_PAGING_PF_FILE_EXT4_VERSION).bsbit
PK_PAGING_PF_FILE_EXT4_OUT         :=
PK_PAGING_PF_FILE_EXT4_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_EXT4_TAR         := $(CONFIG_PK_PAGING_PF_FILE_EXT4_TAR)
PK_PAGING_PF_FILE_EXT4_PACKAGE     :=
PK_PAGING_PF_FILE_EXT4_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_EXT4_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_EXT4_DIR)/build

PK_PAGING_PF_FILE_EXT4_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_EXT4_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_EXT4_NAME)X \
                 $(PK_PAGING_PF_FILE_EXT4_VERSION)X \
                 $(PK_PAGING_PF_FILE_EXT4_SITE)X \
                 $(PK_PAGING_PF_FILE_EXT4_GITHUB)X \
                 $(PK_PAGING_PF_FILE_EXT4_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_EXT4_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_EXT4_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT4_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT4_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT4_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT4_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT4_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT4_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT4_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT4_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_EXT4_BSBIT)X \
                 $(PK_PAGING_PF_FILE_EXT4_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_EXT4_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT4_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT4_CSRCFILE)

$(PK_PAGING_PF_FILE_EXT4_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_EXT4_sh,$(srctree))
