PK_PAGING_PROC_SMAPS_PSS_PACKAGE     :=
PK_PAGING_PROC_SMAPS_PSS_CONFIG      := $(CONFIG_PK_PAGING_PROC_SMAPS_PSS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PROC_SMAPS_PSS_DIR)/build

PK_PAGING_PROC_SMAPS_PSS_sh = $(CONFIG_SHELL) $(PK_PAGING_PROC_SMAPS_PSS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PROC_SMAPS_PSS_NAME)X \
