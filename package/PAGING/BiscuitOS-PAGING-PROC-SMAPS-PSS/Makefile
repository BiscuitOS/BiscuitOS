                 $(PK_PAGING_PROC_SMAPS_PSS_SITE)X \
                 $(PK_PAGING_PROC_SMAPS_PSS_GITHUB)X \
                 $(PK_PAGING_PROC_SMAPS_PSS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_PSS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PROC_SMAPS_PSS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_PSS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_PSS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_PSS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_PSS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_PSS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_PSS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_PSS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_PSS_ASFLAGS)X \
                 $(PK_PAGING_PROC_SMAPS_PSS_BSBIT)X \
                 $(PK_PAGING_PROC_SMAPS_PSS_BUILDARCH)X \
		 $(PK_PAGING_PROC_SMAPS_PSS_CSRCFILE)X \
