PK_PAGING_PROC_SMAPS_PSS_SITE        := $(CONFIG_PK_PAGING_PROC_SMAPS_PSS_URL)
PK_PAGING_PROC_SMAPS_PSS_GITHUB      := $(CONFIG_PK_PAGING_PROC_SMAPS_PSS_GITHUB)
PK_PAGING_PROC_SMAPS_PSS_SUBNAME     :=
PK_PAGING_PROC_SMAPS_PSS_BUILDARCH   := $(CONFIG_PK_PAGING_PROC_SMAPS_PSS_BUILDARCH)
PK_PAGING_PROC_SMAPS_PSS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PROC-SMAPS-PSS
PK_PAGING_PROC_SMAPS_PSS_PATCH       := $(PK_PAGING_PROC_SMAPS_PSS_DIR)/patch
PK_PAGING_PROC_SMAPS_PSS_BSBIT       := BiscuitOS-PAGING-PROC-SMAPS-PSS-$(PK_PAGING_PROC_SMAPS_PSS_VERSION).bsbit
