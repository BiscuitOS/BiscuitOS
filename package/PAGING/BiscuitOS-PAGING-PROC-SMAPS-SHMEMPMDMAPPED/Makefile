PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_NAME        := BiscuitOS-PAGING-PROC-SMAPS-SHMEMPMDMAPPED
PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_VERSION     := $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_VERSION)
PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_SITE        := $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_URL)
PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_GITHUB      := $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_GITHUB)
PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_SUBNAME     :=
PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_BUILDARCH   := $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_BUILDARCH)
PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PROC-SMAPS-SHMEMPMDMAPPED
PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_PATCH       := $(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_DIR)/patch
PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_BSBIT       := BiscuitOS-PAGING-PROC-SMAPS-SHMEMPMDMAPPED-$(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_VERSION).bsbit
PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_OUT         :=
PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_TAR         := $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_TAR)
PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_PACKAGE     :=
PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_CONFIG      := $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_DIR)/build

PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_sh = $(CONFIG_SHELL) $(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_VERSION)X \
                 $(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_SITE)X \
                 $(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_GITHUB)X \
                 $(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_CONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_ASFLAGS)X \
                 $(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_BSBIT)X \
                 $(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_BUILDARCH)X \
		 $(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_CSRCFILE)

$(PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PROC_SMAPS_SHMEMPMDMAPPED_sh,$(srctree))
