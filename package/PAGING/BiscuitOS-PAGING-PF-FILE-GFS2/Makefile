PK_PAGING_PF_FILE_GFS2_NAME        := BiscuitOS-PAGING-PF-FILE-GFS2
PK_PAGING_PF_FILE_GFS2_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_GFS2_VERSION)
PK_PAGING_PF_FILE_GFS2_SITE        := $(CONFIG_PK_PAGING_PF_FILE_GFS2_URL)
PK_PAGING_PF_FILE_GFS2_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_GFS2_GITHUB)
PK_PAGING_PF_FILE_GFS2_SUBNAME     :=
PK_PAGING_PF_FILE_GFS2_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_GFS2_BUILDARCH)
PK_PAGING_PF_FILE_GFS2_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-GFS2
PK_PAGING_PF_FILE_GFS2_PATCH       := $(PK_PAGING_PF_FILE_GFS2_DIR)/patch
PK_PAGING_PF_FILE_GFS2_BSBIT       := BiscuitOS-PAGING-PF-FILE-GFS2-$(PK_PAGING_PF_FILE_GFS2_VERSION).bsbit
PK_PAGING_PF_FILE_GFS2_OUT         :=
PK_PAGING_PF_FILE_GFS2_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_GFS2_TAR         := $(CONFIG_PK_PAGING_PF_FILE_GFS2_TAR)
PK_PAGING_PF_FILE_GFS2_PACKAGE     :=
PK_PAGING_PF_FILE_GFS2_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_GFS2_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_GFS2_DIR)/build

PK_PAGING_PF_FILE_GFS2_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_GFS2_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_GFS2_NAME)X \
                 $(PK_PAGING_PF_FILE_GFS2_VERSION)X \
                 $(PK_PAGING_PF_FILE_GFS2_SITE)X \
                 $(PK_PAGING_PF_FILE_GFS2_GITHUB)X \
                 $(PK_PAGING_PF_FILE_GFS2_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_GFS2_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_GFS2_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_GFS2_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_GFS2_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_GFS2_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_GFS2_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_GFS2_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_GFS2_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_GFS2_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_GFS2_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_GFS2_BSBIT)X \
                 $(PK_PAGING_PF_FILE_GFS2_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_GFS2_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_GFS2_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_GFS2_CSRCFILE)

$(PK_PAGING_PF_FILE_GFS2_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_GFS2_sh,$(srctree))
