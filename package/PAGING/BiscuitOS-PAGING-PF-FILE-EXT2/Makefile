PK_PAGING_PF_FILE_EXT2_NAME        := BiscuitOS-PAGING-PF-FILE-EXT2
PK_PAGING_PF_FILE_EXT2_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_EXT2_VERSION)
PK_PAGING_PF_FILE_EXT2_SITE        := $(CONFIG_PK_PAGING_PF_FILE_EXT2_URL)
PK_PAGING_PF_FILE_EXT2_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_EXT2_GITHUB)
PK_PAGING_PF_FILE_EXT2_SUBNAME     :=
PK_PAGING_PF_FILE_EXT2_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_EXT2_BUILDARCH)
PK_PAGING_PF_FILE_EXT2_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-EXT2
PK_PAGING_PF_FILE_EXT2_PATCH       := $(PK_PAGING_PF_FILE_EXT2_DIR)/patch
PK_PAGING_PF_FILE_EXT2_BSBIT       := BiscuitOS-PAGING-PF-FILE-EXT2-$(PK_PAGING_PF_FILE_EXT2_VERSION).bsbit
PK_PAGING_PF_FILE_EXT2_OUT         :=
PK_PAGING_PF_FILE_EXT2_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_EXT2_TAR         := $(CONFIG_PK_PAGING_PF_FILE_EXT2_TAR)
PK_PAGING_PF_FILE_EXT2_PACKAGE     :=
PK_PAGING_PF_FILE_EXT2_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_EXT2_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_EXT2_DIR)/build

PK_PAGING_PF_FILE_EXT2_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_EXT2_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_EXT2_NAME)X \
                 $(PK_PAGING_PF_FILE_EXT2_VERSION)X \
                 $(PK_PAGING_PF_FILE_EXT2_SITE)X \
                 $(PK_PAGING_PF_FILE_EXT2_GITHUB)X \
                 $(PK_PAGING_PF_FILE_EXT2_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_EXT2_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_EXT2_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT2_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT2_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT2_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT2_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT2_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT2_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT2_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT2_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_EXT2_BSBIT)X \
                 $(PK_PAGING_PF_FILE_EXT2_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_EXT2_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT2_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT2_CSRCFILE)

$(PK_PAGING_PF_FILE_EXT2_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_EXT2_sh,$(srctree))
