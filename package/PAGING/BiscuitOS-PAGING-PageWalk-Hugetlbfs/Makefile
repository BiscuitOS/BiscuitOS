PK_PAGING_PAGEWALK_HUGETLBFS_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_URL)
PK_PAGING_PAGEWALK_HUGETLBFS_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_GITHUB)
PK_PAGING_PAGEWALK_HUGETLBFS_SUBNAME     :=
PK_PAGING_PAGEWALK_HUGETLBFS_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_BUILDARCH)
PK_PAGING_PAGEWALK_HUGETLBFS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-Hugetlbfs
PK_PAGING_PAGEWALK_HUGETLBFS_PATCH       := $(PK_PAGING_PAGEWALK_HUGETLBFS_DIR)/patch
PK_PAGING_PAGEWALK_HUGETLBFS_BSBIT       := BiscuitOS-PAGING-PageWalk-Hugetlbfs-$(PK_PAGING_PAGEWALK_HUGETLBFS_VERSION).bsbit
PK_PAGING_PAGEWALK_HUGETLBFS_OUT         :=
PK_PAGING_PAGEWALK_HUGETLBFS_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_HUGETLBFS_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_TAR)
PK_PAGING_PAGEWALK_HUGETLBFS_PACKAGE     :=
PK_PAGING_PAGEWALK_HUGETLBFS_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_HUGETLBFS_DIR)/build

PK_PAGING_PAGEWALK_HUGETLBFS_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_HUGETLBFS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_HUGETLBFS_NAME)X \
                 $(PK_PAGING_PAGEWALK_HUGETLBFS_VERSION)X \
                 $(PK_PAGING_PAGEWALK_HUGETLBFS_SITE)X \
                 $(PK_PAGING_PAGEWALK_HUGETLBFS_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_HUGETLBFS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_HUGETLBFS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_HUGETLBFS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_HUGETLBFS_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_HUGETLBFS_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_HUGETLBFS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_HUGETLBFS_CSRCFILE)

$(PK_PAGING_PAGEWALK_HUGETLBFS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_HUGETLBFS_sh,$(srctree))
