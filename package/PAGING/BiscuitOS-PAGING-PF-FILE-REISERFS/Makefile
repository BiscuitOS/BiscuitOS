PK_PAGING_PF_FILE_REISERFS_NAME        := BiscuitOS-PAGING-PF-FILE-REISERFS
PK_PAGING_PF_FILE_REISERFS_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_REISERFS_VERSION)
PK_PAGING_PF_FILE_REISERFS_SITE        := $(CONFIG_PK_PAGING_PF_FILE_REISERFS_URL)
PK_PAGING_PF_FILE_REISERFS_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_REISERFS_GITHUB)
PK_PAGING_PF_FILE_REISERFS_SUBNAME     :=
PK_PAGING_PF_FILE_REISERFS_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_REISERFS_BUILDARCH)
PK_PAGING_PF_FILE_REISERFS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-REISERFS
PK_PAGING_PF_FILE_REISERFS_PATCH       := $(PK_PAGING_PF_FILE_REISERFS_DIR)/patch
PK_PAGING_PF_FILE_REISERFS_BSBIT       := BiscuitOS-PAGING-PF-FILE-REISERFS-$(PK_PAGING_PF_FILE_REISERFS_VERSION).bsbit
PK_PAGING_PF_FILE_REISERFS_OUT         :=
PK_PAGING_PF_FILE_REISERFS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_REISERFS_TAR         := $(CONFIG_PK_PAGING_PF_FILE_REISERFS_TAR)
PK_PAGING_PF_FILE_REISERFS_PACKAGE     :=
PK_PAGING_PF_FILE_REISERFS_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_REISERFS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_REISERFS_DIR)/build

PK_PAGING_PF_FILE_REISERFS_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_REISERFS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_REISERFS_NAME)X \
                 $(PK_PAGING_PF_FILE_REISERFS_VERSION)X \
                 $(PK_PAGING_PF_FILE_REISERFS_SITE)X \
                 $(PK_PAGING_PF_FILE_REISERFS_GITHUB)X \
                 $(PK_PAGING_PF_FILE_REISERFS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_REISERFS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_REISERFS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_REISERFS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_REISERFS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_REISERFS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_REISERFS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_REISERFS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_REISERFS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_REISERFS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_REISERFS_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_REISERFS_BSBIT)X \
                 $(PK_PAGING_PF_FILE_REISERFS_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_REISERFS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_REISERFS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_REISERFS_CSRCFILE)

$(PK_PAGING_PF_FILE_REISERFS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_REISERFS_sh,$(srctree))
