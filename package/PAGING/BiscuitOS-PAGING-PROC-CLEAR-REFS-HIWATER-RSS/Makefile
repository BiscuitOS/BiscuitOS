PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_NAME        := BiscuitOS-PAGING-PROC-CLEAR-REFS-HIWATER-RSS
PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_VERSION     := $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_VERSION)
PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_SITE        := $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_URL)
PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_GITHUB      := $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_GITHUB)
PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_SUBNAME     :=
PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_BUILDARCH   := $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_BUILDARCH)
PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PROC-CLEAR-REFS-HIWATER-RSS
PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_PATCH       := $(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_DIR)/patch
PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_BSBIT       := BiscuitOS-PAGING-PROC-CLEAR-REFS-HIWATER-RSS-$(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_VERSION).bsbit
PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_OUT         :=
PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_TAR         := $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_TAR)
PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_PACKAGE     :=
PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_CONFIG      := $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_DIR)/build

PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_sh = $(CONFIG_SHELL) $(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_NAME)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_VERSION)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_SITE)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_GITHUB)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_ASFLAGS)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_BSBIT)X \
                 $(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_BUILDARCH)X \
		 $(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_CSRCFILE)

$(PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PROC_CLEAR_REFS_HIWATER_RSS_sh,$(srctree))
