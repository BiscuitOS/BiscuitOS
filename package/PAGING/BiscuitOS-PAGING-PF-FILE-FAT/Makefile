PK_PAGING_PF_FILE_FAT_NAME        := BiscuitOS-PAGING-PF-FILE-FAT
PK_PAGING_PF_FILE_FAT_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_FAT_VERSION)
PK_PAGING_PF_FILE_FAT_SITE        := $(CONFIG_PK_PAGING_PF_FILE_FAT_URL)
PK_PAGING_PF_FILE_FAT_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_FAT_GITHUB)
PK_PAGING_PF_FILE_FAT_SUBNAME     :=
PK_PAGING_PF_FILE_FAT_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_FAT_BUILDARCH)
PK_PAGING_PF_FILE_FAT_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-FAT
PK_PAGING_PF_FILE_FAT_PATCH       := $(PK_PAGING_PF_FILE_FAT_DIR)/patch
PK_PAGING_PF_FILE_FAT_BSBIT       := BiscuitOS-PAGING-PF-FILE-FAT-$(PK_PAGING_PF_FILE_FAT_VERSION).bsbit
PK_PAGING_PF_FILE_FAT_OUT         :=
PK_PAGING_PF_FILE_FAT_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_FAT_TAR         := $(CONFIG_PK_PAGING_PF_FILE_FAT_TAR)
PK_PAGING_PF_FILE_FAT_PACKAGE     :=
PK_PAGING_PF_FILE_FAT_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_FAT_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_FAT_DIR)/build

PK_PAGING_PF_FILE_FAT_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_FAT_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_FAT_NAME)X \
                 $(PK_PAGING_PF_FILE_FAT_VERSION)X \
                 $(PK_PAGING_PF_FILE_FAT_SITE)X \
                 $(PK_PAGING_PF_FILE_FAT_GITHUB)X \
                 $(PK_PAGING_PF_FILE_FAT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_FAT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_FAT_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_FAT_BSBIT)X \
                 $(PK_PAGING_PF_FILE_FAT_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_FAT_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_CSRCFILE)

$(PK_PAGING_PF_FILE_FAT_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_FAT_sh,$(srctree))
