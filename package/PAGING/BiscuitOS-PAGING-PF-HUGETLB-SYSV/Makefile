PK_PAGING_PF_HUGETLB_SYSV_NAME        := BiscuitOS-PAGING-PF-HUGETLB-SYSV
PK_PAGING_PF_HUGETLB_SYSV_VERSION     := $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_VERSION)
PK_PAGING_PF_HUGETLB_SYSV_SITE        := $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_URL)
PK_PAGING_PF_HUGETLB_SYSV_GITHUB      := $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_GITHUB)
PK_PAGING_PF_HUGETLB_SYSV_SUBNAME     :=
PK_PAGING_PF_HUGETLB_SYSV_BUILDARCH   := $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_BUILDARCH)
PK_PAGING_PF_HUGETLB_SYSV_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-HUGETLB-SYSV
PK_PAGING_PF_HUGETLB_SYSV_PATCH       := $(PK_PAGING_PF_HUGETLB_SYSV_DIR)/patch
PK_PAGING_PF_HUGETLB_SYSV_BSBIT       := BiscuitOS-PAGING-PF-HUGETLB-SYSV-$(PK_PAGING_PF_HUGETLB_SYSV_VERSION).bsbit
PK_PAGING_PF_HUGETLB_SYSV_OUT         :=
PK_PAGING_PF_HUGETLB_SYSV_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_HUGETLB_SYSV_TAR         := $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_TAR)
PK_PAGING_PF_HUGETLB_SYSV_PACKAGE     :=
PK_PAGING_PF_HUGETLB_SYSV_CONFIG      := $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_HUGETLB_SYSV_DIR)/build

PK_PAGING_PF_HUGETLB_SYSV_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_HUGETLB_SYSV_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_HUGETLB_SYSV_NAME)X \
                 $(PK_PAGING_PF_HUGETLB_SYSV_VERSION)X \
                 $(PK_PAGING_PF_HUGETLB_SYSV_SITE)X \
                 $(PK_PAGING_PF_HUGETLB_SYSV_GITHUB)X \
                 $(PK_PAGING_PF_HUGETLB_SYSV_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_HUGETLB_SYSV_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_HUGETLB_SYSV_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_ASFLAGS)X \
                 $(PK_PAGING_PF_HUGETLB_SYSV_BSBIT)X \
                 $(PK_PAGING_PF_HUGETLB_SYSV_BUILDARCH)X \
		 $(PK_PAGING_PF_HUGETLB_SYSV_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_HUGETLB_SYSV_CSRCFILE)

$(PK_PAGING_PF_HUGETLB_SYSV_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_HUGETLB_SYSV_sh,$(srctree))
