PK_PAGING_PF_ECODE_RSVD_NAME        := BiscuitOS-PAGING-PF-ECODE-RSVD
PK_PAGING_PF_ECODE_RSVD_VERSION     := $(CONFIG_PK_PAGING_PF_ECODE_RSVD_VERSION)
PK_PAGING_PF_ECODE_RSVD_SITE        := $(CONFIG_PK_PAGING_PF_ECODE_RSVD_URL)
PK_PAGING_PF_ECODE_RSVD_GITHUB      := $(CONFIG_PK_PAGING_PF_ECODE_RSVD_GITHUB)
PK_PAGING_PF_ECODE_RSVD_SUBNAME     :=
PK_PAGING_PF_ECODE_RSVD_BUILDARCH   := $(CONFIG_PK_PAGING_PF_ECODE_RSVD_BUILDARCH)
PK_PAGING_PF_ECODE_RSVD_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-ECODE-RSVD
PK_PAGING_PF_ECODE_RSVD_PATCH       := $(PK_PAGING_PF_ECODE_RSVD_DIR)/patch
PK_PAGING_PF_ECODE_RSVD_BSBIT       := BiscuitOS-PAGING-PF-ECODE-RSVD-$(PK_PAGING_PF_ECODE_RSVD_VERSION).bsbit
PK_PAGING_PF_ECODE_RSVD_OUT         :=
PK_PAGING_PF_ECODE_RSVD_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PF_ECODE_RSVD_TAR         := $(CONFIG_PK_PAGING_PF_ECODE_RSVD_TAR)
PK_PAGING_PF_ECODE_RSVD_PACKAGE     :=
PK_PAGING_PF_ECODE_RSVD_CONFIG      := $(CONFIG_PK_PAGING_PF_ECODE_RSVD_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_ECODE_RSVD_DIR)/build

PK_PAGING_PF_ECODE_RSVD_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_ECODE_RSVD_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_ECODE_RSVD_NAME)X \
                 $(PK_PAGING_PF_ECODE_RSVD_VERSION)X \
                 $(PK_PAGING_PF_ECODE_RSVD_SITE)X \
                 $(PK_PAGING_PF_ECODE_RSVD_GITHUB)X \
                 $(PK_PAGING_PF_ECODE_RSVD_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_ECODE_RSVD_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_ECODE_RSVD_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_RSVD_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_RSVD_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_RSVD_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_RSVD_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_RSVD_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_RSVD_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_RSVD_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_RSVD_ASFLAGS)X \
                 $(PK_PAGING_PF_ECODE_RSVD_BSBIT)X \
                 $(PK_PAGING_PF_ECODE_RSVD_BUILDARCH)X \
		 $(PK_PAGING_PF_ECODE_RSVD_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_RSVD_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_RSVD_CSRCFILE)

$(PK_PAGING_PF_ECODE_RSVD_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_ECODE_RSVD_sh,$(srctree))
