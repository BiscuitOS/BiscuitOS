PK_PAGING_4L_PAGING_MMAP_4K_SITE        := $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_URL)
PK_PAGING_4L_PAGING_MMAP_4K_GITHUB      := $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_GITHUB)
PK_PAGING_4L_PAGING_MMAP_4K_SUBNAME     :=
PK_PAGING_4L_PAGING_MMAP_4K_BUILDARCH   := $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_BUILDARCH)
PK_PAGING_4L_PAGING_MMAP_4K_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-4level-Paging-MMAP-4KiB
PK_PAGING_4L_PAGING_MMAP_4K_PATCH       := $(PK_PAGING_4L_PAGING_MMAP_4K_DIR)/patch
PK_PAGING_4L_PAGING_MMAP_4K_BSBIT       := BiscuitOS-PAGING-4level-Paging-MMAP-4KiB-$(PK_PAGING_4L_PAGING_MMAP_4K_VERSION).bsbit
PK_PAGING_4L_PAGING_MMAP_4K_OUT         :=
PK_PAGING_4L_PAGING_MMAP_4K_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_4L_PAGING_MMAP_4K_TAR         := $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_TAR)
PK_PAGING_4L_PAGING_MMAP_4K_PACKAGE     :=
PK_PAGING_4L_PAGING_MMAP_4K_CONFIG      := $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_CONFIG)
SUB_TARGET                  += $(PK_PAGING_4L_PAGING_MMAP_4K_DIR)/build

PK_PAGING_4L_PAGING_MMAP_4K_sh = $(CONFIG_SHELL) $(PK_PAGING_4L_PAGING_MMAP_4K_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_4L_PAGING_MMAP_4K_NAME)X \
                 $(PK_PAGING_4L_PAGING_MMAP_4K_VERSION)X \
                 $(PK_PAGING_4L_PAGING_MMAP_4K_SITE)X \
                 $(PK_PAGING_4L_PAGING_MMAP_4K_GITHUB)X \
                 $(PK_PAGING_4L_PAGING_MMAP_4K_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_4L_PAGING_MMAP_4K_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_4L_PAGING_MMAP_4K_SUBNAME)X \
                 $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_CONFIG)X \
                 $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_LIBPATH)X \
                 $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_CFLAGS)X \
                 $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_ASFLAGS)X \
                 $(PK_PAGING_4L_PAGING_MMAP_4K_BSBIT)X \
                 $(PK_PAGING_4L_PAGING_MMAP_4K_BUILDARCH)X \
		 $(PK_PAGING_4L_PAGING_MMAP_4K_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_SRCFILE)X \
                 $(CONFIG_PK_PAGING_4L_PAGING_MMAP_4K_CSRCFILE)

$(PK_PAGING_4L_PAGING_MMAP_4K_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_4L_PAGING_MMAP_4K_sh,$(srctree))
