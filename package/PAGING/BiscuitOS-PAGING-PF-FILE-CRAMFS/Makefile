PK_PAGING_PF_FILE_CRAMFS_NAME        := BiscuitOS-PAGING-PF-FILE-CRAMFS
PK_PAGING_PF_FILE_CRAMFS_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_VERSION)
PK_PAGING_PF_FILE_CRAMFS_SITE        := $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_URL)
PK_PAGING_PF_FILE_CRAMFS_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_GITHUB)
PK_PAGING_PF_FILE_CRAMFS_SUBNAME     :=
PK_PAGING_PF_FILE_CRAMFS_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_BUILDARCH)
PK_PAGING_PF_FILE_CRAMFS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-CRAMFS
PK_PAGING_PF_FILE_CRAMFS_PATCH       := $(PK_PAGING_PF_FILE_CRAMFS_DIR)/patch
PK_PAGING_PF_FILE_CRAMFS_BSBIT       := BiscuitOS-PAGING-PF-FILE-CRAMFS-$(PK_PAGING_PF_FILE_CRAMFS_VERSION).bsbit
PK_PAGING_PF_FILE_CRAMFS_OUT         :=
PK_PAGING_PF_FILE_CRAMFS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_CRAMFS_TAR         := $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_TAR)
PK_PAGING_PF_FILE_CRAMFS_PACKAGE     :=
PK_PAGING_PF_FILE_CRAMFS_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_CRAMFS_DIR)/build

PK_PAGING_PF_FILE_CRAMFS_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_CRAMFS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_CRAMFS_NAME)X \
                 $(PK_PAGING_PF_FILE_CRAMFS_VERSION)X \
                 $(PK_PAGING_PF_FILE_CRAMFS_SITE)X \
                 $(PK_PAGING_PF_FILE_CRAMFS_GITHUB)X \
                 $(PK_PAGING_PF_FILE_CRAMFS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_CRAMFS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_CRAMFS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_CRAMFS_BSBIT)X \
                 $(PK_PAGING_PF_FILE_CRAMFS_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_CRAMFS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_CRAMFS_CSRCFILE)

$(PK_PAGING_PF_FILE_CRAMFS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_CRAMFS_sh,$(srctree))
