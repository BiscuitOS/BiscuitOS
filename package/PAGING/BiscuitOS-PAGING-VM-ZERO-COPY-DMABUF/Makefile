PK_PAGING_VM_ZERO_COPY_DMABUF_NAME        := BiscuitOS-PAGING-VM-ZERO-COPY-DMABUF
PK_PAGING_VM_ZERO_COPY_DMABUF_VERSION     := $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_VERSION)
PK_PAGING_VM_ZERO_COPY_DMABUF_SITE        := $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_URL)
PK_PAGING_VM_ZERO_COPY_DMABUF_GITHUB      := $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_GITHUB)
PK_PAGING_VM_ZERO_COPY_DMABUF_SUBNAME     :=
PK_PAGING_VM_ZERO_COPY_DMABUF_BUILDARCH   := $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_BUILDARCH)
PK_PAGING_VM_ZERO_COPY_DMABUF_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-VM-ZERO-COPY-DMABUF
PK_PAGING_VM_ZERO_COPY_DMABUF_PATCH       := $(PK_PAGING_VM_ZERO_COPY_DMABUF_DIR)/patch
PK_PAGING_VM_ZERO_COPY_DMABUF_BSBIT       := BiscuitOS-PAGING-VM-ZERO-COPY-DMABUF-$(PK_PAGING_VM_ZERO_COPY_DMABUF_VERSION).bsbit
PK_PAGING_VM_ZERO_COPY_DMABUF_OUT         :=
PK_PAGING_VM_ZERO_COPY_DMABUF_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_VM_ZERO_COPY_DMABUF_TAR         := $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_TAR)
PK_PAGING_VM_ZERO_COPY_DMABUF_PACKAGE     :=
PK_PAGING_VM_ZERO_COPY_DMABUF_CONFIG      := $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_CONFIG)
SUB_TARGET                  += $(PK_PAGING_VM_ZERO_COPY_DMABUF_DIR)/build

PK_PAGING_VM_ZERO_COPY_DMABUF_sh = $(CONFIG_SHELL) $(PK_PAGING_VM_ZERO_COPY_DMABUF_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_VM_ZERO_COPY_DMABUF_NAME)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMABUF_VERSION)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMABUF_SITE)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMABUF_GITHUB)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMABUF_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMABUF_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMABUF_SUBNAME)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_CONFIG)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_LIBPATH)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_CFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_ASFLAGS)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMABUF_BSBIT)X \
                 $(PK_PAGING_VM_ZERO_COPY_DMABUF_BUILDARCH)X \
		 $(PK_PAGING_VM_ZERO_COPY_DMABUF_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_SRCFILE)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_COPY_DMABUF_CSRCFILE)

$(PK_PAGING_VM_ZERO_COPY_DMABUF_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_VM_ZERO_COPY_DMABUF_sh,$(srctree))
