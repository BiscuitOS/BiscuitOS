PK_PAGING_PAGEWALK_PERMANENT_NAME        := BiscuitOS-PAGING-PageWalk-Permanent
PK_PAGING_PAGEWALK_PERMANENT_VERSION     := $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_VERSION)
PK_PAGING_PAGEWALK_PERMANENT_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_URL)
PK_PAGING_PAGEWALK_PERMANENT_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_GITHUB)
PK_PAGING_PAGEWALK_PERMANENT_SUBNAME     :=
PK_PAGING_PAGEWALK_PERMANENT_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_BUILDARCH)
PK_PAGING_PAGEWALK_PERMANENT_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-Permanent
PK_PAGING_PAGEWALK_PERMANENT_PATCH       := $(PK_PAGING_PAGEWALK_PERMANENT_DIR)/patch
PK_PAGING_PAGEWALK_PERMANENT_BSBIT       := BiscuitOS-PAGING-PageWalk-Permanent-$(PK_PAGING_PAGEWALK_PERMANENT_VERSION).bsbit
PK_PAGING_PAGEWALK_PERMANENT_OUT         :=
PK_PAGING_PAGEWALK_PERMANENT_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_PERMANENT_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_TAR)
PK_PAGING_PAGEWALK_PERMANENT_PACKAGE     :=
PK_PAGING_PAGEWALK_PERMANENT_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_PERMANENT_DIR)/build

PK_PAGING_PAGEWALK_PERMANENT_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_PERMANENT_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_PERMANENT_NAME)X \
                 $(PK_PAGING_PAGEWALK_PERMANENT_VERSION)X \
                 $(PK_PAGING_PAGEWALK_PERMANENT_SITE)X \
                 $(PK_PAGING_PAGEWALK_PERMANENT_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_PERMANENT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_PERMANENT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_PERMANENT_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_PERMANENT_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_PERMANENT_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_PERMANENT_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_PERMANENT_CSRCFILE)

$(PK_PAGING_PAGEWALK_PERMANENT_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_PERMANENT_sh,$(srctree))
