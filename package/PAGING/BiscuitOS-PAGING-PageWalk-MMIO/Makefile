PK_PAGING_PAGEWALK_MMIO_NAME        := BiscuitOS-PAGING-PageWalk-MMIO
PK_PAGING_PAGEWALK_MMIO_VERSION     := $(CONFIG_PK_PAGING_PAGEWALK_MMIO_VERSION)
PK_PAGING_PAGEWALK_MMIO_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_MMIO_URL)
PK_PAGING_PAGEWALK_MMIO_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_MMIO_GITHUB)
PK_PAGING_PAGEWALK_MMIO_SUBNAME     :=
PK_PAGING_PAGEWALK_MMIO_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_MMIO_BUILDARCH)
PK_PAGING_PAGEWALK_MMIO_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-MMIO
PK_PAGING_PAGEWALK_MMIO_PATCH       := $(PK_PAGING_PAGEWALK_MMIO_DIR)/patch
PK_PAGING_PAGEWALK_MMIO_BSBIT       := BiscuitOS-PAGING-PageWalk-MMIO-$(PK_PAGING_PAGEWALK_MMIO_VERSION).bsbit
PK_PAGING_PAGEWALK_MMIO_OUT         :=
PK_PAGING_PAGEWALK_MMIO_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_MMIO_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_MMIO_TAR)
PK_PAGING_PAGEWALK_MMIO_PACKAGE     :=
PK_PAGING_PAGEWALK_MMIO_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_MMIO_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_MMIO_DIR)/build

PK_PAGING_PAGEWALK_MMIO_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_MMIO_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_MMIO_NAME)X \
                 $(PK_PAGING_PAGEWALK_MMIO_VERSION)X \
                 $(PK_PAGING_PAGEWALK_MMIO_SITE)X \
                 $(PK_PAGING_PAGEWALK_MMIO_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_MMIO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_MMIO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_MMIO_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MMIO_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MMIO_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MMIO_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MMIO_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MMIO_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MMIO_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MMIO_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MMIO_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_MMIO_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_MMIO_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_MMIO_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MMIO_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MMIO_CSRCFILE)

$(PK_PAGING_PAGEWALK_MMIO_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_MMIO_sh,$(srctree))
