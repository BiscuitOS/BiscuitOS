PK_PAGING_PROC_SMAPS_ROLLUP_NAME        := BiscuitOS-PAGING-PROC-SMAPS-ROLLUP
PK_PAGING_PROC_SMAPS_ROLLUP_VERSION     := $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_VERSION)
PK_PAGING_PROC_SMAPS_ROLLUP_SITE        := $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_URL)
PK_PAGING_PROC_SMAPS_ROLLUP_GITHUB      := $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_GITHUB)
PK_PAGING_PROC_SMAPS_ROLLUP_SUBNAME     :=
PK_PAGING_PROC_SMAPS_ROLLUP_BUILDARCH   := $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_BUILDARCH)
PK_PAGING_PROC_SMAPS_ROLLUP_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PROC-SMAPS-ROLLUP
PK_PAGING_PROC_SMAPS_ROLLUP_PATCH       := $(PK_PAGING_PROC_SMAPS_ROLLUP_DIR)/patch
PK_PAGING_PROC_SMAPS_ROLLUP_BSBIT       := BiscuitOS-PAGING-PROC-SMAPS-ROLLUP-$(PK_PAGING_PROC_SMAPS_ROLLUP_VERSION).bsbit
PK_PAGING_PROC_SMAPS_ROLLUP_OUT         :=
PK_PAGING_PROC_SMAPS_ROLLUP_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PROC_SMAPS_ROLLUP_TAR         := $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_TAR)
PK_PAGING_PROC_SMAPS_ROLLUP_PACKAGE     :=
PK_PAGING_PROC_SMAPS_ROLLUP_CONFIG      := $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PROC_SMAPS_ROLLUP_DIR)/build

PK_PAGING_PROC_SMAPS_ROLLUP_sh = $(CONFIG_SHELL) $(PK_PAGING_PROC_SMAPS_ROLLUP_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PROC_SMAPS_ROLLUP_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_ROLLUP_VERSION)X \
                 $(PK_PAGING_PROC_SMAPS_ROLLUP_SITE)X \
                 $(PK_PAGING_PROC_SMAPS_ROLLUP_GITHUB)X \
                 $(PK_PAGING_PROC_SMAPS_ROLLUP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_ROLLUP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PROC_SMAPS_ROLLUP_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_CONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_ASFLAGS)X \
                 $(PK_PAGING_PROC_SMAPS_ROLLUP_BSBIT)X \
                 $(PK_PAGING_PROC_SMAPS_ROLLUP_BUILDARCH)X \
		 $(PK_PAGING_PROC_SMAPS_ROLLUP_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_ROLLUP_CSRCFILE)

$(PK_PAGING_PROC_SMAPS_ROLLUP_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PROC_SMAPS_ROLLUP_sh,$(srctree))
