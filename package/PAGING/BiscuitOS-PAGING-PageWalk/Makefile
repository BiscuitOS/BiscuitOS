################################################
#
# BiscuitOS-PAGING-PageWalk
#
################################################
PK_PAGING_PAGEWALK_NAME        := BiscuitOS-PAGING-PageWalk
PK_PAGING_PAGEWALK_VERSION     := $(CONFIG_PK_PAGING_PAGEWALK_VERSION)
PK_PAGING_PAGEWALK_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_URL)
PK_PAGING_PAGEWALK_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_GITHUB)
PK_PAGING_PAGEWALK_SUBNAME     :=
PK_PAGING_PAGEWALK_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_BUILDARCH)
PK_PAGING_PAGEWALK_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk
PK_PAGING_PAGEWALK_PATCH       := $(PK_PAGING_PAGEWALK_DIR)/patch
PK_PAGING_PAGEWALK_BSBIT       := BiscuitOS-PAGING-PageWalk-$(PK_PAGING_PAGEWALK_VERSION).bsbit
PK_PAGING_PAGEWALK_OUT         :=
PK_PAGING_PAGEWALK_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_TAR)
PK_PAGING_PAGEWALK_PACKAGE     :=
PK_PAGING_PAGEWALK_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_DIR)/build

PK_PAGING_PAGEWALK_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_NAME)X \
                 $(PK_PAGING_PAGEWALK_VERSION)X \
                 $(PK_PAGING_PAGEWALK_SITE)X \
                 $(PK_PAGING_PAGEWALK_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CSRCFILE)

$(PK_PAGING_PAGEWALK_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_sh,$(srctree))
