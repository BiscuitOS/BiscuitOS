PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_NAME        := BiscuitOS-PAGING-PT-K2PT-PAGE-KERNEL-EXEC
PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_VERSION     := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_VERSION)
PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_SITE        := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_URL)
PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_GITHUB      := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_GITHUB)
PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_SUBNAME     :=
PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_BUILDARCH   := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_BUILDARCH)
PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PT-K2PT-PAGE-KERNEL-EXEC
PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_PATCH       := $(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_DIR)/patch
PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_BSBIT       := BiscuitOS-PAGING-PT-K2PT-PAGE-KERNEL-EXEC-$(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_VERSION).bsbit
PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_OUT         :=
PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_TAR         := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_TAR)
PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_PACKAGE     :=
PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_CONFIG      := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_DIR)/build

PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_NAME)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_VERSION)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_SITE)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_GITHUB)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_ASFLAGS)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_BSBIT)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_BUILDARCH)X \
		 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_CSRCFILE)

$(PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PT_K2PT_PAGE_KERNEL_EXEC_sh,$(srctree))
