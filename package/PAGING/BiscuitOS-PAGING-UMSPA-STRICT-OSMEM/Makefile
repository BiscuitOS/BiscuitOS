PK_PAGING_UMSPA_STRICT_OSMEM_NAME        := BiscuitOS-PAGING-UMSPA-STRICT-OSMEM
PK_PAGING_UMSPA_STRICT_OSMEM_VERSION     := $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_VERSION)
PK_PAGING_UMSPA_STRICT_OSMEM_SITE        := $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_URL)
PK_PAGING_UMSPA_STRICT_OSMEM_GITHUB      := $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_GITHUB)
PK_PAGING_UMSPA_STRICT_OSMEM_SUBNAME     :=
PK_PAGING_UMSPA_STRICT_OSMEM_BUILDARCH   := $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_BUILDARCH)
PK_PAGING_UMSPA_STRICT_OSMEM_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-UMSPA-STRICT-OSMEM
PK_PAGING_UMSPA_STRICT_OSMEM_PATCH       := $(PK_PAGING_UMSPA_STRICT_OSMEM_DIR)/patch
PK_PAGING_UMSPA_STRICT_OSMEM_BSBIT       := BiscuitOS-PAGING-UMSPA-STRICT-OSMEM-$(PK_PAGING_UMSPA_STRICT_OSMEM_VERSION).bsbit
PK_PAGING_UMSPA_STRICT_OSMEM_OUT         :=
PK_PAGING_UMSPA_STRICT_OSMEM_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_UMSPA_STRICT_OSMEM_TAR         := $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_TAR)
PK_PAGING_UMSPA_STRICT_OSMEM_PACKAGE     :=
PK_PAGING_UMSPA_STRICT_OSMEM_CONFIG      := $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_CONFIG)
SUB_TARGET                  += $(PK_PAGING_UMSPA_STRICT_OSMEM_DIR)/build

PK_PAGING_UMSPA_STRICT_OSMEM_sh = $(CONFIG_SHELL) $(PK_PAGING_UMSPA_STRICT_OSMEM_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_UMSPA_STRICT_OSMEM_NAME)X \
                 $(PK_PAGING_UMSPA_STRICT_OSMEM_VERSION)X \
                 $(PK_PAGING_UMSPA_STRICT_OSMEM_SITE)X \
                 $(PK_PAGING_UMSPA_STRICT_OSMEM_GITHUB)X \
                 $(PK_PAGING_UMSPA_STRICT_OSMEM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_UMSPA_STRICT_OSMEM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_UMSPA_STRICT_OSMEM_SUBNAME)X \
                 $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_CONFIG)X \
                 $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_LIBPATH)X \
                 $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_CFLAGS)X \
                 $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_ASFLAGS)X \
                 $(PK_PAGING_UMSPA_STRICT_OSMEM_BSBIT)X \
                 $(PK_PAGING_UMSPA_STRICT_OSMEM_BUILDARCH)X \
		 $(PK_PAGING_UMSPA_STRICT_OSMEM_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_SRCFILE)X \
                 $(CONFIG_PK_PAGING_UMSPA_STRICT_OSMEM_CSRCFILE)

$(PK_PAGING_UMSPA_STRICT_OSMEM_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_UMSPA_STRICT_OSMEM_sh,$(srctree))
