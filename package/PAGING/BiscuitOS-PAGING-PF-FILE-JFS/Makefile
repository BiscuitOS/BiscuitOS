PK_PAGING_PF_FILE_JFS_NAME        := BiscuitOS-PAGING-PF-FILE-JFS
PK_PAGING_PF_FILE_JFS_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_JFS_VERSION)
PK_PAGING_PF_FILE_JFS_SITE        := $(CONFIG_PK_PAGING_PF_FILE_JFS_URL)
PK_PAGING_PF_FILE_JFS_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_JFS_GITHUB)
PK_PAGING_PF_FILE_JFS_SUBNAME     :=
PK_PAGING_PF_FILE_JFS_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_JFS_BUILDARCH)
PK_PAGING_PF_FILE_JFS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-JFS
PK_PAGING_PF_FILE_JFS_PATCH       := $(PK_PAGING_PF_FILE_JFS_DIR)/patch
PK_PAGING_PF_FILE_JFS_BSBIT       := BiscuitOS-PAGING-PF-FILE-JFS-$(PK_PAGING_PF_FILE_JFS_VERSION).bsbit
PK_PAGING_PF_FILE_JFS_OUT         :=
PK_PAGING_PF_FILE_JFS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_JFS_TAR         := $(CONFIG_PK_PAGING_PF_FILE_JFS_TAR)
PK_PAGING_PF_FILE_JFS_PACKAGE     :=
PK_PAGING_PF_FILE_JFS_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_JFS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_JFS_DIR)/build

PK_PAGING_PF_FILE_JFS_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_JFS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_JFS_NAME)X \
                 $(PK_PAGING_PF_FILE_JFS_VERSION)X \
                 $(PK_PAGING_PF_FILE_JFS_SITE)X \
                 $(PK_PAGING_PF_FILE_JFS_GITHUB)X \
                 $(PK_PAGING_PF_FILE_JFS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_JFS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_JFS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_JFS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_JFS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_JFS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_JFS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_JFS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_JFS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_JFS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_JFS_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_JFS_BSBIT)X \
                 $(PK_PAGING_PF_FILE_JFS_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_JFS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_JFS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_JFS_CSRCFILE)

$(PK_PAGING_PF_FILE_JFS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_JFS_sh,$(srctree))
