PK_PAGING_PROC_SMAPS_SHARED_NAME        := BiscuitOS-PAGING-PROC-SMAPS-SHARED
PK_PAGING_PROC_SMAPS_SHARED_VERSION     := $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_VERSION)
PK_PAGING_PROC_SMAPS_SHARED_SITE        := $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_URL)
PK_PAGING_PROC_SMAPS_SHARED_GITHUB      := $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_GITHUB)
PK_PAGING_PROC_SMAPS_SHARED_SUBNAME     :=
PK_PAGING_PROC_SMAPS_SHARED_BUILDARCH   := $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_BUILDARCH)
PK_PAGING_PROC_SMAPS_SHARED_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PROC-SMAPS-SHARED
PK_PAGING_PROC_SMAPS_SHARED_PATCH       := $(PK_PAGING_PROC_SMAPS_SHARED_DIR)/patch
PK_PAGING_PROC_SMAPS_SHARED_BSBIT       := BiscuitOS-PAGING-PROC-SMAPS-SHARED-$(PK_PAGING_PROC_SMAPS_SHARED_VERSION).bsbit
PK_PAGING_PROC_SMAPS_SHARED_OUT         :=
PK_PAGING_PROC_SMAPS_SHARED_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PROC_SMAPS_SHARED_TAR         := $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_TAR)
PK_PAGING_PROC_SMAPS_SHARED_PACKAGE     :=
PK_PAGING_PROC_SMAPS_SHARED_CONFIG      := $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PROC_SMAPS_SHARED_DIR)/build

PK_PAGING_PROC_SMAPS_SHARED_sh = $(CONFIG_SHELL) $(PK_PAGING_PROC_SMAPS_SHARED_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_VERSION)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_SITE)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_GITHUB)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_CONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_ASFLAGS)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_BSBIT)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_BUILDARCH)X \
		 $(PK_PAGING_PROC_SMAPS_SHARED_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_CSRCFILE)

$(PK_PAGING_PROC_SMAPS_SHARED_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PROC_SMAPS_SHARED_sh,$(srctree))
