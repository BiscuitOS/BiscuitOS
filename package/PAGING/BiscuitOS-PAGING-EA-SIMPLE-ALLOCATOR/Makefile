PK_PAGING_EA_SIMPLE_ALLOCATOR_NAME        := BiscuitOS-PAGING-EA-SIMPLE-ALLOCATOR
PK_PAGING_EA_SIMPLE_ALLOCATOR_VERSION     := $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_VERSION)
PK_PAGING_EA_SIMPLE_ALLOCATOR_SITE        := $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_URL)
PK_PAGING_EA_SIMPLE_ALLOCATOR_GITHUB      := $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_GITHUB)
PK_PAGING_EA_SIMPLE_ALLOCATOR_SUBNAME     :=
PK_PAGING_EA_SIMPLE_ALLOCATOR_BUILDARCH   := $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_BUILDARCH)
PK_PAGING_EA_SIMPLE_ALLOCATOR_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-EA-SIMPLE-ALLOCATOR
PK_PAGING_EA_SIMPLE_ALLOCATOR_PATCH       := $(PK_PAGING_EA_SIMPLE_ALLOCATOR_DIR)/patch
PK_PAGING_EA_SIMPLE_ALLOCATOR_BSBIT       := BiscuitOS-PAGING-EA-SIMPLE-ALLOCATOR-$(PK_PAGING_EA_SIMPLE_ALLOCATOR_VERSION).bsbit
PK_PAGING_EA_SIMPLE_ALLOCATOR_OUT         :=
PK_PAGING_EA_SIMPLE_ALLOCATOR_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_EA_SIMPLE_ALLOCATOR_TAR         := $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_TAR)
PK_PAGING_EA_SIMPLE_ALLOCATOR_PACKAGE     :=
PK_PAGING_EA_SIMPLE_ALLOCATOR_CONFIG      := $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_CONFIG)
SUB_TARGET                  += $(PK_PAGING_EA_SIMPLE_ALLOCATOR_DIR)/build

PK_PAGING_EA_SIMPLE_ALLOCATOR_sh = $(CONFIG_SHELL) $(PK_PAGING_EA_SIMPLE_ALLOCATOR_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_EA_SIMPLE_ALLOCATOR_NAME)X \
                 $(PK_PAGING_EA_SIMPLE_ALLOCATOR_VERSION)X \
                 $(PK_PAGING_EA_SIMPLE_ALLOCATOR_SITE)X \
                 $(PK_PAGING_EA_SIMPLE_ALLOCATOR_GITHUB)X \
                 $(PK_PAGING_EA_SIMPLE_ALLOCATOR_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_EA_SIMPLE_ALLOCATOR_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_EA_SIMPLE_ALLOCATOR_SUBNAME)X \
                 $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_CONFIG)X \
                 $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_LIBPATH)X \
                 $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_CFLAGS)X \
                 $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_ASFLAGS)X \
                 $(PK_PAGING_EA_SIMPLE_ALLOCATOR_BSBIT)X \
                 $(PK_PAGING_EA_SIMPLE_ALLOCATOR_BUILDARCH)X \
		 $(PK_PAGING_EA_SIMPLE_ALLOCATOR_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_SRCFILE)X \
                 $(CONFIG_PK_PAGING_EA_SIMPLE_ALLOCATOR_CSRCFILE)

$(PK_PAGING_EA_SIMPLE_ALLOCATOR_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_EA_SIMPLE_ALLOCATOR_sh,$(srctree))
