PK_PAGING_PT_CPR_WIPE_NAME        := BiscuitOS-PAGING-PT-CPR-WIPE
PK_PAGING_PT_CPR_WIPE_VERSION     := $(CONFIG_PK_PAGING_PT_CPR_WIPE_VERSION)
PK_PAGING_PT_CPR_WIPE_SITE        := $(CONFIG_PK_PAGING_PT_CPR_WIPE_URL)
PK_PAGING_PT_CPR_WIPE_GITHUB      := $(CONFIG_PK_PAGING_PT_CPR_WIPE_GITHUB)
PK_PAGING_PT_CPR_WIPE_SUBNAME     :=
PK_PAGING_PT_CPR_WIPE_BUILDARCH   := $(CONFIG_PK_PAGING_PT_CPR_WIPE_BUILDARCH)
PK_PAGING_PT_CPR_WIPE_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PT-CPR-WIPE
PK_PAGING_PT_CPR_WIPE_PATCH       := $(PK_PAGING_PT_CPR_WIPE_DIR)/patch
PK_PAGING_PT_CPR_WIPE_BSBIT       := BiscuitOS-PAGING-PT-CPR-WIPE-$(PK_PAGING_PT_CPR_WIPE_VERSION).bsbit
PK_PAGING_PT_CPR_WIPE_OUT         :=
PK_PAGING_PT_CPR_WIPE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PT_CPR_WIPE_TAR         := $(CONFIG_PK_PAGING_PT_CPR_WIPE_TAR)
PK_PAGING_PT_CPR_WIPE_PACKAGE     :=
PK_PAGING_PT_CPR_WIPE_CONFIG      := $(CONFIG_PK_PAGING_PT_CPR_WIPE_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_CPR_WIPE_DIR)/build

PK_PAGING_PT_CPR_WIPE_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_CPR_WIPE_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PT_CPR_WIPE_NAME)X \
                 $(PK_PAGING_PT_CPR_WIPE_VERSION)X \
                 $(PK_PAGING_PT_CPR_WIPE_SITE)X \
                 $(PK_PAGING_PT_CPR_WIPE_GITHUB)X \
                 $(PK_PAGING_PT_CPR_WIPE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_CPR_WIPE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_CPR_WIPE_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_CPR_WIPE_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_CPR_WIPE_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_CPR_WIPE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_WIPE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_CPR_WIPE_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_WIPE_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_WIPE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_WIPE_ASFLAGS)X \
                 $(PK_PAGING_PT_CPR_WIPE_BSBIT)X \
                 $(PK_PAGING_PT_CPR_WIPE_BUILDARCH)X \
		 $(PK_PAGING_PT_CPR_WIPE_CSRCWIPE)X \
                 $(CONFIG_PK_PAGING_PT_CPR_WIPE_SRCWIPE)X \
                 $(CONFIG_PK_PAGING_PT_CPR_WIPE_CSRCWIPE)

$(PK_PAGING_PT_CPR_WIPE_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PT_CPR_WIPE_sh,$(srctree))
