PK_PAGING_PF_UFFD_ZERO_NAME        := BiscuitOS-PAGING-PF-UFFD-ZERO
PK_PAGING_PF_UFFD_ZERO_VERSION     := $(CONFIG_PK_PAGING_PF_UFFD_ZERO_VERSION)
PK_PAGING_PF_UFFD_ZERO_SITE        := $(CONFIG_PK_PAGING_PF_UFFD_ZERO_URL)
PK_PAGING_PF_UFFD_ZERO_GITHUB      := $(CONFIG_PK_PAGING_PF_UFFD_ZERO_GITHUB)
PK_PAGING_PF_UFFD_ZERO_SUBNAME     :=
PK_PAGING_PF_UFFD_ZERO_BUILDARCH   := $(CONFIG_PK_PAGING_PF_UFFD_ZERO_BUILDARCH)
PK_PAGING_PF_UFFD_ZERO_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-UFFD-ZERO
PK_PAGING_PF_UFFD_ZERO_PATCH       := $(PK_PAGING_PF_UFFD_ZERO_DIR)/patch
PK_PAGING_PF_UFFD_ZERO_BSBIT       := BiscuitOS-PAGING-PF-UFFD-ZERO-$(PK_PAGING_PF_UFFD_ZERO_VERSION).bsbit
PK_PAGING_PF_UFFD_ZERO_OUT         :=
PK_PAGING_PF_UFFD_ZERO_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_UFFD_ZERO_TAR         := $(CONFIG_PK_PAGING_PF_UFFD_ZERO_TAR)
PK_PAGING_PF_UFFD_ZERO_PACKAGE     :=
PK_PAGING_PF_UFFD_ZERO_CONFIG      := $(CONFIG_PK_PAGING_PF_UFFD_ZERO_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_UFFD_ZERO_DIR)/build

PK_PAGING_PF_UFFD_ZERO_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_UFFD_ZERO_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_UFFD_ZERO_NAME)X \
                 $(PK_PAGING_PF_UFFD_ZERO_VERSION)X \
                 $(PK_PAGING_PF_UFFD_ZERO_SITE)X \
                 $(PK_PAGING_PF_UFFD_ZERO_GITHUB)X \
                 $(PK_PAGING_PF_UFFD_ZERO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_UFFD_ZERO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_UFFD_ZERO_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_ZERO_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_ZERO_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_ZERO_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_ZERO_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_ZERO_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_ZERO_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_ZERO_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_ZERO_ASFLAGS)X \
                 $(PK_PAGING_PF_UFFD_ZERO_BSBIT)X \
                 $(PK_PAGING_PF_UFFD_ZERO_BUILDARCH)X \
		 $(PK_PAGING_PF_UFFD_ZERO_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_ZERO_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_ZERO_CSRCFILE)

$(PK_PAGING_PF_UFFD_ZERO_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_UFFD_ZERO_sh,$(srctree))
