PK_PAGING_PF_FILE_TMPFS_NAME        := BiscuitOS-PAGING-PF-FILE-TMPFS
PK_PAGING_PF_FILE_TMPFS_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_TMPFS_VERSION)
PK_PAGING_PF_FILE_TMPFS_SITE        := $(CONFIG_PK_PAGING_PF_FILE_TMPFS_URL)
PK_PAGING_PF_FILE_TMPFS_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_TMPFS_GITHUB)
PK_PAGING_PF_FILE_TMPFS_SUBNAME     :=
PK_PAGING_PF_FILE_TMPFS_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_TMPFS_BUILDARCH)
PK_PAGING_PF_FILE_TMPFS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-TMPFS
PK_PAGING_PF_FILE_TMPFS_PATCH       := $(PK_PAGING_PF_FILE_TMPFS_DIR)/patch
PK_PAGING_PF_FILE_TMPFS_BSBIT       := BiscuitOS-PAGING-PF-FILE-TMPFS-$(PK_PAGING_PF_FILE_TMPFS_VERSION).bsbit
PK_PAGING_PF_FILE_TMPFS_OUT         :=
PK_PAGING_PF_FILE_TMPFS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_TMPFS_TAR         := $(CONFIG_PK_PAGING_PF_FILE_TMPFS_TAR)
PK_PAGING_PF_FILE_TMPFS_PACKAGE     :=
PK_PAGING_PF_FILE_TMPFS_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_TMPFS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_TMPFS_DIR)/build

PK_PAGING_PF_FILE_TMPFS_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_TMPFS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_TMPFS_NAME)X \
                 $(PK_PAGING_PF_FILE_TMPFS_VERSION)X \
                 $(PK_PAGING_PF_FILE_TMPFS_SITE)X \
                 $(PK_PAGING_PF_FILE_TMPFS_GITHUB)X \
                 $(PK_PAGING_PF_FILE_TMPFS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_TMPFS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_TMPFS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_TMPFS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_TMPFS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_TMPFS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_TMPFS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_TMPFS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_TMPFS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_TMPFS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_TMPFS_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_TMPFS_BSBIT)X \
                 $(PK_PAGING_PF_FILE_TMPFS_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_TMPFS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_TMPFS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_TMPFS_CSRCFILE)

$(PK_PAGING_PF_FILE_TMPFS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_TMPFS_sh,$(srctree))
