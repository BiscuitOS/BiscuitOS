PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_NAME        := BiscuitOS-PAGING-PT-K2PT-PAGE-KERNEL-VVAR
PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_VERSION     := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_VERSION)
PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_SITE        := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_URL)
PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_GITHUB      := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_GITHUB)
PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_SUBNAME     :=
PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_BUILDARCH   := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_BUILDARCH)
PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PT-K2PT-PAGE-KERNEL-VVAR
PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_PATCH       := $(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_DIR)/patch
PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_BSBIT       := BiscuitOS-PAGING-PT-K2PT-PAGE-KERNEL-VVAR-$(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_VERSION).bsbit
PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_OUT         :=
PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_TAR         := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_TAR)
PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_PACKAGE     :=
PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_CONFIG      := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_DIR)/build

PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_NAME)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_VERSION)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_SITE)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_GITHUB)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_ASFLAGS)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_BSBIT)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_BUILDARCH)X \
		 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_CSRCFILE)

$(PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PT_K2PT_PAGE_KERNEL_VVAR_sh,$(srctree))
