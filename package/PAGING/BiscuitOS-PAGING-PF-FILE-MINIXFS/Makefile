PK_PAGING_PF_FILE_MINIXFS_NAME        := BiscuitOS-PAGING-PF-FILE-MINIXFS
PK_PAGING_PF_FILE_MINIXFS_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_VERSION)
PK_PAGING_PF_FILE_MINIXFS_SITE        := $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_URL)
PK_PAGING_PF_FILE_MINIXFS_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_GITHUB)
PK_PAGING_PF_FILE_MINIXFS_SUBNAME     :=
PK_PAGING_PF_FILE_MINIXFS_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_BUILDARCH)
PK_PAGING_PF_FILE_MINIXFS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-MINIXFS
PK_PAGING_PF_FILE_MINIXFS_PATCH       := $(PK_PAGING_PF_FILE_MINIXFS_DIR)/patch
PK_PAGING_PF_FILE_MINIXFS_BSBIT       := BiscuitOS-PAGING-PF-FILE-MINIXFS-$(PK_PAGING_PF_FILE_MINIXFS_VERSION).bsbit
PK_PAGING_PF_FILE_MINIXFS_OUT         :=
PK_PAGING_PF_FILE_MINIXFS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_MINIXFS_TAR         := $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_TAR)
PK_PAGING_PF_FILE_MINIXFS_PACKAGE     :=
PK_PAGING_PF_FILE_MINIXFS_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_MINIXFS_DIR)/build

PK_PAGING_PF_FILE_MINIXFS_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_MINIXFS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_MINIXFS_NAME)X \
                 $(PK_PAGING_PF_FILE_MINIXFS_VERSION)X \
                 $(PK_PAGING_PF_FILE_MINIXFS_SITE)X \
                 $(PK_PAGING_PF_FILE_MINIXFS_GITHUB)X \
                 $(PK_PAGING_PF_FILE_MINIXFS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_MINIXFS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_MINIXFS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_MINIXFS_BSBIT)X \
                 $(PK_PAGING_PF_FILE_MINIXFS_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_MINIXFS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_MINIXFS_CSRCFILE)

$(PK_PAGING_PF_FILE_MINIXFS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_MINIXFS_sh,$(srctree))
