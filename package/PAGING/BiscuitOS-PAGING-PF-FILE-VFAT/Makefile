PK_PAGING_PF_FILE_VFAT_NAME        := BiscuitOS-PAGING-PF-FILE-VFAT
PK_PAGING_PF_FILE_VFAT_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_VFAT_VERSION)
PK_PAGING_PF_FILE_VFAT_SITE        := $(CONFIG_PK_PAGING_PF_FILE_VFAT_URL)
PK_PAGING_PF_FILE_VFAT_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_VFAT_GITHUB)
PK_PAGING_PF_FILE_VFAT_SUBNAME     :=
PK_PAGING_PF_FILE_VFAT_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_VFAT_BUILDARCH)
PK_PAGING_PF_FILE_VFAT_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-VFAT
PK_PAGING_PF_FILE_VFAT_PATCH       := $(PK_PAGING_PF_FILE_VFAT_DIR)/patch
PK_PAGING_PF_FILE_VFAT_BSBIT       := BiscuitOS-PAGING-PF-FILE-VFAT-$(PK_PAGING_PF_FILE_VFAT_VERSION).bsbit
PK_PAGING_PF_FILE_VFAT_OUT         :=
PK_PAGING_PF_FILE_VFAT_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_VFAT_TAR         := $(CONFIG_PK_PAGING_PF_FILE_VFAT_TAR)
PK_PAGING_PF_FILE_VFAT_PACKAGE     :=
PK_PAGING_PF_FILE_VFAT_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_VFAT_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_VFAT_DIR)/build

PK_PAGING_PF_FILE_VFAT_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_VFAT_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_VFAT_NAME)X \
                 $(PK_PAGING_PF_FILE_VFAT_VERSION)X \
                 $(PK_PAGING_PF_FILE_VFAT_SITE)X \
                 $(PK_PAGING_PF_FILE_VFAT_GITHUB)X \
                 $(PK_PAGING_PF_FILE_VFAT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_VFAT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_VFAT_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_VFAT_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_VFAT_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_VFAT_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_VFAT_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_VFAT_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_VFAT_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_VFAT_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_VFAT_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_VFAT_BSBIT)X \
                 $(PK_PAGING_PF_FILE_VFAT_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_VFAT_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_VFAT_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_VFAT_CSRCFILE)

$(PK_PAGING_PF_FILE_VFAT_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_VFAT_sh,$(srctree))
