PK_PAGING_PF_FILE_DIRECTIO_NAME        := BiscuitOS-PAGING-PF-FILE-DIRECTIO
PK_PAGING_PF_FILE_DIRECTIO_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_VERSION)
PK_PAGING_PF_FILE_DIRECTIO_SITE        := $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_URL)
PK_PAGING_PF_FILE_DIRECTIO_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_GITHUB)
PK_PAGING_PF_FILE_DIRECTIO_SUBNAME     :=
PK_PAGING_PF_FILE_DIRECTIO_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_BUILDARCH)
PK_PAGING_PF_FILE_DIRECTIO_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-DIRECTIO
PK_PAGING_PF_FILE_DIRECTIO_PATCH       := $(PK_PAGING_PF_FILE_DIRECTIO_DIR)/patch
PK_PAGING_PF_FILE_DIRECTIO_BSBIT       := BiscuitOS-PAGING-PF-FILE-DIRECTIO-$(PK_PAGING_PF_FILE_DIRECTIO_VERSION).bsbit
PK_PAGING_PF_FILE_DIRECTIO_OUT         :=
PK_PAGING_PF_FILE_DIRECTIO_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_DIRECTIO_TAR         := $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_TAR)
PK_PAGING_PF_FILE_DIRECTIO_PACKAGE     :=
PK_PAGING_PF_FILE_DIRECTIO_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_DIRECTIO_DIR)/build

PK_PAGING_PF_FILE_DIRECTIO_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_DIRECTIO_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_DIRECTIO_NAME)X \
                 $(PK_PAGING_PF_FILE_DIRECTIO_VERSION)X \
                 $(PK_PAGING_PF_FILE_DIRECTIO_SITE)X \
                 $(PK_PAGING_PF_FILE_DIRECTIO_GITHUB)X \
                 $(PK_PAGING_PF_FILE_DIRECTIO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_DIRECTIO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_DIRECTIO_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_DIRECTIO_BSBIT)X \
                 $(PK_PAGING_PF_FILE_DIRECTIO_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_DIRECTIO_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_DIRECTIO_CSRCFILE)

$(PK_PAGING_PF_FILE_DIRECTIO_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_DIRECTIO_sh,$(srctree))
