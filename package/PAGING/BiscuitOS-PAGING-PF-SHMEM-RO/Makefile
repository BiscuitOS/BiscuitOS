PK_PAGING_PF_SHMEM_RO_NAME        := BiscuitOS-PAGING-PF-SHMEM-RO
PK_PAGING_PF_SHMEM_RO_VERSION     := $(CONFIG_PK_PAGING_PF_SHMEM_RO_VERSION)
PK_PAGING_PF_SHMEM_RO_SITE        := $(CONFIG_PK_PAGING_PF_SHMEM_RO_URL)
PK_PAGING_PF_SHMEM_RO_GITHUB      := $(CONFIG_PK_PAGING_PF_SHMEM_RO_GITHUB)
PK_PAGING_PF_SHMEM_RO_SUBNAME     :=
PK_PAGING_PF_SHMEM_RO_BUILDARCH   := $(CONFIG_PK_PAGING_PF_SHMEM_RO_BUILDARCH)
PK_PAGING_PF_SHMEM_RO_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-SHMEM-RO
PK_PAGING_PF_SHMEM_RO_PATCH       := $(PK_PAGING_PF_SHMEM_RO_DIR)/patch
PK_PAGING_PF_SHMEM_RO_BSBIT       := BiscuitOS-PAGING-PF-SHMEM-RO-$(PK_PAGING_PF_SHMEM_RO_VERSION).bsbit
PK_PAGING_PF_SHMEM_RO_OUT         :=
PK_PAGING_PF_SHMEM_RO_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_SHMEM_RO_TAR         := $(CONFIG_PK_PAGING_PF_SHMEM_RO_TAR)
PK_PAGING_PF_SHMEM_RO_PACKAGE     :=
PK_PAGING_PF_SHMEM_RO_CONFIG      := $(CONFIG_PK_PAGING_PF_SHMEM_RO_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_SHMEM_RO_DIR)/build

PK_PAGING_PF_SHMEM_RO_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_SHMEM_RO_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_SHMEM_RO_NAME)X \
                 $(PK_PAGING_PF_SHMEM_RO_VERSION)X \
                 $(PK_PAGING_PF_SHMEM_RO_SITE)X \
                 $(PK_PAGING_PF_SHMEM_RO_GITHUB)X \
                 $(PK_PAGING_PF_SHMEM_RO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_SHMEM_RO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_SHMEM_RO_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_RO_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_RO_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_RO_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_RO_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_RO_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_RO_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_RO_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_RO_ASFLAGS)X \
                 $(PK_PAGING_PF_SHMEM_RO_BSBIT)X \
                 $(PK_PAGING_PF_SHMEM_RO_BUILDARCH)X \
		 $(PK_PAGING_PF_SHMEM_RO_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_RO_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_RO_CSRCFILE)

$(PK_PAGING_PF_SHMEM_RO_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_SHMEM_RO_sh,$(srctree))
