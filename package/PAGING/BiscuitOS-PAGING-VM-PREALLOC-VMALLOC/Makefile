# BiscuitOS-PAGING-VM-PREALLOC-VMALLOC
#
################################################
PK_PAGING_VM_PREALLOC_VMALLOC_NAME        := BiscuitOS-PAGING-VM-PREALLOC-VMALLOC
PK_PAGING_VM_PREALLOC_VMALLOC_VERSION     := $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_VERSION)
PK_PAGING_VM_PREALLOC_VMALLOC_SITE        := $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_URL)
PK_PAGING_VM_PREALLOC_VMALLOC_GITHUB      := $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_GITHUB)
PK_PAGING_VM_PREALLOC_VMALLOC_SUBNAME     :=
PK_PAGING_VM_PREALLOC_VMALLOC_BUILDARCH   := $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_BUILDARCH)
PK_PAGING_VM_PREALLOC_VMALLOC_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-VM-PREALLOC-VMALLOC
PK_PAGING_VM_PREALLOC_VMALLOC_PATCH       := $(PK_PAGING_VM_PREALLOC_VMALLOC_DIR)/patch
PK_PAGING_VM_PREALLOC_VMALLOC_BSBIT       := BiscuitOS-PAGING-VM-PREALLOC-VMALLOC-$(PK_PAGING_VM_PREALLOC_VMALLOC_VERSION).bsbit
PK_PAGING_VM_PREALLOC_VMALLOC_OUT         :=
PK_PAGING_VM_PREALLOC_VMALLOC_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_VM_PREALLOC_VMALLOC_TAR         := $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_TAR)
PK_PAGING_VM_PREALLOC_VMALLOC_PACKAGE     :=
PK_PAGING_VM_PREALLOC_VMALLOC_CONFIG      := $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_CONFIG)
SUB_TARGET                  += $(PK_PAGING_VM_PREALLOC_VMALLOC_DIR)/build

PK_PAGING_VM_PREALLOC_VMALLOC_sh = $(CONFIG_SHELL) $(PK_PAGING_VM_PREALLOC_VMALLOC_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_VM_PREALLOC_VMALLOC_NAME)X \
                 $(PK_PAGING_VM_PREALLOC_VMALLOC_VERSION)X \
                 $(PK_PAGING_VM_PREALLOC_VMALLOC_SITE)X \
                 $(PK_PAGING_VM_PREALLOC_VMALLOC_GITHUB)X \
                 $(PK_PAGING_VM_PREALLOC_VMALLOC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_VM_PREALLOC_VMALLOC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_VM_PREALLOC_VMALLOC_SUBNAME)X \
                 $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_CONFIG)X \
                 $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_LIBPATH)X \
                 $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_CFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_ASFLAGS)X \
                 $(PK_PAGING_VM_PREALLOC_VMALLOC_BSBIT)X \
                 $(PK_PAGING_VM_PREALLOC_VMALLOC_BUILDARCH)X \
		 $(PK_PAGING_VM_PREALLOC_VMALLOC_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_SRCFILE)X \
                 $(CONFIG_PK_PAGING_VM_PREALLOC_VMALLOC_CSRCFILE)

$(PK_PAGING_VM_PREALLOC_VMALLOC_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_VM_PREALLOC_VMALLOC_sh,$(srctree))
