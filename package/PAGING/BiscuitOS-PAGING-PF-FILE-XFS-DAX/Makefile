PK_PAGING_PF_FILE_XFS_DAX_SITE        := $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_URL)
PK_PAGING_PF_FILE_XFS_DAX_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_GITHUB)
PK_PAGING_PF_FILE_XFS_DAX_SUBNAME     :=
PK_PAGING_PF_FILE_XFS_DAX_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_BUILDARCH)
PK_PAGING_PF_FILE_XFS_DAX_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-XFS-DAX
PK_PAGING_PF_FILE_XFS_DAX_PATCH       := $(PK_PAGING_PF_FILE_XFS_DAX_DIR)/patch
PK_PAGING_PF_FILE_XFS_DAX_BSBIT       := BiscuitOS-PAGING-PF-FILE-XFS-DAX-$(PK_PAGING_PF_FILE_XFS_DAX_VERSION).bsbit
PK_PAGING_PF_FILE_XFS_DAX_OUT         :=
PK_PAGING_PF_FILE_XFS_DAX_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_XFS_DAX_TAR         := $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_TAR)
PK_PAGING_PF_FILE_XFS_DAX_PACKAGE     :=
PK_PAGING_PF_FILE_XFS_DAX_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_XFS_DAX_DIR)/build

PK_PAGING_PF_FILE_XFS_DAX_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_XFS_DAX_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_XFS_DAX_NAME)X \
                 $(PK_PAGING_PF_FILE_XFS_DAX_VERSION)X \
                 $(PK_PAGING_PF_FILE_XFS_DAX_SITE)X \
                 $(PK_PAGING_PF_FILE_XFS_DAX_GITHUB)X \
                 $(PK_PAGING_PF_FILE_XFS_DAX_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_XFS_DAX_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_XFS_DAX_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_XFS_DAX_BSBIT)X \
                 $(PK_PAGING_PF_FILE_XFS_DAX_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_XFS_DAX_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_DAX_CSRCFILE)

$(PK_PAGING_PF_FILE_XFS_DAX_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_XFS_DAX_sh,$(srctree))
