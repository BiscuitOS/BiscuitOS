# BiscuitOS-PAGING-LOOKUP-ADDR-FIXMAP
#
################################################
PK_PAGING_LOOKUP_ADDR_FIXMAP_NAME        := BiscuitOS-PAGING-LOOKUP-ADDR-FIXMAP
PK_PAGING_LOOKUP_ADDR_FIXMAP_VERSION     := $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_VERSION)
PK_PAGING_LOOKUP_ADDR_FIXMAP_SITE        := $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_URL)
PK_PAGING_LOOKUP_ADDR_FIXMAP_GITHUB      := $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_GITHUB)
PK_PAGING_LOOKUP_ADDR_FIXMAP_SUBNAME     :=
PK_PAGING_LOOKUP_ADDR_FIXMAP_BUILDARCH   := $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_BUILDARCH)
PK_PAGING_LOOKUP_ADDR_FIXMAP_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-LOOKUP-ADDR-FIXMAP
PK_PAGING_LOOKUP_ADDR_FIXMAP_PATCH       := $(PK_PAGING_LOOKUP_ADDR_FIXMAP_DIR)/patch
PK_PAGING_LOOKUP_ADDR_FIXMAP_BSBIT       := BiscuitOS-PAGING-LOOKUP-ADDR-FIXMAP-$(PK_PAGING_LOOKUP_ADDR_FIXMAP_VERSION).bsbit
PK_PAGING_LOOKUP_ADDR_FIXMAP_OUT         :=
PK_PAGING_LOOKUP_ADDR_FIXMAP_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_LOOKUP_ADDR_FIXMAP_TAR         := $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_TAR)
PK_PAGING_LOOKUP_ADDR_FIXMAP_PACKAGE     :=
PK_PAGING_LOOKUP_ADDR_FIXMAP_CONFIG      := $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_CONFIG)
SUB_TARGET                  += $(PK_PAGING_LOOKUP_ADDR_FIXMAP_DIR)/build

PK_PAGING_LOOKUP_ADDR_FIXMAP_sh = $(CONFIG_SHELL) $(PK_PAGING_LOOKUP_ADDR_FIXMAP_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_LOOKUP_ADDR_FIXMAP_NAME)X \
                 $(PK_PAGING_LOOKUP_ADDR_FIXMAP_VERSION)X \
                 $(PK_PAGING_LOOKUP_ADDR_FIXMAP_SITE)X \
                 $(PK_PAGING_LOOKUP_ADDR_FIXMAP_GITHUB)X \
                 $(PK_PAGING_LOOKUP_ADDR_FIXMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_LOOKUP_ADDR_FIXMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_LOOKUP_ADDR_FIXMAP_SUBNAME)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_CONFIG)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_LIBPATH)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_CFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_ASFLAGS)X \
                 $(PK_PAGING_LOOKUP_ADDR_FIXMAP_BSBIT)X \
                 $(PK_PAGING_LOOKUP_ADDR_FIXMAP_BUILDARCH)X \
		 $(PK_PAGING_LOOKUP_ADDR_FIXMAP_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_SRCFILE)X \
                 $(CONFIG_PK_PAGING_LOOKUP_ADDR_FIXMAP_CSRCFILE)

$(PK_PAGING_LOOKUP_ADDR_FIXMAP_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_LOOKUP_ADDR_FIXMAP_sh,$(srctree))
