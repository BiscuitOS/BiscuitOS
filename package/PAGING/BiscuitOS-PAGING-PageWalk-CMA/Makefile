PK_PAGING_PAGEWALK_CMA_NAME        := BiscuitOS-PAGING-PageWalk-CMA
PK_PAGING_PAGEWALK_CMA_VERSION     := $(CONFIG_PK_PAGING_PAGEWALK_CMA_VERSION)
PK_PAGING_PAGEWALK_CMA_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_CMA_URL)
PK_PAGING_PAGEWALK_CMA_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_CMA_GITHUB)
PK_PAGING_PAGEWALK_CMA_SUBNAME     :=
PK_PAGING_PAGEWALK_CMA_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_CMA_BUILDARCH)
PK_PAGING_PAGEWALK_CMA_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-CMA
PK_PAGING_PAGEWALK_CMA_PATCH       := $(PK_PAGING_PAGEWALK_CMA_DIR)/patch
PK_PAGING_PAGEWALK_CMA_BSBIT       := BiscuitOS-PAGING-PageWalk-CMA-$(PK_PAGING_PAGEWALK_CMA_VERSION).bsbit
PK_PAGING_PAGEWALK_CMA_OUT         :=
PK_PAGING_PAGEWALK_CMA_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_CMA_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_CMA_TAR)
PK_PAGING_PAGEWALK_CMA_PACKAGE     :=
PK_PAGING_PAGEWALK_CMA_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_CMA_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_CMA_DIR)/build

PK_PAGING_PAGEWALK_CMA_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_CMA_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_CMA_NAME)X \
                 $(PK_PAGING_PAGEWALK_CMA_VERSION)X \
                 $(PK_PAGING_PAGEWALK_CMA_SITE)X \
                 $(PK_PAGING_PAGEWALK_CMA_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_CMA_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_CMA_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_CMA_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CMA_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CMA_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CMA_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CMA_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CMA_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CMA_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CMA_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CMA_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_CMA_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_CMA_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_CMA_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CMA_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CMA_CSRCFILE)

$(PK_PAGING_PAGEWALK_CMA_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_CMA_sh,$(srctree))
