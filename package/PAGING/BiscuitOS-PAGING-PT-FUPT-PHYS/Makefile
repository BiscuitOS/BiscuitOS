PK_PAGING_PT_FUPT_PHYS_NAME        := BiscuitOS-PAGING-PT-FUPT-PHYS
PK_PAGING_PT_FUPT_PHYS_VERSION     := $(CONFIG_PK_PAGING_PT_FUPT_PHYS_VERSION)
PK_PAGING_PT_FUPT_PHYS_SITE        := $(CONFIG_PK_PAGING_PT_FUPT_PHYS_URL)
PK_PAGING_PT_FUPT_PHYS_GITHUB      := $(CONFIG_PK_PAGING_PT_FUPT_PHYS_GITHUB)
PK_PAGING_PT_FUPT_PHYS_SUBNAME     :=
PK_PAGING_PT_FUPT_PHYS_BUILDARCH   := $(CONFIG_PK_PAGING_PT_FUPT_PHYS_BUILDARCH)
PK_PAGING_PT_FUPT_PHYS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PT-FUPT-PHYS
PK_PAGING_PT_FUPT_PHYS_PATCH       := $(PK_PAGING_PT_FUPT_PHYS_DIR)/patch
PK_PAGING_PT_FUPT_PHYS_BSBIT       := BiscuitOS-PAGING-PT-FUPT-PHYS-$(PK_PAGING_PT_FUPT_PHYS_VERSION).bsbit
PK_PAGING_PT_FUPT_PHYS_OUT         :=
PK_PAGING_PT_FUPT_PHYS_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PT_FUPT_PHYS_TAR         := $(CONFIG_PK_PAGING_PT_FUPT_PHYS_TAR)
PK_PAGING_PT_FUPT_PHYS_PACKAGE     :=
PK_PAGING_PT_FUPT_PHYS_CONFIG      := $(CONFIG_PK_PAGING_PT_FUPT_PHYS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_FUPT_PHYS_DIR)/build

PK_PAGING_PT_FUPT_PHYS_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_FUPT_PHYS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PT_FUPT_PHYS_NAME)X \
                 $(PK_PAGING_PT_FUPT_PHYS_VERSION)X \
                 $(PK_PAGING_PT_FUPT_PHYS_SITE)X \
                 $(PK_PAGING_PT_FUPT_PHYS_GITHUB)X \
                 $(PK_PAGING_PT_FUPT_PHYS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_FUPT_PHYS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_FUPT_PHYS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PHYS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PHYS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PHYS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PHYS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PHYS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PHYS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PHYS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PHYS_ASFLAGS)X \
                 $(PK_PAGING_PT_FUPT_PHYS_BSBIT)X \
                 $(PK_PAGING_PT_FUPT_PHYS_BUILDARCH)X \
		 $(PK_PAGING_PT_FUPT_PHYS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PHYS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PHYS_CSRCFILE)

$(PK_PAGING_PT_FUPT_PHYS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PT_FUPT_PHYS_sh,$(srctree))
