PK_PAGING_VMALLOC_HUGE_NAME        := BiscuitOS-PAGING-VMALLOC-HUGE
PK_PAGING_VMALLOC_HUGE_VERSION     := $(CONFIG_PK_PAGING_VMALLOC_HUGE_VERSION)
PK_PAGING_VMALLOC_HUGE_SITE        := $(CONFIG_PK_PAGING_VMALLOC_HUGE_URL)
PK_PAGING_VMALLOC_HUGE_GITHUB      := $(CONFIG_PK_PAGING_VMALLOC_HUGE_GITHUB)
PK_PAGING_VMALLOC_HUGE_SUBNAME     :=
PK_PAGING_VMALLOC_HUGE_BUILDARCH   := $(CONFIG_PK_PAGING_VMALLOC_HUGE_BUILDARCH)
PK_PAGING_VMALLOC_HUGE_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-VMALLOC-HUGE
PK_PAGING_VMALLOC_HUGE_PATCH       := $(PK_PAGING_VMALLOC_HUGE_DIR)/patch
PK_PAGING_VMALLOC_HUGE_BSBIT       := BiscuitOS-PAGING-VMALLOC-HUGE-$(PK_PAGING_VMALLOC_HUGE_VERSION).bsbit
PK_PAGING_VMALLOC_HUGE_OUT         :=
PK_PAGING_VMALLOC_HUGE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_VMALLOC_HUGE_TAR         := $(CONFIG_PK_PAGING_VMALLOC_HUGE_TAR)
PK_PAGING_VMALLOC_HUGE_PACKAGE     :=
PK_PAGING_VMALLOC_HUGE_CONFIG      := $(CONFIG_PK_PAGING_VMALLOC_HUGE_CONFIG)
SUB_TARGET                  += $(PK_PAGING_VMALLOC_HUGE_DIR)/build

PK_PAGING_VMALLOC_HUGE_sh = $(CONFIG_SHELL) $(PK_PAGING_VMALLOC_HUGE_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_VMALLOC_HUGE_NAME)X \
                 $(PK_PAGING_VMALLOC_HUGE_VERSION)X \
                 $(PK_PAGING_VMALLOC_HUGE_SITE)X \
                 $(PK_PAGING_VMALLOC_HUGE_GITHUB)X \
                 $(PK_PAGING_VMALLOC_HUGE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_VMALLOC_HUGE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_VMALLOC_HUGE_SUBNAME)X \
                 $(CONFIG_PK_PAGING_VMALLOC_HUGE_CONFIG)X \
                 $(CONFIG_PK_PAGING_VMALLOC_HUGE_LIBPATH)X \
                 $(CONFIG_PK_PAGING_VMALLOC_HUGE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_VMALLOC_HUGE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_VMALLOC_HUGE_CFLAGS)X \
                 $(CONFIG_PK_PAGING_VMALLOC_HUGE_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_VMALLOC_HUGE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_VMALLOC_HUGE_ASFLAGS)X \
                 $(PK_PAGING_VMALLOC_HUGE_BSBIT)X \
                 $(PK_PAGING_VMALLOC_HUGE_BUILDARCH)X \
		 $(PK_PAGING_VMALLOC_HUGE_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_VMALLOC_HUGE_SRCFILE)X \
                 $(CONFIG_PK_PAGING_VMALLOC_HUGE_CSRCFILE)

$(PK_PAGING_VMALLOC_HUGE_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_VMALLOC_HUGE_sh,$(srctree))
