PK_PAGING_PTDUMP_CURRKERNEL_NAME        := BiscuitOS-PAGING-PTDUMP-CURRKERNEL
PK_PAGING_PTDUMP_CURRKERNEL_VERSION     := $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_VERSION)
PK_PAGING_PTDUMP_CURRKERNEL_SITE        := $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_URL)
PK_PAGING_PTDUMP_CURRKERNEL_GITHUB      := $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_GITHUB)
PK_PAGING_PTDUMP_CURRKERNEL_SUBNAME     :=
PK_PAGING_PTDUMP_CURRKERNEL_BUILDARCH   := $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_BUILDARCH)
PK_PAGING_PTDUMP_CURRKERNEL_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PTDUMP-CURRKERNEL
PK_PAGING_PTDUMP_CURRKERNEL_PATCH       := $(PK_PAGING_PTDUMP_CURRKERNEL_DIR)/patch
PK_PAGING_PTDUMP_CURRKERNEL_BSBIT       := BiscuitOS-PAGING-PTDUMP-CURRKERNEL-$(PK_PAGING_PTDUMP_CURRKERNEL_VERSION).bsbit
PK_PAGING_PTDUMP_CURRKERNEL_OUT         :=
PK_PAGING_PTDUMP_CURRKERNEL_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PTDUMP_CURRKERNEL_TAR         := $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_TAR)
PK_PAGING_PTDUMP_CURRKERNEL_PACKAGE     :=
PK_PAGING_PTDUMP_CURRKERNEL_CONFIG      := $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PTDUMP_CURRKERNEL_DIR)/build

PK_PAGING_PTDUMP_CURRKERNEL_sh = $(CONFIG_SHELL) $(PK_PAGING_PTDUMP_CURRKERNEL_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PTDUMP_CURRKERNEL_NAME)X \
                 $(PK_PAGING_PTDUMP_CURRKERNEL_VERSION)X \
                 $(PK_PAGING_PTDUMP_CURRKERNEL_SITE)X \
                 $(PK_PAGING_PTDUMP_CURRKERNEL_GITHUB)X \
                 $(PK_PAGING_PTDUMP_CURRKERNEL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PTDUMP_CURRKERNEL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PTDUMP_CURRKERNEL_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_CONFIG)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_ASFLAGS)X \
                 $(PK_PAGING_PTDUMP_CURRKERNEL_BSBIT)X \
                 $(PK_PAGING_PTDUMP_CURRKERNEL_BUILDARCH)X \
		 $(PK_PAGING_PTDUMP_CURRKERNEL_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PTDUMP_CURRKERNEL_CSRCFILE)

$(PK_PAGING_PTDUMP_CURRKERNEL_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PTDUMP_CURRKERNEL_sh,$(srctree))
