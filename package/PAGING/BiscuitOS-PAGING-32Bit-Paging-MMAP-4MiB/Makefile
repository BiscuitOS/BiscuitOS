PK_PAGING_32BIT_PAGING_MMAP_4M_NAME        := BiscuitOS-PAGING-32Bit-Paging-MMAP-4MiB
PK_PAGING_32BIT_PAGING_MMAP_4M_VERSION     := $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_VERSION)
PK_PAGING_32BIT_PAGING_MMAP_4M_SITE        := $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_URL)
PK_PAGING_32BIT_PAGING_MMAP_4M_GITHUB      := $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_GITHUB)
PK_PAGING_32BIT_PAGING_MMAP_4M_SUBNAME     :=
PK_PAGING_32BIT_PAGING_MMAP_4M_BUILDARCH   := $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_BUILDARCH)
PK_PAGING_32BIT_PAGING_MMAP_4M_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-32Bit-Paging-MMAP-4MiB
PK_PAGING_32BIT_PAGING_MMAP_4M_PATCH       := $(PK_PAGING_32BIT_PAGING_MMAP_4M_DIR)/patch
PK_PAGING_32BIT_PAGING_MMAP_4M_BSBIT       := BiscuitOS-PAGING-32Bit-Paging-MMAP-4MiB-$(PK_PAGING_32BIT_PAGING_MMAP_4M_VERSION).bsbit
PK_PAGING_32BIT_PAGING_MMAP_4M_OUT         :=
PK_PAGING_32BIT_PAGING_MMAP_4M_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_32BIT_PAGING_MMAP_4M_TAR         := $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_TAR)
PK_PAGING_32BIT_PAGING_MMAP_4M_PACKAGE     :=
PK_PAGING_32BIT_PAGING_MMAP_4M_CONFIG      := $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_CONFIG)
SUB_TARGET                  += $(PK_PAGING_32BIT_PAGING_MMAP_4M_DIR)/build

PK_PAGING_32BIT_PAGING_MMAP_4M_sh = $(CONFIG_SHELL) $(PK_PAGING_32BIT_PAGING_MMAP_4M_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_32BIT_PAGING_MMAP_4M_NAME)X \
                 $(PK_PAGING_32BIT_PAGING_MMAP_4M_VERSION)X \
                 $(PK_PAGING_32BIT_PAGING_MMAP_4M_SITE)X \
                 $(PK_PAGING_32BIT_PAGING_MMAP_4M_GITHUB)X \
                 $(PK_PAGING_32BIT_PAGING_MMAP_4M_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_32BIT_PAGING_MMAP_4M_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_32BIT_PAGING_MMAP_4M_SUBNAME)X \
                 $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_CONFIG)X \
                 $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_LIBPATH)X \
                 $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_CFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_ASFLAGS)X \
                 $(PK_PAGING_32BIT_PAGING_MMAP_4M_BSBIT)X \
                 $(PK_PAGING_32BIT_PAGING_MMAP_4M_BUILDARCH)X \
		 $(PK_PAGING_32BIT_PAGING_MMAP_4M_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_SRCFILE)X \
                 $(CONFIG_PK_PAGING_32BIT_PAGING_MMAP_4M_CSRCFILE)

$(PK_PAGING_32BIT_PAGING_MMAP_4M_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_32BIT_PAGING_MMAP_4M_sh,$(srctree))
