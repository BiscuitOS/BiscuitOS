PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_VERSION     := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_VERSION)
PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_SITE        := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_URL)
PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_GITHUB      := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_GITHUB)
PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_SUBNAME     :=
PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_BUILDARCH   := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_BUILDARCH)
PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PT-K2PT-PAGE-KERNEL-NOCACHE
PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_PATCH       := $(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_DIR)/patch
PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_BSBIT       := BiscuitOS-PAGING-PT-K2PT-PAGE-KERNEL-NOCACHE-$(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_VERSION).bsbit
PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_OUT         :=
PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_TAR         := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_TAR)
PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_PACKAGE     :=
PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_CONFIG      := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_DIR)/build

PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_NAME)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_VERSION)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_SITE)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_GITHUB)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_ASFLAGS)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_BSBIT)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_BUILDARCH)X \
		 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_CSRCFILE)

$(PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PT_K2PT_PAGE_KERNEL_NOCACHE_sh,$(srctree))
