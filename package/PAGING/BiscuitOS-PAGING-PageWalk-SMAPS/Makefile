PK_PAGING_PAGEWALK_SMAPS_NAME        := BiscuitOS-PAGING-PageWalk-SMAPS
PK_PAGING_PAGEWALK_SMAPS_VERSION     := $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_VERSION)
PK_PAGING_PAGEWALK_SMAPS_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_URL)
PK_PAGING_PAGEWALK_SMAPS_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_GITHUB)
PK_PAGING_PAGEWALK_SMAPS_SUBNAME     :=
PK_PAGING_PAGEWALK_SMAPS_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_BUILDARCH)
PK_PAGING_PAGEWALK_SMAPS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-SMAPS
PK_PAGING_PAGEWALK_SMAPS_PATCH       := $(PK_PAGING_PAGEWALK_SMAPS_DIR)/patch
PK_PAGING_PAGEWALK_SMAPS_BSBIT       := BiscuitOS-PAGING-PageWalk-SMAPS-$(PK_PAGING_PAGEWALK_SMAPS_VERSION).bsbit
PK_PAGING_PAGEWALK_SMAPS_OUT         :=
PK_PAGING_PAGEWALK_SMAPS_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_SMAPS_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_TAR)
PK_PAGING_PAGEWALK_SMAPS_PACKAGE     :=
PK_PAGING_PAGEWALK_SMAPS_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_SMAPS_DIR)/build

PK_PAGING_PAGEWALK_SMAPS_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_SMAPS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_SMAPS_NAME)X \
                 $(PK_PAGING_PAGEWALK_SMAPS_VERSION)X \
                 $(PK_PAGING_PAGEWALK_SMAPS_SITE)X \
                 $(PK_PAGING_PAGEWALK_SMAPS_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_SMAPS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_SMAPS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_SMAPS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_SMAPS_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_SMAPS_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_SMAPS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_SMAPS_CSRCFILE)

$(PK_PAGING_PAGEWALK_SMAPS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_SMAPS_sh,$(srctree))
