                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_ASFLAGS)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_BSBIT)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_BUILDARCH)X \
		 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_CSRCFILE)

$(PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_DIR)/build: FORCE
