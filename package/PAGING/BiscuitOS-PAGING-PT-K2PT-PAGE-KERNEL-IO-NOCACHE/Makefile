PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_BSBIT       := BiscuitOS-PAGING-PT-K2PT-PAGE-KERNEL-IO-NOCACHE-$(PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_VERSION).bsbit
PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_OUT         :=
