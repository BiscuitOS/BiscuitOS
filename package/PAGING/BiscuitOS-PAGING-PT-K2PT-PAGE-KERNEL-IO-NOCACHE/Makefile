PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_CONFIG      := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_DIR)/build

PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_K2PT_PAGE_KERNEL_IO_UC_SCRIPTS) \
