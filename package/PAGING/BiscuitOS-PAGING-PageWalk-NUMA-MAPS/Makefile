PK_PAGING_PAGEWALK_NUMA_MAPS_NAME        := BiscuitOS-PAGING-PageWalk-NUMA-MAPS
PK_PAGING_PAGEWALK_NUMA_MAPS_VERSION     := $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_VERSION)
PK_PAGING_PAGEWALK_NUMA_MAPS_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_URL)
PK_PAGING_PAGEWALK_NUMA_MAPS_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_GITHUB)
PK_PAGING_PAGEWALK_NUMA_MAPS_SUBNAME     :=
PK_PAGING_PAGEWALK_NUMA_MAPS_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_BUILDARCH)
PK_PAGING_PAGEWALK_NUMA_MAPS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-NUMA-MAPS
PK_PAGING_PAGEWALK_NUMA_MAPS_PATCH       := $(PK_PAGING_PAGEWALK_NUMA_MAPS_DIR)/patch
PK_PAGING_PAGEWALK_NUMA_MAPS_BSBIT       := BiscuitOS-PAGING-PageWalk-NUMA-MAPS-$(PK_PAGING_PAGEWALK_NUMA_MAPS_VERSION).bsbit
PK_PAGING_PAGEWALK_NUMA_MAPS_OUT         :=
PK_PAGING_PAGEWALK_NUMA_MAPS_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_NUMA_MAPS_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_TAR)
PK_PAGING_PAGEWALK_NUMA_MAPS_PACKAGE     :=
PK_PAGING_PAGEWALK_NUMA_MAPS_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_NUMA_MAPS_DIR)/build

PK_PAGING_PAGEWALK_NUMA_MAPS_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_NUMA_MAPS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_NUMA_MAPS_NAME)X \
                 $(PK_PAGING_PAGEWALK_NUMA_MAPS_VERSION)X \
                 $(PK_PAGING_PAGEWALK_NUMA_MAPS_SITE)X \
                 $(PK_PAGING_PAGEWALK_NUMA_MAPS_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_NUMA_MAPS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_NUMA_MAPS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_NUMA_MAPS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_NUMA_MAPS_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_NUMA_MAPS_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_NUMA_MAPS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_NUMA_MAPS_CSRCFILE)

$(PK_PAGING_PAGEWALK_NUMA_MAPS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_NUMA_MAPS_sh,$(srctree))
