PK_PAGING_PAGEWALK_VMA_NAME        := BiscuitOS-PAGING-PageWalk-VMA
PK_PAGING_PAGEWALK_VMA_VERSION     := $(CONFIG_PK_PAGING_PAGEWALK_VMA_VERSION)
PK_PAGING_PAGEWALK_VMA_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_VMA_URL)
PK_PAGING_PAGEWALK_VMA_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_VMA_GITHUB)
PK_PAGING_PAGEWALK_VMA_SUBNAME     :=
PK_PAGING_PAGEWALK_VMA_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_VMA_BUILDARCH)
PK_PAGING_PAGEWALK_VMA_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-VMA
PK_PAGING_PAGEWALK_VMA_PATCH       := $(PK_PAGING_PAGEWALK_VMA_DIR)/patch
PK_PAGING_PAGEWALK_VMA_BSBIT       := BiscuitOS-PAGING-PageWalk-VMA-$(PK_PAGING_PAGEWALK_VMA_VERSION).bsbit
PK_PAGING_PAGEWALK_VMA_OUT         :=
PK_PAGING_PAGEWALK_VMA_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_VMA_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_VMA_TAR)
PK_PAGING_PAGEWALK_VMA_PACKAGE     :=
PK_PAGING_PAGEWALK_VMA_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_VMA_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_VMA_DIR)/build

PK_PAGING_PAGEWALK_VMA_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_VMA_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_VMA_NAME)X \
                 $(PK_PAGING_PAGEWALK_VMA_VERSION)X \
                 $(PK_PAGING_PAGEWALK_VMA_SITE)X \
                 $(PK_PAGING_PAGEWALK_VMA_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_VMA_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_VMA_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_VMA_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_VMA_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_VMA_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_VMA_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_VMA_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_VMA_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_VMA_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_VMA_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_VMA_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_VMA_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_VMA_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_VMA_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_VMA_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_VMA_CSRCFILE)

$(PK_PAGING_PAGEWALK_VMA_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_VMA_sh,$(srctree))
