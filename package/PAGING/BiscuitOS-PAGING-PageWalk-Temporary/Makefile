PK_PAGING_PAGEWALK_TEMPORARY_NAME        := BiscuitOS-PAGING-PageWalk-Temporary
PK_PAGING_PAGEWALK_TEMPORARY_VERSION     := $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_VERSION)
PK_PAGING_PAGEWALK_TEMPORARY_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_URL)
PK_PAGING_PAGEWALK_TEMPORARY_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_GITHUB)
PK_PAGING_PAGEWALK_TEMPORARY_SUBNAME     :=
PK_PAGING_PAGEWALK_TEMPORARY_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_BUILDARCH)
PK_PAGING_PAGEWALK_TEMPORARY_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-Temporary
PK_PAGING_PAGEWALK_TEMPORARY_PATCH       := $(PK_PAGING_PAGEWALK_TEMPORARY_DIR)/patch
PK_PAGING_PAGEWALK_TEMPORARY_BSBIT       := BiscuitOS-PAGING-PageWalk-Temporary-$(PK_PAGING_PAGEWALK_TEMPORARY_VERSION).bsbit
PK_PAGING_PAGEWALK_TEMPORARY_OUT         :=
PK_PAGING_PAGEWALK_TEMPORARY_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_TEMPORARY_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_TAR)
PK_PAGING_PAGEWALK_TEMPORARY_PACKAGE     :=
PK_PAGING_PAGEWALK_TEMPORARY_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_TEMPORARY_DIR)/build

PK_PAGING_PAGEWALK_TEMPORARY_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_TEMPORARY_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_TEMPORARY_NAME)X \
                 $(PK_PAGING_PAGEWALK_TEMPORARY_VERSION)X \
                 $(PK_PAGING_PAGEWALK_TEMPORARY_SITE)X \
                 $(PK_PAGING_PAGEWALK_TEMPORARY_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_TEMPORARY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_TEMPORARY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_TEMPORARY_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_TEMPORARY_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_TEMPORARY_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_TEMPORARY_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_TEMPORARY_CSRCFILE)

$(PK_PAGING_PAGEWALK_TEMPORARY_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_TEMPORARY_sh,$(srctree))
