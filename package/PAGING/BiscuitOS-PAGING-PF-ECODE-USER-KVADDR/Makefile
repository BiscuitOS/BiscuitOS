PK_PAGING_PF_ECODE_USER_KVADDR_PACKAGE     :=
PK_PAGING_PF_ECODE_USER_KVADDR_CONFIG      := $(CONFIG_PK_PAGING_PF_ECODE_USER_KVADDR_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_ECODE_USER_KVADDR_DIR)/build

PK_PAGING_PF_ECODE_USER_KVADDR_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_ECODE_USER_KVADDR_SCRIPTS) \
