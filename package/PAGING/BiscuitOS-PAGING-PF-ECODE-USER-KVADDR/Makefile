PK_PAGING_PF_ECODE_USER_KVADDR_SUBNAME     :=
PK_PAGING_PF_ECODE_USER_KVADDR_BUILDARCH   := $(CONFIG_PK_PAGING_PF_ECODE_USER_KVADDR_BUILDARCH)
PK_PAGING_PF_ECODE_USER_KVADDR_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-ECODE-USER-KVADDR
PK_PAGING_PF_ECODE_USER_KVADDR_PATCH       := $(PK_PAGING_PF_ECODE_USER_KVADDR_DIR)/patch
PK_PAGING_PF_ECODE_USER_KVADDR_BSBIT       := BiscuitOS-PAGING-PF-ECODE-USER-KVADDR-$(PK_PAGING_PF_ECODE_USER_KVADDR_VERSION).bsbit
