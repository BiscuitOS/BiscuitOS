                 $(PK_PAGING_PF_ECODE_USER_KVADDR_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_ECODE_USER_KVADDR_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_ECODE_USER_KVADDR_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_USER_KVADDR_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_USER_KVADDR_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_USER_KVADDR_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_USER_KVADDR_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_USER_KVADDR_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_USER_KVADDR_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_USER_KVADDR_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_USER_KVADDR_ASFLAGS)X \
                 $(PK_PAGING_PF_ECODE_USER_KVADDR_BSBIT)X \
                 $(PK_PAGING_PF_ECODE_USER_KVADDR_BUILDARCH)X \
		 $(PK_PAGING_PF_ECODE_USER_KVADDR_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_USER_KVADDR_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_USER_KVADDR_CSRCFILE)

$(PK_PAGING_PF_ECODE_USER_KVADDR_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_ECODE_USER_KVADDR_sh,$(srctree))
