PK_PAGING_PF_ANON_STACK_NAME        := BiscuitOS-PAGING-PF-ANON-STACK
PK_PAGING_PF_ANON_STACK_VERSION     := $(CONFIG_PK_PAGING_PF_ANON_STACK_VERSION)
PK_PAGING_PF_ANON_STACK_SITE        := $(CONFIG_PK_PAGING_PF_ANON_STACK_URL)
PK_PAGING_PF_ANON_STACK_GITHUB      := $(CONFIG_PK_PAGING_PF_ANON_STACK_GITHUB)
PK_PAGING_PF_ANON_STACK_SUBNAME     :=
PK_PAGING_PF_ANON_STACK_BUILDARCH   := $(CONFIG_PK_PAGING_PF_ANON_STACK_BUILDARCH)
PK_PAGING_PF_ANON_STACK_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-ANON-STACK
PK_PAGING_PF_ANON_STACK_PATCH       := $(PK_PAGING_PF_ANON_STACK_DIR)/patch
PK_PAGING_PF_ANON_STACK_BSBIT       := BiscuitOS-PAGING-PF-ANON-STACK-$(PK_PAGING_PF_ANON_STACK_VERSION).bsbit
PK_PAGING_PF_ANON_STACK_OUT         :=
PK_PAGING_PF_ANON_STACK_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_ANON_STACK_TAR         := $(CONFIG_PK_PAGING_PF_ANON_STACK_TAR)
PK_PAGING_PF_ANON_STACK_PACKAGE     :=
PK_PAGING_PF_ANON_STACK_CONFIG      := $(CONFIG_PK_PAGING_PF_ANON_STACK_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_ANON_STACK_DIR)/build

PK_PAGING_PF_ANON_STACK_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_ANON_STACK_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_ANON_STACK_NAME)X \
                 $(PK_PAGING_PF_ANON_STACK_VERSION)X \
                 $(PK_PAGING_PF_ANON_STACK_SITE)X \
                 $(PK_PAGING_PF_ANON_STACK_GITHUB)X \
                 $(PK_PAGING_PF_ANON_STACK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_ANON_STACK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_ANON_STACK_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_ANON_STACK_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ANON_STACK_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_ANON_STACK_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_STACK_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ANON_STACK_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_STACK_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_STACK_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_STACK_ASFLAGS)X \
                 $(PK_PAGING_PF_ANON_STACK_BSBIT)X \
                 $(PK_PAGING_PF_ANON_STACK_BUILDARCH)X \
		 $(PK_PAGING_PF_ANON_STACK_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ANON_STACK_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ANON_STACK_CSRCFILE)

$(PK_PAGING_PF_ANON_STACK_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_ANON_STACK_sh,$(srctree))
