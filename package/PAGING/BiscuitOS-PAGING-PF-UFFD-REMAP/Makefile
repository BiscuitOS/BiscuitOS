PK_PAGING_PF_UFFD_REMAP_NAME        := BiscuitOS-PAGING-PF-UFFD-REMAP
PK_PAGING_PF_UFFD_REMAP_VERSION     := $(CONFIG_PK_PAGING_PF_UFFD_REMAP_VERSION)
PK_PAGING_PF_UFFD_REMAP_SITE        := $(CONFIG_PK_PAGING_PF_UFFD_REMAP_URL)
PK_PAGING_PF_UFFD_REMAP_GITHUB      := $(CONFIG_PK_PAGING_PF_UFFD_REMAP_GITHUB)
PK_PAGING_PF_UFFD_REMAP_SUBNAME     :=
PK_PAGING_PF_UFFD_REMAP_BUILDARCH   := $(CONFIG_PK_PAGING_PF_UFFD_REMAP_BUILDARCH)
PK_PAGING_PF_UFFD_REMAP_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-UFFD-REMAP
PK_PAGING_PF_UFFD_REMAP_PATCH       := $(PK_PAGING_PF_UFFD_REMAP_DIR)/patch
PK_PAGING_PF_UFFD_REMAP_BSBIT       := BiscuitOS-PAGING-PF-UFFD-REMAP-$(PK_PAGING_PF_UFFD_REMAP_VERSION).bsbit
PK_PAGING_PF_UFFD_REMAP_OUT         :=
PK_PAGING_PF_UFFD_REMAP_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_UFFD_REMAP_TAR         := $(CONFIG_PK_PAGING_PF_UFFD_REMAP_TAR)
PK_PAGING_PF_UFFD_REMAP_PACKAGE     :=
PK_PAGING_PF_UFFD_REMAP_CONFIG      := $(CONFIG_PK_PAGING_PF_UFFD_REMAP_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_UFFD_REMAP_DIR)/build

PK_PAGING_PF_UFFD_REMAP_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_UFFD_REMAP_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_UFFD_REMAP_NAME)X \
                 $(PK_PAGING_PF_UFFD_REMAP_VERSION)X \
                 $(PK_PAGING_PF_UFFD_REMAP_SITE)X \
                 $(PK_PAGING_PF_UFFD_REMAP_GITHUB)X \
                 $(PK_PAGING_PF_UFFD_REMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_UFFD_REMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_UFFD_REMAP_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMAP_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMAP_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMAP_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMAP_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMAP_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMAP_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMAP_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMAP_ASFLAGS)X \
                 $(PK_PAGING_PF_UFFD_REMAP_BSBIT)X \
                 $(PK_PAGING_PF_UFFD_REMAP_BUILDARCH)X \
		 $(PK_PAGING_PF_UFFD_REMAP_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMAP_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMAP_CSRCFILE)

$(PK_PAGING_PF_UFFD_REMAP_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_UFFD_REMAP_sh,$(srctree))
