PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_NAME        := BiscuitOS-PAGING-PT-K2PT-PAGE-KERNEL-ROX
PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_VERSION     := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_VERSION)
PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_SITE        := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_URL)
PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_GITHUB      := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_GITHUB)
PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_SUBNAME     :=
PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_BUILDARCH   := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_BUILDARCH)
PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PT-K2PT-PAGE-KERNEL-ROX
PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_PATCH       := $(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_DIR)/patch
PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_BSBIT       := BiscuitOS-PAGING-PT-K2PT-PAGE-KERNEL-ROX-$(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_VERSION).bsbit
PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_OUT         :=
PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_TAR         := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_TAR)
PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_PACKAGE     :=
PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_CONFIG      := $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_DIR)/build

PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_NAME)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_VERSION)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_SITE)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_GITHUB)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_ASFLAGS)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_BSBIT)X \
                 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_BUILDARCH)X \
		 $(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_CSRCFILE)

$(PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PT_K2PT_PAGE_KERNEL_ROX_sh,$(srctree))
