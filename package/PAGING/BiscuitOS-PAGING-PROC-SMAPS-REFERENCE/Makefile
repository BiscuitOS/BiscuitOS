PK_PAGING_PROC_SMAPS_REFERENCE_NAME        := BiscuitOS-PAGING-PROC-SMAPS-REFERENCE
PK_PAGING_PROC_SMAPS_REFERENCE_VERSION     := $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_VERSION)
PK_PAGING_PROC_SMAPS_REFERENCE_SITE        := $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_URL)
PK_PAGING_PROC_SMAPS_REFERENCE_GITHUB      := $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_GITHUB)
PK_PAGING_PROC_SMAPS_REFERENCE_SUBNAME     :=
PK_PAGING_PROC_SMAPS_REFERENCE_BUILDARCH   := $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_BUILDARCH)
PK_PAGING_PROC_SMAPS_REFERENCE_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PROC-SMAPS-REFERENCE
PK_PAGING_PROC_SMAPS_REFERENCE_PATCH       := $(PK_PAGING_PROC_SMAPS_REFERENCE_DIR)/patch
PK_PAGING_PROC_SMAPS_REFERENCE_BSBIT       := BiscuitOS-PAGING-PROC-SMAPS-REFERENCE-$(PK_PAGING_PROC_SMAPS_REFERENCE_VERSION).bsbit
PK_PAGING_PROC_SMAPS_REFERENCE_OUT         :=
PK_PAGING_PROC_SMAPS_REFERENCE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PROC_SMAPS_REFERENCE_TAR         := $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_TAR)
PK_PAGING_PROC_SMAPS_REFERENCE_PACKAGE     :=
PK_PAGING_PROC_SMAPS_REFERENCE_CONFIG      := $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PROC_SMAPS_REFERENCE_DIR)/build

PK_PAGING_PROC_SMAPS_REFERENCE_sh = $(CONFIG_SHELL) $(PK_PAGING_PROC_SMAPS_REFERENCE_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PROC_SMAPS_REFERENCE_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_REFERENCE_VERSION)X \
                 $(PK_PAGING_PROC_SMAPS_REFERENCE_SITE)X \
                 $(PK_PAGING_PROC_SMAPS_REFERENCE_GITHUB)X \
                 $(PK_PAGING_PROC_SMAPS_REFERENCE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_REFERENCE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PROC_SMAPS_REFERENCE_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_CONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_ASFLAGS)X \
                 $(PK_PAGING_PROC_SMAPS_REFERENCE_BSBIT)X \
                 $(PK_PAGING_PROC_SMAPS_REFERENCE_BUILDARCH)X \
		 $(PK_PAGING_PROC_SMAPS_REFERENCE_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_REFERENCE_CSRCFILE)

$(PK_PAGING_PROC_SMAPS_REFERENCE_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PROC_SMAPS_REFERENCE_sh,$(srctree))
