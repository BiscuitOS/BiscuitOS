PK_PAGING_PAGEWALK_KERNEL_NAME        := BiscuitOS-PAGING-PageWalk-Kernel
PK_PAGING_PAGEWALK_KERNEL_VERSION     := $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_VERSION)
PK_PAGING_PAGEWALK_KERNEL_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_URL)
PK_PAGING_PAGEWALK_KERNEL_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_GITHUB)
PK_PAGING_PAGEWALK_KERNEL_SUBNAME     :=
PK_PAGING_PAGEWALK_KERNEL_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_BUILDARCH)
PK_PAGING_PAGEWALK_KERNEL_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-Kernel
PK_PAGING_PAGEWALK_KERNEL_PATCH       := $(PK_PAGING_PAGEWALK_KERNEL_DIR)/patch
PK_PAGING_PAGEWALK_KERNEL_BSBIT       := BiscuitOS-PAGING-PageWalk-Kernel-$(PK_PAGING_PAGEWALK_KERNEL_VERSION).bsbit
PK_PAGING_PAGEWALK_KERNEL_OUT         :=
PK_PAGING_PAGEWALK_KERNEL_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_KERNEL_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_TAR)
PK_PAGING_PAGEWALK_KERNEL_PACKAGE     :=
PK_PAGING_PAGEWALK_KERNEL_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_KERNEL_DIR)/build

PK_PAGING_PAGEWALK_KERNEL_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_KERNEL_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_KERNEL_NAME)X \
                 $(PK_PAGING_PAGEWALK_KERNEL_VERSION)X \
                 $(PK_PAGING_PAGEWALK_KERNEL_SITE)X \
                 $(PK_PAGING_PAGEWALK_KERNEL_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_KERNEL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_KERNEL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_KERNEL_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_KERNEL_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_KERNEL_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_KERNEL_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_KERNEL_CSRCFILE)

$(PK_PAGING_PAGEWALK_KERNEL_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_KERNEL_sh,$(srctree))
