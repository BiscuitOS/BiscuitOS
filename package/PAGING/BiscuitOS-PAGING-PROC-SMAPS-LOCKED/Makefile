PK_PAGING_PROC_SMAPS_LOCKED_NAME        := BiscuitOS-PAGING-PROC-SMAPS-LOCKED
PK_PAGING_PROC_SMAPS_LOCKED_VERSION     := $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_VERSION)
PK_PAGING_PROC_SMAPS_LOCKED_SITE        := $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_URL)
PK_PAGING_PROC_SMAPS_LOCKED_GITHUB      := $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_GITHUB)
PK_PAGING_PROC_SMAPS_LOCKED_SUBNAME     :=
PK_PAGING_PROC_SMAPS_LOCKED_BUILDARCH   := $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_BUILDARCH)
PK_PAGING_PROC_SMAPS_LOCKED_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PROC-SMAPS-LOCKED
PK_PAGING_PROC_SMAPS_LOCKED_PATCH       := $(PK_PAGING_PROC_SMAPS_LOCKED_DIR)/patch
PK_PAGING_PROC_SMAPS_LOCKED_BSBIT       := BiscuitOS-PAGING-PROC-SMAPS-LOCKED-$(PK_PAGING_PROC_SMAPS_LOCKED_VERSION).bsbit
PK_PAGING_PROC_SMAPS_LOCKED_OUT         :=
PK_PAGING_PROC_SMAPS_LOCKED_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PROC_SMAPS_LOCKED_TAR         := $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_TAR)
PK_PAGING_PROC_SMAPS_LOCKED_PACKAGE     :=
PK_PAGING_PROC_SMAPS_LOCKED_CONFIG      := $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PROC_SMAPS_LOCKED_DIR)/build

PK_PAGING_PROC_SMAPS_LOCKED_sh = $(CONFIG_SHELL) $(PK_PAGING_PROC_SMAPS_LOCKED_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PROC_SMAPS_LOCKED_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_LOCKED_VERSION)X \
                 $(PK_PAGING_PROC_SMAPS_LOCKED_SITE)X \
                 $(PK_PAGING_PROC_SMAPS_LOCKED_GITHUB)X \
                 $(PK_PAGING_PROC_SMAPS_LOCKED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_LOCKED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PROC_SMAPS_LOCKED_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_CONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_ASFLAGS)X \
                 $(PK_PAGING_PROC_SMAPS_LOCKED_BSBIT)X \
                 $(PK_PAGING_PROC_SMAPS_LOCKED_BUILDARCH)X \
		 $(PK_PAGING_PROC_SMAPS_LOCKED_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_LOCKED_CSRCFILE)

$(PK_PAGING_PROC_SMAPS_LOCKED_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PROC_SMAPS_LOCKED_sh,$(srctree))
