PK_PAGING_PF_ECODE_PRESENT_ZSWAP_SITE        := $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_URL)
PK_PAGING_PF_ECODE_PRESENT_ZSWAP_GITHUB      := $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_GITHUB)
PK_PAGING_PF_ECODE_PRESENT_ZSWAP_SUBNAME     :=
PK_PAGING_PF_ECODE_PRESENT_ZSWAP_BUILDARCH   := $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_BUILDARCH)
PK_PAGING_PF_ECODE_PRESENT_ZSWAP_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-ECODE-PRESENT-ZSWAP
PK_PAGING_PF_ECODE_PRESENT_ZSWAP_PATCH       := $(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_DIR)/patch
PK_PAGING_PF_ECODE_PRESENT_ZSWAP_BSBIT       := BiscuitOS-PAGING-PF-ECODE-PRESENT-ZSWAP-$(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_VERSION).bsbit
PK_PAGING_PF_ECODE_PRESENT_ZSWAP_OUT         :=
PK_PAGING_PF_ECODE_PRESENT_ZSWAP_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_ECODE_PRESENT_ZSWAP_TAR         := $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_TAR)
PK_PAGING_PF_ECODE_PRESENT_ZSWAP_PACKAGE     :=
PK_PAGING_PF_ECODE_PRESENT_ZSWAP_CONFIG      := $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_DIR)/build

PK_PAGING_PF_ECODE_PRESENT_ZSWAP_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_NAME)X \
                 $(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_VERSION)X \
                 $(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_SITE)X \
                 $(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_GITHUB)X \
                 $(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_ASFLAGS)X \
                 $(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_BSBIT)X \
                 $(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_BUILDARCH)X \
		 $(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PRESENT_ZSWAP_CSRCFILE)

$(PK_PAGING_PF_ECODE_PRESENT_ZSWAP_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_ECODE_PRESENT_ZSWAP_sh,$(srctree))
