PK_PAGING_PAGEWALK_CLEANREFS_NAME        := BiscuitOS-PAGING-PageWalk-CLEAR-Reference
PK_PAGING_PAGEWALK_CLEANREFS_VERSION     := $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_VERSION)
PK_PAGING_PAGEWALK_CLEANREFS_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_URL)
PK_PAGING_PAGEWALK_CLEANREFS_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_GITHUB)
PK_PAGING_PAGEWALK_CLEANREFS_SUBNAME     :=
PK_PAGING_PAGEWALK_CLEANREFS_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_BUILDARCH)
PK_PAGING_PAGEWALK_CLEANREFS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-CLEAR-Reference
PK_PAGING_PAGEWALK_CLEANREFS_PATCH       := $(PK_PAGING_PAGEWALK_CLEANREFS_DIR)/patch
PK_PAGING_PAGEWALK_CLEANREFS_BSBIT       := BiscuitOS-PAGING-PageWalk-CLEAR-Reference-$(PK_PAGING_PAGEWALK_CLEANREFS_VERSION).bsbit
PK_PAGING_PAGEWALK_CLEANREFS_OUT         :=
PK_PAGING_PAGEWALK_CLEANREFS_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_CLEANREFS_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_TAR)
PK_PAGING_PAGEWALK_CLEANREFS_PACKAGE     :=
PK_PAGING_PAGEWALK_CLEANREFS_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_CLEANREFS_DIR)/build

PK_PAGING_PAGEWALK_CLEANREFS_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_CLEANREFS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_CLEANREFS_NAME)X \
                 $(PK_PAGING_PAGEWALK_CLEANREFS_VERSION)X \
                 $(PK_PAGING_PAGEWALK_CLEANREFS_SITE)X \
                 $(PK_PAGING_PAGEWALK_CLEANREFS_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_CLEANREFS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_CLEANREFS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_CLEANREFS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_CLEANREFS_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_CLEANREFS_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_CLEANREFS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_CLEANREFS_CSRCFILE)

$(PK_PAGING_PAGEWALK_CLEANREFS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_CLEANREFS_sh,$(srctree))
