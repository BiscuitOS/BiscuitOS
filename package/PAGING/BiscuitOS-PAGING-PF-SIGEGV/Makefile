PK_PAGING_PF_SIGEGV_NAME        := BiscuitOS-PAGING-PF-SIGEGV
PK_PAGING_PF_SIGEGV_VERSION     := $(CONFIG_PK_PAGING_PF_SIGEGV_VERSION)
PK_PAGING_PF_SIGEGV_SITE        := $(CONFIG_PK_PAGING_PF_SIGEGV_URL)
PK_PAGING_PF_SIGEGV_GITHUB      := $(CONFIG_PK_PAGING_PF_SIGEGV_GITHUB)
PK_PAGING_PF_SIGEGV_SUBNAME     :=
PK_PAGING_PF_SIGEGV_BUILDARCH   := $(CONFIG_PK_PAGING_PF_SIGEGV_BUILDARCH)
PK_PAGING_PF_SIGEGV_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-SIGEGV
PK_PAGING_PF_SIGEGV_PATCH       := $(PK_PAGING_PF_SIGEGV_DIR)/patch
PK_PAGING_PF_SIGEGV_BSBIT       := BiscuitOS-PAGING-PF-SIGEGV-$(PK_PAGING_PF_SIGEGV_VERSION).bsbit
PK_PAGING_PF_SIGEGV_OUT         :=
PK_PAGING_PF_SIGEGV_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_SIGEGV_TAR         := $(CONFIG_PK_PAGING_PF_SIGEGV_TAR)
PK_PAGING_PF_SIGEGV_PACKAGE     :=
PK_PAGING_PF_SIGEGV_CONFIG      := $(CONFIG_PK_PAGING_PF_SIGEGV_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_SIGEGV_DIR)/build

PK_PAGING_PF_SIGEGV_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_SIGEGV_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_SIGEGV_NAME)X \
                 $(PK_PAGING_PF_SIGEGV_VERSION)X \
                 $(PK_PAGING_PF_SIGEGV_SITE)X \
                 $(PK_PAGING_PF_SIGEGV_GITHUB)X \
                 $(PK_PAGING_PF_SIGEGV_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_SIGEGV_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_SIGEGV_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_SIGEGV_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SIGEGV_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_SIGEGV_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SIGEGV_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SIGEGV_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SIGEGV_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SIGEGV_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SIGEGV_ASFLAGS)X \
                 $(PK_PAGING_PF_SIGEGV_BSBIT)X \
                 $(PK_PAGING_PF_SIGEGV_BUILDARCH)X \
		 $(PK_PAGING_PF_SIGEGV_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SIGEGV_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SIGEGV_CSRCFILE)

$(PK_PAGING_PF_SIGEGV_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_SIGEGV_sh,$(srctree))
