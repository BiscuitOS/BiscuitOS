PK_PAGING_PT_FUPT_PTE_NAME        := BiscuitOS-PAGING-PT-FUPT-PTE
PK_PAGING_PT_FUPT_PTE_VERSION     := $(CONFIG_PK_PAGING_PT_FUPT_PTE_VERSION)
PK_PAGING_PT_FUPT_PTE_SITE        := $(CONFIG_PK_PAGING_PT_FUPT_PTE_URL)
PK_PAGING_PT_FUPT_PTE_GITHUB      := $(CONFIG_PK_PAGING_PT_FUPT_PTE_GITHUB)
PK_PAGING_PT_FUPT_PTE_SUBNAME     :=
PK_PAGING_PT_FUPT_PTE_BUILDARCH   := $(CONFIG_PK_PAGING_PT_FUPT_PTE_BUILDARCH)
PK_PAGING_PT_FUPT_PTE_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PT-FUPT-PTE
PK_PAGING_PT_FUPT_PTE_PATCH       := $(PK_PAGING_PT_FUPT_PTE_DIR)/patch
PK_PAGING_PT_FUPT_PTE_BSBIT       := BiscuitOS-PAGING-PT-FUPT-PTE-$(PK_PAGING_PT_FUPT_PTE_VERSION).bsbit
PK_PAGING_PT_FUPT_PTE_OUT         :=
PK_PAGING_PT_FUPT_PTE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_PT_FUPT_PTE_TAR         := $(CONFIG_PK_PAGING_PT_FUPT_PTE_TAR)
PK_PAGING_PT_FUPT_PTE_PACKAGE     :=
PK_PAGING_PT_FUPT_PTE_CONFIG      := $(CONFIG_PK_PAGING_PT_FUPT_PTE_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_FUPT_PTE_DIR)/build

PK_PAGING_PT_FUPT_PTE_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_FUPT_PTE_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PT_FUPT_PTE_NAME)X \
                 $(PK_PAGING_PT_FUPT_PTE_VERSION)X \
                 $(PK_PAGING_PT_FUPT_PTE_SITE)X \
                 $(PK_PAGING_PT_FUPT_PTE_GITHUB)X \
                 $(PK_PAGING_PT_FUPT_PTE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_FUPT_PTE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_FUPT_PTE_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PTE_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PTE_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PTE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PTE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PTE_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PTE_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PTE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PTE_ASFLAGS)X \
                 $(PK_PAGING_PT_FUPT_PTE_BSBIT)X \
                 $(PK_PAGING_PT_FUPT_PTE_BUILDARCH)X \
		 $(PK_PAGING_PT_FUPT_PTE_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PTE_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_FUPT_PTE_CSRCFILE)

$(PK_PAGING_PT_FUPT_PTE_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PT_FUPT_PTE_sh,$(srctree))
