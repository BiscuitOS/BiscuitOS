PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_GITHUB      := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_GITHUB)
PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_SUBNAME     :=
PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_BUILDARCH   := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_BUILDARCH)
PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PT-U2PT-PAGE-RW-CLEAR
PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_PATCH       := $(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_DIR)/patch
PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_BSBIT       := BiscuitOS-PAGING-PT-U2PT-PAGE-RW-CLEAR-$(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_VERSION).bsbit
PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_OUT         :=
PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_TAR         := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_TAR)
PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_PACKAGE     :=
PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_CONFIG      := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_DIR)/build

PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_NAME)X \
                 $(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_VERSION)X \
                 $(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_SITE)X \
                 $(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_GITHUB)X \
                 $(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_ASFLAGS)X \
                 $(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_BSBIT)X \
                 $(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_BUILDARCH)X \
		 $(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_CSRCFILE)

$(PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PT_U2PT_PAGE_RW_CLEAR_sh,$(srctree))
