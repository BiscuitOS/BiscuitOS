PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_SITE        := $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_URL)
PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_GITHUB      := $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_GITHUB)
PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_SUBNAME     :=
PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_BUILDARCH   := $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_BUILDARCH)
PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PROC-SMAPS-SHARED-HUGETLB
PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_PATCH       := $(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_DIR)/patch
PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_BSBIT       := BiscuitOS-PAGING-PROC-SMAPS-SHARED-HUGETLB-$(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_VERSION).bsbit
PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_OUT         :=
PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_TAR         := $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_TAR)
PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_PACKAGE     :=
PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_CONFIG      := $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_DIR)/build

PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_sh = $(CONFIG_SHELL) $(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_VERSION)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_SITE)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_GITHUB)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_CONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_ASFLAGS)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_BSBIT)X \
                 $(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_BUILDARCH)X \
		 $(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_CSRCFILE)

$(PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PROC_SMAPS_SHARED_HUGETLB_sh,$(srctree))
