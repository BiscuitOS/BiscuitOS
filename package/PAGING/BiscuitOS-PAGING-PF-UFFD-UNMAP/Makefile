PK_PAGING_PF_UFFD_UNMAP_NAME        := BiscuitOS-PAGING-PF-UFFD-UNMAP
PK_PAGING_PF_UFFD_UNMAP_VERSION     := $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_VERSION)
PK_PAGING_PF_UFFD_UNMAP_SITE        := $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_URL)
PK_PAGING_PF_UFFD_UNMAP_GITHUB      := $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_GITHUB)
PK_PAGING_PF_UFFD_UNMAP_SUBNAME     :=
PK_PAGING_PF_UFFD_UNMAP_BUILDARCH   := $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_BUILDARCH)
PK_PAGING_PF_UFFD_UNMAP_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-UFFD-UNMAP
PK_PAGING_PF_UFFD_UNMAP_PATCH       := $(PK_PAGING_PF_UFFD_UNMAP_DIR)/patch
PK_PAGING_PF_UFFD_UNMAP_BSBIT       := BiscuitOS-PAGING-PF-UFFD-UNMAP-$(PK_PAGING_PF_UFFD_UNMAP_VERSION).bsbit
PK_PAGING_PF_UFFD_UNMAP_OUT         :=
PK_PAGING_PF_UFFD_UNMAP_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_UFFD_UNMAP_TAR         := $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_TAR)
PK_PAGING_PF_UFFD_UNMAP_PACKAGE     :=
PK_PAGING_PF_UFFD_UNMAP_CONFIG      := $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_UFFD_UNMAP_DIR)/build

PK_PAGING_PF_UFFD_UNMAP_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_UFFD_UNMAP_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_UFFD_UNMAP_NAME)X \
                 $(PK_PAGING_PF_UFFD_UNMAP_VERSION)X \
                 $(PK_PAGING_PF_UFFD_UNMAP_SITE)X \
                 $(PK_PAGING_PF_UFFD_UNMAP_GITHUB)X \
                 $(PK_PAGING_PF_UFFD_UNMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_UFFD_UNMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_UFFD_UNMAP_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_ASFLAGS)X \
                 $(PK_PAGING_PF_UFFD_UNMAP_BSBIT)X \
                 $(PK_PAGING_PF_UFFD_UNMAP_BUILDARCH)X \
		 $(PK_PAGING_PF_UFFD_UNMAP_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_UNMAP_CSRCFILE)

$(PK_PAGING_PF_UFFD_UNMAP_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_UFFD_UNMAP_sh,$(srctree))
