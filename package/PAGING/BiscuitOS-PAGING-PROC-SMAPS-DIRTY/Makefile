PK_PAGING_PROC_SMAPS_DIRTY_NAME        := BiscuitOS-PAGING-PROC-SMAPS-DIRTY
PK_PAGING_PROC_SMAPS_DIRTY_VERSION     := $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_VERSION)
PK_PAGING_PROC_SMAPS_DIRTY_SITE        := $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_URL)
PK_PAGING_PROC_SMAPS_DIRTY_GITHUB      := $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_GITHUB)
PK_PAGING_PROC_SMAPS_DIRTY_SUBNAME     :=
PK_PAGING_PROC_SMAPS_DIRTY_BUILDARCH   := $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_BUILDARCH)
PK_PAGING_PROC_SMAPS_DIRTY_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PROC-SMAPS-DIRTY
PK_PAGING_PROC_SMAPS_DIRTY_PATCH       := $(PK_PAGING_PROC_SMAPS_DIRTY_DIR)/patch
PK_PAGING_PROC_SMAPS_DIRTY_BSBIT       := BiscuitOS-PAGING-PROC-SMAPS-DIRTY-$(PK_PAGING_PROC_SMAPS_DIRTY_VERSION).bsbit
PK_PAGING_PROC_SMAPS_DIRTY_OUT         :=
PK_PAGING_PROC_SMAPS_DIRTY_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PROC_SMAPS_DIRTY_TAR         := $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_TAR)
PK_PAGING_PROC_SMAPS_DIRTY_PACKAGE     :=
PK_PAGING_PROC_SMAPS_DIRTY_CONFIG      := $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PROC_SMAPS_DIRTY_DIR)/build

PK_PAGING_PROC_SMAPS_DIRTY_sh = $(CONFIG_SHELL) $(PK_PAGING_PROC_SMAPS_DIRTY_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PROC_SMAPS_DIRTY_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_DIRTY_VERSION)X \
                 $(PK_PAGING_PROC_SMAPS_DIRTY_SITE)X \
                 $(PK_PAGING_PROC_SMAPS_DIRTY_GITHUB)X \
                 $(PK_PAGING_PROC_SMAPS_DIRTY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PROC_SMAPS_DIRTY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PROC_SMAPS_DIRTY_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_CONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_ASFLAGS)X \
                 $(PK_PAGING_PROC_SMAPS_DIRTY_BSBIT)X \
                 $(PK_PAGING_PROC_SMAPS_DIRTY_BUILDARCH)X \
		 $(PK_PAGING_PROC_SMAPS_DIRTY_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PROC_SMAPS_DIRTY_CSRCFILE)

$(PK_PAGING_PROC_SMAPS_DIRTY_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PROC_SMAPS_DIRTY_sh,$(srctree))
