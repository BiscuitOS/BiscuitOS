PK_PAGING_PF_ECODE_WRITE_NAME        := BiscuitOS-PAGING-PF-ECODE-WRITE
PK_PAGING_PF_ECODE_WRITE_VERSION     := $(CONFIG_PK_PAGING_PF_ECODE_WRITE_VERSION)
PK_PAGING_PF_ECODE_WRITE_SITE        := $(CONFIG_PK_PAGING_PF_ECODE_WRITE_URL)
PK_PAGING_PF_ECODE_WRITE_GITHUB      := $(CONFIG_PK_PAGING_PF_ECODE_WRITE_GITHUB)
PK_PAGING_PF_ECODE_WRITE_SUBNAME     :=
PK_PAGING_PF_ECODE_WRITE_BUILDARCH   := $(CONFIG_PK_PAGING_PF_ECODE_WRITE_BUILDARCH)
PK_PAGING_PF_ECODE_WRITE_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-ECODE-WRITE
PK_PAGING_PF_ECODE_WRITE_PATCH       := $(PK_PAGING_PF_ECODE_WRITE_DIR)/patch
PK_PAGING_PF_ECODE_WRITE_BSBIT       := BiscuitOS-PAGING-PF-ECODE-WRITE-$(PK_PAGING_PF_ECODE_WRITE_VERSION).bsbit
PK_PAGING_PF_ECODE_WRITE_OUT         :=
PK_PAGING_PF_ECODE_WRITE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_ECODE_WRITE_TAR         := $(CONFIG_PK_PAGING_PF_ECODE_WRITE_TAR)
PK_PAGING_PF_ECODE_WRITE_PACKAGE     :=
PK_PAGING_PF_ECODE_WRITE_CONFIG      := $(CONFIG_PK_PAGING_PF_ECODE_WRITE_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_ECODE_WRITE_DIR)/build

PK_PAGING_PF_ECODE_WRITE_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_ECODE_WRITE_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_ECODE_WRITE_NAME)X \
                 $(PK_PAGING_PF_ECODE_WRITE_VERSION)X \
                 $(PK_PAGING_PF_ECODE_WRITE_SITE)X \
                 $(PK_PAGING_PF_ECODE_WRITE_GITHUB)X \
                 $(PK_PAGING_PF_ECODE_WRITE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_ECODE_WRITE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_ECODE_WRITE_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_WRITE_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_WRITE_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_WRITE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_WRITE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_WRITE_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_WRITE_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_WRITE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_WRITE_ASFLAGS)X \
                 $(PK_PAGING_PF_ECODE_WRITE_BSBIT)X \
                 $(PK_PAGING_PF_ECODE_WRITE_BUILDARCH)X \
		 $(PK_PAGING_PF_ECODE_WRITE_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_WRITE_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_WRITE_CSRCFILE)

$(PK_PAGING_PF_ECODE_WRITE_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_ECODE_WRITE_sh,$(srctree))
