PK_PAGING_PF_ECODE_PROT_NAME        := BiscuitOS-PAGING-PF-ECODE-PROT
PK_PAGING_PF_ECODE_PROT_VERSION     := $(CONFIG_PK_PAGING_PF_ECODE_PROT_VERSION)
PK_PAGING_PF_ECODE_PROT_SITE        := $(CONFIG_PK_PAGING_PF_ECODE_PROT_URL)
PK_PAGING_PF_ECODE_PROT_GITHUB      := $(CONFIG_PK_PAGING_PF_ECODE_PROT_GITHUB)
PK_PAGING_PF_ECODE_PROT_SUBNAME     :=
PK_PAGING_PF_ECODE_PROT_BUILDARCH   := $(CONFIG_PK_PAGING_PF_ECODE_PROT_BUILDARCH)
PK_PAGING_PF_ECODE_PROT_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-ECODE-PROT
PK_PAGING_PF_ECODE_PROT_PATCH       := $(PK_PAGING_PF_ECODE_PROT_DIR)/patch
PK_PAGING_PF_ECODE_PROT_BSBIT       := BiscuitOS-PAGING-PF-ECODE-PROT-$(PK_PAGING_PF_ECODE_PROT_VERSION).bsbit
PK_PAGING_PF_ECODE_PROT_OUT         :=
PK_PAGING_PF_ECODE_PROT_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_ECODE_PROT_TAR         := $(CONFIG_PK_PAGING_PF_ECODE_PROT_TAR)
PK_PAGING_PF_ECODE_PROT_PACKAGE     :=
PK_PAGING_PF_ECODE_PROT_CONFIG      := $(CONFIG_PK_PAGING_PF_ECODE_PROT_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_ECODE_PROT_DIR)/build

PK_PAGING_PF_ECODE_PROT_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_ECODE_PROT_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_ECODE_PROT_NAME)X \
                 $(PK_PAGING_PF_ECODE_PROT_VERSION)X \
                 $(PK_PAGING_PF_ECODE_PROT_SITE)X \
                 $(PK_PAGING_PF_ECODE_PROT_GITHUB)X \
                 $(PK_PAGING_PF_ECODE_PROT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_ECODE_PROT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_ECODE_PROT_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PROT_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PROT_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PROT_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PROT_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PROT_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PROT_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PROT_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PROT_ASFLAGS)X \
                 $(PK_PAGING_PF_ECODE_PROT_BSBIT)X \
                 $(PK_PAGING_PF_ECODE_PROT_BUILDARCH)X \
		 $(PK_PAGING_PF_ECODE_PROT_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PROT_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ECODE_PROT_CSRCFILE)

$(PK_PAGING_PF_ECODE_PROT_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_ECODE_PROT_sh,$(srctree))
