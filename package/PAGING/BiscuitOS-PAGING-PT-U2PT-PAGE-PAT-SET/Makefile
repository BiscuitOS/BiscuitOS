PK_PAGING_PT_U2PT_PAGE_PAT_SET_SITE        := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_URL)
PK_PAGING_PT_U2PT_PAGE_PAT_SET_GITHUB      := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_GITHUB)
PK_PAGING_PT_U2PT_PAGE_PAT_SET_SUBNAME     :=
PK_PAGING_PT_U2PT_PAGE_PAT_SET_BUILDARCH   := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_BUILDARCH)
PK_PAGING_PT_U2PT_PAGE_PAT_SET_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PT-U2PT-PAGE-PAT-SET
PK_PAGING_PT_U2PT_PAGE_PAT_SET_PATCH       := $(PK_PAGING_PT_U2PT_PAGE_PAT_SET_DIR)/patch
PK_PAGING_PT_U2PT_PAGE_PAT_SET_BSBIT       := BiscuitOS-PAGING-PT-U2PT-PAGE-PAT-SET-$(PK_PAGING_PT_U2PT_PAGE_PAT_SET_VERSION).bsbit
PK_PAGING_PT_U2PT_PAGE_PAT_SET_OUT         :=
PK_PAGING_PT_U2PT_PAGE_PAT_SET_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PT_U2PT_PAGE_PAT_SET_TAR         := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_TAR)
PK_PAGING_PT_U2PT_PAGE_PAT_SET_PACKAGE     :=
PK_PAGING_PT_U2PT_PAGE_PAT_SET_CONFIG      := $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_U2PT_PAGE_PAT_SET_DIR)/build

PK_PAGING_PT_U2PT_PAGE_PAT_SET_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_U2PT_PAGE_PAT_SET_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PT_U2PT_PAGE_PAT_SET_NAME)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PAT_SET_VERSION)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PAT_SET_SITE)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PAT_SET_GITHUB)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PAT_SET_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PAT_SET_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PAT_SET_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_ASFLAGS)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PAT_SET_BSBIT)X \
                 $(PK_PAGING_PT_U2PT_PAGE_PAT_SET_BUILDARCH)X \
		 $(PK_PAGING_PT_U2PT_PAGE_PAT_SET_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_U2PT_PAGE_PAT_SET_CSRCFILE)

$(PK_PAGING_PT_U2PT_PAGE_PAT_SET_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PT_U2PT_PAGE_PAT_SET_sh,$(srctree))
