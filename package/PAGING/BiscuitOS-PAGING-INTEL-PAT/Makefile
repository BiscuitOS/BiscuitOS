################################################
#
# BiscuitOS-PAGING-INTEL-PAT
#
################################################
PK_PAGING_INTEL_PAT_NAME        := BiscuitOS-PAGING-INTEL-PAT
PK_PAGING_INTEL_PAT_VERSION     := $(CONFIG_PK_PAGING_INTEL_PAT_VERSION)
PK_PAGING_INTEL_PAT_SITE        := $(CONFIG_PK_PAGING_INTEL_PAT_URL)
PK_PAGING_INTEL_PAT_GITHUB      := $(CONFIG_PK_PAGING_INTEL_PAT_GITHUB)
PK_PAGING_INTEL_PAT_SUBNAME     :=
PK_PAGING_INTEL_PAT_BUILDARCH   := $(CONFIG_PK_PAGING_INTEL_PAT_BUILDARCH)
PK_PAGING_INTEL_PAT_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-INTEL-PAT
PK_PAGING_INTEL_PAT_PATCH       := $(PK_PAGING_INTEL_PAT_DIR)/patch
PK_PAGING_INTEL_PAT_BSBIT       := BiscuitOS-PAGING-INTEL-PAT-$(PK_PAGING_INTEL_PAT_VERSION).bsbit
PK_PAGING_INTEL_PAT_OUT         :=
PK_PAGING_INTEL_PAT_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_INTEL_PAT_TAR         := $(CONFIG_PK_PAGING_INTEL_PAT_TAR)
PK_PAGING_INTEL_PAT_PACKAGE     :=
PK_PAGING_INTEL_PAT_CONFIG      := $(CONFIG_PK_PAGING_INTEL_PAT_CONFIG)
SUB_TARGET                  += $(PK_PAGING_INTEL_PAT_DIR)/build

PK_PAGING_INTEL_PAT_sh = $(CONFIG_SHELL) $(PK_PAGING_INTEL_PAT_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_INTEL_PAT_NAME)X \
                 $(PK_PAGING_INTEL_PAT_VERSION)X \
                 $(PK_PAGING_INTEL_PAT_SITE)X \
                 $(PK_PAGING_INTEL_PAT_GITHUB)X \
                 $(PK_PAGING_INTEL_PAT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_INTEL_PAT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_INTEL_PAT_SUBNAME)X \
                 $(CONFIG_PK_PAGING_INTEL_PAT_CONFIG)X \
                 $(CONFIG_PK_PAGING_INTEL_PAT_LIBPATH)X \
                 $(CONFIG_PK_PAGING_INTEL_PAT_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_INTEL_PAT_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_INTEL_PAT_CFLAGS)X \
                 $(CONFIG_PK_PAGING_INTEL_PAT_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_INTEL_PAT_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_INTEL_PAT_ASFLAGS)X \
                 $(PK_PAGING_INTEL_PAT_BSBIT)X \
                 $(PK_PAGING_INTEL_PAT_BUILDARCH)X \
		 $(PK_PAGING_INTEL_PAT_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_INTEL_PAT_SRCFILE)X \
                 $(CONFIG_PK_PAGING_INTEL_PAT_CSRCFILE)

$(PK_PAGING_INTEL_PAT_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_INTEL_PAT_sh,$(srctree))
