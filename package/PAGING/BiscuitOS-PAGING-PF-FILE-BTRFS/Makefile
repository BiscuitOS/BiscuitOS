PK_PAGING_PF_FILE_BTRFS_NAME        := BiscuitOS-PAGING-PF-FILE-BTRFS
PK_PAGING_PF_FILE_BTRFS_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_BTRFS_VERSION)
PK_PAGING_PF_FILE_BTRFS_SITE        := $(CONFIG_PK_PAGING_PF_FILE_BTRFS_URL)
PK_PAGING_PF_FILE_BTRFS_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_BTRFS_GITHUB)
PK_PAGING_PF_FILE_BTRFS_SUBNAME     :=
PK_PAGING_PF_FILE_BTRFS_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_BTRFS_BUILDARCH)
PK_PAGING_PF_FILE_BTRFS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-BTRFS
PK_PAGING_PF_FILE_BTRFS_PATCH       := $(PK_PAGING_PF_FILE_BTRFS_DIR)/patch
PK_PAGING_PF_FILE_BTRFS_BSBIT       := BiscuitOS-PAGING-PF-FILE-BTRFS-$(PK_PAGING_PF_FILE_BTRFS_VERSION).bsbit
PK_PAGING_PF_FILE_BTRFS_OUT         :=
PK_PAGING_PF_FILE_BTRFS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_BTRFS_TAR         := $(CONFIG_PK_PAGING_PF_FILE_BTRFS_TAR)
PK_PAGING_PF_FILE_BTRFS_PACKAGE     :=
PK_PAGING_PF_FILE_BTRFS_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_BTRFS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_BTRFS_DIR)/build

PK_PAGING_PF_FILE_BTRFS_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_BTRFS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_BTRFS_NAME)X \
                 $(PK_PAGING_PF_FILE_BTRFS_VERSION)X \
                 $(PK_PAGING_PF_FILE_BTRFS_SITE)X \
                 $(PK_PAGING_PF_FILE_BTRFS_GITHUB)X \
                 $(PK_PAGING_PF_FILE_BTRFS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_BTRFS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_BTRFS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BTRFS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BTRFS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BTRFS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BTRFS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BTRFS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BTRFS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BTRFS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BTRFS_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_BTRFS_BSBIT)X \
                 $(PK_PAGING_PF_FILE_BTRFS_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_BTRFS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BTRFS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_BTRFS_CSRCFILE)

$(PK_PAGING_PF_FILE_BTRFS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_BTRFS_sh,$(srctree))
