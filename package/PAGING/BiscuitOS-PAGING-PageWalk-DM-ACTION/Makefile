PK_PAGING_PAGEWALK_DM_ACTION_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_URL)
PK_PAGING_PAGEWALK_DM_ACTION_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_GITHUB)
PK_PAGING_PAGEWALK_DM_ACTION_SUBNAME     :=
PK_PAGING_PAGEWALK_DM_ACTION_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_BUILDARCH)
PK_PAGING_PAGEWALK_DM_ACTION_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-DM-ACTION
PK_PAGING_PAGEWALK_DM_ACTION_PATCH       := $(PK_PAGING_PAGEWALK_DM_ACTION_DIR)/patch
PK_PAGING_PAGEWALK_DM_ACTION_BSBIT       := BiscuitOS-PAGING-PageWalk-DM-ACTION-$(PK_PAGING_PAGEWALK_DM_ACTION_VERSION).bsbit
PK_PAGING_PAGEWALK_DM_ACTION_OUT         :=
PK_PAGING_PAGEWALK_DM_ACTION_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_DM_ACTION_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_TAR)
PK_PAGING_PAGEWALK_DM_ACTION_PACKAGE     :=
PK_PAGING_PAGEWALK_DM_ACTION_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_DM_ACTION_DIR)/build

PK_PAGING_PAGEWALK_DM_ACTION_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_DM_ACTION_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_DM_ACTION_NAME)X \
                 $(PK_PAGING_PAGEWALK_DM_ACTION_VERSION)X \
                 $(PK_PAGING_PAGEWALK_DM_ACTION_SITE)X \
                 $(PK_PAGING_PAGEWALK_DM_ACTION_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_DM_ACTION_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_DM_ACTION_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_DM_ACTION_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_DM_ACTION_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_DM_ACTION_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_DM_ACTION_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_DM_ACTION_CSRCFILE)

$(PK_PAGING_PAGEWALK_DM_ACTION_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_DM_ACTION_sh,$(srctree))
