PK_PAGING_PF_SHMEM_DEVSHM_NAME        := BiscuitOS-PAGING-PF-SHMEM-DEVSHM
PK_PAGING_PF_SHMEM_DEVSHM_VERSION     := $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_VERSION)
PK_PAGING_PF_SHMEM_DEVSHM_SITE        := $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_URL)
PK_PAGING_PF_SHMEM_DEVSHM_GITHUB      := $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_GITHUB)
PK_PAGING_PF_SHMEM_DEVSHM_SUBNAME     :=
PK_PAGING_PF_SHMEM_DEVSHM_BUILDARCH   := $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_BUILDARCH)
PK_PAGING_PF_SHMEM_DEVSHM_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-SHMEM-DEVSHM
PK_PAGING_PF_SHMEM_DEVSHM_PATCH       := $(PK_PAGING_PF_SHMEM_DEVSHM_DIR)/patch
PK_PAGING_PF_SHMEM_DEVSHM_BSBIT       := BiscuitOS-PAGING-PF-SHMEM-DEVSHM-$(PK_PAGING_PF_SHMEM_DEVSHM_VERSION).bsbit
PK_PAGING_PF_SHMEM_DEVSHM_OUT         :=
PK_PAGING_PF_SHMEM_DEVSHM_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_SHMEM_DEVSHM_TAR         := $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_TAR)
PK_PAGING_PF_SHMEM_DEVSHM_PACKAGE     :=
PK_PAGING_PF_SHMEM_DEVSHM_CONFIG      := $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_SHMEM_DEVSHM_DIR)/build

PK_PAGING_PF_SHMEM_DEVSHM_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_SHMEM_DEVSHM_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_SHMEM_DEVSHM_NAME)X \
                 $(PK_PAGING_PF_SHMEM_DEVSHM_VERSION)X \
                 $(PK_PAGING_PF_SHMEM_DEVSHM_SITE)X \
                 $(PK_PAGING_PF_SHMEM_DEVSHM_GITHUB)X \
                 $(PK_PAGING_PF_SHMEM_DEVSHM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_SHMEM_DEVSHM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_SHMEM_DEVSHM_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_ASFLAGS)X \
                 $(PK_PAGING_PF_SHMEM_DEVSHM_BSBIT)X \
                 $(PK_PAGING_PF_SHMEM_DEVSHM_BUILDARCH)X \
		 $(PK_PAGING_PF_SHMEM_DEVSHM_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_DEVSHM_CSRCFILE)

$(PK_PAGING_PF_SHMEM_DEVSHM_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_SHMEM_DEVSHM_sh,$(srctree))
