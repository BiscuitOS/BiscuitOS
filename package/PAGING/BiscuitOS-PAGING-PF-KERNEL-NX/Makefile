PK_PAGING_PF_KERNEL_NX_NAME        := BiscuitOS-PAGING-PF-KERNEL-NX
PK_PAGING_PF_KERNEL_NX_VERSION     := $(CONFIG_PK_PAGING_PF_KERNEL_NX_VERSION)
PK_PAGING_PF_KERNEL_NX_SITE        := $(CONFIG_PK_PAGING_PF_KERNEL_NX_URL)
PK_PAGING_PF_KERNEL_NX_GITHUB      := $(CONFIG_PK_PAGING_PF_KERNEL_NX_GITHUB)
PK_PAGING_PF_KERNEL_NX_SUBNAME     :=
PK_PAGING_PF_KERNEL_NX_BUILDARCH   := $(CONFIG_PK_PAGING_PF_KERNEL_NX_BUILDARCH)
PK_PAGING_PF_KERNEL_NX_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-KERNEL-NX
PK_PAGING_PF_KERNEL_NX_PATCH       := $(PK_PAGING_PF_KERNEL_NX_DIR)/patch
PK_PAGING_PF_KERNEL_NX_BSBIT       := BiscuitOS-PAGING-PF-KERNEL-NX-$(PK_PAGING_PF_KERNEL_NX_VERSION).bsbit
PK_PAGING_PF_KERNEL_NX_OUT         :=
PK_PAGING_PF_KERNEL_NX_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_PF_KERNEL_NX_TAR         := $(CONFIG_PK_PAGING_PF_KERNEL_NX_TAR)
PK_PAGING_PF_KERNEL_NX_PACKAGE     :=
PK_PAGING_PF_KERNEL_NX_CONFIG      := $(CONFIG_PK_PAGING_PF_KERNEL_NX_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_KERNEL_NX_DIR)/build

PK_PAGING_PF_KERNEL_NX_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_KERNEL_NX_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_KERNEL_NX_NAME)X \
                 $(PK_PAGING_PF_KERNEL_NX_VERSION)X \
                 $(PK_PAGING_PF_KERNEL_NX_SITE)X \
                 $(PK_PAGING_PF_KERNEL_NX_GITHUB)X \
                 $(PK_PAGING_PF_KERNEL_NX_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_KERNEL_NX_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_KERNEL_NX_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_KERNEL_NX_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_KERNEL_NX_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_KERNEL_NX_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_KERNEL_NX_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_KERNEL_NX_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_KERNEL_NX_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_KERNEL_NX_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_KERNEL_NX_ASFLAGS)X \
                 $(PK_PAGING_PF_KERNEL_NX_BSBIT)X \
                 $(PK_PAGING_PF_KERNEL_NX_BUILDARCH)X \
		 $(PK_PAGING_PF_KERNEL_NX_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_KERNEL_NX_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_KERNEL_NX_CSRCFILE)

$(PK_PAGING_PF_KERNEL_NX_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_KERNEL_NX_sh,$(srctree))
