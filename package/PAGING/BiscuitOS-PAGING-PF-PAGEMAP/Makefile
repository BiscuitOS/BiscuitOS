PK_PAGING_PF_PAGEMAP_NAME        := BiscuitOS-PAGING-PF-PAGEMAP
PK_PAGING_PF_PAGEMAP_VERSION     := $(CONFIG_PK_PAGING_PF_PAGEMAP_VERSION)
PK_PAGING_PF_PAGEMAP_SITE        := $(CONFIG_PK_PAGING_PF_PAGEMAP_URL)
PK_PAGING_PF_PAGEMAP_GITHUB      := $(CONFIG_PK_PAGING_PF_PAGEMAP_GITHUB)
PK_PAGING_PF_PAGEMAP_SUBNAME     :=
PK_PAGING_PF_PAGEMAP_BUILDARCH   := $(CONFIG_PK_PAGING_PF_PAGEMAP_BUILDARCH)
PK_PAGING_PF_PAGEMAP_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-PAGEMAP
PK_PAGING_PF_PAGEMAP_PATCH       := $(PK_PAGING_PF_PAGEMAP_DIR)/patch
PK_PAGING_PF_PAGEMAP_BSBIT       := BiscuitOS-PAGING-PF-PAGEMAP-$(PK_PAGING_PF_PAGEMAP_VERSION).bsbit
PK_PAGING_PF_PAGEMAP_OUT         :=
PK_PAGING_PF_PAGEMAP_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_PF_PAGEMAP_TAR         := $(CONFIG_PK_PAGING_PF_PAGEMAP_TAR)
PK_PAGING_PF_PAGEMAP_PACKAGE     :=
PK_PAGING_PF_PAGEMAP_CONFIG      := $(CONFIG_PK_PAGING_PF_PAGEMAP_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_PAGEMAP_DIR)/build

PK_PAGING_PF_PAGEMAP_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_PAGEMAP_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_PAGEMAP_NAME)X \
                 $(PK_PAGING_PF_PAGEMAP_VERSION)X \
                 $(PK_PAGING_PF_PAGEMAP_SITE)X \
                 $(PK_PAGING_PF_PAGEMAP_GITHUB)X \
                 $(PK_PAGING_PF_PAGEMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_PAGEMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_PAGEMAP_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_PAGEMAP_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_PAGEMAP_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_PAGEMAP_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_PAGEMAP_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_PAGEMAP_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_PAGEMAP_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_PAGEMAP_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_PAGEMAP_ASFLAGS)X \
                 $(PK_PAGING_PF_PAGEMAP_BSBIT)X \
                 $(PK_PAGING_PF_PAGEMAP_BUILDARCH)X \
		 $(PK_PAGING_PF_PAGEMAP_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_PAGEMAP_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_PAGEMAP_CSRCFILE)

$(PK_PAGING_PF_PAGEMAP_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_PAGEMAP_sh,$(srctree))
