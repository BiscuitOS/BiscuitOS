PK_PAGING_INTEL_MTRRS_NAME        := BiscuitOS-PAGING-INTEL-MTRRS
PK_PAGING_INTEL_MTRRS_VERSION     := $(CONFIG_PK_PAGING_INTEL_MTRRS_VERSION)
PK_PAGING_INTEL_MTRRS_SITE        := $(CONFIG_PK_PAGING_INTEL_MTRRS_URL)
PK_PAGING_INTEL_MTRRS_GITHUB      := $(CONFIG_PK_PAGING_INTEL_MTRRS_GITHUB)
PK_PAGING_INTEL_MTRRS_SUBNAME     :=
PK_PAGING_INTEL_MTRRS_BUILDARCH   := $(CONFIG_PK_PAGING_INTEL_MTRRS_BUILDARCH)
PK_PAGING_INTEL_MTRRS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-INTEL-MTRRS
PK_PAGING_INTEL_MTRRS_MTRRSCH       := $(PK_PAGING_INTEL_MTRRS_DIR)/patch
PK_PAGING_INTEL_MTRRS_BSBIT       := BiscuitOS-PAGING-INTEL-MTRRS-$(PK_PAGING_INTEL_MTRRS_VERSION).bsbit
PK_PAGING_INTEL_MTRRS_OUT         :=
PK_PAGING_INTEL_MTRRS_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_INTEL_MTRRS_TAR         := $(CONFIG_PK_PAGING_INTEL_MTRRS_TAR)
PK_PAGING_INTEL_MTRRS_PACKAGE     :=
PK_PAGING_INTEL_MTRRS_CONFIG      := $(CONFIG_PK_PAGING_INTEL_MTRRS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_INTEL_MTRRS_DIR)/build

PK_PAGING_INTEL_MTRRS_sh = $(CONFIG_SHELL) $(PK_PAGING_INTEL_MTRRS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_INTEL_MTRRS_NAME)X \
                 $(PK_PAGING_INTEL_MTRRS_VERSION)X \
                 $(PK_PAGING_INTEL_MTRRS_SITE)X \
                 $(PK_PAGING_INTEL_MTRRS_GITHUB)X \
                 $(PK_PAGING_INTEL_MTRRS_MTRRSCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_INTEL_MTRRS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_INTEL_MTRRS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_INTEL_MTRRS_CONFIG)X \
                 $(CONFIG_PK_PAGING_INTEL_MTRRS_LIBMTRRSH)X \
                 $(CONFIG_PK_PAGING_INTEL_MTRRS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_INTEL_MTRRS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_INTEL_MTRRS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_INTEL_MTRRS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_INTEL_MTRRS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_INTEL_MTRRS_ASFLAGS)X \
                 $(PK_PAGING_INTEL_MTRRS_BSBIT)X \
                 $(PK_PAGING_INTEL_MTRRS_BUILDARCH)X \
		 $(PK_PAGING_INTEL_MTRRS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_INTEL_MTRRS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_INTEL_MTRRS_CSRCFILE)

$(PK_PAGING_INTEL_MTRRS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_INTEL_MTRRS_sh,$(srctree))
