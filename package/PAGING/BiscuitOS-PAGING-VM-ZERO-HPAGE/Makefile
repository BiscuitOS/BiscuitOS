PK_PAGING_VM_ZERO_HPAGE_NAME        := BiscuitOS-PAGING-VM-ZERO-HPAGE
PK_PAGING_VM_ZERO_HPAGE_VERSION     := $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_VERSION)
PK_PAGING_VM_ZERO_HPAGE_SITE        := $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_URL)
PK_PAGING_VM_ZERO_HPAGE_GITHUB      := $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_GITHUB)
PK_PAGING_VM_ZERO_HPAGE_SUBNAME     :=
PK_PAGING_VM_ZERO_HPAGE_BUILDARCH   := $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_BUILDARCH)
PK_PAGING_VM_ZERO_HPAGE_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-VM-ZERO-HPAGE
PK_PAGING_VM_ZERO_HPAGE_PATCH       := $(PK_PAGING_VM_ZERO_HPAGE_DIR)/patch
PK_PAGING_VM_ZERO_HPAGE_BSBIT       := BiscuitOS-PAGING-VM-ZERO-HPAGE-$(PK_PAGING_VM_ZERO_HPAGE_VERSION).bsbit
PK_PAGING_VM_ZERO_HPAGE_OUT         :=
PK_PAGING_VM_ZERO_HPAGE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_VM_ZERO_HPAGE_TAR         := $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_TAR)
PK_PAGING_VM_ZERO_HPAGE_PACKAGE     :=
PK_PAGING_VM_ZERO_HPAGE_CONFIG      := $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_CONFIG)
SUB_TARGET                  += $(PK_PAGING_VM_ZERO_HPAGE_DIR)/build

PK_PAGING_VM_ZERO_HPAGE_sh = $(CONFIG_SHELL) $(PK_PAGING_VM_ZERO_HPAGE_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_VM_ZERO_HPAGE_NAME)X \
                 $(PK_PAGING_VM_ZERO_HPAGE_VERSION)X \
                 $(PK_PAGING_VM_ZERO_HPAGE_SITE)X \
                 $(PK_PAGING_VM_ZERO_HPAGE_GITHUB)X \
                 $(PK_PAGING_VM_ZERO_HPAGE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_VM_ZERO_HPAGE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_VM_ZERO_HPAGE_SUBNAME)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_CONFIG)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_LIBPATH)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_CFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_ASFLAGS)X \
                 $(PK_PAGING_VM_ZERO_HPAGE_BSBIT)X \
                 $(PK_PAGING_VM_ZERO_HPAGE_BUILDARCH)X \
		 $(PK_PAGING_VM_ZERO_HPAGE_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_SRCFILE)X \
                 $(CONFIG_PK_PAGING_VM_ZERO_HPAGE_CSRCFILE)

$(PK_PAGING_VM_ZERO_HPAGE_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_VM_ZERO_HPAGE_sh,$(srctree))
