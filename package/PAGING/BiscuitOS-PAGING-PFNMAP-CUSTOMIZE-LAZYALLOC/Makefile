PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_NAME        := BiscuitOS-PAGING-PFNMAP-CUSTOMIZE-LAZYALLOC
PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_VERSION     := $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_VERSION)
PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_SITE        := $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_URL)
PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_GITHUB      := $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_GITHUB)
PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_SUBNAME     :=
PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_BUILDARCH   := $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_BUILDARCH)
PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PFNMAP-CUSTOMIZE-LAZYALLOC
PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_PATCH       := $(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_DIR)/patch
PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_BSBIT       := BiscuitOS-PAGING-PFNMAP-CUSTOMIZE-LAZYALLOC-$(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_VERSION).bsbit
PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_OUT         :=
PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_TAR         := $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_TAR)
PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_PACKAGE     :=
PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_CONFIG      := $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_DIR)/build

PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_sh = $(CONFIG_SHELL) $(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_NAME)X \
                 $(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_VERSION)X \
                 $(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_SITE)X \
                 $(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_GITHUB)X \
                 $(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_CONFIG)X \
                 $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_ASFLAGS)X \
                 $(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_BSBIT)X \
                 $(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_BUILDARCH)X \
		 $(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_CSRCFILE)

$(PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PFNMAP_CUSTOMIZE_LAZYALLOC_sh,$(srctree))
