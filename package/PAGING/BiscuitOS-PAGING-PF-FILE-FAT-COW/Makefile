PK_PAGING_PF_FILE_FAT_COW_NAME        := BiscuitOS-PAGING-PF-FILE-FAT-COW
PK_PAGING_PF_FILE_FAT_COW_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_VERSION)
PK_PAGING_PF_FILE_FAT_COW_SITE        := $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_URL)
PK_PAGING_PF_FILE_FAT_COW_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_GITHUB)
PK_PAGING_PF_FILE_FAT_COW_SUBNAME     :=
PK_PAGING_PF_FILE_FAT_COW_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_BUILDARCH)
PK_PAGING_PF_FILE_FAT_COW_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-FAT-COW
PK_PAGING_PF_FILE_FAT_COW_PATCH       := $(PK_PAGING_PF_FILE_FAT_COW_DIR)/patch
PK_PAGING_PF_FILE_FAT_COW_BSBIT       := BiscuitOS-PAGING-PF-FILE-FAT-COW-$(PK_PAGING_PF_FILE_FAT_COW_VERSION).bsbit
PK_PAGING_PF_FILE_FAT_COW_OUT         :=
PK_PAGING_PF_FILE_FAT_COW_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_FAT_COW_TAR         := $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_TAR)
PK_PAGING_PF_FILE_FAT_COW_PACKAGE     :=
PK_PAGING_PF_FILE_FAT_COW_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_FAT_COW_DIR)/build

PK_PAGING_PF_FILE_FAT_COW_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_FAT_COW_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_FAT_COW_NAME)X \
                 $(PK_PAGING_PF_FILE_FAT_COW_VERSION)X \
                 $(PK_PAGING_PF_FILE_FAT_COW_SITE)X \
                 $(PK_PAGING_PF_FILE_FAT_COW_GITHUB)X \
                 $(PK_PAGING_PF_FILE_FAT_COW_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_FAT_COW_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_FAT_COW_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_FAT_COW_BSBIT)X \
                 $(PK_PAGING_PF_FILE_FAT_COW_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_FAT_COW_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_FAT_COW_CSRCFILE)

$(PK_PAGING_PF_FILE_FAT_COW_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_FAT_COW_sh,$(srctree))
