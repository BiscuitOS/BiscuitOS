PK_PAGING_PF_SHMEM_LOCAL_NAME        := BiscuitOS-PAGING-PF-SHMEM-LOCAL
PK_PAGING_PF_SHMEM_LOCAL_VERSION     := $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_VERSION)
PK_PAGING_PF_SHMEM_LOCAL_SITE        := $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_URL)
PK_PAGING_PF_SHMEM_LOCAL_GITHUB      := $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_GITHUB)
PK_PAGING_PF_SHMEM_LOCAL_SUBNAME     :=
PK_PAGING_PF_SHMEM_LOCAL_BUILDARCH   := $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_BUILDARCH)
PK_PAGING_PF_SHMEM_LOCAL_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-SHMEM-LOCAL
PK_PAGING_PF_SHMEM_LOCAL_PATCH       := $(PK_PAGING_PF_SHMEM_LOCAL_DIR)/patch
PK_PAGING_PF_SHMEM_LOCAL_BSBIT       := BiscuitOS-PAGING-PF-SHMEM-LOCAL-$(PK_PAGING_PF_SHMEM_LOCAL_VERSION).bsbit
PK_PAGING_PF_SHMEM_LOCAL_OUT         :=
PK_PAGING_PF_SHMEM_LOCAL_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_SHMEM_LOCAL_TAR         := $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_TAR)
PK_PAGING_PF_SHMEM_LOCAL_PACKAGE     :=
PK_PAGING_PF_SHMEM_LOCAL_CONFIG      := $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_SHMEM_LOCAL_DIR)/build

PK_PAGING_PF_SHMEM_LOCAL_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_SHMEM_LOCAL_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_SHMEM_LOCAL_NAME)X \
                 $(PK_PAGING_PF_SHMEM_LOCAL_VERSION)X \
                 $(PK_PAGING_PF_SHMEM_LOCAL_SITE)X \
                 $(PK_PAGING_PF_SHMEM_LOCAL_GITHUB)X \
                 $(PK_PAGING_PF_SHMEM_LOCAL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_SHMEM_LOCAL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_SHMEM_LOCAL_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_ASFLAGS)X \
                 $(PK_PAGING_PF_SHMEM_LOCAL_BSBIT)X \
                 $(PK_PAGING_PF_SHMEM_LOCAL_BUILDARCH)X \
		 $(PK_PAGING_PF_SHMEM_LOCAL_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_LOCAL_CSRCFILE)

$(PK_PAGING_PF_SHMEM_LOCAL_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_SHMEM_LOCAL_sh,$(srctree))
