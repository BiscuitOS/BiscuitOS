PK_PAGING_PT_CPR_HUGETLB_NAME        := BiscuitOS-PAGING-PT-CPR-HUGETLB
PK_PAGING_PT_CPR_HUGETLB_VERSION     := $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_VERSION)
PK_PAGING_PT_CPR_HUGETLB_SITE        := $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_URL)
PK_PAGING_PT_CPR_HUGETLB_GITHUB      := $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_GITHUB)
PK_PAGING_PT_CPR_HUGETLB_SUBNAME     :=
PK_PAGING_PT_CPR_HUGETLB_BUILDARCH   := $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_BUILDARCH)
PK_PAGING_PT_CPR_HUGETLB_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PT-CPR-HUGETLB
PK_PAGING_PT_CPR_HUGETLB_PATCH       := $(PK_PAGING_PT_CPR_HUGETLB_DIR)/patch
PK_PAGING_PT_CPR_HUGETLB_BSBIT       := BiscuitOS-PAGING-PT-CPR-HUGETLB-$(PK_PAGING_PT_CPR_HUGETLB_VERSION).bsbit
PK_PAGING_PT_CPR_HUGETLB_OUT         :=
PK_PAGING_PT_CPR_HUGETLB_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PT_CPR_HUGETLB_TAR         := $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_TAR)
PK_PAGING_PT_CPR_HUGETLB_PACKAGE     :=
PK_PAGING_PT_CPR_HUGETLB_CONFIG      := $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_CPR_HUGETLB_DIR)/build

PK_PAGING_PT_CPR_HUGETLB_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_CPR_HUGETLB_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PT_CPR_HUGETLB_NAME)X \
                 $(PK_PAGING_PT_CPR_HUGETLB_VERSION)X \
                 $(PK_PAGING_PT_CPR_HUGETLB_SITE)X \
                 $(PK_PAGING_PT_CPR_HUGETLB_GITHUB)X \
                 $(PK_PAGING_PT_CPR_HUGETLB_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_CPR_HUGETLB_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_CPR_HUGETLB_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_ASFLAGS)X \
                 $(PK_PAGING_PT_CPR_HUGETLB_BSBIT)X \
                 $(PK_PAGING_PT_CPR_HUGETLB_BUILDARCH)X \
		 $(PK_PAGING_PT_CPR_HUGETLB_CSRCHUGETLB)X \
                 $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_SRCHUGETLB)X \
                 $(CONFIG_PK_PAGING_PT_CPR_HUGETLB_CSRCHUGETLB)

$(PK_PAGING_PT_CPR_HUGETLB_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PT_CPR_HUGETLB_sh,$(srctree))
