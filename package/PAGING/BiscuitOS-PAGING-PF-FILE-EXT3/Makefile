PK_PAGING_PF_FILE_EXT3_NAME        := BiscuitOS-PAGING-PF-FILE-EXT3
PK_PAGING_PF_FILE_EXT3_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_EXT3_VERSION)
PK_PAGING_PF_FILE_EXT3_SITE        := $(CONFIG_PK_PAGING_PF_FILE_EXT3_URL)
PK_PAGING_PF_FILE_EXT3_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_EXT3_GITHUB)
PK_PAGING_PF_FILE_EXT3_SUBNAME     :=
PK_PAGING_PF_FILE_EXT3_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_EXT3_BUILDARCH)
PK_PAGING_PF_FILE_EXT3_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-EXT3
PK_PAGING_PF_FILE_EXT3_PATCH       := $(PK_PAGING_PF_FILE_EXT3_DIR)/patch
PK_PAGING_PF_FILE_EXT3_BSBIT       := BiscuitOS-PAGING-PF-FILE-EXT3-$(PK_PAGING_PF_FILE_EXT3_VERSION).bsbit
PK_PAGING_PF_FILE_EXT3_OUT         :=
PK_PAGING_PF_FILE_EXT3_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_EXT3_TAR         := $(CONFIG_PK_PAGING_PF_FILE_EXT3_TAR)
PK_PAGING_PF_FILE_EXT3_PACKAGE     :=
PK_PAGING_PF_FILE_EXT3_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_EXT3_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_EXT3_DIR)/build

PK_PAGING_PF_FILE_EXT3_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_EXT3_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_EXT3_NAME)X \
                 $(PK_PAGING_PF_FILE_EXT3_VERSION)X \
                 $(PK_PAGING_PF_FILE_EXT3_SITE)X \
                 $(PK_PAGING_PF_FILE_EXT3_GITHUB)X \
                 $(PK_PAGING_PF_FILE_EXT3_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_EXT3_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_EXT3_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_EXT3_BSBIT)X \
                 $(PK_PAGING_PF_FILE_EXT3_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_EXT3_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_EXT3_CSRCFILE)

$(PK_PAGING_PF_FILE_EXT3_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_EXT3_sh,$(srctree))
