PK_PAGING_PAGEWALK_MODIFY_NAME        := BiscuitOS-PAGING-PageWalk-Modify
PK_PAGING_PAGEWALK_MODIFY_VERSION     := $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_VERSION)
PK_PAGING_PAGEWALK_MODIFY_SITE        := $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_URL)
PK_PAGING_PAGEWALK_MODIFY_GITHUB      := $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_GITHUB)
PK_PAGING_PAGEWALK_MODIFY_SUBNAME     :=
PK_PAGING_PAGEWALK_MODIFY_BUILDARCH   := $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_BUILDARCH)
PK_PAGING_PAGEWALK_MODIFY_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PageWalk-Modify
PK_PAGING_PAGEWALK_MODIFY_PATCH       := $(PK_PAGING_PAGEWALK_MODIFY_DIR)/patch
PK_PAGING_PAGEWALK_MODIFY_BSBIT       := BiscuitOS-PAGING-PageWalk-Modify-$(PK_PAGING_PAGEWALK_MODIFY_VERSION).bsbit
PK_PAGING_PAGEWALK_MODIFY_OUT         :=
PK_PAGING_PAGEWALK_MODIFY_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_PAGING_PAGEWALK_MODIFY_TAR         := $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_TAR)
PK_PAGING_PAGEWALK_MODIFY_PACKAGE     :=
PK_PAGING_PAGEWALK_MODIFY_CONFIG      := $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PAGEWALK_MODIFY_DIR)/build

PK_PAGING_PAGEWALK_MODIFY_sh = $(CONFIG_SHELL) $(PK_PAGING_PAGEWALK_MODIFY_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PAGEWALK_MODIFY_NAME)X \
                 $(PK_PAGING_PAGEWALK_MODIFY_VERSION)X \
                 $(PK_PAGING_PAGEWALK_MODIFY_SITE)X \
                 $(PK_PAGING_PAGEWALK_MODIFY_GITHUB)X \
                 $(PK_PAGING_PAGEWALK_MODIFY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PAGEWALK_MODIFY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PAGEWALK_MODIFY_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_CONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_ASFLAGS)X \
                 $(PK_PAGING_PAGEWALK_MODIFY_BSBIT)X \
                 $(PK_PAGING_PAGEWALK_MODIFY_BUILDARCH)X \
		 $(PK_PAGING_PAGEWALK_MODIFY_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PAGEWALK_MODIFY_CSRCFILE)

$(PK_PAGING_PAGEWALK_MODIFY_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PAGEWALK_MODIFY_sh,$(srctree))
