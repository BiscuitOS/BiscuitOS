PK_PAGING_TOOLS_KPROBE_NAME        := BiscuitOS-PAGING-TOOLS-KPROBE
PK_PAGING_TOOLS_KPROBE_VERSION     := $(CONFIG_PK_PAGING_TOOLS_KPROBE_VERSION)
PK_PAGING_TOOLS_KPROBE_SITE        := $(CONFIG_PK_PAGING_TOOLS_KPROBE_URL)
PK_PAGING_TOOLS_KPROBE_GITHUB      := $(CONFIG_PK_PAGING_TOOLS_KPROBE_GITHUB)
PK_PAGING_TOOLS_KPROBE_SUBNAME     :=
PK_PAGING_TOOLS_KPROBE_BUILDARCH   := $(CONFIG_PK_PAGING_TOOLS_KPROBE_BUILDARCH)
PK_PAGING_TOOLS_KPROBE_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-TOOLS-KPROBE
PK_PAGING_TOOLS_KPROBE_PATCH       := $(PK_PAGING_TOOLS_KPROBE_DIR)/patch
PK_PAGING_TOOLS_KPROBE_BSBIT       := BiscuitOS-PAGING-TOOLS-KPROBE-$(PK_PAGING_TOOLS_KPROBE_VERSION).bsbit
PK_PAGING_TOOLS_KPROBE_OUT         :=
PK_PAGING_TOOLS_KPROBE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PAGING_TOOLS_KPROBE_TAR         := $(CONFIG_PK_PAGING_TOOLS_KPROBE_TAR)
PK_PAGING_TOOLS_KPROBE_PACKAGE     :=
PK_PAGING_TOOLS_KPROBE_CONFIG      := $(CONFIG_PK_PAGING_TOOLS_KPROBE_CONFIG)
SUB_TARGET                  += $(PK_PAGING_TOOLS_KPROBE_DIR)/build

PK_PAGING_TOOLS_KPROBE_sh = $(CONFIG_SHELL) $(PK_PAGING_TOOLS_KPROBE_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_TOOLS_KPROBE_NAME)X \
                 $(PK_PAGING_TOOLS_KPROBE_VERSION)X \
                 $(PK_PAGING_TOOLS_KPROBE_SITE)X \
                 $(PK_PAGING_TOOLS_KPROBE_GITHUB)X \
                 $(PK_PAGING_TOOLS_KPROBE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_TOOLS_KPROBE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_TOOLS_KPROBE_SUBNAME)X \
                 $(CONFIG_PK_PAGING_TOOLS_KPROBE_CONFIG)X \
                 $(CONFIG_PK_PAGING_TOOLS_KPROBE_LIBPATH)X \
                 $(CONFIG_PK_PAGING_TOOLS_KPROBE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_TOOLS_KPROBE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_TOOLS_KPROBE_CFLAGS)X \
                 $(CONFIG_PK_PAGING_TOOLS_KPROBE_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_TOOLS_KPROBE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_TOOLS_KPROBE_ASFLAGS)X \
                 $(PK_PAGING_TOOLS_KPROBE_BSBIT)X \
                 $(PK_PAGING_TOOLS_KPROBE_BUILDARCH)X \
		 $(PK_PAGING_TOOLS_KPROBE_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_TOOLS_KPROBE_SRCFILE)X \
                 $(CONFIG_PK_PAGING_TOOLS_KPROBE_CSRCFILE)

$(PK_PAGING_TOOLS_KPROBE_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_TOOLS_KPROBE_sh,$(srctree))
