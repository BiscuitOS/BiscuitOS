PK_PAGING_PF_FILE_SQUASHFS_NAME        := BiscuitOS-PAGING-PF-FILE-SQUASHFS
PK_PAGING_PF_FILE_SQUASHFS_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_VERSION)
PK_PAGING_PF_FILE_SQUASHFS_SITE        := $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_URL)
PK_PAGING_PF_FILE_SQUASHFS_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_GITHUB)
PK_PAGING_PF_FILE_SQUASHFS_SUBNAME     :=
PK_PAGING_PF_FILE_SQUASHFS_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_BUILDARCH)
PK_PAGING_PF_FILE_SQUASHFS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-SQUASHFS
PK_PAGING_PF_FILE_SQUASHFS_PATCH       := $(PK_PAGING_PF_FILE_SQUASHFS_DIR)/patch
PK_PAGING_PF_FILE_SQUASHFS_BSBIT       := BiscuitOS-PAGING-PF-FILE-SQUASHFS-$(PK_PAGING_PF_FILE_SQUASHFS_VERSION).bsbit
PK_PAGING_PF_FILE_SQUASHFS_OUT         :=
PK_PAGING_PF_FILE_SQUASHFS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_SQUASHFS_TAR         := $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_TAR)
PK_PAGING_PF_FILE_SQUASHFS_PACKAGE     :=
PK_PAGING_PF_FILE_SQUASHFS_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_SQUASHFS_DIR)/build

PK_PAGING_PF_FILE_SQUASHFS_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_SQUASHFS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_SQUASHFS_NAME)X \
                 $(PK_PAGING_PF_FILE_SQUASHFS_VERSION)X \
                 $(PK_PAGING_PF_FILE_SQUASHFS_SITE)X \
                 $(PK_PAGING_PF_FILE_SQUASHFS_GITHUB)X \
                 $(PK_PAGING_PF_FILE_SQUASHFS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_SQUASHFS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_SQUASHFS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_SQUASHFS_BSBIT)X \
                 $(PK_PAGING_PF_FILE_SQUASHFS_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_SQUASHFS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_SQUASHFS_CSRCFILE)

$(PK_PAGING_PF_FILE_SQUASHFS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_SQUASHFS_sh,$(srctree))
