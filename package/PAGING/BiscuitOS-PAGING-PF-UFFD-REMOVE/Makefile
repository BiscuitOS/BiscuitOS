PK_PAGING_PF_UFFD_REMOVE_NAME        := BiscuitOS-PAGING-PF-UFFD-REMOVE
PK_PAGING_PF_UFFD_REMOVE_VERSION     := $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_VERSION)
PK_PAGING_PF_UFFD_REMOVE_SITE        := $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_URL)
PK_PAGING_PF_UFFD_REMOVE_GITHUB      := $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_GITHUB)
PK_PAGING_PF_UFFD_REMOVE_SUBNAME     :=
PK_PAGING_PF_UFFD_REMOVE_BUILDARCH   := $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_BUILDARCH)
PK_PAGING_PF_UFFD_REMOVE_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-UFFD-REMOVE
PK_PAGING_PF_UFFD_REMOVE_PATCH       := $(PK_PAGING_PF_UFFD_REMOVE_DIR)/patch
PK_PAGING_PF_UFFD_REMOVE_BSBIT       := BiscuitOS-PAGING-PF-UFFD-REMOVE-$(PK_PAGING_PF_UFFD_REMOVE_VERSION).bsbit
PK_PAGING_PF_UFFD_REMOVE_OUT         :=
PK_PAGING_PF_UFFD_REMOVE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_UFFD_REMOVE_TAR         := $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_TAR)
PK_PAGING_PF_UFFD_REMOVE_PACKAGE     :=
PK_PAGING_PF_UFFD_REMOVE_CONFIG      := $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_UFFD_REMOVE_DIR)/build

PK_PAGING_PF_UFFD_REMOVE_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_UFFD_REMOVE_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_UFFD_REMOVE_NAME)X \
                 $(PK_PAGING_PF_UFFD_REMOVE_VERSION)X \
                 $(PK_PAGING_PF_UFFD_REMOVE_SITE)X \
                 $(PK_PAGING_PF_UFFD_REMOVE_GITHUB)X \
                 $(PK_PAGING_PF_UFFD_REMOVE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_UFFD_REMOVE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_UFFD_REMOVE_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_ASFLAGS)X \
                 $(PK_PAGING_PF_UFFD_REMOVE_BSBIT)X \
                 $(PK_PAGING_PF_UFFD_REMOVE_BUILDARCH)X \
		 $(PK_PAGING_PF_UFFD_REMOVE_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_UFFD_REMOVE_CSRCFILE)

$(PK_PAGING_PF_UFFD_REMOVE_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_UFFD_REMOVE_sh,$(srctree))
