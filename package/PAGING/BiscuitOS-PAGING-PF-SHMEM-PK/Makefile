PK_PAGING_PF_SHMEM_PK_NAME        := BiscuitOS-PAGING-PF-SHMEM-PK
PK_PAGING_PF_SHMEM_PK_VERSION     := $(CONFIG_PK_PAGING_PF_SHMEM_PK_VERSION)
PK_PAGING_PF_SHMEM_PK_SITE        := $(CONFIG_PK_PAGING_PF_SHMEM_PK_URL)
PK_PAGING_PF_SHMEM_PK_GITHUB      := $(CONFIG_PK_PAGING_PF_SHMEM_PK_GITHUB)
PK_PAGING_PF_SHMEM_PK_SUBNAME     :=
PK_PAGING_PF_SHMEM_PK_BUILDARCH   := $(CONFIG_PK_PAGING_PF_SHMEM_PK_BUILDARCH)
PK_PAGING_PF_SHMEM_PK_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-SHMEM-PK
PK_PAGING_PF_SHMEM_PK_PATCH       := $(PK_PAGING_PF_SHMEM_PK_DIR)/patch
PK_PAGING_PF_SHMEM_PK_BSBIT       := BiscuitOS-PAGING-PF-SHMEM-PK-$(PK_PAGING_PF_SHMEM_PK_VERSION).bsbit
PK_PAGING_PF_SHMEM_PK_OUT         :=
PK_PAGING_PF_SHMEM_PK_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_SHMEM_PK_TAR         := $(CONFIG_PK_PAGING_PF_SHMEM_PK_TAR)
PK_PAGING_PF_SHMEM_PK_PACKAGE     :=
PK_PAGING_PF_SHMEM_PK_CONFIG      := $(CONFIG_PK_PAGING_PF_SHMEM_PK_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_SHMEM_PK_DIR)/build

PK_PAGING_PF_SHMEM_PK_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_SHMEM_PK_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_SHMEM_PK_NAME)X \
                 $(PK_PAGING_PF_SHMEM_PK_VERSION)X \
                 $(PK_PAGING_PF_SHMEM_PK_SITE)X \
                 $(PK_PAGING_PF_SHMEM_PK_GITHUB)X \
                 $(PK_PAGING_PF_SHMEM_PK_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_SHMEM_PK_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_SHMEM_PK_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_PK_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_PK_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_PK_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_PK_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_PK_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_PK_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_PK_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_PK_ASFLAGS)X \
                 $(PK_PAGING_PF_SHMEM_PK_BSBIT)X \
                 $(PK_PAGING_PF_SHMEM_PK_BUILDARCH)X \
		 $(PK_PAGING_PF_SHMEM_PK_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_PK_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_SHMEM_PK_CSRCFILE)

$(PK_PAGING_PF_SHMEM_PK_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_SHMEM_PK_sh,$(srctree))
