PK_PAGING_PT_CPR_NONAV_NAME        := BiscuitOS-PAGING-PT-CPR-NONAV
PK_PAGING_PT_CPR_NONAV_VERSION     := $(CONFIG_PK_PAGING_PT_CPR_NONAV_VERSION)
PK_PAGING_PT_CPR_NONAV_SITE        := $(CONFIG_PK_PAGING_PT_CPR_NONAV_URL)
PK_PAGING_PT_CPR_NONAV_GITHUB      := $(CONFIG_PK_PAGING_PT_CPR_NONAV_GITHUB)
PK_PAGING_PT_CPR_NONAV_SUBNAME     :=
PK_PAGING_PT_CPR_NONAV_BUILDARCH   := $(CONFIG_PK_PAGING_PT_CPR_NONAV_BUILDARCH)
PK_PAGING_PT_CPR_NONAV_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PT-CPR-NONAV
PK_PAGING_PT_CPR_NONAV_PATCH       := $(PK_PAGING_PT_CPR_NONAV_DIR)/patch
PK_PAGING_PT_CPR_NONAV_BSBIT       := BiscuitOS-PAGING-PT-CPR-NONAV-$(PK_PAGING_PT_CPR_NONAV_VERSION).bsbit
PK_PAGING_PT_CPR_NONAV_OUT         :=
PK_PAGING_PT_CPR_NONAV_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PT_CPR_NONAV_TAR         := $(CONFIG_PK_PAGING_PT_CPR_NONAV_TAR)
PK_PAGING_PT_CPR_NONAV_PACKAGE     :=
PK_PAGING_PT_CPR_NONAV_CONFIG      := $(CONFIG_PK_PAGING_PT_CPR_NONAV_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PT_CPR_NONAV_DIR)/build

PK_PAGING_PT_CPR_NONAV_sh = $(CONFIG_SHELL) $(PK_PAGING_PT_CPR_NONAV_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PT_CPR_NONAV_NAME)X \
                 $(PK_PAGING_PT_CPR_NONAV_VERSION)X \
                 $(PK_PAGING_PT_CPR_NONAV_SITE)X \
                 $(PK_PAGING_PT_CPR_NONAV_GITHUB)X \
                 $(PK_PAGING_PT_CPR_NONAV_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PT_CPR_NONAV_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PT_CPR_NONAV_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PT_CPR_NONAV_CONFIG)X \
                 $(CONFIG_PK_PAGING_PT_CPR_NONAV_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PT_CPR_NONAV_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_NONAV_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PT_CPR_NONAV_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_NONAV_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_NONAV_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PT_CPR_NONAV_ASFLAGS)X \
                 $(PK_PAGING_PT_CPR_NONAV_BSBIT)X \
                 $(PK_PAGING_PT_CPR_NONAV_BUILDARCH)X \
		 $(PK_PAGING_PT_CPR_NONAV_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_CPR_NONAV_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PT_CPR_NONAV_CSRCFILE)

$(PK_PAGING_PT_CPR_NONAV_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PT_CPR_NONAV_sh,$(srctree))
