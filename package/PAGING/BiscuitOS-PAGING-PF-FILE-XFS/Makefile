PK_PAGING_PF_FILE_XFS_NAME        := BiscuitOS-PAGING-PF-FILE-XFS
PK_PAGING_PF_FILE_XFS_VERSION     := $(CONFIG_PK_PAGING_PF_FILE_XFS_VERSION)
PK_PAGING_PF_FILE_XFS_SITE        := $(CONFIG_PK_PAGING_PF_FILE_XFS_URL)
PK_PAGING_PF_FILE_XFS_GITHUB      := $(CONFIG_PK_PAGING_PF_FILE_XFS_GITHUB)
PK_PAGING_PF_FILE_XFS_SUBNAME     :=
PK_PAGING_PF_FILE_XFS_BUILDARCH   := $(CONFIG_PK_PAGING_PF_FILE_XFS_BUILDARCH)
PK_PAGING_PF_FILE_XFS_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-FILE-XFS
PK_PAGING_PF_FILE_XFS_PATCH       := $(PK_PAGING_PF_FILE_XFS_DIR)/patch
PK_PAGING_PF_FILE_XFS_BSBIT       := BiscuitOS-PAGING-PF-FILE-XFS-$(PK_PAGING_PF_FILE_XFS_VERSION).bsbit
PK_PAGING_PF_FILE_XFS_OUT         :=
PK_PAGING_PF_FILE_XFS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_FILE_XFS_TAR         := $(CONFIG_PK_PAGING_PF_FILE_XFS_TAR)
PK_PAGING_PF_FILE_XFS_PACKAGE     :=
PK_PAGING_PF_FILE_XFS_CONFIG      := $(CONFIG_PK_PAGING_PF_FILE_XFS_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_FILE_XFS_DIR)/build

PK_PAGING_PF_FILE_XFS_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_FILE_XFS_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_FILE_XFS_NAME)X \
                 $(PK_PAGING_PF_FILE_XFS_VERSION)X \
                 $(PK_PAGING_PF_FILE_XFS_SITE)X \
                 $(PK_PAGING_PF_FILE_XFS_GITHUB)X \
                 $(PK_PAGING_PF_FILE_XFS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_FILE_XFS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_FILE_XFS_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_ASFLAGS)X \
                 $(PK_PAGING_PF_FILE_XFS_BSBIT)X \
                 $(PK_PAGING_PF_FILE_XFS_BUILDARCH)X \
		 $(PK_PAGING_PF_FILE_XFS_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_FILE_XFS_CSRCFILE)

$(PK_PAGING_PF_FILE_XFS_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_FILE_XFS_sh,$(srctree))
