PK_PAGING_PF_ANON_WP_REUSE_NAME        := BiscuitOS-PAGING-PF-ANON-WP-REUSE
PK_PAGING_PF_ANON_WP_REUSE_VERSION     := $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_VERSION)
PK_PAGING_PF_ANON_WP_REUSE_SITE        := $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_URL)
PK_PAGING_PF_ANON_WP_REUSE_GITHUB      := $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_GITHUB)
PK_PAGING_PF_ANON_WP_REUSE_SUBNAME     :=
PK_PAGING_PF_ANON_WP_REUSE_BUILDARCH   := $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_BUILDARCH)
PK_PAGING_PF_ANON_WP_REUSE_DIR         := $(srctree)/package/PAGING/BiscuitOS-PAGING-PF-ANON-WP-REUSE
PK_PAGING_PF_ANON_WP_REUSE_PATCH       := $(PK_PAGING_PF_ANON_WP_REUSE_DIR)/patch
PK_PAGING_PF_ANON_WP_REUSE_BSBIT       := BiscuitOS-PAGING-PF-ANON-WP-REUSE-$(PK_PAGING_PF_ANON_WP_REUSE_VERSION).bsbit
PK_PAGING_PF_ANON_WP_REUSE_OUT         :=
PK_PAGING_PF_ANON_WP_REUSE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_PF_ANON_WP_REUSE_TAR         := $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_TAR)
PK_PAGING_PF_ANON_WP_REUSE_PACKAGE     :=
PK_PAGING_PF_ANON_WP_REUSE_CONFIG      := $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_CONFIG)
SUB_TARGET                  += $(PK_PAGING_PF_ANON_WP_REUSE_DIR)/build

PK_PAGING_PF_ANON_WP_REUSE_sh = $(CONFIG_SHELL) $(PK_PAGING_PF_ANON_WP_REUSE_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_PF_ANON_WP_REUSE_NAME)X \
                 $(PK_PAGING_PF_ANON_WP_REUSE_VERSION)X \
                 $(PK_PAGING_PF_ANON_WP_REUSE_SITE)X \
                 $(PK_PAGING_PF_ANON_WP_REUSE_GITHUB)X \
                 $(PK_PAGING_PF_ANON_WP_REUSE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_PF_ANON_WP_REUSE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_PF_ANON_WP_REUSE_SUBNAME)X \
                 $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_CONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_LIBPATH)X \
                 $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_CFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_ASFLAGS)X \
                 $(PK_PAGING_PF_ANON_WP_REUSE_BSBIT)X \
                 $(PK_PAGING_PF_ANON_WP_REUSE_BUILDARCH)X \
		 $(PK_PAGING_PF_ANON_WP_REUSE_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_SRCFILE)X \
                 $(CONFIG_PK_PAGING_PF_ANON_WP_REUSE_CSRCFILE)

$(PK_PAGING_PF_ANON_WP_REUSE_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_PF_ANON_WP_REUSE_sh,$(srctree))
