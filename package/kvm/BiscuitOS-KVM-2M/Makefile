PK_BISCUITOS_KVM_2M_NAME        := BiscuitOS-KVM-2M
PK_BISCUITOS_KVM_2M_VERSION     := $(CONFIG_PK_BISCUITOS_KVM_2M_VERSION)
PK_BISCUITOS_KVM_2M_SITE        := $(CONFIG_PK_BISCUITOS_KVM_2M_URL)
PK_BISCUITOS_KVM_2M_GITHUB      := $(CONFIG_PK_BISCUITOS_KVM_2M_GITHUB)
PK_BISCUITOS_KVM_2M_SUBNAME     :=
PK_BISCUITOS_KVM_2M_BUILDARCH   := $(CONFIG_PK_BISCUITOS_KVM_2M_BUILDARCH)
PK_BISCUITOS_KVM_2M_DIR         := $(srctree)/package/kvm/BiscuitOS-KVM-2M
PK_BISCUITOS_KVM_2M_PATCH       := $(PK_BISCUITOS_KVM_2M_DIR)/patch
PK_BISCUITOS_KVM_2M_BSBIT       := BiscuitOS-KVM-2M-$(PK_BISCUITOS_KVM_2M_VERSION).bsbit
PK_BISCUITOS_KVM_2M_OUT         :=
PK_BISCUITOS_KVM_2M_SCRIPTS     := $(srctree)/scripts/package/package-BiscuitOS-kvm.sh
PK_BISCUITOS_KVM_2M_TAR         := $(CONFIG_PK_BISCUITOS_KVM_2M_TAR)
PK_BISCUITOS_KVM_2M_PACKAGE     :=
PK_BISCUITOS_KVM_2M_CONFIG      := $(CONFIG_PK_BISCUITOS_KVM_2M_CONFIG)
SUB_TARGET                  += $(PK_BISCUITOS_KVM_2M_DIR)/build

PK_BISCUITOS_KVM_2M_sh = $(CONFIG_SHELL) $(PK_BISCUITOS_KVM_2M_SCRIPTS) \
                 $1X \
                 $(PK_BISCUITOS_KVM_2M_NAME)X \
                 $(PK_BISCUITOS_KVM_2M_VERSION)X \
                 $(PK_BISCUITOS_KVM_2M_SITE)X \
                 $(PK_BISCUITOS_KVM_2M_GITHUB)X \
                 $(PK_BISCUITOS_KVM_2M_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_BISCUITOS_KVM_2M_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_BISCUITOS_KVM_2M_SUBNAME)X \
                 $(CONFIG_PK_BISCUITOS_KVM_2M_CONFIG)X \
                 $(CONFIG_PK_BISCUITOS_KVM_2M_LIBPATH)X \
                 $(CONFIG_PK_BISCUITOS_KVM_2M_CPPFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_KVM_2M_DPKCONFIG)X \
                 $(CONFIG_PK_BISCUITOS_KVM_2M_CFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_KVM_2M_LDFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_KVM_2M_CXXFLAGS)X \
                 $(CONFIG_PK_BISCUITOS_KVM_2M_ASFLAGS)X \
                 $(PK_BISCUITOS_KVM_2M_BSBIT)X \
                 $(PK_BISCUITOS_KVM_2M_BUILDARCH)X \
		 $(PK_BISCUITOS_KVM_2M_CSRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_KVM_2M_SRCFILE)X \
                 $(CONFIG_PK_BISCUITOS_KVM_2M_CSRCFILE)

$(PK_BISCUITOS_KVM_2M_DIR)/build: FORCE
	$(Q)$(call PK_BISCUITOS_KVM_2M_sh,$(srctree))
