PK_MEMBARRIER_NOLOCK_SYNC_CNT_NAME        := BiscuitOS-MEMORY-BARRIER-NOLOCKING-SYNC-COUNTER
PK_MEMBARRIER_NOLOCK_SYNC_CNT_VERSION     := $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_VERSION)
PK_MEMBARRIER_NOLOCK_SYNC_CNT_SITE        := $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_URL)
PK_MEMBARRIER_NOLOCK_SYNC_CNT_GITHUB      := $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_GITHUB)
PK_MEMBARRIER_NOLOCK_SYNC_CNT_SUBNAME     :=
PK_MEMBARRIER_NOLOCK_SYNC_CNT_BUILDARCH   := $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_BUILDARCH)
PK_MEMBARRIER_NOLOCK_SYNC_CNT_DIR         := $(srctree)/package/MEMORY-BARRIER/BiscuitOS-MEMORY-BARRIER-NOLOCKING-SYNC-COUNTER
PK_MEMBARRIER_NOLOCK_SYNC_CNT_PATCH       := $(PK_MEMBARRIER_NOLOCK_SYNC_CNT_DIR)/patch
PK_MEMBARRIER_NOLOCK_SYNC_CNT_BSBIT       := BiscuitOS-MEMORY-BARRIER-NOLOCKING-SYNC-COUNTERT-$(PK_MEMBARRIER_NOLOCK_SYNC_CNT_VERSION).bsbit
PK_MEMBARRIER_NOLOCK_SYNC_CNT_OUT         :=
PK_MEMBARRIER_NOLOCK_SYNC_CNT_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMBARRIER_NOLOCK_SYNC_CNT_TAR         := $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_TAR)
PK_MEMBARRIER_NOLOCK_SYNC_CNT_PACKAGE     :=
PK_MEMBARRIER_NOLOCK_SYNC_CNT_CONFIG      := $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_CONFIG)
SUB_TARGET                  += $(PK_MEMBARRIER_NOLOCK_SYNC_CNT_DIR)/build

PK_MEMBARRIER_NOLOCK_SYNC_CNT_sh = $(CONFIG_SHELL) $(PK_MEMBARRIER_NOLOCK_SYNC_CNT_SCRIPTS) \
                 $1X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_CNT_NAME)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_CNT_VERSION)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_CNT_SITE)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_CNT_GITHUB)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_CNT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_CNT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_CNT_SUBNAME)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_CONFIG)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_LIBPATH)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_CFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_LDFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_ASFLAGS)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_CNT_BSBIT)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_CNT_BUILDARCH)X \
		 $(PK_MEMBARRIER_NOLOCK_SYNC_CNT_CSRCFILE)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_SRCFILE)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT_CSRCFILE)

$(PK_MEMBARRIER_NOLOCK_SYNC_CNT_DIR)/build: FORCE
	$(Q)$(call PK_MEMBARRIER_NOLOCK_SYNC_CNT_sh,$(srctree))
