PK_MEMBARRIER_NOLOCK_SYNC_NAME        := BiscuitOS-MEMORY-BARRIER-NOLOCKING-SYNC
PK_MEMBARRIER_NOLOCK_SYNC_VERSION     := $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_VERSION)
PK_MEMBARRIER_NOLOCK_SYNC_SITE        := $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_URL)
PK_MEMBARRIER_NOLOCK_SYNC_GITHUB      := $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_GITHUB)
PK_MEMBARRIER_NOLOCK_SYNC_SUBNAME     :=
PK_MEMBARRIER_NOLOCK_SYNC_BUILDARCH   := $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_BUILDARCH)
PK_MEMBARRIER_NOLOCK_SYNC_DIR         := $(srctree)/package/MEMORY-BARRIER/BiscuitOS-MEMORY-BARRIER-NOLOCKING-SYNC
PK_MEMBARRIER_NOLOCK_SYNC_PATCH       := $(PK_MEMBARRIER_NOLOCK_SYNC_DIR)/patch
PK_MEMBARRIER_NOLOCK_SYNC_BSBIT       := BiscuitOS-MEMORY-BARRIER-NOLOCKING-SYNC-$(PK_MEMBARRIER_NOLOCK_SYNC_VERSION).bsbit
PK_MEMBARRIER_NOLOCK_SYNC_OUT         :=
PK_MEMBARRIER_NOLOCK_SYNC_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMBARRIER_NOLOCK_SYNC_TAR         := $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_TAR)
PK_MEMBARRIER_NOLOCK_SYNC_PACKAGE     :=
PK_MEMBARRIER_NOLOCK_SYNC_CONFIG      := $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CONFIG)
SUB_TARGET                  += $(PK_MEMBARRIER_NOLOCK_SYNC_DIR)/build

PK_MEMBARRIER_NOLOCK_SYNC_sh = $(CONFIG_SHELL) $(PK_MEMBARRIER_NOLOCK_SYNC_SCRIPTS) \
                 $1X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_NAME)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_VERSION)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_SITE)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_GITHUB)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_SUBNAME)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CONFIG)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_LIBPATH)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_LDFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_ASFLAGS)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_BSBIT)X \
                 $(PK_MEMBARRIER_NOLOCK_SYNC_BUILDARCH)X \
		 $(PK_MEMBARRIER_NOLOCK_SYNC_CSRCFILE)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_SRCFILE)X \
                 $(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CSRCFILE)

$(PK_MEMBARRIER_NOLOCK_SYNC_DIR)/build: FORCE
	$(Q)$(call PK_MEMBARRIER_NOLOCK_SYNC_sh,$(srctree))
