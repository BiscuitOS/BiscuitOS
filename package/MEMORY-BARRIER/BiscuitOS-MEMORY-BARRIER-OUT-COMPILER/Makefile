# BiscuitOS-MEMORY-BARRIER-OUT-COMPILER
#
################################################
PK_MEMBARRIER_OUT_COMPILER_NAME        := BiscuitOS-MEMORY-BARRIER-OUT-COMPILER
PK_MEMBARRIER_OUT_COMPILER_VERSION     := $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_VERSION)
PK_MEMBARRIER_OUT_COMPILER_SITE        := $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_URL)
PK_MEMBARRIER_OUT_COMPILER_GITHUB      := $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_GITHUB)
PK_MEMBARRIER_OUT_COMPILER_SUBNAME     :=
PK_MEMBARRIER_OUT_COMPILER_BUILDARCH   := $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_BUILDARCH)
PK_MEMBARRIER_OUT_COMPILER_DIR         := $(srctree)/package/MEMORY-BARRIER/BiscuitOS-MEMORY-BARRIER-OUT-COMPILER
PK_MEMBARRIER_OUT_COMPILER_PATCH       := $(PK_MEMBARRIER_OUT_COMPILER_DIR)/patch
PK_MEMBARRIER_OUT_COMPILER_BSBIT       := BiscuitOS-MEMORY-BARRIER-OUT-COMPILER-$(PK_MEMBARRIER_OUT_COMPILER_VERSION).bsbit
PK_MEMBARRIER_OUT_COMPILER_OUT         :=
PK_MEMBARRIER_OUT_COMPILER_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_MEMBARRIER_OUT_COMPILER_TAR         := $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_TAR)
PK_MEMBARRIER_OUT_COMPILER_PACKAGE     :=
PK_MEMBARRIER_OUT_COMPILER_CONFIG      := $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_CONFIG)
SUB_TARGET                  += $(PK_MEMBARRIER_OUT_COMPILER_DIR)/build

PK_MEMBARRIER_OUT_COMPILER_sh = $(CONFIG_SHELL) $(PK_MEMBARRIER_OUT_COMPILER_SCRIPTS) \
                 $1X \
                 $(PK_MEMBARRIER_OUT_COMPILER_NAME)X \
                 $(PK_MEMBARRIER_OUT_COMPILER_VERSION)X \
                 $(PK_MEMBARRIER_OUT_COMPILER_SITE)X \
                 $(PK_MEMBARRIER_OUT_COMPILER_GITHUB)X \
                 $(PK_MEMBARRIER_OUT_COMPILER_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBARRIER_OUT_COMPILER_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBARRIER_OUT_COMPILER_SUBNAME)X \
                 $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_CONFIG)X \
                 $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_LIBPATH)X \
                 $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_CFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_LDFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_ASFLAGS)X \
                 $(PK_MEMBARRIER_OUT_COMPILER_BSBIT)X \
                 $(PK_MEMBARRIER_OUT_COMPILER_BUILDARCH)X \
		 $(PK_MEMBARRIER_OUT_COMPILER_CSRCFILE)X \
                 $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_SRCFILE)X \
                 $(CONFIG_PK_MEMBARRIER_OUT_COMPILER_CSRCFILE)

$(PK_MEMBARRIER_OUT_COMPILER_DIR)/build: FORCE
	$(Q)$(call PK_MEMBARRIER_OUT_COMPILER_sh,$(srctree))
