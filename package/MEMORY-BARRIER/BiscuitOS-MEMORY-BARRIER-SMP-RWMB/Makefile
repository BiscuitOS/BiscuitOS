PK_MEMBARRIER_SMP_RWMB_NAME        := BiscuitOS-MEMORY-BARRIER-SMP-RWMB
PK_MEMBARRIER_SMP_RWMB_VERSION     := $(CONFIG_PK_MEMBARRIER_SMP_RWMB_VERSION)
PK_MEMBARRIER_SMP_RWMB_SITE        := $(CONFIG_PK_MEMBARRIER_SMP_RWMB_URL)
PK_MEMBARRIER_SMP_RWMB_GITHUB      := $(CONFIG_PK_MEMBARRIER_SMP_RWMB_GITHUB)
PK_MEMBARRIER_SMP_RWMB_SUBNAME     :=
PK_MEMBARRIER_SMP_RWMB_BUILDARCH   := $(CONFIG_PK_MEMBARRIER_SMP_RWMB_BUILDARCH)
PK_MEMBARRIER_SMP_RWMB_DIR         := $(srctree)/package/MEMORY-BARRIER/BiscuitOS-MEMORY-BARRIER-SMP-RWMB
PK_MEMBARRIER_SMP_RWMB_PATCH       := $(PK_MEMBARRIER_SMP_RWMB_DIR)/patch
PK_MEMBARRIER_SMP_RWMB_BSBIT       := BiscuitOS-MEMORY-BARRIER-SMP-RWMB-$(PK_MEMBARRIER_SMP_RWMB_VERSION).bsbit
PK_MEMBARRIER_SMP_RWMB_OUT         :=
PK_MEMBARRIER_SMP_RWMB_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_MEMBARRIER_SMP_RWMB_TAR         := $(CONFIG_PK_MEMBARRIER_SMP_RWMB_TAR)
PK_MEMBARRIER_SMP_RWMB_PACKAGE     :=
PK_MEMBARRIER_SMP_RWMB_CONFIG      := $(CONFIG_PK_MEMBARRIER_SMP_RWMB_CONFIG)
SUB_TARGET                  += $(PK_MEMBARRIER_SMP_RWMB_DIR)/build

PK_MEMBARRIER_SMP_RWMB_sh = $(CONFIG_SHELL) $(PK_MEMBARRIER_SMP_RWMB_SCRIPTS) \
                 $1X \
                 $(PK_MEMBARRIER_SMP_RWMB_NAME)X \
                 $(PK_MEMBARRIER_SMP_RWMB_VERSION)X \
                 $(PK_MEMBARRIER_SMP_RWMB_SITE)X \
                 $(PK_MEMBARRIER_SMP_RWMB_GITHUB)X \
                 $(PK_MEMBARRIER_SMP_RWMB_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_MEMBARRIER_SMP_RWMB_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_MEMBARRIER_SMP_RWMB_SUBNAME)X \
                 $(CONFIG_PK_MEMBARRIER_SMP_RWMB_CONFIG)X \
                 $(CONFIG_PK_MEMBARRIER_SMP_RWMB_LIBPATH)X \
                 $(CONFIG_PK_MEMBARRIER_SMP_RWMB_CPPFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_SMP_RWMB_DPKCONFIG)X \
                 $(CONFIG_PK_MEMBARRIER_SMP_RWMB_CFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_SMP_RWMB_LDFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_SMP_RWMB_CXXFLAGS)X \
                 $(CONFIG_PK_MEMBARRIER_SMP_RWMB_ASFLAGS)X \
                 $(PK_MEMBARRIER_SMP_RWMB_BSBIT)X \
                 $(PK_MEMBARRIER_SMP_RWMB_BUILDARCH)X \
		 $(PK_MEMBARRIER_SMP_RWMB_CSRCFILE)X \
                 $(CONFIG_PK_MEMBARRIER_SMP_RWMB_SRCFILE)X \
                 $(CONFIG_PK_MEMBARRIER_SMP_RWMB_CSRCFILE)

$(PK_MEMBARRIER_SMP_RWMB_DIR)/build: FORCE
	$(Q)$(call PK_MEMBARRIER_SMP_RWMB_sh,$(srctree))
