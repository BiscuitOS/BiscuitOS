MEMBARRIER-$(CONFIG_PK_MEMBARRIER_SMP_RWMB)			+= BiscuitOS-MEMORY-BARRIER-SMP-RWMB/
MEMBARRIER-$(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC)			+= BiscuitOS-MEMORY-BARRIER-NOLOCKING-SYNC/
MEMBARRIER-$(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_WMB)		+= BiscuitOS-MEMORY-BARRIER-NOLOCKING-SYNC-WMB/
MEMBARRIER-$(CONFIG_PK_MEMBARRIER_NOLOCK_SYNC_CNT)		+= BiscuitOS-MEMORY-BARRIER-NOLOCKING-SYNC-COUNTER/

PACKAGE_MAKEDIR := $(patsubst %/, $(srctree)/package/MEMORY-BARRIER/%/Makefile, $(MEMBARRIER-y))

include $(PACKAGE_MAKEDIR)
