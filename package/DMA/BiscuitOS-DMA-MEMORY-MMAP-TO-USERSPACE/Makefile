PK_DMA_MEM_MMAP_TO_USERSPACE_NAME        := BiscuitOS-DMA-MEMORY-MMAP-TO-USERSPACE
PK_DMA_MEM_MMAP_TO_USERSPACE_VERSION     := $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_VERSION)
PK_DMA_MEM_MMAP_TO_USERSPACE_SITE        := $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_URL)
PK_DMA_MEM_MMAP_TO_USERSPACE_GITHUB      := $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_GITHUB)
PK_DMA_MEM_MMAP_TO_USERSPACE_SUBNAME     :=
PK_DMA_MEM_MMAP_TO_USERSPACE_BUILDARCH   := $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_BUILDARCH)
PK_DMA_MEM_MMAP_TO_USERSPACE_DIR         := $(srctree)/package/DMA/BiscuitOS-DMA-MEMORY-MMAP-TO-USERSPACE
PK_DMA_MEM_MMAP_TO_USERSPACE_PATCH       := $(PK_DMA_MEM_MMAP_TO_USERSPACE_DIR)/patch
PK_DMA_MEM_MMAP_TO_USERSPACE_BSBIT       := BiscuitOS-DMA-MEMORY-MMAP-TO-USERSPACE-$(PK_DMA_MEM_MMAP_TO_USERSPACE_VERSION).bsbit
PK_DMA_MEM_MMAP_TO_USERSPACE_OUT         :=
PK_DMA_MEM_MMAP_TO_USERSPACE_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_DMA_MEM_MMAP_TO_USERSPACE_TAR         := $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_TAR)
PK_DMA_MEM_MMAP_TO_USERSPACE_PACKAGE     :=
PK_DMA_MEM_MMAP_TO_USERSPACE_CONFIG      := $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_CONFIG)
SUB_TARGET                  += $(PK_DMA_MEM_MMAP_TO_USERSPACE_DIR)/build

PK_DMA_MEM_MMAP_TO_USERSPACE_sh = $(CONFIG_SHELL) $(PK_DMA_MEM_MMAP_TO_USERSPACE_SCRIPTS) \
                 $1X \
                 $(PK_DMA_MEM_MMAP_TO_USERSPACE_NAME)X \
                 $(PK_DMA_MEM_MMAP_TO_USERSPACE_VERSION)X \
                 $(PK_DMA_MEM_MMAP_TO_USERSPACE_SITE)X \
                 $(PK_DMA_MEM_MMAP_TO_USERSPACE_GITHUB)X \
                 $(PK_DMA_MEM_MMAP_TO_USERSPACE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_DMA_MEM_MMAP_TO_USERSPACE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_DMA_MEM_MMAP_TO_USERSPACE_SUBNAME)X \
                 $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_CONFIG)X \
                 $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_LIBPATH)X \
                 $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_CPPFLAGS)X \
                 $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_DPKCONFIG)X \
                 $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_CFLAGS)X \
                 $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_LDFLAGS)X \
                 $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_CXXFLAGS)X \
                 $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_ASFLAGS)X \
                 $(PK_DMA_MEM_MMAP_TO_USERSPACE_BSBIT)X \
                 $(PK_DMA_MEM_MMAP_TO_USERSPACE_BUILDARCH)X \
		 $(PK_DMA_MEM_MMAP_TO_USERSPACE_CSRCFILE)X \
                 $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_SRCFILE)X \
                 $(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE_CSRCFILE)

$(PK_DMA_MEM_MMAP_TO_USERSPACE_DIR)/build: FORCE
	$(Q)$(call PK_DMA_MEM_MMAP_TO_USERSPACE_sh,$(srctree))
