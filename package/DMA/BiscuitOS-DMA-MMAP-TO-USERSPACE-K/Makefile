PK_DMA_MMAP_TO_USERSPACE_K_NAME        := BiscuitOS-DMA-MMAP-TO-USERSPACE-K
PK_DMA_MMAP_TO_USERSPACE_K_VERSION     := $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_VERSION)
PK_DMA_MMAP_TO_USERSPACE_K_SITE        := $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_URL)
PK_DMA_MMAP_TO_USERSPACE_K_GITHUB      := $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_GITHUB)
PK_DMA_MMAP_TO_USERSPACE_K_SUBNAME     :=
PK_DMA_MMAP_TO_USERSPACE_K_BUILDARCH   := $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_BUILDARCH)
PK_DMA_MMAP_TO_USERSPACE_K_DIR         := $(srctree)/package/DMA/BiscuitOS-DMA-MMAP-TO-USERSPACE-K
PK_DMA_MMAP_TO_USERSPACE_K_PATCH       := $(PK_DMA_MMAP_TO_USERSPACE_K_DIR)/patch
PK_DMA_MMAP_TO_USERSPACE_K_BSBIT       := BiscuitOS-DMA-MMAP-TO-USERSPACE-K-$(PK_DMA_MMAP_TO_USERSPACE_K_VERSION).bsbit
PK_DMA_MMAP_TO_USERSPACE_K_OUT         :=
PK_DMA_MMAP_TO_USERSPACE_K_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_DMA_MMAP_TO_USERSPACE_K_TAR         := $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_TAR)
PK_DMA_MMAP_TO_USERSPACE_K_PACKAGE     :=
PK_DMA_MMAP_TO_USERSPACE_K_CONFIG      := $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_CONFIG)
SUB_TARGET                  += $(PK_DMA_MMAP_TO_USERSPACE_K_DIR)/build

PK_DMA_MMAP_TO_USERSPACE_K_sh = $(CONFIG_SHELL) $(PK_DMA_MMAP_TO_USERSPACE_K_SCRIPTS) \
                 $1X \
                 $(PK_DMA_MMAP_TO_USERSPACE_K_NAME)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_K_VERSION)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_K_SITE)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_K_GITHUB)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_K_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_K_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_K_SUBNAME)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_CONFIG)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_LIBPATH)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_CPPFLAGS)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_DPKCONFIG)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_CFLAGS)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_LDFLAGS)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_CXXFLAGS)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_ASFLAGS)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_K_BSBIT)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_K_BUILDARCH)X \
		 $(PK_DMA_MMAP_TO_USERSPACE_K_CSRCFILE)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_SRCFILE)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_K_CSRCFILE)

$(PK_DMA_MMAP_TO_USERSPACE_K_DIR)/build: FORCE
	$(Q)$(call PK_DMA_MMAP_TO_USERSPACE_K_sh,$(srctree))
