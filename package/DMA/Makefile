DMA-$(CONFIG_PK_DMA_DEV_MMAP_TO_USERSPACE)		+= BiscuitOS-DMA-DEVICE-MMAP-TO-USERSPACE/
DMA-$(CONFIG_PK_DMA_DEV_USE_ON_USERSPACE)		+= BiscuitOS-DMA-DEVICE-USE-ON-USERSPACE/
DMA-$(CONFIG_PK_DMA_MEM_MMAP_TO_USERSPACE)		+= BiscuitOS-DMA-MEMORY-MMAP-TO-USERSPACE/
DMA-$(CONFIG_PK_DMA_MEM_USE_ON_USERSPACE)		+= BiscuitOS-DMA-MEMORY-USE-ON-USERSPACE/
DMA-$(CONFIG_PK_DMA_DMABUF)				+= BiscuitOS-DMA-DMABUF/
DMA-$(CONFIG_PK_DMA_DMABUF_QEMU_DEVICE)			+= BiscuitOS-DMA-DMABUF-with-QEMU-Device/

PACKAGE_MAKEDIR := $(patsubst %/, $(srctree)/package/DMA/%/Makefile, $(DMA-y))

include $(PACKAGE_MAKEDIR)
