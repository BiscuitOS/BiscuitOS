PK_DMA_MMAP_TO_USERSPACE_U_NAME        := BiscuitOS-DMA-MMAP-TO-USERSPACE
PK_DMA_MMAP_TO_USERSPACE_U_VERSION     := $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_VERSION)
PK_DMA_MMAP_TO_USERSPACE_U_SITE        := $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_URL)
PK_DMA_MMAP_TO_USERSPACE_U_GITHUB      := $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_GITHUB)
PK_DMA_MMAP_TO_USERSPACE_U_SUBNAME     :=
PK_DMA_MMAP_TO_USERSPACE_U_BUILDARCH   := $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_BUILDARCH)
PK_DMA_MMAP_TO_USERSPACE_U_DIR         := $(srctree)/package/DMA/BiscuitOS-DMA-MMAP-TO-USERSPACE
PK_DMA_MMAP_TO_USERSPACE_U_PATCH       := $(PK_DMA_MMAP_TO_USERSPACE_U_DIR)/patch
PK_DMA_MMAP_TO_USERSPACE_U_BSBIT       := BiscuitOS-DMA-MMAP-TO-USERSPACE-$(PK_DMA_MMAP_TO_USERSPACE_U_VERSION).bsbit
PK_DMA_MMAP_TO_USERSPACE_U_OUT         :=
PK_DMA_MMAP_TO_USERSPACE_U_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_DMA_MMAP_TO_USERSPACE_U_TAR         := $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_TAR)
PK_DMA_MMAP_TO_USERSPACE_U_PACKAGE     :=
PK_DMA_MMAP_TO_USERSPACE_U_CONFIG      := $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_CONFIG)
SUB_TARGET                  += $(PK_DMA_MMAP_TO_USERSPACE_U_DIR)/build

PK_DMA_MMAP_TO_USERSPACE_U_sh = $(CONFIG_SHELL) $(PK_DMA_MMAP_TO_USERSPACE_U_SCRIPTS) \
                 $1X \
                 $(PK_DMA_MMAP_TO_USERSPACE_U_NAME)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_U_VERSION)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_U_SITE)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_U_GITHUB)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_U_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_U_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_U_SUBNAME)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_CONFIG)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_LIBPATH)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_CPPFLAGS)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_DPKCONFIG)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_CFLAGS)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_LDFLAGS)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_CXXFLAGS)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_ASFLAGS)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_U_BSBIT)X \
                 $(PK_DMA_MMAP_TO_USERSPACE_U_BUILDARCH)X \
		 $(PK_DMA_MMAP_TO_USERSPACE_U_CSRCFILE)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_SRCFILE)X \
                 $(CONFIG_PK_DMA_MMAP_TO_USERSPACE_U_CSRCFILE)

$(PK_DMA_MMAP_TO_USERSPACE_U_DIR)/build: FORCE
	$(Q)$(call PK_DMA_MMAP_TO_USERSPACE_U_sh,$(srctree))
