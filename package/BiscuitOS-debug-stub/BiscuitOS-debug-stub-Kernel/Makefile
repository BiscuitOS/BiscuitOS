################################################
#
# BiscuitOS-debug-stub-Kernel
#
################################################
PK_DEBUG_STUB_KERNEL_NAME        := BiscuitOS-debug-stub-Kernel
PK_DEBUG_STUB_KERNEL_VERSION     := $(CONFIG_PK_DEBUG_STUB_KERNEL_VERSION)
PK_DEBUG_STUB_KERNEL_SITE        := $(CONFIG_PK_DEBUG_STUB_KERNEL_URL)
PK_DEBUG_STUB_KERNEL_GITHUB      := $(CONFIG_PK_DEBUG_STUB_KERNEL_GITHUB)
PK_DEBUG_STUB_KERNEL_SUBNAME     :=
PK_DEBUG_STUB_KERNEL_BUILDARCH   := $(CONFIG_PK_DEBUG_STUB_KERNEL_BUILDARCH)
PK_DEBUG_STUB_KERNEL_DIR         := $(srctree)/package/BiscuitOS-debug-stub/BiscuitOS-debug-stub-Kernel
PK_DEBUG_STUB_KERNEL_PATCH       := $(PK_DEBUG_STUB_KERNEL_DIR)/patch
PK_DEBUG_STUB_KERNEL_BSBIT       := BiscuitOS-debug-stub-Kernel-$(PK_DEBUG_STUB_KERNEL_VERSION).bsbit
PK_DEBUG_STUB_KERNEL_OUT         :=
PK_DEBUG_STUB_KERNEL_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_DEBUG_STUB_KERNEL_TAR         := $(CONFIG_PK_DEBUG_STUB_KERNEL_TAR)
PK_DEBUG_STUB_KERNEL_PACKAGE     :=
PK_DEBUG_STUB_KERNEL_CONFIG      := $(CONFIG_PK_DEBUG_STUB_KERNEL_CONFIG)
SUB_TARGET                  += $(PK_DEBUG_STUB_KERNEL_DIR)/build

PK_DEBUG_STUB_KERNEL_sh = $(CONFIG_SHELL) $(PK_DEBUG_STUB_KERNEL_SCRIPTS) \
                 $1X \
                 $(PK_DEBUG_STUB_KERNEL_NAME)X \
                 $(PK_DEBUG_STUB_KERNEL_VERSION)X \
                 $(PK_DEBUG_STUB_KERNEL_SITE)X \
                 $(PK_DEBUG_STUB_KERNEL_GITHUB)X \
                 $(PK_DEBUG_STUB_KERNEL_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_DEBUG_STUB_KERNEL_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_DEBUG_STUB_KERNEL_SUBNAME)X \
                 $(CONFIG_PK_DEBUG_STUB_KERNEL_CONFIG)X \
                 $(CONFIG_PK_DEBUG_STUB_KERNEL_LIBPATH)X \
                 $(CONFIG_PK_DEBUG_STUB_KERNEL_CPPFLAGS)X \
                 $(CONFIG_PK_DEBUG_STUB_KERNEL_DPKCONFIG)X \
                 $(CONFIG_PK_DEBUG_STUB_KERNEL_CFLAGS)X \
                 $(CONFIG_PK_DEBUG_STUB_KERNEL_LDFLAGS)X \
                 $(CONFIG_PK_DEBUG_STUB_KERNEL_CXXFLAGS)X \
                 $(CONFIG_PK_DEBUG_STUB_KERNEL_ASFLAGS)X \
                 $(PK_DEBUG_STUB_KERNEL_BSBIT)X \
                 $(PK_DEBUG_STUB_KERNEL_BUILDARCH)X \
		 $(PK_DEBUG_STUB_KERNEL_CSRCFILE)X \
                 $(CONFIG_PK_DEBUG_STUB_KERNEL_SRCFILE)X \
                 $(CONFIG_PK_DEBUG_STUB_KERNEL_CSRCFILE)

$(PK_DEBUG_STUB_KERNEL_DIR)/build: FORCE
	$(Q)$(call PK_DEBUG_STUB_KERNEL_sh,$(srctree))
