################################################
#
# BiscuitOS-MEMORY-FLUID
#
################################################
PK_DEBUG_MEMORY_FLUID_NAME        := BiscuitOS-MEMORY-FLUID
PK_DEBUG_MEMORY_FLUID_VERSION     := $(CONFIG_PK_DEBUG_MEMORY_FLUID_VERSION)
PK_DEBUG_MEMORY_FLUID_SITE        := $(CONFIG_PK_DEBUG_MEMORY_FLUID_URL)
PK_DEBUG_MEMORY_FLUID_GITHUB      := $(CONFIG_PK_DEBUG_MEMORY_FLUID_GITHUB)
PK_DEBUG_MEMORY_FLUID_SUBNAME     :=
PK_DEBUG_MEMORY_FLUID_BUILDARCH   := $(CONFIG_PK_DEBUG_MEMORY_FLUID_BUILDARCH)
PK_DEBUG_MEMORY_FLUID_DIR         := $(srctree)/package/BiscuitOS-debug-stub/BiscuitOS-MEMORY-FLUID
PK_DEBUG_MEMORY_FLUID_PATCH       := $(PK_DEBUG_MEMORY_FLUID_DIR)/patch
PK_DEBUG_MEMORY_FLUID_BSBIT       := BiscuitOS-MEMORY-FLUID-$(PK_DEBUG_MEMORY_FLUID_VERSION).bsbit
PK_DEBUG_MEMORY_FLUID_OUT         :=
PK_DEBUG_MEMORY_FLUID_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_DEBUG_MEMORY_FLUID_TAR         := $(CONFIG_PK_DEBUG_MEMORY_FLUID_TAR)
PK_DEBUG_MEMORY_FLUID_PACKAGE     :=
PK_DEBUG_MEMORY_FLUID_CONFIG      := $(CONFIG_PK_DEBUG_MEMORY_FLUID_CONFIG)
SUB_TARGET                  += $(PK_DEBUG_MEMORY_FLUID_DIR)/build

PK_DEBUG_MEMORY_FLUID_sh = $(CONFIG_SHELL) $(PK_DEBUG_MEMORY_FLUID_SCRIPTS) \
                 $1X \
                 $(PK_DEBUG_MEMORY_FLUID_NAME)X \
                 $(PK_DEBUG_MEMORY_FLUID_VERSION)X \
                 $(PK_DEBUG_MEMORY_FLUID_SITE)X \
                 $(PK_DEBUG_MEMORY_FLUID_GITHUB)X \
                 $(PK_DEBUG_MEMORY_FLUID_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_DEBUG_MEMORY_FLUID_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_DEBUG_MEMORY_FLUID_SUBNAME)X \
                 $(CONFIG_PK_DEBUG_MEMORY_FLUID_CONFIG)X \
                 $(CONFIG_PK_DEBUG_MEMORY_FLUID_LIBPATH)X \
                 $(CONFIG_PK_DEBUG_MEMORY_FLUID_CPPFLAGS)X \
                 $(CONFIG_PK_DEBUG_MEMORY_FLUID_DPKCONFIG)X \
                 $(CONFIG_PK_DEBUG_MEMORY_FLUID_CFLAGS)X \
                 $(CONFIG_PK_DEBUG_MEMORY_FLUID_LDFLAGS)X \
                 $(CONFIG_PK_DEBUG_MEMORY_FLUID_CXXFLAGS)X \
                 $(CONFIG_PK_DEBUG_MEMORY_FLUID_ASFLAGS)X \
                 $(PK_DEBUG_MEMORY_FLUID_BSBIT)X \
                 $(PK_DEBUG_MEMORY_FLUID_BUILDARCH)X \
		 $(PK_DEBUG_MEMORY_FLUID_CSRCFILE)X \
                 $(CONFIG_PK_DEBUG_MEMORY_FLUID_SRCFILE)X \
                 $(CONFIG_PK_DEBUG_MEMORY_FLUID_CSRCFILE)

$(PK_DEBUG_MEMORY_FLUID_DIR)/build: FORCE
	$(Q)$(call PK_DEBUG_MEMORY_FLUID_sh,$(srctree))
