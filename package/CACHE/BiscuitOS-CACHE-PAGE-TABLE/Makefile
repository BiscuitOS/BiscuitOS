################################################
#
# BiscuitOS-CACHE-PAGE-TABLE
#
################################################
PK_CACHE_PAGE_TABLE_NAME        := BiscuitOS-CACHE-PAGE-TABLE
PK_CACHE_PAGE_TABLE_VERSION     := $(CONFIG_PK_CACHE_PAGE_TABLE_VERSION)
PK_CACHE_PAGE_TABLE_SITE        := $(CONFIG_PK_CACHE_PAGE_TABLE_URL)
PK_CACHE_PAGE_TABLE_GITHUB      := $(CONFIG_PK_CACHE_PAGE_TABLE_GITHUB)
PK_CACHE_PAGE_TABLE_SUBNAME     :=
PK_CACHE_PAGE_TABLE_BUILDARCH   := $(CONFIG_PK_CACHE_PAGE_TABLE_BUILDARCH)
PK_CACHE_PAGE_TABLE_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-PAGE-TABLE
PK_CACHE_PAGE_TABLE_PATCH       := $(PK_CACHE_PAGE_TABLE_DIR)/patch
PK_CACHE_PAGE_TABLE_BSBIT       := BiscuitOS-CACHE-PAGE-TABLE-$(PK_CACHE_PAGE_TABLE_VERSION).bsbit
PK_CACHE_PAGE_TABLE_OUT         :=
PK_CACHE_PAGE_TABLE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_PAGE_TABLE_TAR         := $(CONFIG_PK_CACHE_PAGE_TABLE_TAR)
PK_CACHE_PAGE_TABLE_PACKAGE     :=
PK_CACHE_PAGE_TABLE_CONFIG      := $(CONFIG_PK_CACHE_PAGE_TABLE_CONFIG)
SUB_TARGET                  += $(PK_CACHE_PAGE_TABLE_DIR)/build

PK_CACHE_PAGE_TABLE_sh = $(CONFIG_SHELL) $(PK_CACHE_PAGE_TABLE_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_PAGE_TABLE_NAME)X \
                 $(PK_CACHE_PAGE_TABLE_VERSION)X \
                 $(PK_CACHE_PAGE_TABLE_SITE)X \
                 $(PK_CACHE_PAGE_TABLE_GITHUB)X \
                 $(PK_CACHE_PAGE_TABLE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_PAGE_TABLE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_PAGE_TABLE_SUBNAME)X \
                 $(CONFIG_PK_CACHE_PAGE_TABLE_CONFIG)X \
                 $(CONFIG_PK_CACHE_PAGE_TABLE_LIBPATH)X \
                 $(CONFIG_PK_CACHE_PAGE_TABLE_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_PAGE_TABLE_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_PAGE_TABLE_CFLAGS)X \
                 $(CONFIG_PK_CACHE_PAGE_TABLE_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_PAGE_TABLE_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_PAGE_TABLE_ASFLAGS)X \
                 $(PK_CACHE_PAGE_TABLE_BSBIT)X \
                 $(PK_CACHE_PAGE_TABLE_BUILDARCH)X \
		 $(PK_CACHE_PAGE_TABLE_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_PAGE_TABLE_SRCFILE)X \
                 $(CONFIG_PK_CACHE_PAGE_TABLE_CSRCFILE)

$(PK_CACHE_PAGE_TABLE_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_PAGE_TABLE_sh,$(srctree))
