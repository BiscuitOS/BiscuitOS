################################################
#
# BiscuitOS-CACHE-RAM-RSVDMEM
#
################################################
PK_CACHE_RAM_RSVDMEM_NAME        := BiscuitOS-CACHE-RAM-RSVDMEM
PK_CACHE_RAM_RSVDMEM_VERSION     := $(CONFIG_PK_CACHE_RAM_RSVDMEM_VERSION)
PK_CACHE_RAM_RSVDMEM_SITE        := $(CONFIG_PK_CACHE_RAM_RSVDMEM_URL)
PK_CACHE_RAM_RSVDMEM_GITHUB      := $(CONFIG_PK_CACHE_RAM_RSVDMEM_GITHUB)
PK_CACHE_RAM_RSVDMEM_SUBNAME     :=
PK_CACHE_RAM_RSVDMEM_BUILDARCH   := $(CONFIG_PK_CACHE_RAM_RSVDMEM_BUILDARCH)
PK_CACHE_RAM_RSVDMEM_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-RAM-RSVDMEM
PK_CACHE_RAM_RSVDMEM_PATCH       := $(PK_CACHE_RAM_RSVDMEM_DIR)/patch
PK_CACHE_RAM_RSVDMEM_BSBIT       := BiscuitOS-CACHE-RAM-RSVDMEM-$(PK_CACHE_RAM_RSVDMEM_VERSION).bsbit
PK_CACHE_RAM_RSVDMEM_OUT         :=
PK_CACHE_RAM_RSVDMEM_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_RAM_RSVDMEM_TAR         := $(CONFIG_PK_CACHE_RAM_RSVDMEM_TAR)
PK_CACHE_RAM_RSVDMEM_PACKAGE     :=
PK_CACHE_RAM_RSVDMEM_CONFIG      := $(CONFIG_PK_CACHE_RAM_RSVDMEM_CONFIG)
SUB_TARGET                  += $(PK_CACHE_RAM_RSVDMEM_DIR)/build

PK_CACHE_RAM_RSVDMEM_sh = $(CONFIG_SHELL) $(PK_CACHE_RAM_RSVDMEM_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_RAM_RSVDMEM_NAME)X \
                 $(PK_CACHE_RAM_RSVDMEM_VERSION)X \
                 $(PK_CACHE_RAM_RSVDMEM_SITE)X \
                 $(PK_CACHE_RAM_RSVDMEM_GITHUB)X \
                 $(PK_CACHE_RAM_RSVDMEM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_RAM_RSVDMEM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_RAM_RSVDMEM_SUBNAME)X \
                 $(CONFIG_PK_CACHE_RAM_RSVDMEM_CONFIG)X \
                 $(CONFIG_PK_CACHE_RAM_RSVDMEM_LIBPATH)X \
                 $(CONFIG_PK_CACHE_RAM_RSVDMEM_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_RAM_RSVDMEM_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_RAM_RSVDMEM_CFLAGS)X \
                 $(CONFIG_PK_CACHE_RAM_RSVDMEM_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_RAM_RSVDMEM_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_RAM_RSVDMEM_ASFLAGS)X \
                 $(PK_CACHE_RAM_RSVDMEM_BSBIT)X \
                 $(PK_CACHE_RAM_RSVDMEM_BUILDARCH)X \
		 $(PK_CACHE_RAM_RSVDMEM_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_RAM_RSVDMEM_SRCFILE)X \
                 $(CONFIG_PK_CACHE_RAM_RSVDMEM_CSRCFILE)

$(PK_CACHE_RAM_RSVDMEM_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_RAM_RSVDMEM_sh,$(srctree))
