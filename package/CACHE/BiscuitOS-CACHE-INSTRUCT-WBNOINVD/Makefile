PK_CACHE_INSTRUCT_WBNOINVD_NAME        := BiscuitOS-CACHE-INSTRUCT-WBNOINVD
PK_CACHE_INSTRUCT_WBNOINVD_VERSION     := $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_VERSION)
PK_CACHE_INSTRUCT_WBNOINVD_SITE        := $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_URL)
PK_CACHE_INSTRUCT_WBNOINVD_GITHUB      := $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_GITHUB)
PK_CACHE_INSTRUCT_WBNOINVD_SUBNAME     :=
PK_CACHE_INSTRUCT_WBNOINVD_BUILDARCH   := $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_BUILDARCH)
PK_CACHE_INSTRUCT_WBNOINVD_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-INSTRUCT-WBNOINVD
PK_CACHE_INSTRUCT_WBNOINVD_PATCH       := $(PK_CACHE_INSTRUCT_WBNOINVD_DIR)/patch
PK_CACHE_INSTRUCT_WBNOINVD_BSBIT       := BiscuitOS-CACHE-INSTRUCT-WBNOINVD-$(PK_CACHE_INSTRUCT_WBNOINVD_VERSION).bsbit
PK_CACHE_INSTRUCT_WBNOINVD_OUT         :=
PK_CACHE_INSTRUCT_WBNOINVD_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_INSTRUCT_WBNOINVD_TAR         := $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_TAR)
PK_CACHE_INSTRUCT_WBNOINVD_PACKAGE     :=
PK_CACHE_INSTRUCT_WBNOINVD_CONFIG      := $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_CONFIG)
SUB_TARGET                  += $(PK_CACHE_INSTRUCT_WBNOINVD_DIR)/build

PK_CACHE_INSTRUCT_WBNOINVD_sh = $(CONFIG_SHELL) $(PK_CACHE_INSTRUCT_WBNOINVD_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_INSTRUCT_WBNOINVD_NAME)X \
                 $(PK_CACHE_INSTRUCT_WBNOINVD_VERSION)X \
                 $(PK_CACHE_INSTRUCT_WBNOINVD_SITE)X \
                 $(PK_CACHE_INSTRUCT_WBNOINVD_GITHUB)X \
                 $(PK_CACHE_INSTRUCT_WBNOINVD_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_INSTRUCT_WBNOINVD_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_INSTRUCT_WBNOINVD_SUBNAME)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_CONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_LIBPATH)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_CFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_ASFLAGS)X \
                 $(PK_CACHE_INSTRUCT_WBNOINVD_BSBIT)X \
                 $(PK_CACHE_INSTRUCT_WBNOINVD_BUILDARCH)X \
		 $(PK_CACHE_INSTRUCT_WBNOINVD_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_SRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBNOINVD_CSRCFILE)

$(PK_CACHE_INSTRUCT_WBNOINVD_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_INSTRUCT_WBNOINVD_sh,$(srctree))
