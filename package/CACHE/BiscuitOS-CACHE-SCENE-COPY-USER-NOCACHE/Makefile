PK_CACHE_SCENE_COPY_USER_NC_NAME        := BiscuitOS-CACHE-SCENE-COPY-USER-NOCACHE
PK_CACHE_SCENE_COPY_USER_NC_VERSION     := $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_VERSION)
PK_CACHE_SCENE_COPY_USER_NC_SITE        := $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_URL)
PK_CACHE_SCENE_COPY_USER_NC_GITHUB      := $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_GITHUB)
PK_CACHE_SCENE_COPY_USER_NC_SUBNAME     :=
PK_CACHE_SCENE_COPY_USER_NC_BUILDARCH   := $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_BUILDARCH)
PK_CACHE_SCENE_COPY_USER_NC_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-SCENE-COPY-USER-NOCACHE
PK_CACHE_SCENE_COPY_USER_NC_PATCH       := $(PK_CACHE_SCENE_COPY_USER_NC_DIR)/patch
PK_CACHE_SCENE_COPY_USER_NC_BSBIT       := BiscuitOS-CACHE-SCENE-COPY-USER-NOCACHE-$(PK_CACHE_SCENE_COPY_USER_NC_VERSION).bsbit
PK_CACHE_SCENE_COPY_USER_NC_OUT         :=
PK_CACHE_SCENE_COPY_USER_NC_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_SCENE_COPY_USER_NC_TAR         := $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_TAR)
PK_CACHE_SCENE_COPY_USER_NC_PACKAGE     :=
PK_CACHE_SCENE_COPY_USER_NC_CONFIG      := $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_CONFIG)
SUB_TARGET                  += $(PK_CACHE_SCENE_COPY_USER_NC_DIR)/build

PK_CACHE_SCENE_COPY_USER_NC_sh = $(CONFIG_SHELL) $(PK_CACHE_SCENE_COPY_USER_NC_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_SCENE_COPY_USER_NC_NAME)X \
                 $(PK_CACHE_SCENE_COPY_USER_NC_VERSION)X \
                 $(PK_CACHE_SCENE_COPY_USER_NC_SITE)X \
                 $(PK_CACHE_SCENE_COPY_USER_NC_GITHUB)X \
                 $(PK_CACHE_SCENE_COPY_USER_NC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_SCENE_COPY_USER_NC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_SCENE_COPY_USER_NC_SUBNAME)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_CONFIG)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_LIBPATH)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_CFLAGS)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_ASFLAGS)X \
                 $(PK_CACHE_SCENE_COPY_USER_NC_BSBIT)X \
                 $(PK_CACHE_SCENE_COPY_USER_NC_BUILDARCH)X \
		 $(PK_CACHE_SCENE_COPY_USER_NC_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_SRCFILE)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_NC_CSRCFILE)

$(PK_CACHE_SCENE_COPY_USER_NC_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_SCENE_COPY_USER_NC_sh,$(srctree))
