PK_CACHE_USER_PAGE_USERSPACE_NAME        := BiscuitOS-CACHE-USER-PAGE-USERSPACE
PK_CACHE_USER_PAGE_USERSPACE_VERSION     := $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_VERSION)
PK_CACHE_USER_PAGE_USERSPACE_SITE        := $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_URL)
PK_CACHE_USER_PAGE_USERSPACE_GITHUB      := $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_GITHUB)
PK_CACHE_USER_PAGE_USERSPACE_SUBNAME     :=
PK_CACHE_USER_PAGE_USERSPACE_BUILDARCH   := $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_BUILDARCH)
PK_CACHE_USER_PAGE_USERSPACE_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-USER-PAGE-USERSPACE
PK_CACHE_USER_PAGE_USERSPACE_PATCH       := $(PK_CACHE_USER_PAGE_USERSPACE_DIR)/patch
PK_CACHE_USER_PAGE_USERSPACE_BSBIT       := BiscuitOS-CACHE-USER-PAGE-USERSPACE-$(PK_CACHE_USER_PAGE_USERSPACE_VERSION).bsbit
PK_CACHE_USER_PAGE_USERSPACE_OUT         :=
PK_CACHE_USER_PAGE_USERSPACE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_CACHE_USER_PAGE_USERSPACE_TAR         := $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_TAR)
PK_CACHE_USER_PAGE_USERSPACE_PACKAGE     :=
PK_CACHE_USER_PAGE_USERSPACE_CONFIG      := $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_CONFIG)
SUB_TARGET                  += $(PK_CACHE_USER_PAGE_USERSPACE_DIR)/build

PK_CACHE_USER_PAGE_USERSPACE_sh = $(CONFIG_SHELL) $(PK_CACHE_USER_PAGE_USERSPACE_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_USER_PAGE_USERSPACE_NAME)X \
                 $(PK_CACHE_USER_PAGE_USERSPACE_VERSION)X \
                 $(PK_CACHE_USER_PAGE_USERSPACE_SITE)X \
                 $(PK_CACHE_USER_PAGE_USERSPACE_GITHUB)X \
                 $(PK_CACHE_USER_PAGE_USERSPACE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_USER_PAGE_USERSPACE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_USER_PAGE_USERSPACE_SUBNAME)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_CONFIG)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_LIBPATH)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_CFLAGS)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_ASFLAGS)X \
                 $(PK_CACHE_USER_PAGE_USERSPACE_BSBIT)X \
                 $(PK_CACHE_USER_PAGE_USERSPACE_BUILDARCH)X \
		 $(PK_CACHE_USER_PAGE_USERSPACE_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_SRCFILE)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_USERSPACE_CSRCFILE)

$(PK_CACHE_USER_PAGE_USERSPACE_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_USER_PAGE_USERSPACE_sh,$(srctree))
