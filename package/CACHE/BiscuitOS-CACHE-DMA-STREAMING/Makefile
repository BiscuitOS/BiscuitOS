################################################
#
# BiscuitOS-CACHE-DMA-STREAMING
#
################################################
PK_CACHE_DMA_STREAMING_NAME        := BiscuitOS-CACHE-DMA-STREAMING
PK_CACHE_DMA_STREAMING_VERSION     := $(CONFIG_PK_CACHE_DMA_STREAMING_VERSION)
PK_CACHE_DMA_STREAMING_SITE        := $(CONFIG_PK_CACHE_DMA_STREAMING_URL)
PK_CACHE_DMA_STREAMING_GITHUB      := $(CONFIG_PK_CACHE_DMA_STREAMING_GITHUB)
PK_CACHE_DMA_STREAMING_SUBNAME     :=
PK_CACHE_DMA_STREAMING_BUILDARCH   := $(CONFIG_PK_CACHE_DMA_STREAMING_BUILDARCH)
PK_CACHE_DMA_STREAMING_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-DMA-STREAMING
PK_CACHE_DMA_STREAMING_PATCH       := $(PK_CACHE_DMA_STREAMING_DIR)/patch
PK_CACHE_DMA_STREAMING_BSBIT       := BiscuitOS-CACHE-DMA-STREAMING-$(PK_CACHE_DMA_STREAMING_VERSION).bsbit
PK_CACHE_DMA_STREAMING_OUT         :=
PK_CACHE_DMA_STREAMING_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_DMA_STREAMING_TAR         := $(CONFIG_PK_CACHE_DMA_STREAMING_TAR)
PK_CACHE_DMA_STREAMING_PACKAGE     :=
PK_CACHE_DMA_STREAMING_CONFIG      := $(CONFIG_PK_CACHE_DMA_STREAMING_CONFIG)
SUB_TARGET                  += $(PK_CACHE_DMA_STREAMING_DIR)/build

PK_CACHE_DMA_STREAMING_sh = $(CONFIG_SHELL) $(PK_CACHE_DMA_STREAMING_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_DMA_STREAMING_NAME)X \
                 $(PK_CACHE_DMA_STREAMING_VERSION)X \
                 $(PK_CACHE_DMA_STREAMING_SITE)X \
                 $(PK_CACHE_DMA_STREAMING_GITHUB)X \
                 $(PK_CACHE_DMA_STREAMING_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_DMA_STREAMING_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_DMA_STREAMING_SUBNAME)X \
                 $(CONFIG_PK_CACHE_DMA_STREAMING_CONFIG)X \
                 $(CONFIG_PK_CACHE_DMA_STREAMING_LIBPATH)X \
                 $(CONFIG_PK_CACHE_DMA_STREAMING_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_DMA_STREAMING_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_DMA_STREAMING_CFLAGS)X \
                 $(CONFIG_PK_CACHE_DMA_STREAMING_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_DMA_STREAMING_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_DMA_STREAMING_ASFLAGS)X \
                 $(PK_CACHE_DMA_STREAMING_BSBIT)X \
                 $(PK_CACHE_DMA_STREAMING_BUILDARCH)X \
		 $(PK_CACHE_DMA_STREAMING_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_DMA_STREAMING_SRCFILE)X \
                 $(CONFIG_PK_CACHE_DMA_STREAMING_CSRCFILE)

$(PK_CACHE_DMA_STREAMING_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_DMA_STREAMING_sh,$(srctree))
