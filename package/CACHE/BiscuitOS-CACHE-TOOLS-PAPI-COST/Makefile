PK_CACHE_TOOLS_PAPI_COST_NAME        := BiscuitOS-CACHE-TOOLS-PAPI-COST
PK_CACHE_TOOLS_PAPI_COST_VERSION     := $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_VERSION)
PK_CACHE_TOOLS_PAPI_COST_SITE        := $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_URL)
PK_CACHE_TOOLS_PAPI_COST_GITHUB      := $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_GITHUB)
PK_CACHE_TOOLS_PAPI_COST_SUBNAME     :=
PK_CACHE_TOOLS_PAPI_COST_BUILDARCH   := $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_BUILDARCH)
PK_CACHE_TOOLS_PAPI_COST_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-TOOLS-PAPI-COST
PK_CACHE_TOOLS_PAPI_COST_PATCH       := $(PK_CACHE_TOOLS_PAPI_COST_DIR)/patch
PK_CACHE_TOOLS_PAPI_COST_BSBIT       := BiscuitOS-CACHE-TOOLS-PAPI-COST-$(PK_CACHE_TOOLS_PAPI_COST_VERSION).bsbit
PK_CACHE_TOOLS_PAPI_COST_OUT         :=
PK_CACHE_TOOLS_PAPI_COST_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_CACHE_TOOLS_PAPI_COST_TAR         := $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_TAR)
PK_CACHE_TOOLS_PAPI_COST_PACKAGE     :=
PK_CACHE_TOOLS_PAPI_COST_CONFIG      := $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_CONFIG)
SUB_TARGET                  += $(PK_CACHE_TOOLS_PAPI_COST_DIR)/build

PK_CACHE_TOOLS_PAPI_COST_sh = $(CONFIG_SHELL) $(PK_CACHE_TOOLS_PAPI_COST_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_TOOLS_PAPI_COST_NAME)X \
                 $(PK_CACHE_TOOLS_PAPI_COST_VERSION)X \
                 $(PK_CACHE_TOOLS_PAPI_COST_SITE)X \
                 $(PK_CACHE_TOOLS_PAPI_COST_GITHUB)X \
                 $(PK_CACHE_TOOLS_PAPI_COST_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_TOOLS_PAPI_COST_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_TOOLS_PAPI_COST_SUBNAME)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_CONFIG)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_LIBPATH)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_CFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_ASFLAGS)X \
                 $(PK_CACHE_TOOLS_PAPI_COST_BSBIT)X \
                 $(PK_CACHE_TOOLS_PAPI_COST_BUILDARCH)X \
		 $(PK_CACHE_TOOLS_PAPI_COST_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_SRCFILE)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_COST_CSRCFILE)

$(PK_CACHE_TOOLS_PAPI_COST_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_TOOLS_PAPI_COST_sh,$(srctree))
