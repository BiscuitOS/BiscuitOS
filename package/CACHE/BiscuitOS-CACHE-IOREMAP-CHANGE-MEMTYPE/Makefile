################################################
#
# BiscuitOS-CACHE-IOREMAP-CHANGE-MEMTYPE
#
################################################
PK_CACHE_IOREMAP_CHANGE_MEMTYPE_NAME        := BiscuitOS-CACHE-IOREMAP-CHANGE-MEMTYPE
PK_CACHE_IOREMAP_CHANGE_MEMTYPE_VERSION     := $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_VERSION)
PK_CACHE_IOREMAP_CHANGE_MEMTYPE_SITE        := $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_URL)
PK_CACHE_IOREMAP_CHANGE_MEMTYPE_GITHUB      := $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_GITHUB)
PK_CACHE_IOREMAP_CHANGE_MEMTYPE_SUBNAME     :=
PK_CACHE_IOREMAP_CHANGE_MEMTYPE_BUILDARCH   := $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_BUILDARCH)
PK_CACHE_IOREMAP_CHANGE_MEMTYPE_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-IOREMAP-CHANGE-MEMTYPE
PK_CACHE_IOREMAP_CHANGE_MEMTYPE_PATCH       := $(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_DIR)/patch
PK_CACHE_IOREMAP_CHANGE_MEMTYPE_BSBIT       := BiscuitOS-CACHE-IOREMAP-CHANGE-MEMTYPE-$(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_VERSION).bsbit
PK_CACHE_IOREMAP_CHANGE_MEMTYPE_OUT         :=
PK_CACHE_IOREMAP_CHANGE_MEMTYPE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_IOREMAP_CHANGE_MEMTYPE_TAR         := $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_TAR)
PK_CACHE_IOREMAP_CHANGE_MEMTYPE_PACKAGE     :=
PK_CACHE_IOREMAP_CHANGE_MEMTYPE_CONFIG      := $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_CONFIG)
SUB_TARGET                  += $(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_DIR)/build

PK_CACHE_IOREMAP_CHANGE_MEMTYPE_sh = $(CONFIG_SHELL) $(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_NAME)X \
                 $(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_VERSION)X \
                 $(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_SITE)X \
                 $(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_GITHUB)X \
                 $(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_SUBNAME)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_CONFIG)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_LIBPATH)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_CFLAGS)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_ASFLAGS)X \
                 $(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_BSBIT)X \
                 $(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_BUILDARCH)X \
		 $(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_SRCFILE)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CHANGE_MEMTYPE_CSRCFILE)

$(PK_CACHE_IOREMAP_CHANGE_MEMTYPE_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_IOREMAP_CHANGE_MEMTYPE_sh,$(srctree))
