################################################
#
# BiscuitOS-CACHE-ALIGN-FALSE-SHARING
#
################################################
PK_CACHE_ALIGN_FALSE_SHARING_NAME        := BiscuitOS-CACHE-ALIGN-FALSE-SHARING
PK_CACHE_ALIGN_FALSE_SHARING_VERSION     := $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_VERSION)
PK_CACHE_ALIGN_FALSE_SHARING_SITE        := $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_URL)
PK_CACHE_ALIGN_FALSE_SHARING_GITHUB      := $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_GITHUB)
PK_CACHE_ALIGN_FALSE_SHARING_SUBNAME     :=
PK_CACHE_ALIGN_FALSE_SHARING_BUILDARCH   := $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_BUILDARCH)
PK_CACHE_ALIGN_FALSE_SHARING_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-ALIGN-FALSE-SHARING
PK_CACHE_ALIGN_FALSE_SHARING_PATCH       := $(PK_CACHE_ALIGN_FALSE_SHARING_DIR)/patch
PK_CACHE_ALIGN_FALSE_SHARING_BSBIT       := BiscuitOS-CACHE-ALIGN-FALSE-SHARING-$(PK_CACHE_ALIGN_FALSE_SHARING_VERSION).bsbit
PK_CACHE_ALIGN_FALSE_SHARING_OUT         :=
PK_CACHE_ALIGN_FALSE_SHARING_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_ALIGN_FALSE_SHARING_TAR         := $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_TAR)
PK_CACHE_ALIGN_FALSE_SHARING_PACKAGE     :=
PK_CACHE_ALIGN_FALSE_SHARING_CONFIG      := $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_CONFIG)
SUB_TARGET                  += $(PK_CACHE_ALIGN_FALSE_SHARING_DIR)/build

PK_CACHE_ALIGN_FALSE_SHARING_sh = $(CONFIG_SHELL) $(PK_CACHE_ALIGN_FALSE_SHARING_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_ALIGN_FALSE_SHARING_NAME)X \
                 $(PK_CACHE_ALIGN_FALSE_SHARING_VERSION)X \
                 $(PK_CACHE_ALIGN_FALSE_SHARING_SITE)X \
                 $(PK_CACHE_ALIGN_FALSE_SHARING_GITHUB)X \
                 $(PK_CACHE_ALIGN_FALSE_SHARING_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_ALIGN_FALSE_SHARING_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_ALIGN_FALSE_SHARING_SUBNAME)X \
                 $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_CONFIG)X \
                 $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_LIBPATH)X \
                 $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_CFLAGS)X \
                 $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_ASFLAGS)X \
                 $(PK_CACHE_ALIGN_FALSE_SHARING_BSBIT)X \
                 $(PK_CACHE_ALIGN_FALSE_SHARING_BUILDARCH)X \
		 $(PK_CACHE_ALIGN_FALSE_SHARING_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_SRCFILE)X \
                 $(CONFIG_PK_CACHE_ALIGN_FALSE_SHARING_CSRCFILE)

$(PK_CACHE_ALIGN_FALSE_SHARING_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_ALIGN_FALSE_SHARING_sh,$(srctree))
