# BiscuitOS-CACHE-MTRR-IOCTL-READ
#
################################################
PK_CACHE_MTRR_IOCTL_READ_NAME        := BiscuitOS-CACHE-MTRR-IOCTL-READ
PK_CACHE_MTRR_IOCTL_READ_VERSION     := $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_VERSION)
PK_CACHE_MTRR_IOCTL_READ_SITE        := $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_URL)
PK_CACHE_MTRR_IOCTL_READ_GITHUB      := $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_GITHUB)
PK_CACHE_MTRR_IOCTL_READ_SUBNAME     :=
PK_CACHE_MTRR_IOCTL_READ_BUILDARCH   := $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_BUILDARCH)
PK_CACHE_MTRR_IOCTL_READ_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-MTRR-IOCTL-READ
PK_CACHE_MTRR_IOCTL_READ_PATCH       := $(PK_CACHE_MTRR_IOCTL_READ_DIR)/patch
PK_CACHE_MTRR_IOCTL_READ_BSBIT       := BiscuitOS-CACHE-MTRR-IOCTL-READ-$(PK_CACHE_MTRR_IOCTL_READ_VERSION).bsbit
PK_CACHE_MTRR_IOCTL_READ_OUT         :=
PK_CACHE_MTRR_IOCTL_READ_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_CACHE_MTRR_IOCTL_READ_TAR         := $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_TAR)
PK_CACHE_MTRR_IOCTL_READ_PACKAGE     :=
PK_CACHE_MTRR_IOCTL_READ_CONFIG      := $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_CONFIG)
SUB_TARGET                  += $(PK_CACHE_MTRR_IOCTL_READ_DIR)/build

PK_CACHE_MTRR_IOCTL_READ_sh = $(CONFIG_SHELL) $(PK_CACHE_MTRR_IOCTL_READ_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_MTRR_IOCTL_READ_NAME)X \
                 $(PK_CACHE_MTRR_IOCTL_READ_VERSION)X \
                 $(PK_CACHE_MTRR_IOCTL_READ_SITE)X \
                 $(PK_CACHE_MTRR_IOCTL_READ_GITHUB)X \
                 $(PK_CACHE_MTRR_IOCTL_READ_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_MTRR_IOCTL_READ_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_MTRR_IOCTL_READ_SUBNAME)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_CONFIG)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_LIBPATH)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_CFLAGS)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_ASFLAGS)X \
                 $(PK_CACHE_MTRR_IOCTL_READ_BSBIT)X \
                 $(PK_CACHE_MTRR_IOCTL_READ_BUILDARCH)X \
		 $(PK_CACHE_MTRR_IOCTL_READ_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_SRCFILE)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_READ_CSRCFILE)

$(PK_CACHE_MTRR_IOCTL_READ_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_MTRR_IOCTL_READ_sh,$(srctree))
