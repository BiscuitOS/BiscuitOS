	string "GitHub"

config PK_CACHE_MTRR_IOCTL_READ_SRCFILE
	default "main.c Makefile"
	string "Source code list"

config PK_CACHE_MTRR_IOCTL_READ_CSRCFILE
	string "Compile source code list"

config PK_CACHE_MTRR_IOCTL_READ_CONFIG
	string "CONFIG: Project Configure Options"

