PK_CACHE_MODE_COHERENT_OSMEM_NAME        := BiscuitOS-CACHE-MODE-COHERENT-OSMEM
PK_CACHE_MODE_COHERENT_OSMEM_VERSION     := $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_VERSION)
PK_CACHE_MODE_COHERENT_OSMEM_SITE        := $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_URL)
PK_CACHE_MODE_COHERENT_OSMEM_GITHUB      := $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_GITHUB)
PK_CACHE_MODE_COHERENT_OSMEM_SUBNAME     :=
PK_CACHE_MODE_COHERENT_OSMEM_BUILDARCH   := $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_BUILDARCH)
PK_CACHE_MODE_COHERENT_OSMEM_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-MODE-COHERENT-OSMEM
PK_CACHE_MODE_COHERENT_OSMEM_PATCH       := $(PK_CACHE_MODE_COHERENT_OSMEM_DIR)/patch
PK_CACHE_MODE_COHERENT_OSMEM_BSBIT       := BiscuitOS-CACHE-MODE-COHERENT-OSMEM-$(PK_CACHE_MODE_COHERENT_OSMEM_VERSION).bsbit
PK_CACHE_MODE_COHERENT_OSMEM_OUT         :=
PK_CACHE_MODE_COHERENT_OSMEM_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_MODE_COHERENT_OSMEM_TAR         := $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_TAR)
PK_CACHE_MODE_COHERENT_OSMEM_PACKAGE     :=
PK_CACHE_MODE_COHERENT_OSMEM_CONFIG      := $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_CONFIG)
SUB_TARGET                  += $(PK_CACHE_MODE_COHERENT_OSMEM_DIR)/build

PK_CACHE_MODE_COHERENT_OSMEM_sh = $(CONFIG_SHELL) $(PK_CACHE_MODE_COHERENT_OSMEM_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_MODE_COHERENT_OSMEM_NAME)X \
                 $(PK_CACHE_MODE_COHERENT_OSMEM_VERSION)X \
                 $(PK_CACHE_MODE_COHERENT_OSMEM_SITE)X \
                 $(PK_CACHE_MODE_COHERENT_OSMEM_GITHUB)X \
                 $(PK_CACHE_MODE_COHERENT_OSMEM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_MODE_COHERENT_OSMEM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_MODE_COHERENT_OSMEM_SUBNAME)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_CONFIG)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_LIBPATH)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_CFLAGS)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_ASFLAGS)X \
                 $(PK_CACHE_MODE_COHERENT_OSMEM_BSBIT)X \
                 $(PK_CACHE_MODE_COHERENT_OSMEM_BUILDARCH)X \
		 $(PK_CACHE_MODE_COHERENT_OSMEM_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_SRCFILE)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_OSMEM_CSRCFILE)

$(PK_CACHE_MODE_COHERENT_OSMEM_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_MODE_COHERENT_OSMEM_sh,$(srctree))
