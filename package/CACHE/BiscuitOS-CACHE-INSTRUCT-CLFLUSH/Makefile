PK_CACHE_INSTRUCT_CLFLUSH_NAME        := BiscuitOS-CACHE-INSTRUCT-CLFLUSH
PK_CACHE_INSTRUCT_CLFLUSH_VERSION     := $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_VERSION)
PK_CACHE_INSTRUCT_CLFLUSH_SITE        := $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_URL)
PK_CACHE_INSTRUCT_CLFLUSH_GITHUB      := $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_GITHUB)
PK_CACHE_INSTRUCT_CLFLUSH_SUBNAME     :=
PK_CACHE_INSTRUCT_CLFLUSH_BUILDARCH   := $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_BUILDARCH)
PK_CACHE_INSTRUCT_CLFLUSH_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-INSTRUCT-CLFLUSH
PK_CACHE_INSTRUCT_CLFLUSH_PATCH       := $(PK_CACHE_INSTRUCT_CLFLUSH_DIR)/patch
PK_CACHE_INSTRUCT_CLFLUSH_BSBIT       := BiscuitOS-CACHE-INSTRUCT-CLFLUSH-$(PK_CACHE_INSTRUCT_CLFLUSH_VERSION).bsbit
PK_CACHE_INSTRUCT_CLFLUSH_OUT         :=
PK_CACHE_INSTRUCT_CLFLUSH_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_INSTRUCT_CLFLUSH_TAR         := $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_TAR)
PK_CACHE_INSTRUCT_CLFLUSH_PACKAGE     :=
PK_CACHE_INSTRUCT_CLFLUSH_CONFIG      := $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_CONFIG)
SUB_TARGET                  += $(PK_CACHE_INSTRUCT_CLFLUSH_DIR)/build

PK_CACHE_INSTRUCT_CLFLUSH_sh = $(CONFIG_SHELL) $(PK_CACHE_INSTRUCT_CLFLUSH_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_INSTRUCT_CLFLUSH_NAME)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSH_VERSION)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSH_SITE)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSH_GITHUB)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSH_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSH_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSH_SUBNAME)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_CONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_LIBPATH)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_CFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_ASFLAGS)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSH_BSBIT)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSH_BUILDARCH)X \
		 $(PK_CACHE_INSTRUCT_CLFLUSH_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_SRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSH_CSRCFILE)

$(PK_CACHE_INSTRUCT_CLFLUSH_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_INSTRUCT_CLFLUSH_sh,$(srctree))
