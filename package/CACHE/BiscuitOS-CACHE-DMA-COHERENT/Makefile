################################################
#
# BiscuitOS-CACHE-DMA-COHERENT
#
################################################
PK_CACHE_DMA_COHERENT_NAME        := BiscuitOS-CACHE-DMA-COHERENT
PK_CACHE_DMA_COHERENT_VERSION     := $(CONFIG_PK_CACHE_DMA_COHERENT_VERSION)
PK_CACHE_DMA_COHERENT_SITE        := $(CONFIG_PK_CACHE_DMA_COHERENT_URL)
PK_CACHE_DMA_COHERENT_GITHUB      := $(CONFIG_PK_CACHE_DMA_COHERENT_GITHUB)
PK_CACHE_DMA_COHERENT_SUBNAME     :=
PK_CACHE_DMA_COHERENT_BUILDARCH   := $(CONFIG_PK_CACHE_DMA_COHERENT_BUILDARCH)
PK_CACHE_DMA_COHERENT_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-DMA-COHERENT
PK_CACHE_DMA_COHERENT_PATCH       := $(PK_CACHE_DMA_COHERENT_DIR)/patch
PK_CACHE_DMA_COHERENT_BSBIT       := BiscuitOS-CACHE-DMA-COHERENT-$(PK_CACHE_DMA_COHERENT_VERSION).bsbit
PK_CACHE_DMA_COHERENT_OUT         :=
PK_CACHE_DMA_COHERENT_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_DMA_COHERENT_TAR         := $(CONFIG_PK_CACHE_DMA_COHERENT_TAR)
PK_CACHE_DMA_COHERENT_PACKAGE     :=
PK_CACHE_DMA_COHERENT_CONFIG      := $(CONFIG_PK_CACHE_DMA_COHERENT_CONFIG)
SUB_TARGET                  += $(PK_CACHE_DMA_COHERENT_DIR)/build

PK_CACHE_DMA_COHERENT_sh = $(CONFIG_SHELL) $(PK_CACHE_DMA_COHERENT_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_DMA_COHERENT_NAME)X \
                 $(PK_CACHE_DMA_COHERENT_VERSION)X \
                 $(PK_CACHE_DMA_COHERENT_SITE)X \
                 $(PK_CACHE_DMA_COHERENT_GITHUB)X \
                 $(PK_CACHE_DMA_COHERENT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_DMA_COHERENT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_DMA_COHERENT_SUBNAME)X \
                 $(CONFIG_PK_CACHE_DMA_COHERENT_CONFIG)X \
                 $(CONFIG_PK_CACHE_DMA_COHERENT_LIBPATH)X \
                 $(CONFIG_PK_CACHE_DMA_COHERENT_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_DMA_COHERENT_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_DMA_COHERENT_CFLAGS)X \
                 $(CONFIG_PK_CACHE_DMA_COHERENT_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_DMA_COHERENT_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_DMA_COHERENT_ASFLAGS)X \
                 $(PK_CACHE_DMA_COHERENT_BSBIT)X \
                 $(PK_CACHE_DMA_COHERENT_BUILDARCH)X \
		 $(PK_CACHE_DMA_COHERENT_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_DMA_COHERENT_SRCFILE)X \
                 $(CONFIG_PK_CACHE_DMA_COHERENT_CSRCFILE)

$(PK_CACHE_DMA_COHERENT_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_DMA_COHERENT_sh,$(srctree))
