################################################
#
# BiscuitOS-CACHE-MODE-COHERENT-DEVMEM
#
################################################
PK_CACHE_MODE_COHERENT_DEVMEM_NAME        := BiscuitOS-CACHE-MODE-COHERENT-DEVMEM
PK_CACHE_MODE_COHERENT_DEVMEM_VERSION     := $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_VERSION)
PK_CACHE_MODE_COHERENT_DEVMEM_SITE        := $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_URL)
PK_CACHE_MODE_COHERENT_DEVMEM_GITHUB      := $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_GITHUB)
PK_CACHE_MODE_COHERENT_DEVMEM_SUBNAME     :=
PK_CACHE_MODE_COHERENT_DEVMEM_BUILDARCH   := $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_BUILDARCH)
PK_CACHE_MODE_COHERENT_DEVMEM_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-MODE-COHERENT-DEVMEM
PK_CACHE_MODE_COHERENT_DEVMEM_PATCH       := $(PK_CACHE_MODE_COHERENT_DEVMEM_DIR)/patch
PK_CACHE_MODE_COHERENT_DEVMEM_BSBIT       := BiscuitOS-CACHE-MODE-COHERENT-DEVMEM-$(PK_CACHE_MODE_COHERENT_DEVMEM_VERSION).bsbit
PK_CACHE_MODE_COHERENT_DEVMEM_OUT         :=
PK_CACHE_MODE_COHERENT_DEVMEM_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_MODE_COHERENT_DEVMEM_TAR         := $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_TAR)
PK_CACHE_MODE_COHERENT_DEVMEM_PACKAGE     :=
PK_CACHE_MODE_COHERENT_DEVMEM_CONFIG      := $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_CONFIG)
SUB_TARGET                  += $(PK_CACHE_MODE_COHERENT_DEVMEM_DIR)/build

PK_CACHE_MODE_COHERENT_DEVMEM_sh = $(CONFIG_SHELL) $(PK_CACHE_MODE_COHERENT_DEVMEM_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_MODE_COHERENT_DEVMEM_NAME)X \
                 $(PK_CACHE_MODE_COHERENT_DEVMEM_VERSION)X \
                 $(PK_CACHE_MODE_COHERENT_DEVMEM_SITE)X \
                 $(PK_CACHE_MODE_COHERENT_DEVMEM_GITHUB)X \
                 $(PK_CACHE_MODE_COHERENT_DEVMEM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_MODE_COHERENT_DEVMEM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_MODE_COHERENT_DEVMEM_SUBNAME)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_CONFIG)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_LIBPATH)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_CFLAGS)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_ASFLAGS)X \
                 $(PK_CACHE_MODE_COHERENT_DEVMEM_BSBIT)X \
                 $(PK_CACHE_MODE_COHERENT_DEVMEM_BUILDARCH)X \
		 $(PK_CACHE_MODE_COHERENT_DEVMEM_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_SRCFILE)X \
                 $(CONFIG_PK_CACHE_MODE_COHERENT_DEVMEM_CSRCFILE)

$(PK_CACHE_MODE_COHERENT_DEVMEM_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_MODE_COHERENT_DEVMEM_sh,$(srctree))
