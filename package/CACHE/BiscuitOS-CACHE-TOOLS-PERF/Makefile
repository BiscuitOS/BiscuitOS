################################################
#
# BiscuitOS-CACHE-TOOLS-PERF
#
################################################
PK_CACHE_TOOLS_PERF_NAME        := BiscuitOS-CACHE-TOOLS-PERF
PK_CACHE_TOOLS_PERF_VERSION     := $(CONFIG_PK_CACHE_TOOLS_PERF_VERSION)
PK_CACHE_TOOLS_PERF_SITE        := $(CONFIG_PK_CACHE_TOOLS_PERF_URL)
PK_CACHE_TOOLS_PERF_GITHUB      := $(CONFIG_PK_CACHE_TOOLS_PERF_GITHUB)
PK_CACHE_TOOLS_PERF_SUBNAME     :=
PK_CACHE_TOOLS_PERF_BUILDARCH   := $(CONFIG_PK_CACHE_TOOLS_PERF_BUILDARCH)
PK_CACHE_TOOLS_PERF_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-TOOLS-PERF
PK_CACHE_TOOLS_PERF_PATCH       := $(PK_CACHE_TOOLS_PERF_DIR)/patch
PK_CACHE_TOOLS_PERF_BSBIT       := BiscuitOS-CACHE-TOOLS-PERF-$(PK_CACHE_TOOLS_PERF_VERSION).bsbit
PK_CACHE_TOOLS_PERF_OUT         :=
PK_CACHE_TOOLS_PERF_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_CACHE_TOOLS_PERF_TAR         := $(CONFIG_PK_CACHE_TOOLS_PERF_TAR)
PK_CACHE_TOOLS_PERF_PACKAGE     :=
PK_CACHE_TOOLS_PERF_CONFIG      := $(CONFIG_PK_CACHE_TOOLS_PERF_CONFIG)
SUB_TARGET                  += $(PK_CACHE_TOOLS_PERF_DIR)/build

PK_CACHE_TOOLS_PERF_sh = $(CONFIG_SHELL) $(PK_CACHE_TOOLS_PERF_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_TOOLS_PERF_NAME)X \
                 $(PK_CACHE_TOOLS_PERF_VERSION)X \
                 $(PK_CACHE_TOOLS_PERF_SITE)X \
                 $(PK_CACHE_TOOLS_PERF_GITHUB)X \
                 $(PK_CACHE_TOOLS_PERF_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_TOOLS_PERF_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_TOOLS_PERF_SUBNAME)X \
                 $(CONFIG_PK_CACHE_TOOLS_PERF_CONFIG)X \
                 $(CONFIG_PK_CACHE_TOOLS_PERF_LIBPATH)X \
                 $(CONFIG_PK_CACHE_TOOLS_PERF_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_PERF_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_TOOLS_PERF_CFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_PERF_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_PERF_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_PERF_ASFLAGS)X \
                 $(PK_CACHE_TOOLS_PERF_BSBIT)X \
                 $(PK_CACHE_TOOLS_PERF_BUILDARCH)X \
		 $(PK_CACHE_TOOLS_PERF_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_TOOLS_PERF_SRCFILE)X \
                 $(CONFIG_PK_CACHE_TOOLS_PERF_CSRCFILE)

$(PK_CACHE_TOOLS_PERF_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_TOOLS_PERF_sh,$(srctree))
