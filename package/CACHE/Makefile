CACHE-$(CONFIG_PK_CACHE_INTEL_CMT_CAT)		+= BiscuitOS-Intel-CMT-CAT/
CACHE-$(CONFIG_PK_CACHE_UCD_MMIO_U)		+= BiscuitOS-CACHE-UNCACHED-Mapping-MMIO-Userspace/
CACHE-$(CONFIG_PK_CACHE_UCD_IOREMAP_MMIO)	+= BiscuitOS-CACHE-UNCACHED-IOREMAP-with-MMIO/
CACHE-$(CONFIG_PK_CACHE_UCD_IOREMAP_MEM)	+= BiscuitOS-CACHE-UNCACHED-IOREMAP-with-MEM/
CACHE-$(CONFIG_PK_CACHE_UC_IOREMAP_MMIO)	+= BiscuitOS-CACHE-UNCACHE-IOREMAP-with-MMIO/
CACHE-$(CONFIG_PK_CACHE_UC_IOREMAP_MEM)		+= BiscuitOS-CACHE-UNCACHE-IOREMAP-with-MEM/
CACHE-$(CONFIG_PK_CACHE_WC_IOREMAP_MMIO)	+= BiscuitOS-CACHE-WC-IOREMAP-with-MMIO/
CACHE-$(CONFIG_PK_CACHE_WC_IOREMAP_MEM)		+= BiscuitOS-CACHE-WC-IOREMAP-with-MEM/
CACHE-$(CONFIG_PK_CACHE_USER_PAGE_U)		+= BiscuitOS-CACHE-USER-PAGE-U/
CACHE-$(CONFIG_PK_CACHE_USER_PAGE_K)		+= BiscuitOS-CACHE-USER-PAGE-K/

PACKAGE_MAKEDIR := $(patsubst %/, $(srctree)/package/CACHE/%/Makefile, $(CACHE-y))

include $(PACKAGE_MAKEDIR)
