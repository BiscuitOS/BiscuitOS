################################################
#
# BiscuitOS-CACHE-INSTRUCT-INVD
#
################################################
PK_CACHE_INSTRUCT_INVD_NAME        := BiscuitOS-CACHE-INSTRUCT-INVD
PK_CACHE_INSTRUCT_INVD_VERSION     := $(CONFIG_PK_CACHE_INSTRUCT_INVD_VERSION)
PK_CACHE_INSTRUCT_INVD_SITE        := $(CONFIG_PK_CACHE_INSTRUCT_INVD_URL)
PK_CACHE_INSTRUCT_INVD_GITHUB      := $(CONFIG_PK_CACHE_INSTRUCT_INVD_GITHUB)
PK_CACHE_INSTRUCT_INVD_SUBNAME     :=
PK_CACHE_INSTRUCT_INVD_BUILDARCH   := $(CONFIG_PK_CACHE_INSTRUCT_INVD_BUILDARCH)
PK_CACHE_INSTRUCT_INVD_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-INSTRUCT-INVD
PK_CACHE_INSTRUCT_INVD_PATCH       := $(PK_CACHE_INSTRUCT_INVD_DIR)/patch
PK_CACHE_INSTRUCT_INVD_BSBIT       := BiscuitOS-CACHE-INSTRUCT-INVD-$(PK_CACHE_INSTRUCT_INVD_VERSION).bsbit
PK_CACHE_INSTRUCT_INVD_OUT         :=
PK_CACHE_INSTRUCT_INVD_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_INSTRUCT_INVD_TAR         := $(CONFIG_PK_CACHE_INSTRUCT_INVD_TAR)
PK_CACHE_INSTRUCT_INVD_PACKAGE     :=
PK_CACHE_INSTRUCT_INVD_CONFIG      := $(CONFIG_PK_CACHE_INSTRUCT_INVD_CONFIG)
SUB_TARGET                  += $(PK_CACHE_INSTRUCT_INVD_DIR)/build

PK_CACHE_INSTRUCT_INVD_sh = $(CONFIG_SHELL) $(PK_CACHE_INSTRUCT_INVD_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_INSTRUCT_INVD_NAME)X \
                 $(PK_CACHE_INSTRUCT_INVD_VERSION)X \
                 $(PK_CACHE_INSTRUCT_INVD_SITE)X \
                 $(PK_CACHE_INSTRUCT_INVD_GITHUB)X \
                 $(PK_CACHE_INSTRUCT_INVD_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_INSTRUCT_INVD_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_INSTRUCT_INVD_SUBNAME)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_INVD_CONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_INVD_LIBPATH)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_INVD_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_INVD_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_INVD_CFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_INVD_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_INVD_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_INVD_ASFLAGS)X \
                 $(PK_CACHE_INSTRUCT_INVD_BSBIT)X \
                 $(PK_CACHE_INSTRUCT_INVD_BUILDARCH)X \
		 $(PK_CACHE_INSTRUCT_INVD_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_INVD_SRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_INVD_CSRCFILE)

$(PK_CACHE_INSTRUCT_INVD_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_INSTRUCT_INVD_sh,$(srctree))
