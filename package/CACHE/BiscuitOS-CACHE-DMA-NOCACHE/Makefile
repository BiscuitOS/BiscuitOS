################################################
#
# BiscuitOS-CACHE-DMA-NOCACHE
#
################################################
PK_CACHE_DMA_NOCACHE_NAME        := BiscuitOS-CACHE-DMA-NOCACHE
PK_CACHE_DMA_NOCACHE_VERSION     := $(CONFIG_PK_CACHE_DMA_NOCACHE_VERSION)
PK_CACHE_DMA_NOCACHE_SITE        := $(CONFIG_PK_CACHE_DMA_NOCACHE_URL)
PK_CACHE_DMA_NOCACHE_GITHUB      := $(CONFIG_PK_CACHE_DMA_NOCACHE_GITHUB)
PK_CACHE_DMA_NOCACHE_SUBNAME     :=
PK_CACHE_DMA_NOCACHE_BUILDARCH   := $(CONFIG_PK_CACHE_DMA_NOCACHE_BUILDARCH)
PK_CACHE_DMA_NOCACHE_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-DMA-NOCACHE
PK_CACHE_DMA_NOCACHE_PATCH       := $(PK_CACHE_DMA_NOCACHE_DIR)/patch
PK_CACHE_DMA_NOCACHE_BSBIT       := BiscuitOS-CACHE-DMA-NOCACHE-$(PK_CACHE_DMA_NOCACHE_VERSION).bsbit
PK_CACHE_DMA_NOCACHE_OUT         :=
PK_CACHE_DMA_NOCACHE_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_DMA_NOCACHE_TAR         := $(CONFIG_PK_CACHE_DMA_NOCACHE_TAR)
PK_CACHE_DMA_NOCACHE_PACKAGE     :=
PK_CACHE_DMA_NOCACHE_CONFIG      := $(CONFIG_PK_CACHE_DMA_NOCACHE_CONFIG)
SUB_TARGET                  += $(PK_CACHE_DMA_NOCACHE_DIR)/build

PK_CACHE_DMA_NOCACHE_sh = $(CONFIG_SHELL) $(PK_CACHE_DMA_NOCACHE_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_DMA_NOCACHE_NAME)X \
                 $(PK_CACHE_DMA_NOCACHE_VERSION)X \
                 $(PK_CACHE_DMA_NOCACHE_SITE)X \
                 $(PK_CACHE_DMA_NOCACHE_GITHUB)X \
                 $(PK_CACHE_DMA_NOCACHE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_DMA_NOCACHE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_DMA_NOCACHE_SUBNAME)X \
                 $(CONFIG_PK_CACHE_DMA_NOCACHE_CONFIG)X \
                 $(CONFIG_PK_CACHE_DMA_NOCACHE_LIBPATH)X \
                 $(CONFIG_PK_CACHE_DMA_NOCACHE_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_DMA_NOCACHE_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_DMA_NOCACHE_CFLAGS)X \
                 $(CONFIG_PK_CACHE_DMA_NOCACHE_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_DMA_NOCACHE_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_DMA_NOCACHE_ASFLAGS)X \
                 $(PK_CACHE_DMA_NOCACHE_BSBIT)X \
                 $(PK_CACHE_DMA_NOCACHE_BUILDARCH)X \
		 $(PK_CACHE_DMA_NOCACHE_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_DMA_NOCACHE_SRCFILE)X \
                 $(CONFIG_PK_CACHE_DMA_NOCACHE_CSRCFILE)

$(PK_CACHE_DMA_NOCACHE_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_DMA_NOCACHE_sh,$(srctree))
