################################################
#
# BiscuitOS-CACHE-FLUSH-CACHE-THREAD
#
################################################
PK_CACHE_FLUSH_CACHE_THREAD_NAME        := BiscuitOS-CACHE-FLUSH-CACHE-THREAD
PK_CACHE_FLUSH_CACHE_THREAD_VERSION     := $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_VERSION)
PK_CACHE_FLUSH_CACHE_THREAD_SITE        := $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_URL)
PK_CACHE_FLUSH_CACHE_THREAD_GITHUB      := $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_GITHUB)
PK_CACHE_FLUSH_CACHE_THREAD_SUBNAME     :=
PK_CACHE_FLUSH_CACHE_THREAD_BUILDARCH   := $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_BUILDARCH)
PK_CACHE_FLUSH_CACHE_THREAD_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-FLUSH-CACHE-THREAD
PK_CACHE_FLUSH_CACHE_THREAD_PATCH       := $(PK_CACHE_FLUSH_CACHE_THREAD_DIR)/patch
PK_CACHE_FLUSH_CACHE_THREAD_BSBIT       := BiscuitOS-CACHE-FLUSH-CACHE-THREAD-$(PK_CACHE_FLUSH_CACHE_THREAD_VERSION).bsbit
PK_CACHE_FLUSH_CACHE_THREAD_OUT         :=
PK_CACHE_FLUSH_CACHE_THREAD_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_CACHE_FLUSH_CACHE_THREAD_TAR         := $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_TAR)
PK_CACHE_FLUSH_CACHE_THREAD_PACKAGE     :=
PK_CACHE_FLUSH_CACHE_THREAD_CONFIG      := $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_CONFIG)
SUB_TARGET                  += $(PK_CACHE_FLUSH_CACHE_THREAD_DIR)/build

PK_CACHE_FLUSH_CACHE_THREAD_sh = $(CONFIG_SHELL) $(PK_CACHE_FLUSH_CACHE_THREAD_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_FLUSH_CACHE_THREAD_NAME)X \
                 $(PK_CACHE_FLUSH_CACHE_THREAD_VERSION)X \
                 $(PK_CACHE_FLUSH_CACHE_THREAD_SITE)X \
                 $(PK_CACHE_FLUSH_CACHE_THREAD_GITHUB)X \
                 $(PK_CACHE_FLUSH_CACHE_THREAD_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_FLUSH_CACHE_THREAD_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_FLUSH_CACHE_THREAD_SUBNAME)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_CONFIG)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_LIBPATH)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_CFLAGS)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_ASFLAGS)X \
                 $(PK_CACHE_FLUSH_CACHE_THREAD_BSBIT)X \
                 $(PK_CACHE_FLUSH_CACHE_THREAD_BUILDARCH)X \
		 $(PK_CACHE_FLUSH_CACHE_THREAD_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_SRCFILE)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_THREAD_CSRCFILE)

$(PK_CACHE_FLUSH_CACHE_THREAD_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_FLUSH_CACHE_THREAD_sh,$(srctree))
