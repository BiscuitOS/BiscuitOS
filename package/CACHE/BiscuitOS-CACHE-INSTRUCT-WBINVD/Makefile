PK_CACHE_INSTRUCT_WBINVD_NAME        := BiscuitOS-CACHE-INSTRUCT-WBINVD
PK_CACHE_INSTRUCT_WBINVD_VERSION     := $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_VERSION)
PK_CACHE_INSTRUCT_WBINVD_SITE        := $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_URL)
PK_CACHE_INSTRUCT_WBINVD_GITHUB      := $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_GITHUB)
PK_CACHE_INSTRUCT_WBINVD_SUBNAME     :=
PK_CACHE_INSTRUCT_WBINVD_BUILDARCH   := $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_BUILDARCH)
PK_CACHE_INSTRUCT_WBINVD_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-INSTRUCT-WBINVD
PK_CACHE_INSTRUCT_WBINVD_PATCH       := $(PK_CACHE_INSTRUCT_WBINVD_DIR)/patch
PK_CACHE_INSTRUCT_WBINVD_BSBIT       := BiscuitOS-CACHE-INSTRUCT-WBINVD-$(PK_CACHE_INSTRUCT_WBINVD_VERSION).bsbit
PK_CACHE_INSTRUCT_WBINVD_OUT         :=
PK_CACHE_INSTRUCT_WBINVD_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_INSTRUCT_WBINVD_TAR         := $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_TAR)
PK_CACHE_INSTRUCT_WBINVD_PACKAGE     :=
PK_CACHE_INSTRUCT_WBINVD_CONFIG      := $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_CONFIG)
SUB_TARGET                  += $(PK_CACHE_INSTRUCT_WBINVD_DIR)/build

PK_CACHE_INSTRUCT_WBINVD_sh = $(CONFIG_SHELL) $(PK_CACHE_INSTRUCT_WBINVD_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_INSTRUCT_WBINVD_NAME)X \
                 $(PK_CACHE_INSTRUCT_WBINVD_VERSION)X \
                 $(PK_CACHE_INSTRUCT_WBINVD_SITE)X \
                 $(PK_CACHE_INSTRUCT_WBINVD_GITHUB)X \
                 $(PK_CACHE_INSTRUCT_WBINVD_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_INSTRUCT_WBINVD_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_INSTRUCT_WBINVD_SUBNAME)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_CONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_LIBPATH)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_CFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_ASFLAGS)X \
                 $(PK_CACHE_INSTRUCT_WBINVD_BSBIT)X \
                 $(PK_CACHE_INSTRUCT_WBINVD_BUILDARCH)X \
		 $(PK_CACHE_INSTRUCT_WBINVD_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_SRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_WBINVD_CSRCFILE)

$(PK_CACHE_INSTRUCT_WBINVD_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_INSTRUCT_WBINVD_sh,$(srctree))
