PK_CACHE_RAM_DEVMEM_PF_NAME        := BiscuitOS-CACHE-RAM-DEVMEM-PAGE-FAULT
PK_CACHE_RAM_DEVMEM_PF_VERSION     := $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_VERSION)
PK_CACHE_RAM_DEVMEM_PF_SITE        := $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_URL)
PK_CACHE_RAM_DEVMEM_PF_GITHUB      := $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_GITHUB)
PK_CACHE_RAM_DEVMEM_PF_SUBNAME     :=
PK_CACHE_RAM_DEVMEM_PF_BUILDARCH   := $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_BUILDARCH)
PK_CACHE_RAM_DEVMEM_PF_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-RAM-DEVMEM-PAGE-FAULT
PK_CACHE_RAM_DEVMEM_PF_PATCH       := $(PK_CACHE_RAM_DEVMEM_PF_DIR)/patch
PK_CACHE_RAM_DEVMEM_PF_BSBIT       := BiscuitOS-CACHE-RAM-DEVMEM-PAGE-FAULT-$(PK_CACHE_RAM_DEVMEM_PF_VERSION).bsbit
PK_CACHE_RAM_DEVMEM_PF_OUT         :=
PK_CACHE_RAM_DEVMEM_PF_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_RAM_DEVMEM_PF_TAR         := $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_TAR)
PK_CACHE_RAM_DEVMEM_PF_PACKAGE     :=
PK_CACHE_RAM_DEVMEM_PF_CONFIG      := $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_CONFIG)
SUB_TARGET                  += $(PK_CACHE_RAM_DEVMEM_PF_DIR)/build

PK_CACHE_RAM_DEVMEM_PF_sh = $(CONFIG_SHELL) $(PK_CACHE_RAM_DEVMEM_PF_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_RAM_DEVMEM_PF_NAME)X \
                 $(PK_CACHE_RAM_DEVMEM_PF_VERSION)X \
                 $(PK_CACHE_RAM_DEVMEM_PF_SITE)X \
                 $(PK_CACHE_RAM_DEVMEM_PF_GITHUB)X \
                 $(PK_CACHE_RAM_DEVMEM_PF_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_RAM_DEVMEM_PF_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_RAM_DEVMEM_PF_SUBNAME)X \
                 $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_CONFIG)X \
                 $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_LIBPATH)X \
                 $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_CFLAGS)X \
                 $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_ASFLAGS)X \
                 $(PK_CACHE_RAM_DEVMEM_PF_BSBIT)X \
                 $(PK_CACHE_RAM_DEVMEM_PF_BUILDARCH)X \
		 $(PK_CACHE_RAM_DEVMEM_PF_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_SRCFILE)X \
                 $(CONFIG_PK_CACHE_RAM_DEVMEM_PF_CSRCFILE)

$(PK_CACHE_RAM_DEVMEM_PF_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_RAM_DEVMEM_PF_sh,$(srctree))
