# BiscuitOS-CACHE-MTRR-IOCTL-WRITE
#
################################################
PK_CACHE_MTRR_IOCTL_WRITE_NAME        := BiscuitOS-CACHE-MTRR-IOCTL-WRITE
PK_CACHE_MTRR_IOCTL_WRITE_VERSION     := $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_VERSION)
PK_CACHE_MTRR_IOCTL_WRITE_SITE        := $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_URL)
PK_CACHE_MTRR_IOCTL_WRITE_GITHUB      := $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_GITHUB)
PK_CACHE_MTRR_IOCTL_WRITE_SUBNAME     :=
PK_CACHE_MTRR_IOCTL_WRITE_BUILDARCH   := $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_BUILDARCH)
PK_CACHE_MTRR_IOCTL_WRITE_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-MTRR-IOCTL-WRITE
PK_CACHE_MTRR_IOCTL_WRITE_PATCH       := $(PK_CACHE_MTRR_IOCTL_WRITE_DIR)/patch
PK_CACHE_MTRR_IOCTL_WRITE_BSBIT       := BiscuitOS-CACHE-MTRR-IOCTL-WRITE-$(PK_CACHE_MTRR_IOCTL_WRITE_VERSION).bsbit
PK_CACHE_MTRR_IOCTL_WRITE_OUT         :=
PK_CACHE_MTRR_IOCTL_WRITE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_CACHE_MTRR_IOCTL_WRITE_TAR         := $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_TAR)
PK_CACHE_MTRR_IOCTL_WRITE_PACKAGE     :=
PK_CACHE_MTRR_IOCTL_WRITE_CONFIG      := $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_CONFIG)
SUB_TARGET                  += $(PK_CACHE_MTRR_IOCTL_WRITE_DIR)/build

PK_CACHE_MTRR_IOCTL_WRITE_sh = $(CONFIG_SHELL) $(PK_CACHE_MTRR_IOCTL_WRITE_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_MTRR_IOCTL_WRITE_NAME)X \
                 $(PK_CACHE_MTRR_IOCTL_WRITE_VERSION)X \
                 $(PK_CACHE_MTRR_IOCTL_WRITE_SITE)X \
                 $(PK_CACHE_MTRR_IOCTL_WRITE_GITHUB)X \
                 $(PK_CACHE_MTRR_IOCTL_WRITE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_MTRR_IOCTL_WRITE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_MTRR_IOCTL_WRITE_SUBNAME)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_CONFIG)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_LIBPATH)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_CFLAGS)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_ASFLAGS)X \
                 $(PK_CACHE_MTRR_IOCTL_WRITE_BSBIT)X \
                 $(PK_CACHE_MTRR_IOCTL_WRITE_BUILDARCH)X \
		 $(PK_CACHE_MTRR_IOCTL_WRITE_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_SRCFILE)X \
                 $(CONFIG_PK_CACHE_MTRR_IOCTL_WRITE_CSRCFILE)

$(PK_CACHE_MTRR_IOCTL_WRITE_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_MTRR_IOCTL_WRITE_sh,$(srctree))
