PK_CACHE_INSTRUCT_CLFLUSHOPT_NAME        := BiscuitOS-CACHE-INSTRUCT-CLFLUSHOPT
PK_CACHE_INSTRUCT_CLFLUSHOPT_VERSION     := $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_VERSION)
PK_CACHE_INSTRUCT_CLFLUSHOPT_SITE        := $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_URL)
PK_CACHE_INSTRUCT_CLFLUSHOPT_GITHUB      := $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_GITHUB)
PK_CACHE_INSTRUCT_CLFLUSHOPT_SUBNAME     :=
PK_CACHE_INSTRUCT_CLFLUSHOPT_BUILDARCH   := $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_BUILDARCH)
PK_CACHE_INSTRUCT_CLFLUSHOPT_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-INSTRUCT-CLFLUSHOPT
PK_CACHE_INSTRUCT_CLFLUSHOPT_PATCH       := $(PK_CACHE_INSTRUCT_CLFLUSHOPT_DIR)/patch
PK_CACHE_INSTRUCT_CLFLUSHOPT_BSBIT       := BiscuitOS-CACHE-INSTRUCT-CLFLUSHOPT-$(PK_CACHE_INSTRUCT_CLFLUSHOPT_VERSION).bsbit
PK_CACHE_INSTRUCT_CLFLUSHOPT_OUT         :=
PK_CACHE_INSTRUCT_CLFLUSHOPT_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_INSTRUCT_CLFLUSHOPT_TAR         := $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_TAR)
PK_CACHE_INSTRUCT_CLFLUSHOPT_PACKAGE     :=
PK_CACHE_INSTRUCT_CLFLUSHOPT_CONFIG      := $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_CONFIG)
SUB_TARGET                  += $(PK_CACHE_INSTRUCT_CLFLUSHOPT_DIR)/build

PK_CACHE_INSTRUCT_CLFLUSHOPT_sh = $(CONFIG_SHELL) $(PK_CACHE_INSTRUCT_CLFLUSHOPT_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_INSTRUCT_CLFLUSHOPT_NAME)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSHOPT_VERSION)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSHOPT_SITE)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSHOPT_GITHUB)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSHOPT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSHOPT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSHOPT_SUBNAME)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_CONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_LIBPATH)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_CFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_ASFLAGS)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSHOPT_BSBIT)X \
                 $(PK_CACHE_INSTRUCT_CLFLUSHOPT_BUILDARCH)X \
		 $(PK_CACHE_INSTRUCT_CLFLUSHOPT_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_SRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLFLUSHOPT_CSRCFILE)

$(PK_CACHE_INSTRUCT_CLFLUSHOPT_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_INSTRUCT_CLFLUSHOPT_sh,$(srctree))
