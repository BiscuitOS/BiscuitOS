################################################
#
# BiscuitOS-CACHE-RAM-DIRECT-MAPPING
#
################################################
PK_CACHE_RAM_DIRECTM_NAME        := BiscuitOS-CACHE-RAM-DIRECT-MAPPING
PK_CACHE_RAM_DIRECTM_VERSION     := $(CONFIG_PK_CACHE_RAM_DIRECTM_VERSION)
PK_CACHE_RAM_DIRECTM_SITE        := $(CONFIG_PK_CACHE_RAM_DIRECTM_URL)
PK_CACHE_RAM_DIRECTM_GITHUB      := $(CONFIG_PK_CACHE_RAM_DIRECTM_GITHUB)
PK_CACHE_RAM_DIRECTM_SUBNAME     :=
PK_CACHE_RAM_DIRECTM_BUILDARCH   := $(CONFIG_PK_CACHE_RAM_DIRECTM_BUILDARCH)
PK_CACHE_RAM_DIRECTM_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-RAM-DIRECT-MAPPING
PK_CACHE_RAM_DIRECTM_PATCH       := $(PK_CACHE_RAM_DIRECTM_DIR)/patch
PK_CACHE_RAM_DIRECTM_BSBIT       := BiscuitOS-CACHE-RAM-DIRECT-MAPPING-$(PK_CACHE_RAM_DIRECTM_VERSION).bsbit
PK_CACHE_RAM_DIRECTM_OUT         :=
PK_CACHE_RAM_DIRECTM_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_CACHE_RAM_DIRECTM_TAR         := $(CONFIG_PK_CACHE_RAM_DIRECTM_TAR)
PK_CACHE_RAM_DIRECTM_PACKAGE     :=
PK_CACHE_RAM_DIRECTM_CONFIG      := $(CONFIG_PK_CACHE_RAM_DIRECTM_CONFIG)
SUB_TARGET                  += $(PK_CACHE_RAM_DIRECTM_DIR)/build

PK_CACHE_RAM_DIRECTM_sh = $(CONFIG_SHELL) $(PK_CACHE_RAM_DIRECTM_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_RAM_DIRECTM_NAME)X \
                 $(PK_CACHE_RAM_DIRECTM_VERSION)X \
                 $(PK_CACHE_RAM_DIRECTM_SITE)X \
                 $(PK_CACHE_RAM_DIRECTM_GITHUB)X \
                 $(PK_CACHE_RAM_DIRECTM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_RAM_DIRECTM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_RAM_DIRECTM_SUBNAME)X \
                 $(CONFIG_PK_CACHE_RAM_DIRECTM_CONFIG)X \
                 $(CONFIG_PK_CACHE_RAM_DIRECTM_LIBPATH)X \
                 $(CONFIG_PK_CACHE_RAM_DIRECTM_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_RAM_DIRECTM_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_RAM_DIRECTM_CFLAGS)X \
                 $(CONFIG_PK_CACHE_RAM_DIRECTM_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_RAM_DIRECTM_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_RAM_DIRECTM_ASFLAGS)X \
                 $(PK_CACHE_RAM_DIRECTM_BSBIT)X \
                 $(PK_CACHE_RAM_DIRECTM_BUILDARCH)X \
		 $(PK_CACHE_RAM_DIRECTM_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_RAM_DIRECTM_SRCFILE)X \
                 $(CONFIG_PK_CACHE_RAM_DIRECTM_CSRCFILE)

$(PK_CACHE_RAM_DIRECTM_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_RAM_DIRECTM_sh,$(srctree))
