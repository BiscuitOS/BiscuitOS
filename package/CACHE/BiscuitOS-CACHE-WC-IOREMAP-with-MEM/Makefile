PK_CACHE_WC_IOREMAP_MEM_NAME        := BiscuitOS-CACHE-WC-IOREMAP-with-MEM
PK_CACHE_WC_IOREMAP_MEM_VERSION     := $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_VERSION)
PK_CACHE_WC_IOREMAP_MEM_SITE        := $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_URL)
PK_CACHE_WC_IOREMAP_MEM_GITHUB      := $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_GITHUB)
PK_CACHE_WC_IOREMAP_MEM_SUBNAME     :=
PK_CACHE_WC_IOREMAP_MEM_BUILDARCH   := $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_BUILDARCH)
PK_CACHE_WC_IOREMAP_MEM_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-WC-IOREMAP-with-MEM
PK_CACHE_WC_IOREMAP_MEM_PATCH       := $(PK_CACHE_WC_IOREMAP_MEM_DIR)/patch
PK_CACHE_WC_IOREMAP_MEM_BSBIT       := BiscuitOS-CACHE-WC-IOREMAP-with-MEM-$(PK_CACHE_WC_IOREMAP_MEM_VERSION).bsbit
PK_CACHE_WC_IOREMAP_MEM_OUT         :=
PK_CACHE_WC_IOREMAP_MEM_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_WC_IOREMAP_MEM_TAR         := $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_TAR)
PK_CACHE_WC_IOREMAP_MEM_PACKAGE     :=
PK_CACHE_WC_IOREMAP_MEM_CONFIG      := $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_CONFIG)
SUB_TARGET                  += $(PK_CACHE_WC_IOREMAP_MEM_DIR)/build

PK_CACHE_WC_IOREMAP_MEM_sh = $(CONFIG_SHELL) $(PK_CACHE_WC_IOREMAP_MEM_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_WC_IOREMAP_MEM_NAME)X \
                 $(PK_CACHE_WC_IOREMAP_MEM_VERSION)X \
                 $(PK_CACHE_WC_IOREMAP_MEM_SITE)X \
                 $(PK_CACHE_WC_IOREMAP_MEM_GITHUB)X \
                 $(PK_CACHE_WC_IOREMAP_MEM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_WC_IOREMAP_MEM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_WC_IOREMAP_MEM_SUBNAME)X \
                 $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_CONFIG)X \
                 $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_LIBPATH)X \
                 $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_CFLAGS)X \
                 $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_ASFLAGS)X \
                 $(PK_CACHE_WC_IOREMAP_MEM_BSBIT)X \
                 $(PK_CACHE_WC_IOREMAP_MEM_BUILDARCH)X \
		 $(PK_CACHE_WC_IOREMAP_MEM_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_SRCFILE)X \
                 $(CONFIG_PK_CACHE_WC_IOREMAP_MEM_CSRCFILE)

$(PK_CACHE_WC_IOREMAP_MEM_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_WC_IOREMAP_MEM_sh,$(srctree))
