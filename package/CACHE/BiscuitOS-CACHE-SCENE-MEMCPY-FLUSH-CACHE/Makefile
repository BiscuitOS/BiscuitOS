PK_CACHE_SCENE_MEMCPY_FC_NAME        := BiscuitOS-CACHE-SCENE-MEMCPY-FLUSH-CACHE
PK_CACHE_SCENE_MEMCPY_FC_VERSION     := $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_VERSION)
PK_CACHE_SCENE_MEMCPY_FC_SITE        := $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_URL)
PK_CACHE_SCENE_MEMCPY_FC_GITHUB      := $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_GITHUB)
PK_CACHE_SCENE_MEMCPY_FC_SUBNAME     :=
PK_CACHE_SCENE_MEMCPY_FC_BUILDARCH   := $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_BUILDARCH)
PK_CACHE_SCENE_MEMCPY_FC_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-SCENE-MEMCPY-FLUSH-CACHE
PK_CACHE_SCENE_MEMCPY_FC_PATCH       := $(PK_CACHE_SCENE_MEMCPY_FC_DIR)/patch
PK_CACHE_SCENE_MEMCPY_FC_BSBIT       := BiscuitOS-CACHE-SCENE-MEMCPY-FLUSH-CACHE-$(PK_CACHE_SCENE_MEMCPY_FC_VERSION).bsbit
PK_CACHE_SCENE_MEMCPY_FC_OUT         :=
PK_CACHE_SCENE_MEMCPY_FC_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_SCENE_MEMCPY_FC_TAR         := $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_TAR)
PK_CACHE_SCENE_MEMCPY_FC_PACKAGE     :=
PK_CACHE_SCENE_MEMCPY_FC_CONFIG      := $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_CONFIG)
SUB_TARGET                  += $(PK_CACHE_SCENE_MEMCPY_FC_DIR)/build

PK_CACHE_SCENE_MEMCPY_FC_sh = $(CONFIG_SHELL) $(PK_CACHE_SCENE_MEMCPY_FC_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_SCENE_MEMCPY_FC_NAME)X \
                 $(PK_CACHE_SCENE_MEMCPY_FC_VERSION)X \
                 $(PK_CACHE_SCENE_MEMCPY_FC_SITE)X \
                 $(PK_CACHE_SCENE_MEMCPY_FC_GITHUB)X \
                 $(PK_CACHE_SCENE_MEMCPY_FC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_SCENE_MEMCPY_FC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_SCENE_MEMCPY_FC_SUBNAME)X \
                 $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_CONFIG)X \
                 $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_LIBPATH)X \
                 $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_CFLAGS)X \
                 $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_ASFLAGS)X \
                 $(PK_CACHE_SCENE_MEMCPY_FC_BSBIT)X \
                 $(PK_CACHE_SCENE_MEMCPY_FC_BUILDARCH)X \
		 $(PK_CACHE_SCENE_MEMCPY_FC_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_SRCFILE)X \
                 $(CONFIG_PK_CACHE_SCENE_MEMCPY_FC_CSRCFILE)

$(PK_CACHE_SCENE_MEMCPY_FC_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_SCENE_MEMCPY_FC_sh,$(srctree))
