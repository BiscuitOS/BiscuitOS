PK_CACHE_USER_PAGE_KERNEL_STUB_SCRIPTS     := $(srctree)/scripts/package/package-kernel.sh
PK_CACHE_USER_PAGE_KERNEL_STUB_TAR         := $(CONFIG_PK_CACHE_USER_PAGE_KERNEL_STUB_TAR)
PK_CACHE_USER_PAGE_KERNEL_STUB_PACKAGE     :=
PK_CACHE_USER_PAGE_KERNEL_STUB_CONFIG      := $(CONFIG_PK_CACHE_USER_PAGE_KERNEL_STUB_CONFIG)
SUB_TARGET                  += $(PK_CACHE_USER_PAGE_KERNEL_STUB_DIR)/build

PK_CACHE_USER_PAGE_KERNEL_STUB_sh = $(CONFIG_SHELL) $(PK_CACHE_USER_PAGE_KERNEL_STUB_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_USER_PAGE_KERNEL_STUB_NAME)X \
                 $(PK_CACHE_USER_PAGE_KERNEL_STUB_VERSION)X \
                 $(PK_CACHE_USER_PAGE_KERNEL_STUB_SITE)X \
                 $(PK_CACHE_USER_PAGE_KERNEL_STUB_GITHUB)X \
                 $(PK_CACHE_USER_PAGE_KERNEL_STUB_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_USER_PAGE_KERNEL_STUB_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_USER_PAGE_KERNEL_STUB_SUBNAME)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_KERNEL_STUB_CONFIG)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_KERNEL_STUB_LIBPATH)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_KERNEL_STUB_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_KERNEL_STUB_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_KERNEL_STUB_CFLAGS)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_KERNEL_STUB_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_KERNEL_STUB_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_USER_PAGE_KERNEL_STUB_ASFLAGS)X \
                 $(PK_CACHE_USER_PAGE_KERNEL_STUB_BSBIT)X \
                 $(PK_CACHE_USER_PAGE_KERNEL_STUB_BUILDARCH)X \
		 $(PK_CACHE_USER_PAGE_KERNEL_STUB_CSRCFILE)X \
