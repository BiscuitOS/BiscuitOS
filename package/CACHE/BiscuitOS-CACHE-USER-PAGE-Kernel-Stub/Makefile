PK_CACHE_USER_PAGE_KERNEL_STUB_SITE        := $(CONFIG_PK_CACHE_USER_PAGE_KERNEL_STUB_URL)
PK_CACHE_USER_PAGE_KERNEL_STUB_GITHUB      := $(CONFIG_PK_CACHE_USER_PAGE_KERNEL_STUB_GITHUB)
PK_CACHE_USER_PAGE_KERNEL_STUB_SUBNAME     :=
PK_CACHE_USER_PAGE_KERNEL_STUB_BUILDARCH   := $(CONFIG_PK_CACHE_USER_PAGE_KERNEL_STUB_BUILDARCH)
PK_CACHE_USER_PAGE_KERNEL_STUB_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-USER-PAGE-Kernel-Stub
PK_CACHE_USER_PAGE_KERNEL_STUB_PATCH       := $(PK_CACHE_USER_PAGE_KERNEL_STUB_DIR)/patch
PK_CACHE_USER_PAGE_KERNEL_STUB_BSBIT       := BiscuitOS-CACHE-USER-PAGE-Kernel-Stub-$(PK_CACHE_USER_PAGE_KERNEL_STUB_VERSION).bsbit
