################################################
#
# BiscuitOS-CACHE-PERFORMANCE-PREFETCH
#
################################################
PK_CACHE_PERFORMANCE_PREFETCH_NAME        := BiscuitOS-CACHE-PERFORMANCE-PREFETCH
PK_CACHE_PERFORMANCE_PREFETCH_VERSION     := $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_VERSION)
PK_CACHE_PERFORMANCE_PREFETCH_SITE        := $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_URL)
PK_CACHE_PERFORMANCE_PREFETCH_GITHUB      := $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_GITHUB)
PK_CACHE_PERFORMANCE_PREFETCH_SUBNAME     :=
PK_CACHE_PERFORMANCE_PREFETCH_BUILDARCH   := $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_BUILDARCH)
PK_CACHE_PERFORMANCE_PREFETCH_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-PERFORMANCE-PREFETCH
PK_CACHE_PERFORMANCE_PREFETCH_PATCH       := $(PK_CACHE_PERFORMANCE_PREFETCH_DIR)/patch
PK_CACHE_PERFORMANCE_PREFETCH_BSBIT       := BiscuitOS-CACHE-PERFORMANCE-PREFETCH-$(PK_CACHE_PERFORMANCE_PREFETCH_VERSION).bsbit
PK_CACHE_PERFORMANCE_PREFETCH_OUT         :=
PK_CACHE_PERFORMANCE_PREFETCH_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_CACHE_PERFORMANCE_PREFETCH_TAR         := $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_TAR)
PK_CACHE_PERFORMANCE_PREFETCH_PACKAGE     :=
PK_CACHE_PERFORMANCE_PREFETCH_CONFIG      := $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_CONFIG)
SUB_TARGET                  += $(PK_CACHE_PERFORMANCE_PREFETCH_DIR)/build

PK_CACHE_PERFORMANCE_PREFETCH_sh = $(CONFIG_SHELL) $(PK_CACHE_PERFORMANCE_PREFETCH_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_PERFORMANCE_PREFETCH_NAME)X \
                 $(PK_CACHE_PERFORMANCE_PREFETCH_VERSION)X \
                 $(PK_CACHE_PERFORMANCE_PREFETCH_SITE)X \
                 $(PK_CACHE_PERFORMANCE_PREFETCH_GITHUB)X \
                 $(PK_CACHE_PERFORMANCE_PREFETCH_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_PERFORMANCE_PREFETCH_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_PERFORMANCE_PREFETCH_SUBNAME)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_CONFIG)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_LIBPATH)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_CFLAGS)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_ASFLAGS)X \
                 $(PK_CACHE_PERFORMANCE_PREFETCH_BSBIT)X \
                 $(PK_CACHE_PERFORMANCE_PREFETCH_BUILDARCH)X \
		 $(PK_CACHE_PERFORMANCE_PREFETCH_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_SRCFILE)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_PREFETCH_CSRCFILE)

$(PK_CACHE_PERFORMANCE_PREFETCH_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_PERFORMANCE_PREFETCH_sh,$(srctree))
