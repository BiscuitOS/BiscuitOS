################################################
#
# BiscuitOS-CACHE-COLOURING
#
################################################
PK_CACHE_COLOURING_NAME        := BiscuitOS-CACHE-COLOURING
PK_CACHE_COLOURING_VERSION     := $(CONFIG_PK_CACHE_COLOURING_VERSION)
PK_CACHE_COLOURING_SITE        := $(CONFIG_PK_CACHE_COLOURING_URL)
PK_CACHE_COLOURING_GITHUB      := $(CONFIG_PK_CACHE_COLOURING_GITHUB)
PK_CACHE_COLOURING_SUBNAME     :=
PK_CACHE_COLOURING_BUILDARCH   := $(CONFIG_PK_CACHE_COLOURING_BUILDARCH)
PK_CACHE_COLOURING_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-COLOURING
PK_CACHE_COLOURING_PATCH       := $(PK_CACHE_COLOURING_DIR)/patch
PK_CACHE_COLOURING_BSBIT       := BiscuitOS-CACHE-COLOURING-$(PK_CACHE_COLOURING_VERSION).bsbit
PK_CACHE_COLOURING_OUT         :=
PK_CACHE_COLOURING_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_CACHE_COLOURING_TAR         := $(CONFIG_PK_CACHE_COLOURING_TAR)
PK_CACHE_COLOURING_PACKAGE     :=
PK_CACHE_COLOURING_CONFIG      := $(CONFIG_PK_CACHE_COLOURING_CONFIG)
SUB_TARGET                  += $(PK_CACHE_COLOURING_DIR)/build

PK_CACHE_COLOURING_sh = $(CONFIG_SHELL) $(PK_CACHE_COLOURING_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_COLOURING_NAME)X \
                 $(PK_CACHE_COLOURING_VERSION)X \
                 $(PK_CACHE_COLOURING_SITE)X \
                 $(PK_CACHE_COLOURING_GITHUB)X \
                 $(PK_CACHE_COLOURING_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_COLOURING_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_COLOURING_SUBNAME)X \
                 $(CONFIG_PK_CACHE_COLOURING_CONFIG)X \
                 $(CONFIG_PK_CACHE_COLOURING_LIBPATH)X \
                 $(CONFIG_PK_CACHE_COLOURING_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_COLOURING_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_COLOURING_CFLAGS)X \
                 $(CONFIG_PK_CACHE_COLOURING_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_COLOURING_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_COLOURING_ASFLAGS)X \
                 $(PK_CACHE_COLOURING_BSBIT)X \
                 $(PK_CACHE_COLOURING_BUILDARCH)X \
		 $(PK_CACHE_COLOURING_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_COLOURING_SRCFILE)X \
                 $(CONFIG_PK_CACHE_COLOURING_CSRCFILE)

$(PK_CACHE_COLOURING_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_COLOURING_sh,$(srctree))
