PK_CACHE_INSTRUCT_CLWB_NAME        := BiscuitOS-CACHE-INSTRUCT-CLWB
PK_CACHE_INSTRUCT_CLWB_VERSION     := $(CONFIG_PK_CACHE_INSTRUCT_CLWB_VERSION)
PK_CACHE_INSTRUCT_CLWB_SITE        := $(CONFIG_PK_CACHE_INSTRUCT_CLWB_URL)
PK_CACHE_INSTRUCT_CLWB_GITHUB      := $(CONFIG_PK_CACHE_INSTRUCT_CLWB_GITHUB)
PK_CACHE_INSTRUCT_CLWB_SUBNAME     :=
PK_CACHE_INSTRUCT_CLWB_BUILDARCH   := $(CONFIG_PK_CACHE_INSTRUCT_CLWB_BUILDARCH)
PK_CACHE_INSTRUCT_CLWB_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-INSTRUCT-CLWB
PK_CACHE_INSTRUCT_CLWB_PATCH       := $(PK_CACHE_INSTRUCT_CLWB_DIR)/patch
PK_CACHE_INSTRUCT_CLWB_BSBIT       := BiscuitOS-CACHE-INSTRUCT-CLWB-$(PK_CACHE_INSTRUCT_CLWB_VERSION).bsbit
PK_CACHE_INSTRUCT_CLWB_OUT         :=
PK_CACHE_INSTRUCT_CLWB_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_INSTRUCT_CLWB_TAR         := $(CONFIG_PK_CACHE_INSTRUCT_CLWB_TAR)
PK_CACHE_INSTRUCT_CLWB_PACKAGE     :=
PK_CACHE_INSTRUCT_CLWB_CONFIG      := $(CONFIG_PK_CACHE_INSTRUCT_CLWB_CONFIG)
SUB_TARGET                  += $(PK_CACHE_INSTRUCT_CLWB_DIR)/build

PK_CACHE_INSTRUCT_CLWB_sh = $(CONFIG_SHELL) $(PK_CACHE_INSTRUCT_CLWB_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_INSTRUCT_CLWB_NAME)X \
                 $(PK_CACHE_INSTRUCT_CLWB_VERSION)X \
                 $(PK_CACHE_INSTRUCT_CLWB_SITE)X \
                 $(PK_CACHE_INSTRUCT_CLWB_GITHUB)X \
                 $(PK_CACHE_INSTRUCT_CLWB_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_INSTRUCT_CLWB_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_INSTRUCT_CLWB_SUBNAME)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLWB_CONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLWB_LIBPATH)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLWB_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLWB_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLWB_CFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLWB_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLWB_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLWB_ASFLAGS)X \
                 $(PK_CACHE_INSTRUCT_CLWB_BSBIT)X \
                 $(PK_CACHE_INSTRUCT_CLWB_BUILDARCH)X \
		 $(PK_CACHE_INSTRUCT_CLWB_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLWB_SRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_CLWB_CSRCFILE)

$(PK_CACHE_INSTRUCT_CLWB_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_INSTRUCT_CLWB_sh,$(srctree))
