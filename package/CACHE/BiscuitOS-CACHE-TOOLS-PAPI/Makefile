################################################
#
# BiscuitOS-CACHE-TOOLS-PAPI
#
################################################
PK_CACHE_TOOLS_PAPI_NAME        := BiscuitOS-CACHE-TOOLS-PAPI
PK_CACHE_TOOLS_PAPI_VERSION     := $(CONFIG_PK_CACHE_TOOLS_PAPI_VERSION)
PK_CACHE_TOOLS_PAPI_SITE        := $(CONFIG_PK_CACHE_TOOLS_PAPI_URL)
PK_CACHE_TOOLS_PAPI_GITHUB      := $(CONFIG_PK_CACHE_TOOLS_PAPI_GITHUB)
PK_CACHE_TOOLS_PAPI_SUBNAME     :=
PK_CACHE_TOOLS_PAPI_BUILDARCH   := $(CONFIG_PK_CACHE_TOOLS_PAPI_BUILDARCH)
PK_CACHE_TOOLS_PAPI_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-TOOLS-PAPI
PK_CACHE_TOOLS_PAPI_PATCH       := $(PK_CACHE_TOOLS_PAPI_DIR)/patch
PK_CACHE_TOOLS_PAPI_BSBIT       := BiscuitOS-CACHE-TOOLS-PAPI-$(PK_CACHE_TOOLS_PAPI_VERSION).bsbit
PK_CACHE_TOOLS_PAPI_OUT         :=
PK_CACHE_TOOLS_PAPI_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_CACHE_TOOLS_PAPI_TAR         := $(CONFIG_PK_CACHE_TOOLS_PAPI_TAR)
PK_CACHE_TOOLS_PAPI_PACKAGE     :=
PK_CACHE_TOOLS_PAPI_CONFIG      := $(CONFIG_PK_CACHE_TOOLS_PAPI_CONFIG)
SUB_TARGET                  += $(PK_CACHE_TOOLS_PAPI_DIR)/build

PK_CACHE_TOOLS_PAPI_sh = $(CONFIG_SHELL) $(PK_CACHE_TOOLS_PAPI_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_TOOLS_PAPI_NAME)X \
                 $(PK_CACHE_TOOLS_PAPI_VERSION)X \
                 $(PK_CACHE_TOOLS_PAPI_SITE)X \
                 $(PK_CACHE_TOOLS_PAPI_GITHUB)X \
                 $(PK_CACHE_TOOLS_PAPI_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_TOOLS_PAPI_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_TOOLS_PAPI_SUBNAME)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_CONFIG)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_LIBPATH)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_CFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_ASFLAGS)X \
                 $(PK_CACHE_TOOLS_PAPI_BSBIT)X \
                 $(PK_CACHE_TOOLS_PAPI_BUILDARCH)X \
		 $(PK_CACHE_TOOLS_PAPI_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_SRCFILE)X \
                 $(CONFIG_PK_CACHE_TOOLS_PAPI_CSRCFILE)

$(PK_CACHE_TOOLS_PAPI_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_TOOLS_PAPI_sh,$(srctree))
