################################################
#
# BiscuitOS-CACHE-IOREMAP
#
################################################
PK_CACHE_IOREMAP_NAME        := BiscuitOS-CACHE-IOREMAP
PK_CACHE_IOREMAP_VERSION     := $(CONFIG_PK_CACHE_IOREMAP_VERSION)
PK_CACHE_IOREMAP_SITE        := $(CONFIG_PK_CACHE_IOREMAP_URL)
PK_CACHE_IOREMAP_GITHUB      := $(CONFIG_PK_CACHE_IOREMAP_GITHUB)
PK_CACHE_IOREMAP_SUBNAME     :=
PK_CACHE_IOREMAP_BUILDARCH   := $(CONFIG_PK_CACHE_IOREMAP_BUILDARCH)
PK_CACHE_IOREMAP_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-IOREMAP
PK_CACHE_IOREMAP_PATCH       := $(PK_CACHE_IOREMAP_DIR)/patch
PK_CACHE_IOREMAP_BSBIT       := BiscuitOS-CACHE-IOREMAP-$(PK_CACHE_IOREMAP_VERSION).bsbit
PK_CACHE_IOREMAP_OUT         :=
PK_CACHE_IOREMAP_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_IOREMAP_TAR         := $(CONFIG_PK_CACHE_IOREMAP_TAR)
PK_CACHE_IOREMAP_PACKAGE     :=
PK_CACHE_IOREMAP_CONFIG      := $(CONFIG_PK_CACHE_IOREMAP_CONFIG)
SUB_TARGET                  += $(PK_CACHE_IOREMAP_DIR)/build

PK_CACHE_IOREMAP_sh = $(CONFIG_SHELL) $(PK_CACHE_IOREMAP_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_IOREMAP_NAME)X \
                 $(PK_CACHE_IOREMAP_VERSION)X \
                 $(PK_CACHE_IOREMAP_SITE)X \
                 $(PK_CACHE_IOREMAP_GITHUB)X \
                 $(PK_CACHE_IOREMAP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_IOREMAP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_IOREMAP_SUBNAME)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CONFIG)X \
                 $(CONFIG_PK_CACHE_IOREMAP_LIBPATH)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_IOREMAP_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CFLAGS)X \
                 $(CONFIG_PK_CACHE_IOREMAP_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_IOREMAP_ASFLAGS)X \
                 $(PK_CACHE_IOREMAP_BSBIT)X \
                 $(PK_CACHE_IOREMAP_BUILDARCH)X \
		 $(PK_CACHE_IOREMAP_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_IOREMAP_SRCFILE)X \
                 $(CONFIG_PK_CACHE_IOREMAP_CSRCFILE)

$(PK_CACHE_IOREMAP_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_IOREMAP_sh,$(srctree))
