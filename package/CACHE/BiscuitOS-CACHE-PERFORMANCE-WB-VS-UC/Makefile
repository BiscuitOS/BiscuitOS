PK_CACHE_PERFORMANCE_WB_UC_NAME        := BiscuitOS-CACHE-PERFORMANCE-WB-VS-UC
PK_CACHE_PERFORMANCE_WB_UC_VERSION     := $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_VERSION)
PK_CACHE_PERFORMANCE_WB_UC_SITE        := $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_URL)
PK_CACHE_PERFORMANCE_WB_UC_GITHUB      := $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_GITHUB)
PK_CACHE_PERFORMANCE_WB_UC_SUBNAME     :=
PK_CACHE_PERFORMANCE_WB_UC_BUILDARCH   := $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_BUILDARCH)
PK_CACHE_PERFORMANCE_WB_UC_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-PERFORMANCE-WB-VS-UC
PK_CACHE_PERFORMANCE_WB_UC_PATCH       := $(PK_CACHE_PERFORMANCE_WB_UC_DIR)/patch
PK_CACHE_PERFORMANCE_WB_UC_BSBIT       := BiscuitOS-CACHE-PERFORMANCE-WB-VS-UC-$(PK_CACHE_PERFORMANCE_WB_UC_VERSION).bsbit
PK_CACHE_PERFORMANCE_WB_UC_OUT         :=
PK_CACHE_PERFORMANCE_WB_UC_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_CACHE_PERFORMANCE_WB_UC_TAR         := $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_TAR)
PK_CACHE_PERFORMANCE_WB_UC_PACKAGE     :=
PK_CACHE_PERFORMANCE_WB_UC_CONFIG      := $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_CONFIG)
SUB_TARGET                  += $(PK_CACHE_PERFORMANCE_WB_UC_DIR)/build

PK_CACHE_PERFORMANCE_WB_UC_sh = $(CONFIG_SHELL) $(PK_CACHE_PERFORMANCE_WB_UC_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_PERFORMANCE_WB_UC_NAME)X \
                 $(PK_CACHE_PERFORMANCE_WB_UC_VERSION)X \
                 $(PK_CACHE_PERFORMANCE_WB_UC_SITE)X \
                 $(PK_CACHE_PERFORMANCE_WB_UC_GITHUB)X \
                 $(PK_CACHE_PERFORMANCE_WB_UC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_PERFORMANCE_WB_UC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_PERFORMANCE_WB_UC_SUBNAME)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_CONFIG)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_LIBPATH)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_CFLAGS)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_ASFLAGS)X \
                 $(PK_CACHE_PERFORMANCE_WB_UC_BSBIT)X \
                 $(PK_CACHE_PERFORMANCE_WB_UC_BUILDARCH)X \
		 $(PK_CACHE_PERFORMANCE_WB_UC_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_SRCFILE)X \
                 $(CONFIG_PK_CACHE_PERFORMANCE_WB_UC_CSRCFILE)

$(PK_CACHE_PERFORMANCE_WB_UC_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_PERFORMANCE_WB_UC_sh,$(srctree))
