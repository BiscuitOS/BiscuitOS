################################################
#
# BiscuitOS-Intel-CMT-CAT
#
################################################
PK_CACHE_INTEL_CMT_CAT_NAME	:= BiscuitOS-Intel-CMT-CAT
PK_CACHE_INTEL_CMT_CAT_VERSION	:= $(CONFIG_PK_CACHE_INTEL_CMT_CAT_VERSION)
PK_CACHE_INTEL_CMT_CAT_SITE	:= $(CONFIG_PK_CACHE_INTEL_CMT_CAT_URL)
PK_CACHE_INTEL_CMT_CAT_GITHUB	:= $(CONFIG_PK_CACHE_INTEL_CMT_CAT_GITHUB)
PK_CACHE_INTEL_CMT_CAT_SUBNAME	:=
PK_CACHE_INTEL_CMT_CAT_BUILDARCH	:= $(CONFIG_PK_CACHE_INTEL_CMT_CAT_BUILDARCH)
PK_CACHE_INTEL_CMT_CAT_DIR	:= $(srctree)/package/CACHE/BiscuitOS-Intel-CMT-CAT
PK_CACHE_INTEL_CMT_CAT_PATCH	:= $(PK_CACHE_INTEL_CMT_CAT_DIR)/patch
PK_CACHE_INTEL_CMT_CAT_BSBIT	:= BiscuitOS-Intel-CMT-CAT-$(PK_CACHE_INTEL_CMT_CAT_VERSION).bsbit
PK_CACHE_INTEL_CMT_CAT_OUT	:=
PK_CACHE_INTEL_CMT_CAT_SCRIPTS	:= $(srctree)/scripts/package/package-CMTCAT.sh
PK_CACHE_INTEL_CMT_CAT_TAR	:= $(CONFIG_PK_CACHE_INTEL_CMT_CAT_TAR)
PK_CACHE_INTEL_CMT_CAT_PACKAGE	:=
PK_CACHE_INTEL_CMT_CAT_CONFIG	:= $(CONFIG_PK_CACHE_INTEL_CMT_CAT_CONFIG)
SUB_TARGET		+= $(PK_CACHE_INTEL_CMT_CAT_DIR)/build

PK_CACHE_INTEL_CMT_CAT_sh = $(CONFIG_SHELL) $(PK_CACHE_INTEL_CMT_CAT_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_INTEL_CMT_CAT_NAME)X \
                 $(PK_CACHE_INTEL_CMT_CAT_VERSION)X \
                 $(PK_CACHE_INTEL_CMT_CAT_SITE)X \
                 $(PK_CACHE_INTEL_CMT_CAT_GITHUB)X \
                 $(PK_CACHE_INTEL_CMT_CAT_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_INTEL_CMT_CAT_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_INTEL_CMT_CAT_SUBNAME)X \
                 $(CONFIG_PK_CACHE_INTEL_CMT_CAT_CONFIG)X \
                 $(CONFIG_PK_CACHE_INTEL_CMT_CAT_LIBPATH)X \
                 $(CONFIG_PK_CACHE_INTEL_CMT_CAT_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_INTEL_CMT_CAT_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_INTEL_CMT_CAT_CFLAGS)X \
                 $(CONFIG_PK_CACHE_INTEL_CMT_CAT_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_INTEL_CMT_CAT_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_INTEL_CMT_CAT_ASFLAGS)X \
                 $(PK_CACHE_INTEL_CMT_CAT_BSBIT)X \
                 $(PK_CACHE_INTEL_CMT_CAT_BUILDARCH)X

$(PK_CACHE_INTEL_CMT_CAT_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_INTEL_CMT_CAT_sh,$(srctree))
