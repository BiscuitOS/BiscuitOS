PK_CACHE_INSTRUCT_PREFETCHW_NAME        := BiscuitOS-CACHE-INSTRUCT-PREFETCHW
PK_CACHE_INSTRUCT_PREFETCHW_VERSION     := $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_VERSION)
PK_CACHE_INSTRUCT_PREFETCHW_SITE        := $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_URL)
PK_CACHE_INSTRUCT_PREFETCHW_GITHUB      := $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_GITHUB)
PK_CACHE_INSTRUCT_PREFETCHW_SUBNAME     :=
PK_CACHE_INSTRUCT_PREFETCHW_BUILDARCH   := $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_BUILDARCH)
PK_CACHE_INSTRUCT_PREFETCHW_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-INSTRUCT-PREFETCHW
PK_CACHE_INSTRUCT_PREFETCHW_PATCH       := $(PK_CACHE_INSTRUCT_PREFETCHW_DIR)/patch
PK_CACHE_INSTRUCT_PREFETCHW_BSBIT       := BiscuitOS-CACHE-INSTRUCT-PREFETCHW-$(PK_CACHE_INSTRUCT_PREFETCHW_VERSION).bsbit
PK_CACHE_INSTRUCT_PREFETCHW_OUT         :=
PK_CACHE_INSTRUCT_PREFETCHW_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_INSTRUCT_PREFETCHW_TAR         := $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_TAR)
PK_CACHE_INSTRUCT_PREFETCHW_PACKAGE     :=
PK_CACHE_INSTRUCT_PREFETCHW_CONFIG      := $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_CONFIG)
SUB_TARGET                  += $(PK_CACHE_INSTRUCT_PREFETCHW_DIR)/build

PK_CACHE_INSTRUCT_PREFETCHW_sh = $(CONFIG_SHELL) $(PK_CACHE_INSTRUCT_PREFETCHW_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_INSTRUCT_PREFETCHW_NAME)X \
                 $(PK_CACHE_INSTRUCT_PREFETCHW_VERSION)X \
                 $(PK_CACHE_INSTRUCT_PREFETCHW_SITE)X \
                 $(PK_CACHE_INSTRUCT_PREFETCHW_GITHUB)X \
                 $(PK_CACHE_INSTRUCT_PREFETCHW_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_INSTRUCT_PREFETCHW_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_INSTRUCT_PREFETCHW_SUBNAME)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_CONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_LIBPATH)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_CFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_ASFLAGS)X \
                 $(PK_CACHE_INSTRUCT_PREFETCHW_BSBIT)X \
                 $(PK_CACHE_INSTRUCT_PREFETCHW_BUILDARCH)X \
		 $(PK_CACHE_INSTRUCT_PREFETCHW_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_SRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_PREFETCHW_CSRCFILE)

$(PK_CACHE_INSTRUCT_PREFETCHW_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_INSTRUCT_PREFETCHW_sh,$(srctree))
