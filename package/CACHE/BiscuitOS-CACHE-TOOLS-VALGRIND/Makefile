################################################
#
# BiscuitOS-CACHE-TOOLS-VALGRIND
#
################################################
PK_CACHE_TOOLS_VALGRIND_NAME        := BiscuitOS-CACHE-TOOLS-VALGRIND
PK_CACHE_TOOLS_VALGRIND_VERSION     := $(CONFIG_PK_CACHE_TOOLS_VALGRIND_VERSION)
PK_CACHE_TOOLS_VALGRIND_SITE        := $(CONFIG_PK_CACHE_TOOLS_VALGRIND_URL)
PK_CACHE_TOOLS_VALGRIND_GITHUB      := $(CONFIG_PK_CACHE_TOOLS_VALGRIND_GITHUB)
PK_CACHE_TOOLS_VALGRIND_SUBNAME     :=
PK_CACHE_TOOLS_VALGRIND_BUILDARCH   := $(CONFIG_PK_CACHE_TOOLS_VALGRIND_BUILDARCH)
PK_CACHE_TOOLS_VALGRIND_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-TOOLS-VALGRIND
PK_CACHE_TOOLS_VALGRIND_PATCH       := $(PK_CACHE_TOOLS_VALGRIND_DIR)/patch
PK_CACHE_TOOLS_VALGRIND_BSBIT       := BiscuitOS-CACHE-TOOLS-VALGRIND-$(PK_CACHE_TOOLS_VALGRIND_VERSION).bsbit
PK_CACHE_TOOLS_VALGRIND_OUT         :=
PK_CACHE_TOOLS_VALGRIND_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_CACHE_TOOLS_VALGRIND_TAR         := $(CONFIG_PK_CACHE_TOOLS_VALGRIND_TAR)
PK_CACHE_TOOLS_VALGRIND_PACKAGE     :=
PK_CACHE_TOOLS_VALGRIND_CONFIG      := $(CONFIG_PK_CACHE_TOOLS_VALGRIND_CONFIG)
SUB_TARGET                  += $(PK_CACHE_TOOLS_VALGRIND_DIR)/build

PK_CACHE_TOOLS_VALGRIND_sh = $(CONFIG_SHELL) $(PK_CACHE_TOOLS_VALGRIND_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_TOOLS_VALGRIND_NAME)X \
                 $(PK_CACHE_TOOLS_VALGRIND_VERSION)X \
                 $(PK_CACHE_TOOLS_VALGRIND_SITE)X \
                 $(PK_CACHE_TOOLS_VALGRIND_GITHUB)X \
                 $(PK_CACHE_TOOLS_VALGRIND_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_TOOLS_VALGRIND_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_TOOLS_VALGRIND_SUBNAME)X \
                 $(CONFIG_PK_CACHE_TOOLS_VALGRIND_CONFIG)X \
                 $(CONFIG_PK_CACHE_TOOLS_VALGRIND_LIBPATH)X \
                 $(CONFIG_PK_CACHE_TOOLS_VALGRIND_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_VALGRIND_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_TOOLS_VALGRIND_CFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_VALGRIND_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_VALGRIND_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_TOOLS_VALGRIND_ASFLAGS)X \
                 $(PK_CACHE_TOOLS_VALGRIND_BSBIT)X \
                 $(PK_CACHE_TOOLS_VALGRIND_BUILDARCH)X \
		 $(PK_CACHE_TOOLS_VALGRIND_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_TOOLS_VALGRIND_SRCFILE)X \
                 $(CONFIG_PK_CACHE_TOOLS_VALGRIND_CSRCFILE)

$(PK_CACHE_TOOLS_VALGRIND_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_TOOLS_VALGRIND_sh,$(srctree))
