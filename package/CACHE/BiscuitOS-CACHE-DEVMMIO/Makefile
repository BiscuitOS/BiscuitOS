################################################
#
# BiscuitOS-CACHE-DEVMMIO
#
################################################
PK_CACHE_DEVMMIO_NAME        := BiscuitOS-CACHE-DEVMMIO
PK_CACHE_DEVMMIO_VERSION     := $(CONFIG_PK_CACHE_DEVMMIO_VERSION)
PK_CACHE_DEVMMIO_SITE        := $(CONFIG_PK_CACHE_DEVMMIO_URL)
PK_CACHE_DEVMMIO_GITHUB      := $(CONFIG_PK_CACHE_DEVMMIO_GITHUB)
PK_CACHE_DEVMMIO_SUBNAME     :=
PK_CACHE_DEVMMIO_BUILDARCH   := $(CONFIG_PK_CACHE_DEVMMIO_BUILDARCH)
PK_CACHE_DEVMMIO_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-DEVMMIO
PK_CACHE_DEVMMIO_PATCH       := $(PK_CACHE_DEVMMIO_DIR)/patch
PK_CACHE_DEVMMIO_BSBIT       := BiscuitOS-CACHE-DEVMMIO-$(PK_CACHE_DEVMMIO_VERSION).bsbit
PK_CACHE_DEVMMIO_OUT         :=
PK_CACHE_DEVMMIO_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_DEVMMIO_TAR         := $(CONFIG_PK_CACHE_DEVMMIO_TAR)
PK_CACHE_DEVMMIO_PACKAGE     :=
PK_CACHE_DEVMMIO_CONFIG      := $(CONFIG_PK_CACHE_DEVMMIO_CONFIG)
SUB_TARGET                  += $(PK_CACHE_DEVMMIO_DIR)/build

PK_CACHE_DEVMMIO_sh = $(CONFIG_SHELL) $(PK_CACHE_DEVMMIO_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_DEVMMIO_NAME)X \
                 $(PK_CACHE_DEVMMIO_VERSION)X \
                 $(PK_CACHE_DEVMMIO_SITE)X \
                 $(PK_CACHE_DEVMMIO_GITHUB)X \
                 $(PK_CACHE_DEVMMIO_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_DEVMMIO_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_DEVMMIO_SUBNAME)X \
                 $(CONFIG_PK_CACHE_DEVMMIO_CONFIG)X \
                 $(CONFIG_PK_CACHE_DEVMMIO_LIBPATH)X \
                 $(CONFIG_PK_CACHE_DEVMMIO_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_DEVMMIO_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_DEVMMIO_CFLAGS)X \
                 $(CONFIG_PK_CACHE_DEVMMIO_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_DEVMMIO_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_DEVMMIO_ASFLAGS)X \
                 $(PK_CACHE_DEVMMIO_BSBIT)X \
                 $(PK_CACHE_DEVMMIO_BUILDARCH)X \
		 $(PK_CACHE_DEVMMIO_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_DEVMMIO_SRCFILE)X \
                 $(CONFIG_PK_CACHE_DEVMMIO_CSRCFILE)

$(PK_CACHE_DEVMMIO_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_DEVMMIO_sh,$(srctree))
