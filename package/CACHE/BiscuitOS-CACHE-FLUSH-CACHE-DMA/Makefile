################################################
#
# BiscuitOS-CACHE-FLUSH-CACHE-DMA
#
################################################
PK_CACHE_FLUSH_CACHE_DMA_NAME        := BiscuitOS-CACHE-FLUSH-CACHE-DMA
PK_CACHE_FLUSH_CACHE_DMA_VERSION     := $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_VERSION)
PK_CACHE_FLUSH_CACHE_DMA_SITE        := $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_URL)
PK_CACHE_FLUSH_CACHE_DMA_GITHUB      := $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_GITHUB)
PK_CACHE_FLUSH_CACHE_DMA_SUBNAME     :=
PK_CACHE_FLUSH_CACHE_DMA_BUILDARCH   := $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_BUILDARCH)
PK_CACHE_FLUSH_CACHE_DMA_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-FLUSH-CACHE-DMA
PK_CACHE_FLUSH_CACHE_DMA_PATCH       := $(PK_CACHE_FLUSH_CACHE_DMA_DIR)/patch
PK_CACHE_FLUSH_CACHE_DMA_BSBIT       := BiscuitOS-CACHE-FLUSH-CACHE-DMA-$(PK_CACHE_FLUSH_CACHE_DMA_VERSION).bsbit
PK_CACHE_FLUSH_CACHE_DMA_OUT         :=
PK_CACHE_FLUSH_CACHE_DMA_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_FLUSH_CACHE_DMA_TAR         := $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_TAR)
PK_CACHE_FLUSH_CACHE_DMA_PACKAGE     :=
PK_CACHE_FLUSH_CACHE_DMA_CONFIG      := $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_CONFIG)
SUB_TARGET                  += $(PK_CACHE_FLUSH_CACHE_DMA_DIR)/build

PK_CACHE_FLUSH_CACHE_DMA_sh = $(CONFIG_SHELL) $(PK_CACHE_FLUSH_CACHE_DMA_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_FLUSH_CACHE_DMA_NAME)X \
                 $(PK_CACHE_FLUSH_CACHE_DMA_VERSION)X \
                 $(PK_CACHE_FLUSH_CACHE_DMA_SITE)X \
                 $(PK_CACHE_FLUSH_CACHE_DMA_GITHUB)X \
                 $(PK_CACHE_FLUSH_CACHE_DMA_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_FLUSH_CACHE_DMA_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_FLUSH_CACHE_DMA_SUBNAME)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_CONFIG)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_LIBPATH)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_CFLAGS)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_ASFLAGS)X \
                 $(PK_CACHE_FLUSH_CACHE_DMA_BSBIT)X \
                 $(PK_CACHE_FLUSH_CACHE_DMA_BUILDARCH)X \
		 $(PK_CACHE_FLUSH_CACHE_DMA_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_SRCFILE)X \
                 $(CONFIG_PK_CACHE_FLUSH_CACHE_DMA_CSRCFILE)

$(PK_CACHE_FLUSH_CACHE_DMA_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_FLUSH_CACHE_DMA_sh,$(srctree))
