PK_CACHE_INSTRUCT_MOVNTI_NAME        := BiscuitOS-CACHE-INSTRUCT-MOVNTI
PK_CACHE_INSTRUCT_MOVNTI_VERSION     := $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_VERSION)
PK_CACHE_INSTRUCT_MOVNTI_SITE        := $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_URL)
PK_CACHE_INSTRUCT_MOVNTI_GITHUB      := $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_GITHUB)
PK_CACHE_INSTRUCT_MOVNTI_SUBNAME     :=
PK_CACHE_INSTRUCT_MOVNTI_BUILDARCH   := $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_BUILDARCH)
PK_CACHE_INSTRUCT_MOVNTI_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-INSTRUCT-MOVNTI
PK_CACHE_INSTRUCT_MOVNTI_PATCH       := $(PK_CACHE_INSTRUCT_MOVNTI_DIR)/patch
PK_CACHE_INSTRUCT_MOVNTI_BSBIT       := BiscuitOS-CACHE-INSTRUCT-MOVNTI-$(PK_CACHE_INSTRUCT_MOVNTI_VERSION).bsbit
PK_CACHE_INSTRUCT_MOVNTI_OUT         :=
PK_CACHE_INSTRUCT_MOVNTI_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_INSTRUCT_MOVNTI_TAR         := $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_TAR)
PK_CACHE_INSTRUCT_MOVNTI_PACKAGE     :=
PK_CACHE_INSTRUCT_MOVNTI_CONFIG      := $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_CONFIG)
SUB_TARGET                  += $(PK_CACHE_INSTRUCT_MOVNTI_DIR)/build

PK_CACHE_INSTRUCT_MOVNTI_sh = $(CONFIG_SHELL) $(PK_CACHE_INSTRUCT_MOVNTI_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_INSTRUCT_MOVNTI_NAME)X \
                 $(PK_CACHE_INSTRUCT_MOVNTI_VERSION)X \
                 $(PK_CACHE_INSTRUCT_MOVNTI_SITE)X \
                 $(PK_CACHE_INSTRUCT_MOVNTI_GITHUB)X \
                 $(PK_CACHE_INSTRUCT_MOVNTI_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_INSTRUCT_MOVNTI_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_INSTRUCT_MOVNTI_SUBNAME)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_CONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_LIBPATH)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_CFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_ASFLAGS)X \
                 $(PK_CACHE_INSTRUCT_MOVNTI_BSBIT)X \
                 $(PK_CACHE_INSTRUCT_MOVNTI_BUILDARCH)X \
		 $(PK_CACHE_INSTRUCT_MOVNTI_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_SRCFILE)X \
                 $(CONFIG_PK_CACHE_INSTRUCT_MOVNTI_CSRCFILE)

$(PK_CACHE_INSTRUCT_MOVNTI_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_INSTRUCT_MOVNTI_sh,$(srctree))
