################################################
#
# BiscuitOS-CACHE-SCENE-COPY-USER
#
################################################
PK_CACHE_SCENE_COPY_USER_NAME        := BiscuitOS-CACHE-SCENE-COPY-USER
PK_CACHE_SCENE_COPY_USER_VERSION     := $(CONFIG_PK_CACHE_SCENE_COPY_USER_VERSION)
PK_CACHE_SCENE_COPY_USER_SITE        := $(CONFIG_PK_CACHE_SCENE_COPY_USER_URL)
PK_CACHE_SCENE_COPY_USER_GITHUB      := $(CONFIG_PK_CACHE_SCENE_COPY_USER_GITHUB)
PK_CACHE_SCENE_COPY_USER_SUBNAME     :=
PK_CACHE_SCENE_COPY_USER_BUILDARCH   := $(CONFIG_PK_CACHE_SCENE_COPY_USER_BUILDARCH)
PK_CACHE_SCENE_COPY_USER_DIR         := $(srctree)/package/CACHE/BiscuitOS-CACHE-SCENE-COPY-USER
PK_CACHE_SCENE_COPY_USER_PATCH       := $(PK_CACHE_SCENE_COPY_USER_DIR)/patch
PK_CACHE_SCENE_COPY_USER_BSBIT       := BiscuitOS-CACHE-SCENE-COPY-USER-$(PK_CACHE_SCENE_COPY_USER_VERSION).bsbit
PK_CACHE_SCENE_COPY_USER_OUT         :=
PK_CACHE_SCENE_COPY_USER_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_CACHE_SCENE_COPY_USER_TAR         := $(CONFIG_PK_CACHE_SCENE_COPY_USER_TAR)
PK_CACHE_SCENE_COPY_USER_PACKAGE     :=
PK_CACHE_SCENE_COPY_USER_CONFIG      := $(CONFIG_PK_CACHE_SCENE_COPY_USER_CONFIG)
SUB_TARGET                  += $(PK_CACHE_SCENE_COPY_USER_DIR)/build

PK_CACHE_SCENE_COPY_USER_sh = $(CONFIG_SHELL) $(PK_CACHE_SCENE_COPY_USER_SCRIPTS) \
                 $1X \
                 $(PK_CACHE_SCENE_COPY_USER_NAME)X \
                 $(PK_CACHE_SCENE_COPY_USER_VERSION)X \
                 $(PK_CACHE_SCENE_COPY_USER_SITE)X \
                 $(PK_CACHE_SCENE_COPY_USER_GITHUB)X \
                 $(PK_CACHE_SCENE_COPY_USER_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_CACHE_SCENE_COPY_USER_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_CACHE_SCENE_COPY_USER_SUBNAME)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_CONFIG)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_LIBPATH)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_CPPFLAGS)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_DPKCONFIG)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_CFLAGS)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_LDFLAGS)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_CXXFLAGS)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_ASFLAGS)X \
                 $(PK_CACHE_SCENE_COPY_USER_BSBIT)X \
                 $(PK_CACHE_SCENE_COPY_USER_BUILDARCH)X \
		 $(PK_CACHE_SCENE_COPY_USER_CSRCFILE)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_SRCFILE)X \
                 $(CONFIG_PK_CACHE_SCENE_COPY_USER_CSRCFILE)

$(PK_CACHE_SCENE_COPY_USER_DIR)/build: FORCE
	$(Q)$(call PK_CACHE_SCENE_COPY_USER_sh,$(srctree))
